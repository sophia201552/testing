    <style id="cssMain">
        html {
            height: 100%;
            font-family: Helvetica,"Microsoft Yahei";
        }
        body {
            height:100%;
            overflow: hidden;
            position: relative;
            font-family: Helvetica,"Microsoft Yahei";
        }
        #indexMain {
            height: 100%;
            width: 100%;
            position: relative;
            overflow: auto;
        }
        #outerContainer {
            height: 100%;
            position: relative;
            overflow: hidden;
            -webkit-overflow-scrolling:touch;
        }
        #innerContainer {
            height: 100%;
            overflow-y: auto;
            overflow-x: hidden;
        }
        #divAlert {
            position: fixed;
            top: 40%;
            text-align: center;
            width: 80%;
            left: 10%;
            z-index: 100;
        }
        #topBlank,#bottomBlank{
            position: relative;
        }
        #navStatus {
            background-color: #061624;
        }
        #navTop {
            width: 100%;
            height: 0;
            display: none;
            position: absolute;
            z-index: 3;
            padding: 0 0.75rem;
            /*background-color: rgba(132, 185, 244, 0.85);*/
			background-color: #22252E;
            color:white;
            overflow: hidden;
        }
        #navTop.active{
            display:flex;
            display:-webkit-flex;
            align-items: center;
            -webkit-align-items: center;
        }
        #navBottom {
            /*background-color: #272D45;*/
            width: 100%;
            height: 4.9rem;
            display: none;
            position: absolute;
            bottom: 0;
            z-index: 1000;
            background-color: #fbfbf9;
            border-top: 1px solid #eee;
            padding: 0.2rem;
            margin: 0;
        }
        #navBottom.active {
            display:flex;
        }
        .top-nav #navTop {
            display: flex;
            display: -webkit-flex;
        }
        .bottom-nav #navBottom {
            display: block;
        }
        .topNavLeft{
            color: white;
            font-size: 14px;
            position: relative;
            z-index: 1;
            height: 100%;
            display: inline-flex;
            display: -webkit-inline-flex;
            align-items: center;
            -webkit-align-items: center;
        }
        .noBtnBack .topNavLeft{
            display: inline-flex;
            display: -webkit-inline-flex;
        }
        .noBtnBack #btnBack{
            display: none;
        }
        .topNavTitle{
            color: white;
            font-size: 18px;
            font-weight: 500;
            width: 100%;
            height: 100%;
            display: inline-flex;
            display: -webkit-inline-flex;
            align-items: center;
            -webkit-align-items: center;
            justify-content: center;
            -webkit-justify-content: center;
            position: absolute;
            left: 0;
            top: 0;
        }
        .topNavInput{
            position: absolute;
            width: 100%;
            text-align: center;
            height: 100%;
            left: 0;
            display: inline-flex;
            display: -webkit-inline-flex;
            align-items: center;
            -webkit-align-items: center;
            justify-content: center;
            -webkit-justify-content: center;
        }
        .topNavInput input {
            width: 50%;
            border: none;
            border-radius: 0.2rem;
            padding-left: 0.5rem;
            position: relative;
        }
        .topNavInput input:focus {
            outline: none;
        }
        .topNavRight {
            color: white;
            height: 100%;
            z-index: 1;
            position: absolute;
            display: inline-flex;
            display: -webkit-inline-flex;
            align-items: center;
            -webkit-align-items: center;
            right: 15px;
        }
        .topTool .btnIcon {
            background-image: url('static/app/dashboard/img/icon.png');
            background-size: auto 100%;
            background-repeat: no-repeat;
            background-position-y: 0;
            height: 24px;
            width: 24px;
            display: inline-block;
        }
        #btnBack .btnIcon{
            background: none;
        }
        .topNavLeft .topTip{
            display: inline-block;
            position: relative;
            max-width: 7.5rem;
            margin-left: 6px;
        }
        .bottomTool  {
            color: #ccc;
            position: relative;
            display: inline-block;
            height: 100%;
            text-align: center;
        }
        .bottomTool .btnTitle{
            position: relative;
            top: 0.2rem;
            font-size: 0.8rem;
            display: block;
            white-space: nowrap;
            color:#646464
        }
        .bottomTool.selected .btnTitle{
            color:#36a0ff
        }
        .bottomTool .btnIcon{
            height: 24px;
            width: 24px;
            display: inline-block;
            position: relative;
            top: 5px;
        }
        .bottomTool.selected .on{
            display: inline-block;
        }
        .bottomTool.selected .off{
            display: none;
        }
        .bottomTool:not(.selected) .on{
            display: none;
        }
        .bottomTool .on path{
            stroke: #36a0ff;
            fill: #36a0ff;
        }
        .bottomTool .off path{
            stroke: #7a7e83;
            fill: #7a7e83;
        }

        .panel-title{
            font-size: 1.4rem;
        }
        #reportSelect button, #reportSelect button:focus, #reportSelect button:active{
            background: none;
            border: none;
            color: #fff;
            outline-color: transparent;
        }
        #reportSelect .dropdown-menu{
            left: 50%;
            margin-left: -80px;
            background: rgba(10, 22, 36,0.9);
            top: 95%;
        }
        #reportSelect .dropdown-menu li{
            padding: .2rem .8rem;
            line-height: 2rem;
        }
        #btnLogin .spinner {
            margin-top: -10px;
            width: 50px;
            height: 40px;
            text-align: center;
            font-size: 10px;
            display: none;
        }

        .spinner > div {
            background-color: rgb(217, 104, 49);
            height: 100%;
            width: 6px;
            display: inline-block;
            animation: sk-stretchdelay 1.2s infinite ease-in-out;
            -webkit-animation: sk-stretchdelay 1.2s infinite ease-in-out;
        }

        .spinner .rect2 {
            animation-delay: -1.1s;
            -webkit-animation-delay: -1.1s;
        }

        .spinner .rect3 {
            animation-delay: -1.0s;
            -webkit-animation-delay: -1.0s;
        }

        .spinner .rect4 {
            animation-delay: -0.9s;
            -webkit-animation-delay: -0.9s;
        }

        .spinner .rect5 {
            animation-delay: -0.8s;
            -webkit-animation-delay: -0.8s;
        }

        @keyframes sk-stretchdelay {
            0%, 40%, 100% {
                transform: scaleY(0.4);
                -webkit-transform: scaleY(0.4);
            }

            20% {
                transform: scaleY(1.0);
                -webkit-transform: scaleY(1.0);
            }
        }
        @-webkit-keyframes sk-stretchdelay {
            0%, 40%, 100% {
                transform: scaleY(0.4);
                -webkit-transform: scaleY(0.4);
            }

            20% {
                transform: scaleY(1.0);
                -webkit-transform: scaleY(1.0);
            }
        }
        #divSpinner {
            position: fixed;
            height: 100%;
            width: 100%;
            top: 0;
            display: none;
            z-index: 200;
        }
        #divModal {
            height:100%
        }
        #divModal .close{
            width: 3rem;
            height: 3rem;
            position: relative;
            top: -0.4rem;
        }
        #divModal .modal-dialog {
            height: 70%;
            top: 15%;
            margin-left:0;
            margin-right:0;
        }
        #divModal .modal-content {
            height: 100%;
            overflow: hidden;
        }
        #divModal .modal-body {
            height: calc(100% - 121px);
            height: -webkit-calc(100% - 121px);
            overflow-x: hidden;
            -webkit-overflow-scrolling:touch;
        }
        #divModal .modal-footer {
            position: relative;
            text-align: right;
            height: 6.6rem;
        }
        .title {
            color:#323232
        }
        .text{
            color:#646464
        }
        .additionText {
            color:#909090
        }
        .infoBox {
            width:95%;
            margin-left:2.5%;
        }
        </style>
    </head>
