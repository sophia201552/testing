<style>
    .MdWrap {
        width: 100%;
        height: 100%;
        background: rgb(237, 240, 249);
        padding: 10px 20px;
    }

    .MDLeft {
        width: 40%;
        height: 100%;
        float: left;
        padding-right: 10px;
    }

    .MDRight {
        width: 60%;
        height: 100%;
        float: left;
    }

    .MDBody {
        width: 100%;
        height: 100%;
        background: rgba(255, 255, 255, 0.90);
        box-shadow: 0 0 6px 2px rgba(193, 205, 223, 0.40);
        border-radius: 4px;
        padding: 10px 15px;
    }

    .MDParamWrap {
        width: 100%;
        height: 70%;
        position:relative;
    }

    .MDUsingWrap {
        width: 100%;
        height: 30%;
        position: relative;
    }

    .MDParamHeader {
        height: 30px;
        position: relative;
    }

    .MDParamCtn {
        width: 100%;
        height: calc(100% - 30px);
        padding: 10px 0;
    }

    .MDUsingHeader {
        height: 30px;
    }

    .MDUsingCtn {
        height: calc(100% - 30px);
        position: relative;
    }

    .MDHeaderSpan {
        height: 30px;
        display: inline-block;
        line-height: 30px;
        font-size: 16px;
        color: #25272B;
    }

    .statisTime {
        position: absolute;
        right: 0px;
        top: 0px;
    }

    #queryTimIpt {
        display: inline-block;
        border: none;
        padding: 3px;
        border: none;
        box-shadow: none;
        position: relative;
        top: 1px;
        width: 70px;
        height: 30px;
    }

    .MDParamCtnTd {
        width: 25%;
        float: left;
        height: 100%;
        border-top: 1px solid #C7D2E4;
        border-left: 1px solid #C7D2E4;
        /* border: 1px solid #C7D2E4; */
        text-align: center;
        vertical-align: middle;
        display: inline-block;
    }
    .MDParamCtnTd:last-child{
        border-right: 1px solid #C7D2E4;
    }
    .MDParamCtnRow:last-child .MDParamCtnTd{
        border-bottom: 1px solid #C7D2E4;
    }
    .MDParamCtnRow {
        width: 100%;
        height: calc(100%/6);
    }

    .MDParamCtnTdRight {
        position: relative;
    }

    .MDParamCtnTdRight span {
        top: 25%;
        transform: translateY(-50%);
        text-align: right;
        right: -13%;
    }

    .MDParamCtnSpan {
        display: inline-block;
        position: relative;
        top: 50%;   
        transform: translateY(-50%);
        width: 100%;
        color: #354052;
    }
    .MDParamCtnSpanColor{
        color: #25272B;
    }

    .MDParamCtnTdLine {
        position: absolute;
        width: 100%;
        height: 1px;
        background: #E0E4EA;
        transform: rotate(32deg);
        top: 50%;
    }
    @media (max-width: 1366px) {
        .MDParamCtnSpan {
        font-size: 10px;
    }
    }
    .MDParamCtnSpanColor1 {
        color: rgb(84, 138, 225)
    }

    .MDParamCtnRow:nth-child(2n-1) .MDParamCtnTd {
        background: #F2F5F8;
    }

    .MDParamCtnTable {
        width: 100%;
        height: 100%;
    }

    .MDEchartWrap {
        width: 100%;
        height: 33.33%;
        padding-bottom: 10px;
    }

    .MDEchartWrap:last-child {
        padding-bottom: 0px;
    }

    .MDEchartBody {
        width: 100%;
        height: 100%;
        background: rgba(255, 255, 255, 0.90);
        box-shadow: 0 0 6px 2px rgba(193, 205, 223, 0.40);
        border-radius: 4px;
        padding: 10px;
        position: relative;
    }

    .MDEchartCtn {
        width: 100%;
        height: calc(100% - 30px);
        position: relative;
    }
    #queryDailyTimIpt,#queryMonthlyTimIpt,#queryYearTimIpt{
        display: inline-block;
        border: none;
        padding: 3px;
        border: none;
        box-shadow: none;
        position: relative;
        top: 1px;
        width: 70px;
        height: 30px;
    }
    #queryDailyTimIpt{
        width: 90px;
    }
    .btnQuery{
        padding: 4px 12px;
    }
    .MDParamCtnSpanNextDemand{
        padding-right: 5px;
    }
    .MDParamCtnTd.disableEdit .iconfont{
        display:none;
    }
</style>

<div class="MdWrap">
