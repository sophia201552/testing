const UNDEF = undefined;

const keyMirror = obj => {
  let result = {};
  if (!obj || typeof obj !== 'object' || Array.isArray(obj)) {
    throw new Error("keyMirror: 'obj' must be an pure object.");
  }
  Object.keys(obj).forEach(key => {
    if (obj.hasOwnProperty(key)) {
      result[key] = key;
    }
  });
  return result;
};

