.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.dataMonitoringPanel {
  width: 100%;
  height: 100%;
  padding: 16px;
  overflow: hidden;
}
.checkGroupWrap {
  background-color: #fff;
  padding: 16px;
}
.tableWrap {
  width: 100%;
  background-color: #fff;
  padding: 16px;
  position: relative;
}
.pageWrap {
  width: 100%;
  height: 44px;
  padding: 8px 16px;
  background-color: #fff;
}
.tableWrap .toolbar {
  width: 100%;
  height: 48px;
  background: #f0f0f0;
  position: relative;
}
.tableWrap .toolbar .toolbarL {
  float: left;
  width: 20%;
  height: 100%;
  padding: 16px 0 16px 16px;
}
.tableWrap .toolbar .toolbarC {
  float: left;
  width: 45%;
  height: 100%;
  padding: 16px 0;
}
.tableWrap .toolbar .toolbarR {
  float: left;
  width: 35%;
  height: 100%;
  padding: 8px 16px 8px 0;
}
.tableWrap .toolbar .toolbarL .item {
  float: left;
  margin-right: 8px;
}
.tableWrap .toolbar .toolbarC .item {
  float: left;
  margin-right: 8px;
}
.tableWrap .toolbar .toolbarC .item.disabled span,
.tableWrap .toolbar .toolbarC .item.disabled i {
  cursor: not-allowed;
  opacity: 0.6;
}
.tableWrap .toolbar .toolbarC .item i {
  color: rgb(51, 51, 51);
  vertical-align: middle;
}
.tableWrap .toolbar .toolbarC .item span {
  color: rgb(51, 51, 51);
  margin-top: 0px;
  margin-right: 4px;
  margin-bottom: 0px;
  margin-left: 4px;
  font-size: 14px;
  cursor: pointer;
  font-weight: 400;
  user-select: none;
}
.tableWrap .toolbar .toolbarC .item:not(.disabled):hover span,
.tableWrap .toolbar .toolbarC .item:not(.disabled):hover i {
  color: rgb(0, 0, 0);
}

.rulePanel :global(.ms-Panel-content) {
  height: 100%;
}
/* checkbox 样式 */
.item :global(.is-checked) i {
  background: #4c83ff;
  border-color: #4c83ff;
}
.item > button:hover > label > div {
  border-color: rgb(166, 166, 166) !important;
}
.item > button > label > div {
  width: 14px;
  height: 14px;
  opacity: 0.8;
  border-radius: 2px;
  border: solid 1px rgb(166, 166, 166);
}
.item > button > span {
  color: #555 !important;
  font-size: 12px !important;
}
.item .arrowWrap {
  position: absolute;
  right: 0;
  top: 2px;
}
/* 表格样式 */
.tableWrap .content {
  width: 100%;
  height: calc(100% - 48px);
  position: relative;
}
.tableWrap .content :global(.react-grid-Header) {
  background-color: #7da2f5;
}
.tableWrap .content :global(.react-grid-Header) div {
  background: transparent;
}
.tableWrap .content :global(.react-grid-Header) div {
  background: transparent;
  text-align: center;
  color: #fff;
}
.tableWrap
  .content
  :global(.react-grid-checkbox:checked)
  + :global(.react-grid-checkbox-label:before) {
  background: #4c83ff;
  box-shadow: inset 0 0 0 5px #fff;
}
.tableWrap
  .content
  :global(.radio-custom)
  + :global(.radio-custom-label:before),
.tableWrap
  .content
  :global(.react-grid-checkbox)
  + :global(.react-grid-checkbox-label:before) {
  border-radius: 2px;
  border: none;
}
.tableWrap
  .content
  :global(.react-grid-checkbox:focus)
  + :global(.react-grid-checkbox-label) {
  outline: none;
}
.tableWrap .content :global(.react-grid-Canvas) {
  overflow-y: auto !important;
}
.tableWrap .content :global(.react-grid-checkbox-label) {
  border: 1px solid #bdd1ff;
  border-radius: 2px;
}

.tableWrap .content :global(.widget-HeaderCell__value) {
  line-height: 28px;
}
.right .contentWrap .rulePanel :global(.ms-Panel-content) {
  height: 100%;
}

.tableWrap .content :global(.react-grid-Canvas) {
  overflow-y: auto !important;
}
.tableWrap .content :global(.react-grid-checkbox-label) {
  border: 1px solid #bdd1ff;
  border-radius: 2px;
}
.tableWrap .content :global(.react-grid-Row:hover) :global(.react-grid-Cell) {
  background-color: rgba(125, 162, 245, 0.15);
}

.tableWrap .content :global(.react-grid-Row) :global(.react-grid-Cell) {
  border-color: #bdd1ff;
  color: #7f7f7f;
}
.originalPointNameWrap {
  width: 100%;
  position: relative;
  padding-right: 14px;
}
.originalPointNameWrap .btnWrap {
  width: 14px;
  position: absolute;
  right: 0;
  top: 0;
  cursor: pointer;
}
/* tag */
.tagUl {
  padding-top: 4px;
  width: 100%;
  height: 100%;
  overflow-y: auto;
}
.tagUl .tagLi {
  user-select: none;
  height: 12px;
  line-height: 12px;
  border-radius: 6px;
  background-color: rgba(99, 207, 65, 0.1);
  color: #63cf41;
  cursor: default;
  float: left;
  margin-right: 4px;
  max-width: 99%;
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  transition: padding 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  padding: 0 10px 0 10px;
  text-align: center;
  margin-bottom: 4px;
}
