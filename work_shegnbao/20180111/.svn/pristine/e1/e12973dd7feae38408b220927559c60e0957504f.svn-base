#taskModalWrap{
    width:100%;
    height:500px;
    position:absolute;
    bottom:-500px;
    left:0;
    background: rgb(40,41,46);
    z-index: 1049;
}
#taskModalWrap .taskHead{
    height: 39px;
    padding: 0 28px;
    border-bottom: 1px solid rgb(25,26,31);
}
#taskModalWrap .taskHead>div{
    float: left;
    height: 14px;
    line-height: 14px;
}
#taskModalWrap .taskHead .title{
    width:30%;
    color:#fff;
    border-left: 2px solid #fff;
    text-indent: 8px;
    margin-top: 12px;
}
#taskModalWrap .taskHead .tabWrap{
    width:40%;
    margin-top: 12px;
}
#taskModalWrap .taskHead .btnWrap{
    width:auto;
    height: auto;
    float: right;
}
#taskModalWrap .taskHead .btnWrap button{
    width: 90px;
    height: 24px;
    line-height: 12px;
    margin-top: 7px;
    margin-left: 8px;
}
#taskModalWrap .taskHead .btnWrap .actionModalClose{
    color: #aaaab2;
    background-color: transparent;
    border-color: rgb(88,89,94);
}
#taskModalWrap .taskHead .btnWrap .actionModalSubmit{
    background-color: rgb(0, 166, 206);
    /* border-color: #aaaab2; */
}
#taskModalWrap #taskInfoSendWO{
    background-color: rgb(0, 166, 206);
}
#taskModalWrap .taskHead .btnWrap::after{
    content: '';
    display: block;
    height: 0;
    clear: both;
}
#taskModalWrap .taskHead .tabWrap #taskTab{
    border: none;
}
#taskModalWrap .taskHead .tabWrap li{
    width: 50%;
}
#taskModalWrap .taskHead .tabWrap li a{
    float: left;
    width: auto;
    padding: 0;
    margin: 0 0 0 12px;
    border: 0;
    height: 14px;
    line-height: 14px;
    color: rgb(88,89,94);
    font-weight: 400;
}
#taskModalWrap .taskHead .tabWrap li a:hover{
    background: transparent;
    border: none;
    color: rgb(0, 166, 206);
}
#taskModalWrap .taskHead .tabWrap li:first-of-type a{
    float: right;
    margin: 0 12px 0 0;
}
#taskModalWrap .taskHead .tabWrap .active a{
    color: rgb(0, 166, 206);
    background-color: transparent;
    border: none;
}
#taskModalWrap .taskHead .tabWrap .active a::after{
    content: '';
    display: block;
    border-bottom: 1px solid;
    position: absolute;
    width: fill-available;
    width: -webkit-fill-available;
    left: 0;
    top: 26px;
}

#taskModalWrap .taskContent{
    height: calc(100% - 39px);
    width: 100%;
    color: #aaaab2;
}
#taskModalWrap .taskContent .form-control{
    height: 28px;
    color: #aaaab2;
    background-color: transparent;
    border-color: #aaaab2;
}
#taskModalWrap .taskContent .form-control:disabled{
    background-color: #666;
}
#taskModalWrap .taskContent select.form-control{
    padding: 0 12px;
}
#taskModalWrap .taskContent .leftContent{
    width: 50%;
    float: left;
    border-right: 1px solid rgb(28,29,34);
    height: 100%;
    padding: 8px 8px 8px 28px;
}
#taskModalWrap .taskContent .rightContent{
    width: 50%;
    float: left;
    height: 100%;
    padding: 8px 28px 8px 8px;
}
#taskModalWrap .taskContent .rightContent::after{
    content: '';
    display: block;
    height: 0;
    clear: both;
}
#taskModalWrap .taskContent .tab-pane{
    height: 100%;
}
#taskModalWrap .taskContent .commentWrap{
    height: 100%;
    width: 100%;
    overflow: auto;
}
#taskModalWrap .taskContent .commentWrap .media-left img{
    height: 40px;
    margin-right: 8px;
}
#taskModalWrap .taskContent .commentWrap .media-body>span{
    display: block;
    height: 20px;
    width: 100%;
    line-height: 20px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
#taskModalWrap .taskContent .commentWrap .media-body .nameWrap{
    width: 100%;
    margin-bottom: 8px;
}
#taskModalWrap .taskContent .commentWrap .media-body span.name{
    display: inline-block;
    color: #fff;
    font-size: 14px;
    font-weight: 400;
}
#taskModalWrap .taskContent .commentWrap .media-body span.createTime{
    display: inline-block;
    color: rgba(255,255,255,.4);
    font-size: 12px;
}
#taskModalWrap .taskContent .commentWrap .media-body span.glyphicon{
    display: inline-block;
    width: auto;
}
