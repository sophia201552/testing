/* main frame */
body,
html {
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
    -ms-content-zooming: none;
    font-size: 12px;
    vertical-align: middle;
    overflow: hidden;
    font-family:'Helvetica','Microsoft YaHei';
}
#mainframe {
    width: 100%;
    height: 100%;
}

/* layout */
.diagnosis-v2-layout {
    display: flex;
    flex-direction: column;
    flex: auto;
    overflow: hidden;
    position: relative;
}
.diagnosis-v2-layout.has-sider {
    flex-direction: row;
}
.diagnosis-v2-header {
    height: 64px;
    line-height: 64px;
}
.diagnosis-v2-content {
    position: absolute;
    top: 64px;
    bottom: 0;
    left: 0;
    right: 0;
}
#diagnosisV2Container {
    height: 100%;
}
#diagnosisV2Sider {
    flex: 0 0 350px;
}
/*工具栏按钮*/
#diagnosisV2Detail{
    background: #28292e;
}
#diagnosisV2Detail .diagnosis-v2-header{
    color: #e5e6e8;
    position: relative;
    padding: 0 15px 0 28px;
}
.diagnosis-v2-header .toolBox{
    display: flex;
    align-items: center;
    height: 100%;
}
.diagnosis-v2-header .toolBox > div{
    height: 20px;
    line-height: 20px;
    display: flex;
    align-items: center;
    cursor: pointer;
}
.diagnosis-v2-header .toolBox > div:hover > span,
.diagnosis-v2-header .toolBox > div.highLight > span {
    color: #eee;
}
.diagnosis-v2-header .toolBox span.iconfont{
    font-size: 1.6667rem;
    padding-left: 20px;
    color: #727480;
    margin-right: 4px;
}
.diagnosis-v2-header .toolBox span.toolName{
    padding-right: 20px;
    border-right: 1px solid rgba(131, 131, 137, 0.2);
    color: #838389;
}
.diagnosis-v2-header .saveRoiBtnCtn{
    position: absolute;
    top: 0;
    left: 350px;
    display: none;
}
.diagnosis-v2-header ul{
    margin: 0;
    padding: 0;
    list-style: none;
    position: absolute;
    right: 0;
    top: 0;
}
.diagnosis-v2-header ul li{
    float: left;
    padding: 0 16px;
    cursor: pointer;
    font-size: 1.1667rem;
}
.diagnosis-v2-header ul li.active{
    background: #ffffff;
    color: #111111;
}
/*faultTable 样式 start*/
#diagnosisV2PageContainer .tableCtn{
    width: 100%;
    height: 100%;
    background: #ffffff;
}
#diagnosisV2PageContainer .tableCtn.caseRecordTable{
    background: rgb(236,238,247)
}
#diagnosisV2PageContainer .tableCtn .conditionSearchCtn{
    padding: 0 15px;
}
#diagnosisV2PageContainer .conditionCtn{
    min-height: 40px;
    color: #666666;
    width: calc(100% - 130px);
    float: left;
}
#diagnosisV2PageContainer .roiTable .conditionCtn{
    min-height: 40px;
    color: #666666;
    width: calc(100% - 240px);
    float: left;
}
#diagnosisV2PageContainer .conditionCtn .singleCondition{
    float: left;
    border: 1px solid #cccccc;
    border-radius: 4px;
    margin: 0 6px;
    line-height: 26px;
    padding: 0 4px;
    margin: 4px 2px;
}
#diagnosisV2PageContainer .conditionCtn .name{
    color: #999;
}
#diagnosisV2PageContainer .conditionCtn .singleCondition .glyphicon-remove{
    cursor: pointer;
}
#diagnosisV2PageContainer .roiToolBox{
    height: 40px;
    display: flex;
    align-items: center;
    float: right;
    max-width: 225px;
    padding-right: 15px;
}
#diagnosisV2PageContainer .roiToolBox .buttonBox{
    border-right: 1px solid #ccc;
}
#diagnosisV2PageContainer .roiToolBox .buttonBox span{
    padding-right: 6px;
}
#diagnosisV2PageContainer .searchBox{
    position: relative;
    height: 40px;
    min-width: 40px;
    display: flex;
    align-items: center;
    padding: 0 0 0 6px;
}
#diagnosisV2PageContainer .roiTable .searchBox{
    padding: 0 0 0 6px;
}
#diagnosisV2PageContainer .searchBox input{
    border-radius: 4px;
    height: 26px;
    padding: 6px 24px 6px 12px;
    max-width: 130px;
}
#diagnosisV2PageContainer .searchBox span{
    position: absolute;
    top: 7px;
    right: 6px;
    line-height: 26px;
    font-size: 1.6667rem;
}
#diagnosisV2PageContainer .faultTable{
    width: 100%;
    /*max-height: calc(100% - 40px);*/
    /* height: calc(100% - 40px); */
    height: 100%;
    padding-bottom: 15px;
    clear: both;
    /*overflow-y: auto;
    overflow-x: hidden;*/
}
#diagnosisV2PageContainer .faultTable table{
    width: 100%;
    color: #333333;
}
#diagnosisV2PageContainer .faultTable .consequenceChart{
    padding: 0;
    overflow: visible;
}
#diagnosisV2PageContainer .faultTable table tbody tr:hover,
#faultModal table tbody tr:hover{
    background-color: #ebeef7;
}
#diagnosisV2PageContainer .faultTable table td,#diagnosisV2PageContainer .faultTable table th{
    padding: 8px 8px 8px 8px;
    margin-right: -23px;
    border-top: none;
}
#diagnosisV2PageContainer .faultTable table td:first-child,
#diagnosisV2PageContainer .faultTable table th:first-child{
    padding-left: 28px;
}
#diagnosisV2PageContainer .faultTable table .isSelected{
    background: url(../static/app/Diagnosis/themes/default/images/isSelected.png);
    background-repeat: no-repeat;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background-position: -22px -1px;
}
#diagnosisV2PageContainer .faultTable table tbody tr.selected,
#faultModal table tbody tr.selected{
    background-color: #00a7ce;
}
#diagnosisV2PageContainer .faultTable table tbody tr.selectedHover,
#faultModal table tbody tr.selectedHover{
    background-color: #03c0ec!important;
}
#diagnosisV2PageContainer .faultTable table .selected .isSelected{
    background-position: -2px -1px;
}
#diagnosisV2PageContainer .faultTable thead tr{
    height: 30px;
    background: #ebeef7;    
    color: #69779f;
}
#diagnosisV2PageContainer .faultTable tbody tr:nth-child(2n){
    background: #F3F5FA;
}
#diagnosisV2PageContainer .faultTable tbody tr{
    height: 40px;
    cursor: pointer;
}
#diagnosisV2PageContainer .diagnosisFaultDetailPanel{
    height: 300px;
    display: none;
}
/*faultTable 样式 end*/
/*diagnosisFaultDetailPanel start*/
#diagnosisFaultDetailPanel{
    height: 280px;
    /* background-color: #1C1E22; */
    font-size: 1rem;
    position: relative;
}
#diagnosisFaultDetailPanelContent{
    height: 100%;
    position: relative;
}
#diagnosisFaultDetailPanelContent .pTitle{
    color: #0289AA;
    margin: 0 0 4px;
}
#diagnosisFaultDetailPanelContent .equipmentInfo,
#diagnosisFaultDetailPanelContent .faultNumbers,
#diagnosisFaultDetailPanelContent .savingMoney,
#diagnosisFaultDetailPanelContent .time,
#diagnosisFaultDetailPanelContent .faultDetail{
    color: #fff;
}
#diagnosisFaultDetailPanelContent .faultDetail {
    word-wrap: break-word;
}
#faultSelect{
    width: 100%;
    background-color: #fff;
    border-radius: 4px;
    border-width: 1px;
    border-style: solid;
    padding: 6px;
}
#diagnosisFaultDetailPanelContent .divFault{
    margin: 10px 0;
}
#diagnosisFaultDetailPanelContent .divEquipment{
    margin-bottom: 10px;
}.caseRecordTable #diagnosisFaultDetailPanelContent  .divFault{
    margin: 5px 0;
    border-bottom: 1px solid #EAEFF7;
    height: 28px;
}
#diagnosisFaultDetailPanelContent .divFault1{
    margin: 10px 0;
    border-bottom: 1px solid #EAEFF7;
    height: 35px;
}
#diagnosisFaultDetailPanelContent .divFaultNew{
    margin: 0px 0;
    border-bottom: 1px solid #EAEFF7;
    height: 26px;
    clear: both;
    padding-top: 2px;
}
#diagnosisFaultDetailPanelContent .divFaultNew.divFaultBig{
    height: 40px;
}
#diagnosisFaultDetailPanelContent .divFaultNew .pTitleLeft{
    display: inline-block;
    float: left;
    width: 100px;
    padding: 3px;
    font-size: 12px;
    color: #4D8BE4;
    padding-left: 0px;
    padding-top: 4px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    margin: 0;
}
#diagnosisFaultDetailPanelContent .divFault .pTitleLeft{
    display: inline-block;
    float: left;
    width: 100px;
    padding: 6px;
    font-size: 12px;
    color: #4D8BE4;
    padding-left: 0px;
    padding-top: 0px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding-top: 2px;
    margin: 0;
}#diagnosisFaultDetailPanelContent .divFaultInfo{
    margin-bottom: 10px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    border-bottom: 1px solid #EAEFF7;
    padding-bottom: 5px;
}
#diagnosisFaultDetailPanelContent .divFaultInfo >div {
    justify-content: space-between;
}
#diagnosisFaultDetailPanelContent .divFaultInfo .divFaultNumbers{
    padding-left: 0;
}
#diagnosisFaultDetailPanelContent .divFaultInfo .divFaultNumbers .pTitle{
    line-height:20px;
}
#leftFaultDetailPanel,#rightFaultDetailPanel{
    height: 100%;
}
.caseRecordTable #rightFaultDetailPanel{
    background: #1B1E22;
    box-shadow: 0 0 6px 2px rgba(193,205,223,0.20);
}
#btnClose{
    position: absolute;
    right: 16px;
    top: 5px;
    color: #aaa;
    font-size: 1.333rem;
    display: inline-block;
    cursor: pointer;
    z-index: 1;
    background-color: rgba(170, 170, 170, 0.18);
    border-radius: 50%;
    width: 20px;
    height: 20px;
    text-align: center;
}
#btnExportPoint {
    position: absolute;
    top: 35px;
    width: 20px;
    height: 20px;
    font-size: 10px;
    right: 15px;
    color: #aaa;
    background-color: rgba(170, 170, 170, 0.18);
    text-align: center;
    border-radius: 50%;
    padding-top: 4px;
    cursor: pointer;
    z-index: 10;
}
#taskInfoCurve + #btnExportPoint {
    top: 0;
}
#diagnosisFaultDetailPanel.caseRecordDetail .divChart[data-type="history"] #btnExportPoint {
    top: 0;
    right: 0;
}
/*diagnosisFaultDetailPanel end*/
@media screen and (max-width:1400px) {
    #diagnosisV2Sider {
        flex: 0 0 250px;
    }
}

/*侧边栏*/
#diagnosisV2Sider,
#diagnosisV2Sider div{
    width: 100%;
}
#diagnosisV2Sider #timePicker,
#faultModal #timePicker{
    height: 100%;
    background: rgb(0,167,206);
    position: relative;
    font-size: 1.333rem;
    color: #444;
}
#diagnosisV2Sider #timePicker #pickerWrap,
#faultModal #timePicker #pickerWrap{
    display: inline-block;
    height: 28px;
    width: 100%;
    padding: 0 21px 0 18px;
    line-height: 28px;
}
#diagnosisV2Sider #timePicker #picker,
#faultModal #timePicker #picker{
    display: inline-block;
    font-size: 1rem;
    height: 28px;
    border: none;
    padding: 1px;
    width: calc(100% - 20px);
    line-height: 28px;
    text-align: left;
    color: #28292e;
    border-radius: 4px;
    border-color: transparent;
    outline: none;
    background: transparent;
    transition: all 0.25s;
}
#diagnosisV2Sider #timePicker #picker.active,
#faultModal #timePicker #picker.active{
    box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102, 175, 233, 0.6);
    background: #fff;
}
#diagnosisV2Sider #timePicker #picker+label,
#faultModal #timePicker #picker+label{
    width: auto;
    height: auto;
    float: right;
    font-size: 1rem;
    font-weight: normal;
    color: #28292e;
    margin-top: 7px;
    cursor: pointer;
}
#diagnosisV2Sider #listWrap{
    height: 100%;
}
#faultModal .structure{
    height: 40%;
}
#diagnosisV2Sider #structure{
    height:60%
}
#diagnosisV2Sider #category,
#faultModal .consequence,
#faultModal .category{
    height: 30%;
}
#diagnosisV2Sider #fault{
    height: 40%;
}

#diagnosisV2Sider .itemHead,
#faultModal .itemHead{
    font-size: 1.1667rem;
    height: 30px;
    line-height: 30px;
    padding: 0 20px 0 12px;
    background-color: #111111;
    color: #00a7ce;
}
#diagnosisV2Sider .itemHead .headIcon,
#faultModal .itemHead .headIcon{
    display: inline-block;
    width: 12px;
    height: 12px;
    position: relative;
    margin-right: 10px;
}
#diagnosisV2Sider .itemHead .headIcon .out,
#faultModal .itemHead .headIcon .out{
    display: inline-block;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    width: 11px;
    height: 11px;
    border-radius: 100%;
    border: 1px solid #40c7dd;
    box-shadow: 0 0 10px #40c7dd;
}
#diagnosisV2Sider .itemHead .headIcon .in,
#faultModal .itemHead .headIcon .in{
    display: inline-block;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    width: 11px;
    height: 11px;
    border-radius: 100%;
    background: #40c7dd;
    opacity: 0.3;
}
#diagnosisV2Sider .itemHead .allBtn,
#diagnosisV2Sider .itemHead .clearBtn,
#diagnosisV2Sider #fault .itemList .item .removeBtn{
    float: right;
    width: auto;
    height: auto;
    color: #727480;
    font-size: 1rem;
    font-weight: lighter;
    /*line-height: 16px;*/
    text-align: center;
    margin-top: 9px;
    /*border-radius: 4px;
    border: solid 1px rgba(241,68,76,.2);*/
    cursor: pointer;
}
#diagnosisV2Sider .itemHead .clearBtn{
    /*width: 40px;*/
    margin-right: 20px;
}
#diagnosisV2Sider .itemList,
#faultModal .itemList{
    list-style: none;
    font-size: 1rem;
    height: calc( 100% - 30px );
    overflow-y: auto; 
    background-color: #17181c;
    padding: 0;
}
#diagnosisV2Sider .itemList::-webkit-scrollbar,
#faultModal .itemList::-webkit-scrollbar{
    width: 6px;
}
#diagnosisV2Sider .itemList::-webkit-scrollbar-thumb,
#faultModal .itemList::-webkit-scrollbar-thumb{
    background-color: rgba(40,41,46,0.6);
}
#diagnosisV2Sider .itemList::-webkit-scrollbar-track,
#faultModal .itemList::-webkit-scrollbar-track{
    background-color: #F5F5F5;
}
/*ztree*/
#diagnosisV2Sider .itemList li,
#faultModal .itemList li{
    position: relative;
}
#diagnosisV2Sider .itemList li ul,
#faultModal .itemList li ul{
    padding: 0;
}
#diagnosisV2Sider .itemList li span.itemEnd,
#faultModal .itemList li span.itemEnd{
    text-align: right;
    padding-right: 8px;
    width: 40px;
}
#diagnosisV2Sider .itemList li span.button.switch,
#faultModal .itemList li span.button.switch{
    width: auto;
    height: auto;
    position: absolute;
    right: 20px;
    top: 8px;
    z-index: 999;
    color: #cacaca;
    transition: all 0.5s;
}
/*#diagnosisV2Sider .itemList li:hover>span.button.switch{
    color: #ffffff;
}*/
#diagnosisV2Sider .itemList li a,
#faultModal .itemList li a{
    width: 100%;
    height: 31px;
    line-height: 30px;
    font-size: 1rem;
    color: #aaaab2;
    padding: 0 20px 0 12.25px;
    border-bottom: 1px solid rgba(88,88,88,.1);
    transition: all 0.5s;
    border-left: none;
    border-right: none;
    border-top: none;
}
#diagnosisV2Sider .itemList li a:hover,
#faultModal .itemList li a:hover{
    background: #373c50;
    color: #fff;
    text-decoration: none;
}
#diagnosisV2Sider .itemList li a:hover span.iconfont,
#diagnosisV2Sider .itemList li a.curSelectedNode span.iconfont,
#faultModal .itemList li a:hover span.iconfont,
#faultModal .itemList li a.curSelectedNode span.iconfont{
    color: #fff;
}
#diagnosisV2Sider .itemList li a.curSelectedNode,
#faultModal .itemList li a.curSelectedNode{
    background: #373c50;
    color: #fff;
    border: none;
    opacity: 1;
    border-bottom: 1px solid rgba(88,88,88,.1);
}
#diagnosisV2Sider .itemList li a span,
#faultModal .itemList li a span{
    display: inline-block;
    width: calc( 100% - 75px);
    line-height: 30px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 1rem;
    margin-right: 0;
}
#diagnosisV2Sider .itemList li a span.iconfont ,
#diagnosisV2Sider .itemList li span.button.noline_open,
#diagnosisV2Sider .itemList li span.button.noline_close,
#faultModal .itemList li a span.iconfont ,
#faultModal .itemList li span.button.noline_open,
#faultModal .itemList li span.button.noline_close{
    width: auto;
    height: auto;
    font-size: 0.916rem;
    line-height: 1;
    color: #cacaca;
    background: none;
    font-family: 'Glyphicons Halflings';
    transition: all 0.5s;
    margin-top: 1.4px;
}
#diagnosisV2Sider .itemList li a span.iconfont,
#faultModal .itemList li a span.iconfont{
    float: left;
    margin: 9.5px 8px 0 0;
}
#diagnosisV2Sider .itemList li span.button.noline_close::before,
#faultModal .itemList li span.button.noline_close::before{
    content: "\e259";
}
#diagnosisV2Sider .itemList li span.button.noline_open::before,
#faultModal .itemList li span.button.noline_open::before{
    content: "\e260";
}
#diagnosisV2Sider .itemList li li a,
#faultModal .itemList li li a{
    padding-left: 34px;
}
#diagnosisV2Sider .itemList li li li a,
#faultModal .itemList li li li a{
    padding-left: 56px;
}
#diagnosisV2Sider .itemList li li li li a,
#faultModal .itemList li li li li a{
    padding-left: 78px;
}
#diagnosisV2Sider .itemList li li li li li a,
#faultModal .itemList li li li li li a{
    padding-left: 100px;
}
/*ztree end*/
#diagnosisV2Sider .itemList .item,
#faultModal .itemList .item{
    height: 31px;
    line-height: 30px;
    font-size: 1rem;
    color: #aaaab2;
    border-bottom: 1px solid rgba(88,88,88,.1);
    padding: 0 20px 0 12.25px;
    cursor: pointer;
    transition: all 0.5s;
}
#diagnosisV2Sider #category .itemList .item,
#faultModal .category .itemList .item{
    display: inline-block;
    width: 50%;
    padding-right: 12.25px;
}
#diagnosisV2Sider .itemList .item:hover,
#diagnosisV2Sider .itemList .item.selected,
#faultModal .itemList .item:hover,
#faultModal .itemList .item.selected{
    background: #373c50;
    color: #fff;
}
#diagnosisV2Sider .itemList .item .itemText,
#faultModal .itemList .item .itemText{
    display: inline;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
#diagnosisV2Sider .itemList .item .itemIcon,
#faultModal .itemList .item .itemIcon{
    float: left;
    width: auto;
    height: auto;
    font-size: 0.916rem;
    margin: 0 8px 0 0;
}
#diagnosisV2Sider #fault .itemList .item .itemText,
#faultModal .fault .itemList .item .itemText{
    display: inline-block;
    width: calc(100% - 45px);
}
#diagnosisV2Sider #category .itemList .item .itemText,
#faultModal .category .itemList .item .itemText{
    width: calc(100% - 72px);
}
#diagnosisV2Sider #category .itemList .item .itemText{
    display: inline-block;
}
#diagnosisV2Sider .itemList .item .itemEnd,
#faultModal .itemList .item .itemEnd{
    display: inline-block;
    float: right;
    width: 60px;
    font-weight: lighter;
    text-align: right;
    color: #28b570;
}
#diagnosisV2Sider #fault .itemList .item:hover .removeBtn{
    display: inline-block;
}
#diagnosisV2Sider #fault .itemList .item .removeBtn{
    display: none;
    margin-top: 9px;
}
#diagnosisV2Sider #category .itemList .item .itemEnd,
#faultModal .category .itemList .item .itemEnd{
    width: 43px;
}
#diagnosisV2Sider .itemList .item .itemEnd.error,
#faultModal .itemList .item .itemEnd.error{
    color: #f1444c;
}
/*faultModal fault摸态框*/
#faultModal {

}
#faultModal .fade{
    z-index: 0;
}
#faultModal .modal-dialog{
    width: 1366px;
    height: 768px;
}
#faultModal .modal-content{
    height: 100%;
    background-color: #fff;
    border-radius: 0;
}
#faultModal .modal-header{
    overflow: auto;
    border: none;
    padding: 10px 15px;
    background-color: #111;
}
#faultModal .modal-header button{
    margin-right: 10px;
    float: right;
    border-radius: 6px;
    color: #fff;
    padding: 4px 18px;
}
#faultModal .modal-body{
    height: 100%;
    padding: 0;
}
#faultModal #modal-nav{
    width: 300px;
    height: 100%;
    float: left;
}
#faultModal #modal-nav .listHead{
    height: 64px;
    line-height: 64px;
}
#faultModal #modal-nav .listWrap{
    height: calc( 100% - 64px );
}
#faultModal .category .itemList .item{
    display: block;
    width: 100%;
}
#faultModal #modal-faulttable{
    width: calc(100% - 300px);
    height: 100%;
    float: left;
}
#faultModal .btn-xs{
    padding: 1px 5px;
    font-size: 1rem;
    line-height: 1.5;
    border-radius: 3px;
}
/*FaultPanelTable start*/
#modal-faulttable .modal-faulttable-content{
    width: 100%;
    height: calc(100% - 36px);
    overflow-y: auto;
}
#modal-faulttable .modal-fault-table-top {
    height: 60px;
    line-height: 60px;
    
}
#modal-faulttable .modal-fault-table-top .leftGroup{
    float: left;
    margin-left: 16px;
}
#modal-faulttable .modal-fault-table-top .rightGroup{
    float: right;
    margin-right: 16px;
}
#faultPanelTable{
    color: #333;
