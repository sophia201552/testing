.pythonPanel {
  width: 100%;
  height: 100%;
}
.pythonPanel .left {
  float: left;
  width: 80%;
  height: 100%;
}
.pythonPanel .top {
  width: 100%;
  height: 60%;
  border-bottom: 1px solid #333;
}
.pythonPanel .bottom {
  width: 100%;
  height: 40%;
  position: relative;
}
.pythonPanel .bottom .runButton {
  position: absolute;
  top: 0;
  right: 15px;
  color: rgb(136, 172, 255);
  display: flex;
  align-items: center;
  height: 40px;
  cursor: pointer;
  padding: 0 10px;
}
.pythonPanel .bottom .runButton:hover{
    background-color: #292e48;
    color: #17a4df;
}
.pythonPanel .bottom .runButton i {
  margin-right: 6px;
  font-size: 14px;
}
.pythonPanel .right {
  float: right;
  height: 100%;
  width: 20%;
  position: relative;
  background: #171d34;
  /* background-image: linear-gradient(to bottom, #282f48, #171d34); */
}
.pythonPanel .top .title {
  background-image: linear-gradient(to right, #282f48, #171d34);
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 20px;
  font-weight: 600;
  color: #fff;
  padding-left: 16px;
}
.pythonPanel .top .content {
  height: calc(100% - 40px);
  width: 100%;
}

.pythonResult {
  width: 100%;
  height: 100%;
  overflow-y: auto;
  background-color: #fff;
  padding: 20px;
  color: #7a7a7a;
  font-size: 16px;
}
