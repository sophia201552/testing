  .add-strategy-main {
    width: 100%;
    padding-top: 20px; 
  }

  .header{
      overflow: hidden;
  }
  .pdlr20{
      padding:0 20px;
  }

  .titleName {
    width: 60%;
    font-size: 18px;
    float: left;
    font-weight: 600;
  }

  .eventButton {
    text-align: right;
    width: 40%;
    float: right;
  }
  
  .eventButton button{
    width: 80px !important;
    font-size: 12px !important;
    padding: 0 6px !important;
    border-radius: 5px !important;
    font-weight: normal;
  }

  .hide-strategy-btn , .show-strategy-btn{
    border:1px solid #D3D3D3 !important;
    background-color: #EBEEF7 !important;
  }

  .hide-strategy-btn:hover ,.show-strategy-btn:hover{
    background-color: #EBEEF7 !important;
    color: none;
  }
  
  .more-strategy-btn{
      margin-left: 10px;
      background-color: #799DF3 !important;
      color: #fff !important;
  }
  
  .select-strategy-main {
      padding: 10px;
  }

  .blank{
    width: 100%;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    text-align: center;
  }

  .blank >i {
      font-size: 44px;
      color: #5282f1;
  }

  .select-strategy-main ul {
    list-style: none;
    margin: 0;
    padding: 0;
    flex-wrap: wrap;
    display: flex;
  }
  
  .select-strategy-main ul li {
    flex: 1;
    text-align: center;
    height: 170px;
  }
  @media (min-width: 1608px) {
    .select-strategy-main ul li {
      height: 170px;
    }
  }

  .select-strategy-main ul li > div {
    /* box-shadow: 0 0 4px 4px rgba(243, 243, 243, 1); */
    text-align: center;
    height: calc(100% - 40px);
    display: table;
    width: 94%;
    margin: 5px auto 15px auto;
    cursor: pointer;
    border-radius: 4px;
    background-color: #feffff;
    border: solid 1px #d3d3d3;
    position: relative;
  }

  .select-strategy-main ul li > div:hover {
    transform: translateY(-6px);
    -webkit-transform: translateY(-6px);
    box-shadow: 0px 6px 20px 0 rgba(23, 28, 53, 0.2);
    -webkit-box-shadow: 0px 6px 20px 0 rgba(23, 28, 53, 0.2);
    -webkit-transition: transform 0.3s ease;
    transition: transform 0.3s ease;
  }

  .select-strategy-main ul li > div > span {
    display: table-cell;
    vertical-align: middle;
  }

  .select-strategy-main ul li .itemName {
    line-height: 20px;
    color: #666666;
    font-size: 14px;
  }
  
  