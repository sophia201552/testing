.mb-dtu-container {
    color: #f6f6f6;
    height: 100%;
    left: 10px;
}

#mb_dtu_list_ul.ztree li a {
    color: #EEEEEE;
}

#mb_dtu_list_ul {
    height: 98%;
    overflow-y: auto;
    overflow-x: hidden;
}

#mb_dtu_list_ul li {
    line-height: 24px;
}

#mb_dtu_list_ul li a {
    height: 24px;
    padding: 0 4px;
    border-radius: 4px;
}

#mb_dtu_list_ul li a:hover {
    text-decoration: none;
    background-color: #fcb813;
    color: #fff;
}

#mb_dtu_list_ul li a.curSelectedNode {
    border: none;
    background-color: #fcb813 !important;
    color: #fff !important;
}

#mb_dtu_list_ul a .button:not(.ico_close):not(.ico_open) {
    margin: 0 15px 0 0;
    text-decoration: none;
}

#mb_dtu_list_ul a .button:not(.ico_close):not(.ico_open):hover {
    color: #5cb85c;
}

#mb_dtu_list_ul li span.button.ico_open, #mb_dtu_list_ul li span.button.ico_close,
#mb_dtu_list_ul .treeCustomIcon {
    background: none;
    font-family: 'Glyphicons Halflings';
    vertical-align: middle;
    width: 0;
    height: 0;
}

#mb_dtu_list_ul li span.button.ico_open, #mb_dtu_list_ul li span.button.ico_close {
    margin: 0 22px 0 0;
}

#mb_dtu_list_ul li span.button.ico_close:before, #mb_dtu_list_ul li span.button.ico_open:before {
    font-size: 15px;
}

#mb_dtu_list_ul .ico_docu {
    margin: 5px 3px 0 0 !important;
}

#mb_dtu_list_ul li span.button.ico_close:before {
    content: "\e117";
}

#mb_dtu_list_ul li span.button.ico_open:before {
    content: "\e118";
}

#mb_dtu_list_ul li .removeNode:before {
    content: "\e014";
}

#mb_dtu_list_ul li .edit:before {
    content: "\270f";
}

#mb_dtu_list_ul li .copy:before {
    content: "\e015";
}

#mb_dtu_list_ul li .debugger:before {
    content: "\e019";
}

#modbusContainer #mb_dtu_list_ul .isOnline {
    display: inline-block;
    margin-right: 2px;
    width: 10px;
    height: 10px;
    border-radius: 20px;
}

#mb_dtu_list_ul .online {
    background: #008000;
}

#mb_dtu_list_ul .offline {
    background: #fff;
}

#mb_dtu_list_ul .ztree li span {
    margin-right: 5px;
}

/*modbus_tree样式start*/
#modbus_tree_copy .modal-content,
#modbus_tree_edit .modal-content,
#export_point_modal .modal-content,
#modbus_tree_update .modal-content {
    width: 38%;
    margin: 60px auto;
    padding: 20px 10px;
}

#modbus_tree_copy .copy_content_title {
    padding: 8px 12px;
}

#modbus_tree_copy .copy-point-table {
    margin: 10px 40px;
}

#modbus_tree_copy .copy-point-table tr th,
#modbus_tree_copy .copy-point-table tr td {
    min-width: 150px;
    padding: 5px 8px;
}

/*modbus_tree样式end*/

/*modbus_panel样式start*/
#panel-nav {
    text-align: center;
    margin-top: 15px;
    overflow: hidden;
    padding: 15px;
}

#mbPointsTableBox .panel-shortcut {
    float: left;
    box-shadow: 0 0 15px #CCC;
    color: #ffffff;
    font-size: 14px;
    font-family: "Microsoft YaHei", "微软雅黑", "Helvetica", "arial", "sans-serif";
    cursor: pointer;
    padding: 1px;
    border-radius: 6px;
    background-color: #ffffff;
    margin: 10px 20px 10px 0;
    width: 190px;
    height: 200px;
    text-align: center;
    text-decoration: none;
}

#mbPointsTableBox .icon-container {
    margin: 4px 3px 10px 3px;
    background-color: #F2F2F2;
    height: 145px;
    width: 180px;
    padding: 1px;
}

#mbPointsTableBox .icon-container .icon.iconfont {
    width: 110px;
    height: 110px;
    display: block;
    line-height: 110px;
    font-size: 41px;
    background-color: #e5e5e5;
    color: #00bbc3;
    border-radius: 50%;
    margin: 16px auto;
}

#mbPointsTableBox .panel-nav-text {
    width: 142px;
    height: 32px;
    line-height: 32px;
    background-color: #00bbc3;
    border: 1px solid #00a7ae;
    border-radius: 15px;
    margin: 8px auto;
}

#export-container {
    text-align: center;
    font-size: 16px;
    font-family: "Microsoft YaHei", "微软雅黑", "Helvetica", "arial", "sans-serif";
    padding: 5px 10px;
    min-height: 100px;
}

#export-container .export-content {
    display: inline-block;
    margin-right: 24px;
    vertical-align: middle;
    margin-top: 30px;
}

#export-container .note-help {
    display: block;
    margin: 3px;
    padding-top: 5px;
    text-align: left;
    padding-left: 44px;
}

#new-equipment {
    margin: 50px;
}

#export-container .errorMsg {
    line-height: 14px;
    padding: 5px;
    color: #ff0000;
    font-size: 12px;
    display: block;
}
/*modbus_panel样式end*/