.container {
  position: relative;
  width: 100%;
  height: 100%;
}
.message-bar-container {
  width: 100%;
  height: 32px;
  line-height: 32px;
  position: absolute;
  top: 0;
  color: #a80000;
  background: #fde7e9;
  padding: 0 20px;
  overflow: hidden
}

.ErrorBadgeWrap {
  float: left;
  font-size: 16px;
  margin-right: 8px;
}
.cancleWrap {
  float: right;
}
.container >div:global(.cancle) {
  display: none;
}
