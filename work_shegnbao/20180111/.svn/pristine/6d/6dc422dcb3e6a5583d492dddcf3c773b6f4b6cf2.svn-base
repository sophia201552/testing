.slider-container {
  float: left;
  width: 200px;
  height: 100%;
  color: #555;
  padding-top: 24px;
}
.slider-header-text {
  width: 100%;
  height: 42px;
}
.slider-header-text h3 {
  height: 100%;
  margin-left: 14px;
  font-size: 20px;
  font-weight: 500;
  color: rgb(29, 29, 29);
}
.timePickerWrap {
  width: 100%;
  padding: 8px 0 0 14px;
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
}
.timePickerWrap > div {
  margin-bottom: 8px;
}

.timePickerWrap :global(.ms-TextField-fieldGroup) {
  width: 172px;
  height: 30px;
  border-radius: 4px;
  font-size: 12px;
  background-color: #f2f2f2;
  border-color: #e8e8e8 !important;
}
.timePickerWrap :global(.ms-TextField-fieldGroup:hover) {
  border-color: #e8e8e8;
}
.timePickerWrap > div > div input {
  background: #ffffff;
  border: 1px solid #a6a6a6;
  cursor: pointer;
  display: block;
  height: 24px;
  line-height: 22px;
  padding: 0 12px 0 12px;
  width: 100%;
  border-radius: 4px;
  font-size: 12px;
}

.timePickerWrap :global(.ms-TextField-fieldGroup) > i {
  font-size: 12px;
  line-height: 14px;
  right: 6px;
}
.slider-assess-ways {
  height: 100px;
  overflow: hidden;
}

.slider-assess-ways > div > button {
  margin: 6px;
}
.slider-assess-ways :global(.ms-Checkbox:hover) > label > div {
  border-color: rgb(166, 166, 166);
}
.slider-assess-ways :global(.ms-Checkbox:focus) > label > span,
.slider-assess-ways :global(.ms-Checkbox) > label > span,
.slider-assess-ways :global(.ms-Checkbox:hover) > label > span {
  color: #999;
  font-size: 12px;
}
.slider-assess-ways :global(.ms-Checkbox) > label > div > i {
  /* line-height: 15px; */
  font-size: 13px;
  line-height: 14px;
  font-weight: 500;
}
.slider-assess-ways :global(.ms-Checkbox.is-checked) > label > div > i {
  background: #4c83ff;
  border: none;
}
.slider-assess-ways :global(.ms-Checkbox-checkbox) {
  width: 14px;
  height: 14px;
  border-radius: 2px;
  border: none;
}
.slider-title {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  font-weight: 600;
  letter-spacing: 0;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
  margin-bottom: 12px;
  color: rgb(29, 29, 29);
}
.slider-assess-ways,
.slider-sample {
  padding-top: 20px;
  padding-left: 14px;
}

.sample-list {
  width: 172px;
  font-size: 10px;
  color: #6c95ff;
}

.sample-list .sample-item {
  padding-left: 7px;
  line-height: 24px;
  border-radius: 4px;
}
.sample-item:hover {
  background-color: #c7d8fe;
  color: #10b8f9;
}
.slider-run-test {
  width: 160px;
  height: 32px;
  line-height: 32px;
  border-radius: 4px;
  margin-top: 111px;
  margin-left: 20px;
  background-color: #5b8af7;
  text-align: center;
  font-weight: 500;
  color: #fff;
}
