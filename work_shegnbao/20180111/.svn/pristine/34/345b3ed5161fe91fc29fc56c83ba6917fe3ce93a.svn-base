.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.logicAnalysisWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.left > .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left>.treeWrap{
  width: 100%;
  height: calc(100% - 20px);
  padding-top: 8px;
}
.right {
  float: left;
  width: 82%;
  height: 100%;
}
.right .t {
  width: 100%;
  height: calc(100% - 44px);
  padding: 8px;
  overflow-y: auto;
}
.right .b {
  width: 100%;
  height: 44px;
  padding: 8px 0;
}
.listAddWrap {
  width: 100%;
  height: 28px;
  position: relative;
  border-radius: 4px;
  background: #333;
  opacity: 0.5;
}
.listAddWrap .listAdd {
  width: 30px;
  height: 30px;
  line-height: 27px;
  text-align: center;
  font-size: 30px;
  color: #fff;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  cursor: pointer;
  user-select: none;
}
.wrapLv1,
.wrapLv2 {
  width: 100%;
  position: relative;
}
.wrapLv1{
  border-radius: 4px;
  border: 1px solid #333;
  margin-bottom: 8px;
}
.wrapLv1 .wrapLv2:first-child{
  border-bottom: 1px dashed #666;
}
.wrapLv2 > .listRemove {
  width: 20px;
  height: 20px;
  line-height: 17px;
  color: #333;
  text-align: center;
  font-size: 14px;
  position: absolute;
  left: 0;
  top: 0;
  cursor: pointer;
  border-radius: 100%;
  user-select: none;
}
.wrapLv2 > .name {
  float: left;
  width: 60px;
  min-height: 1px;
}
.wrapLv2 > .name > span{
  display: block;
  width: 60px;
  height: 12px;
  overflow: hidden;
  font-size: 12px;
  line-height: 1;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -6px;
}
.wrapLv2 > .content {
  float: left;
  width: calc(100% - 60px);
}
.wrapLv2 > .content .itemWrap {
  float: left;
  position: relative;
  width: 33%;
  padding-right: 16px;
  height: 56px;
  line-height: 56px;
  overflow: hidden;
  margin: 4px 0;
}
.wrapLv2 > .content .itemWrap:hover .remove {
  display: block;
}
.wrapLv2 > .content .itemWrap .remove {
  display: none;
  position: absolute;
  top: 0;
  right: 0;
  width: 16px;
  height: 16px;
  border-radius: 100%;
  /* background-color: rgb(245, 106, 0); */
  line-height: 16px;
  text-align: center;
  cursor: pointer;
  user-select: none;
}
.wrapLv2 > .content .itemWrap .addWrap {
  width: 100%;
  height: 100%;
  position: relative;
  border: 1px dashed #333;
  border-radius: 4px;
}
.wrapLv2 > .content .itemWrap .add {
  width: 30px;
  height: 30px;
  line-height: 27px;
  text-align: center;
  font-size: 30px;
  opacity: 0.5;
  border-radius: 100%;
  background: #333;
  color: #fff;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  cursor: pointer;
  user-select: none;
}
.wrapLv2 > .content .itemWrap .itemL {
  float: left;
  width: 76px;
  height: 56px;
  border: 1px solid #333;
  border-right: none;
  border-radius: 4px 0 0 4px;
  overflow: hidden;
}
.wrapLv2 > .content .itemWrap .itemR {
  float: left;
  width: calc(100% - 76px);
  height: 56px;
  border: 1px solid #333;
  border-radius: 0 4px 4px 0;
  overflow: hidden;
}
.wrapLv2 > .content .itemWrap .itemT {
  width: 100%;
  height: 29px;
  border-bottom: 1px solid #333;
}
.wrapLv2 > .content .itemWrap .itemB {
  width: 100%;
  height: 28px;
}
.wrapLv2 > .content .itemWrap .itemBL {
  float: left;
  width: 78px;
  height: 28px;
  border-right: 1px solid #333;
}
.wrapLv2 > .content .itemWrap .itemBR {
  float: left;
  width: calc(100% - 78px);
  height: 28px;
}
