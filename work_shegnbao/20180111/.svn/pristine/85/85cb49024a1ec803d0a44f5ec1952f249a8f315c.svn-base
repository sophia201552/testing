import pytestfrom beopWeb.mod_admin import reportControllersimport jsonfrom beopWeb.mod_oss.ossapi import *from beopWeb.mod_common.Utils import *from flask import current_app@pytest.mark.p0@pytest.mark.parametrize(('rq_data'), [    ({"reportData":"test send_report_email","recipients":"2899718379@qq.com"}),])def test_send_report_email(rq_data):    if not current_app:        app_ctx = app.app_context()        app_ctx.push()    rt=reportControllers.do_send_report_email(rq_data)    rt=json.loads(bytes.decode(rt.data))    assert rt.get('success') == True, 'send_report_email fail'