.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.over {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.FeatureSelectionPanel {
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding-top: 24px;
}

.left-panel {
  width: 18%;
  height: 100%;
  overflow-y: auto;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.right-panel {
  width: 82%;
  height: 100%;
}
.left-panel > .title {
  width: 100%;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.methodsWrap {
  padding-top: 16px;
  padding-left: 16px;
  height: 60%;
  position: relative;
  overflow-y: auto;
}

.methodsWrap > .title {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
  margin-bottom: 4px;
  font-weight: 600
}
.methodsWrap > .contentWrap{
  height: calc(100% - 20px);
  width: 100%;
  overflow-y: auto;
}
.methodsWrap .item {
  margin-top: 8px;
  padding: 0 16px 0 0;
}
.methodsWrap .inputWrap {
  padding-left: 22px;
}
.datasetWrap {
  padding: 16px 0 0 16px;
  height: calc(40% - 20px);
}
.right-panel-top {
  width: 100%;
  height: 50%;
}
.right-panel-bottom {
  width: 100%;
  height: 50%;
}
.tableWrap {
  width: 100%;
  height: 100%;
  overflow: auto;
}
.tableWrap .thead {
  height: 84px;
  border-bottom: 1px solid rgba(153, 153, 153, 0.3);
  min-width: 100%;
}
.tableWrap .tbody {
  height: calc(100% - 84px);
  min-width: 100%;
}
.tableWrap .tr,
.tableWrap .th,
.tableWrap .td {
  height: 42px;
  line-height: 42px;
}
.tableWrap .th,
.tableWrap .td {
  float: left;
  width: 120px;
  text-align: center;
  user-select: none;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.tableWrap .th {
  font-size: 14px;
  font-weight: 500;
}

.tableWrap .col-3 {
  width: 360px;
}
.tableWrap .col-4 {
  width: 480px;
}
.tableWrap .col-2 {
  width: 240px;
}
.green {
  color: green;
}
.red {
  color: red;
}
.bgLight {
  background: rgba(235, 238, 247, 0.7);
}
.bd-l {
  border-left: 1px solid #ccc;
}
/* ms 样式 */
.FeatureSelectionPanel :global(.ms-Label) {
  color: #999999;
}
.FeatureSelectionPanel :global(.ms-TextField-fieldGroup) {
  height: 24px;
  line-height: 22px;
  border-radius: 4px;
  background: #ffffff;
  border: 1px solid rgba(204, 204, 204, 0.3);
}
.FeatureSelectionPanel :global(.ms-Dropdown-title) {
  background: #ffffff;
  border: 1px solid rgba(204, 204, 204, 0.3);
  border-radius: 4px;
}
.FeatureSelectionPanel :global(.ms-Dropdown) > span {
  height: 24px;
  line-height: 24px;
}
.checkbox {
  height: 24px;
  width: 100%;
}
.checkbox > label{
  width: 100%;
}
.checkbox > label > div {
  width: 14px;
  height: 14px;
  border-radius: 2px;
  border-color: #ccc;
}
.checkbox:hover > label > div:global(.ms-Checkbox-checkbox) {
  border-color: #ccc;
  
}

.checkbox:global(.is-checked:hover) > label > div,
.checkbox:global(.is-checked) > label > div {
  background-color: #4c84ff;
}
.checkbox:hover > label > span:global(.ms-Checkbox-text),
.checkbox > label > span {
  font-size: 12px;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.checkbox > label > div > i {
  font-size: 12px;
}
.labelWrap {
  color: #999;
  word-wrap: break-word;
  overflow-wrap: break-word;
  padding: 5px 0;
  display: inline-block;
}
.labelWrap > div > div {
  margin-top: 2px;
}
.checkboxWrap {
  position: relative;
}
.checkboxWrap > div:global(.ms-TooltipHost) {
  position: absolute;
  top: 6px;
}
.checkboxWrap > button {
  float: left;
}
