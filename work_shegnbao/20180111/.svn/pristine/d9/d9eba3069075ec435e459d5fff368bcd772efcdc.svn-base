/*Overview Begin*/
.overviewScreen {
    padding: 5px;
    background-color: #ebeef7;
    width: 100%;
    height: 100%;
}

.overviewScreen .clickShadow.selected {
    box-shadow: 0px 0px 20px #00a7ce!important;
}

.panelTop {
    width: 100%;
    height: 60%;
}

.panelBottom {
    width: 100%;
    height: 40%;
}

.faultRatioContainer {
    width: 60%;
    height: 130px;
    float: left;
    padding: 10px 5px 5px 10px;
    padding-right: 5px;
}

.influenceRatioContainer {
    width: 40%;
    height: 130px;
    float: left;
    padding: 10px 5px 5px 5px;
}

.ChartContainer {
    width: 60%;
    height: calc(100% - 130px);
    position: relative;
    float: left;
    padding: 6px;
    padding-left: 10px;
}

.pieWithLineContainer {
    width: 40%;
    height: calc(100% - 130px);
    padding: 6px;
    float: left;
    position: relative;
}

.summaryContainer {
    width: 30%;
    height: 100%;
    float: left;
    padding: 6px;
    padding-left: 10px;
}

.pieWidthBarContainer {
    width: 30%;
    height: 100%;
    padding: 6px;
    float: left;
}

.tableContainer {
    width: 40%;
    height: 100%;
    padding: 6px;
    float: left;
}

.faultBox {
    width: 100%;
    height: 100%;
}

.influenceBox {
    width: 100%;
    height: 100%;
}

.chartBox {
    width: 100%;
    height: 100%;
    border-radius: 4px;
    background-color: #ffffff;
    box-shadow: 0px 2px 12px 0  rgba(164,173,201,0.4);
}

.pieWithLineContainBox {
    width: 100%;
    height: 100%;
    border-radius: 4px;
    background-color: #ffffff;
    box-shadow: 0px 2px 12px 0 rgba(164,173,201,0.4);
}

.BlackBox {
    width: 100%;
    height: 100%;
    border-radius: 4px;
    background-color: #28292e;
    box-shadow: 0px 2px 12px 0  rgba(164,173,201,0.4);
}

.faultTitle {
    width: 100%;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    color: #00a7ce;
}
.boxPlane{
    height: 100%;
    display: inline-block;
    width: 24%;
    position: relative;
}
.overPlane {
    height: 100%;
    /* display: inline-block; */
    width: 100%;
    position: relative; 
}
.influencePanelContainer .overPlane {
    transition: transform .8s linear;
    transform-style: preserve-3d;
}
.influencePanelContainer .overPlane:hover {
    transform: rotateY(180deg);
}
.influencePanelContainer .overPlane .front { 
    transform: translateZ(0);
}
.influencePanelContainer .overPlane .back {   
    transform: rotateY(-180deg) translateZ(0);
}
.back .gotoBtn{
    z-index: 5;
}

.singlePanel {
    float: left;
    background-color: #fff;
    height: 100%;
    box-shadow: 0px 2px 12px 0 rgba(164,173,201,0.4);
    border-radius: 4px;
    cursor: pointer;
    position: relative;
}
.faultPanelContainer{
    height: calc(100% - 30px);
}
.faultPanelContainer .singlePanel {
    width: 12%;
}
.influencePanelContainer {
    overflow: hidden;
    height: calc(100% - 30px);
}
.influencePanelContainer .singlePanel {
    width: 100%;
    position: absolute!important;
     transition:transform 2s;  
}

.faultPanelContainer .singlePanel:not(:last-child) {
    margin-right: .54%;
}
.influencePanelContainer .boxPlane:not(:last-child) {
    margin-right: 1.2%;
}
.panelName {
    text-align: center;
    margin: 16px 0 8px;
    font-size: 12px;
    color: #69779f;
    font-weight: 500;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.panelPower {
    color: #333333;
    font-size: 18px;
    text-align: center;
    margin-bottom: 16px;
}

.panelBar {
    width: 100%;
    padding: 0 6px;
    position: absolute;
    bottom: 6px;
}

.progress1 {
    height: 3px;
    background: #ebeef7;
    border-left: 1px solid transparent;
    border-right: 1px solid transparent;
    border-radius: 1px;
}

.progress .blue {
    background: #6c90ca;
    box-shadow: 0 0 4px 0 rgba(108, 144, 202, 0.7);
}

.progress .pink {
    background: #df92e6;
    box-shadow: 0 0 4px 0 rgba(223, 146, 230, 0.7);
}

.progress .red {
    background: #e9728f;
    box-shadow: 0 0 4px 0 rgba(233, 114, 143, 0.7);
}

.progress .orange {
    background: #fcb192;
    box-shadow: 0 0 4px 0 rgba(252, 177, 146, 0.7);
}

.progress .skyblue {
    background: #57c9ff;
