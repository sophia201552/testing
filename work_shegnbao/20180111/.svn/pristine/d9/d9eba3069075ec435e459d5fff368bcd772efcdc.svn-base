.clickShadow:hover{
    box-shadow: 0px 2px 12px 0 #929cb9;
}
.clickAddShadow{
    box-shadow: 0px 2px 12px 0 #929cb9;
}
.clickRemoveShadow{
    box-shadow: 0px 2px 12px 0 rgba(164,173,201,0.4);
}
.clickShadow.goto{
    position: relative;
}
.goto .gotoBtn{
    display: none;
    font-size: 16px;
    font-weight: bold;
    width: auto;
    height: 16px;
    line-height: 16px;
    text-align: center;
    color: #57c9ff;
}
.goto .gotoBtn:hover{
    color: #00a7ce;
}
.goto:hover .gotoBtn{
    display: block;
    position: absolute;
    right: 6px;
    top: 4px;
    cursor: pointer;
}

