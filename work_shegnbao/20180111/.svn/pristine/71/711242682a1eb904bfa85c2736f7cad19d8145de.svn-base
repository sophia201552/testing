.tab-container {
  position: relative;
  height: 100%;
}
.tab-menu {
  height: 32px;
  background: rgb(47, 59, 85);
  border-top: 1px solid rgb(25, 31, 55);
}
.tab-pane {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 32px;
  background: #f5f8f9;
}
.tab-menu-item {
  display: inline-block;
  padding: 0 16px;
  height: 32px;
  line-height: 32px;
  color: #fff;
  cursor: pointer;
  user-select: none;
}
.tab-menu-item.active {
  background: #fff;
  color: rgb(15, 184, 249);
  cursor: pointer;
}
:global(.ms-Icon).menu-item-icon {
  height: 32px;
  vertical-align: bottom;
  padding-right: 4px;
}