.configTabPane {
  width: 100%;
  height: 100%;
  padding-top: 16px;
}
.configTabPane .propertyCtn{
  height: calc(100% - 80px);
}
.configTabPane .buttonCtn{
  height: 80px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.configTabPane .buttonCtn button {
  width: 81px;
  margin-left: 12px;
  height: 31px;
  line-height: 31px;
  border-radius: 4px;
  background-color: transparent;
  border: solid 1px rgba(211, 211, 211, .5);
  cursor: pointer;
  padding: 0 16px;
  color: #999999;
  margin-left: 12px;
}
.configTabPane .buttonCtn > button:first-child:hover {
  background: transparent;
}
.configTabPane .buttonCtn > button:last-child {
  border-color: rgba(81, 130, 241, .8);
  background-color: rgba(81, 130, 241, .8);
  color: #ffffff;
}
.configTabPane .group {
  width: 100%;
  padding: 8px 16px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.configTabPane .group:first-child {
  margin-top: 0;
}
.configTabPane .group::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.configTabPane .group label {
  float: left;
  color: #fff;
  width: 36%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.configTabPane .group .dropdownlistWrap {
  width: 64%;
  height: 24px;
}
.configTabPane .group .dropdownlistWrap input {
  background-color: rgb(25, 31, 55);
  height: 24px;
  width: 100%;
  line-height: 24px;
  padding: 0 12px;
  border-radius: 4px;
  border: none;
  color: rgb(110, 119, 150);
  font-size: 12px;
}
.configTabPane .group .dropdownlistWrap input:focus {
  outline: none;
}
.configTabPane .group .dropdownlistWrap > div > span,
.configTabPane .group .dropdownlistWrap > div > i{
  height: 24px!important;
  line-height: 24px!important;
  border-radius: 4px;
}
.configTabPane .group .dropdownlistWrap > div > div > div > div > div{
  background: rgb(25, 31, 55);
  height: 24px;
  border: none;
  border-radius: 4px;
}
.configTabPane .group .dropdownlistWrap > div > div > div > div > div > i{
  top: 4px;
}