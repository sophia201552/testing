import pytestfrom beopWeb.mod_admin import controllersfrom beopWeb.mod_admin import Userimport jsonimport osfrom werkzeug.datastructures import FileStorage@pytest.mark.p0@pytest.mark.parametrize(('user_id','expected'), [    (2265,True),    (None,False),])def test_updateAvatar(user_id,expected):    filename = os.getcwd() + "/beopWeb" + User.default_user_pic_path + "19.png"    with open(filename, 'rb') as fp:        file = FileStorage(fp)        rt = controllers.do_update_avatar(file, user_id)    rt=json.loads(bytes.decode(rt.data))    assert rt.get('success') == expected, ' not equal .actual={0},expected={1}'.format(rt.get('data'), expected)    if expected:        userpic_after=User.query_user_by_id(user_id, 'userpic').get('userpic', '')        assert userpic_after==rt.get('data'),'update fail: userpic in database is {0},not {1}'.format(userpic_after,rt.get("data"))