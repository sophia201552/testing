.clear::after {
    display: block;
    content: '';
    clear: both;
    width: 0;
    height: 1px;
  }
  .datasetWrap {
    width: 100%;
    height: 100%;
  }
  .datasetWrap > .title {
    width: 100%;
    height: 16px;
    line-height: 16px;
    font-size: 14px;
    color: #1d1d1d;
    padding: 0 0 0 8px;
    border-left: 4px solid #1d1d1d;
    margin-bottom: 4px;
    font-weight: 600;
  }
  .datasetWrap > .content {
    width: 100%;
    height: calc(100% - 20px);
    overflow-y: auto;
    padding-top: 16px;
    padding-right: 16px;
  }
  .datasetWrap > .content .item {
    float: left;
    width: 48%;
    height: 28px;
    padding: 0 8px;
    line-height: 26px;
    border-radius: 4px;
    border: solid 1px rgba(204, 204, 204, 0.3);
    background-color: rgba(204, 204, 204, 0.3);
    margin-right: 4%;
    font-size: 12px;
    text-align: center;
    color: #1d1d1d;
    cursor: pointer;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 8px;
    user-select: none;
  }
  .datasetWrap > .content .item:hover,
  .datasetWrap > .content .item.active {
    background-color: #fff;
    border: solid 1px #10b8f9;
    color: #10b8f9;
  }
  .datasetWrap > .content .item:nth-child(even) {
    margin-right: 0;
  }
  