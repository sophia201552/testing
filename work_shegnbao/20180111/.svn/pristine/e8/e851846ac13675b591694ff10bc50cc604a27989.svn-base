            list-style: none;
            width: 120px;
            padding: 0;
            height: 330px;
            overflow-y: auto;
        }

        #offDtuHistoryUl label {
            margin-bottom: 0;
        }

        #offDtuHistoryUl .allDtuBox {
            border: 1px solid rgba(124, 156, 212, 0.8);
            height: 26px;
            line-height: 26px;
            padding-left: 4px;
            border-bottom: none;
            border-right: none;
        }

        #offDtuHistoryUl .allDtuBox:hover {
            background-color: #1e78e6;
        }

        #offDtuHistoryUl .dtuInput {
            position: relative;
            top: -6px;
        }

        #offDtuHistoryUl li {
            border: 1px solid rgba(124, 156, 212, 0.8);
            height: 26px;
            line-height: 26px;
            padding-left: 4px;
            border-right: none;
        }

        #offDtuHistoryUl li:not(:last-child) {
            border-bottom: none;
        }

        #offDtuHistoryUl li:hover {
            background-color: #1e78e6;
        }

        #offDtuHistoryUl li .offDtu {
            display: inline-block;
            width: 90px;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        #offDtuHistoryUl li.active .offDtu {
            border-right: none;
            position: relative;
            left: 1px;
            background: deepskyblue;
            color: #fff;
        }

        #backBtn {
            position: absolute;
            top: -22px;
            right: -1px;
            color: red;
        }

        #DtuHistoryDemo {
            width: calc(100% - 120px);
            border: 1px solid rgba(124, 156, 212, 0.8);
            padding: 5px;
            min-height: 330px;
            max-height: 338px;
        }

        #offDtuTable, #offDtuHistory {
            color: #E7F1FF;
        }

        #offDtuTable thead {
            display: inline-block;
            width: 100%;
        }

        #offDtuTable tbody {
            display: inline-block;
            width: 100%;
            max-height: 316px;
            overflow-y: auto;
        }

        #offDtuTable table tbody tr:hover {
            background-color: #1e78e6;
        }

        .pd0 {
            padding: 0;
        }

        .lineStateLabel {
            width: 165px;
        }
        #ikenBtn{
            height:53px;
            width:42px;
            position:fixed;
            left:127px;
            top:0;
            display:none;
            z-index:10000;
            cursor:pointer;
        }
        #ikenBtn i{
            line-height: 54px;
            font-size: 17px;
            opacity: 0.9;
            text-shadow: 0 0 1px rgba(255, 255, 255, 0.2);
        }
        #ikenBtn i:hover{
            color: rgb(234, 193, 36);
            opacity: 0.9;
        }
        @media (max-width: 1366px) {
            #iconList #btnDropdownNavList.dropdown-menu {
                padding: 10px 10px 5px 10px;
                width: 410px;
                max-width: 410px;
            }

            #btnDropdownNavList span.glyphicon:before {
                font-size: 21px;
            }

            #iconList .dropdown-menu {
                right: -70px;
            }

