            }

            #iconList .dropdown-menu li .dropdownNav {
                font-size: 12px;
                line-height: 14px;
            }

            #btnDropdownNavList .arrow {
                left: 314px;
                margin-left: -10px;
            }

        }
        @media (min-width: 768px) {
            #right-nav {
                margin-right:5px !important;
            }
        }

