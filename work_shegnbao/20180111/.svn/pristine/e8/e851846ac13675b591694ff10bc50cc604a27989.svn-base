            width: 100%;
            height: 100%;
            margin: 0;
            overflow: hidden;
        }

        #indexMain {
            padding: 0;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            z-index: 1;
        }

        .indexContent {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }

        .nav-btn-a {
            padding: 0 10px 0 10px !important;
            line-height: 38px;
        }

        #divPages {
            position: relative;
            float: left;
            width: calc(100% - 550px);
            height: 53px;
        }

        #divPages > span {
            position: absolute;
            font-size: 14px;
            color: #3893c6;
            top: 18px;
            display: none;
            cursor: pointer;
        }

        #divPages > span:first-child {
            left: 0;
        }

        #divPages > span:last-child {
            right: 0;
        }

        #scrollPages {
            width: 797px;
            position: absolute;
            left: 20px;
            overflow: hidden;
        }

        #ulPages {
            position: relative;
            left: 0;
        }

        #ulPages .active .nav-icon-svg-g {
            fill: #eee;
        }

        #ulPages .dropdown-menu.popover {
            max-width:initial;
        }
        .nav-icon-svg {
            width: 16px;
            height: 16px;
            vertical-align: middle;
            border-radius: 4px;
            position: relative;
            top: -2px;
        }

        .nav-icon-svg-g {
            fill: #3893c6;
        }
        #ulPages .icon{
            font-size: 16px;
            margin-right: 5px;
            position: absolute;
            left: 15px;
            line-height: 38px;
            top: 0;
            height:100%;
