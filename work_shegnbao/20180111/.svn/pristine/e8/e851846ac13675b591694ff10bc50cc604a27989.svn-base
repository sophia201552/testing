            border-bottom-left-radius: 4px;
        }

        #sltLanguage .vMiddle {
            text-align: left;
        }

        #sltLanguage li {
            float: none !important;
            border-radius: 3px;
            height: 30px;
        }

        li.liHover:hover {
            background: #16599C;
        }

        li.liHover:hover span {
            color: #fff !important;
        }

        #iconList .dropdown-menu li.iconWrap:hover {
            background-color: rgba(0, 0, 0, 0.1);
            border-radius: 4px;
        }

