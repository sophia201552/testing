            border-radius: 4px;
            opacity: 0.7;
            z-index: 10;
            cursor: pointer;
            font-size: 13px;
        }

        .divLanguage span {
            color: #eee;
        }

        .divLanguage:hover #sltLanguage {
            display: block;
        }

        .spanlngText {
            margin-bottom: 0 !important;
            display: inline-block !important;
            padding-left: 5px !important;
            padding-right: 0 !important;
        }

        .triangle {
            display: inline-block;
            width: 0;
            height: 0;
            margin-left: 2px;
            vertical-align: middle;
            border-top: 4px solid #3893c6;
            border-right: 4px solid transparent;
            border-left: 4px solid transparent;
        }

        #btnSelectLanguage a > span {
            float: none;
            padding-left: 5px;
        }

        #sltLanguage {
            display: none;
            position: absolute;
