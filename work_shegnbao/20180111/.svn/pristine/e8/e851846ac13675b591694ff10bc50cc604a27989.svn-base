            position: relative;
        }
        .nav-btn-text {
            margin-left: 3px;
            padding-left: 25px;
        }

        #txtProjectTitle,
        .txtProjectTitle {
            margin-top: 20px;
            float: left;
            line-height: 18px;
            font-weight: bold;
            width: 100px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }

        .vMiddle {
            height: 19px;
            line-height: 19px;
            vertical-align: middle;
            display: inline-block;
        }

        /* alarm bell part */

        .alarm-bell > a {
            position: relative;
            z-index: 1;
        }

        #liProjectAlarm .badge {
            position: absolute;
            top: -6px;
            right: -6px;
            background: #ed5565;
            color: #fff;
            border-radius: 4px;
            z-index: 2;
            padding: 2px 5px;
        }

        #paneWorkflow > .badge {
            position: absolute;
            top: 6px;
            right: 16px;
            background: #ed5565;
            color: #fff;
            border-radius: 4px;
            z-index: 2;
            padding: 2px 5px;
        }

        .alarm-bell > a > .badge,
        #iconList .badge:empty {
            display: none !important;
        }

        .alarm-bell > a > .glyphicon-bell {
            font-size: 18px;
        }

        #ulAlarmPanel {
            padding: 10px 0 3px;
            display: none;
            position: absolute;
            width: 340px;
            height: calc(100% - 84px);
            top: 80px;
