<!DOCTYPE html>
<html lang="zh">

<head>
    {% if icon %}
        <link rel="shortcut icon" type="image/ico" href="/static/images/{{ icon }}.ico">
    {% else %}
        <link rel="shortcut icon" type="image/ico" href="/static/images/favicon.ico">
    {% endif %}
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    {% if title %}
        <title>{{ title }}</title>
    {% else %}
        <title>BeOP</title>
    {% endif %}


    {% assets "css_index" %}
        <link rel="stylesheet" type="text/css" href="{{ ASSET_URL }}"> {% endassets %}

    <script>
        var loginCode = '{{ code }}';
        if ('{{ lang }}' === 'en' || '{{ lang }}' === 'zh') {
            // window.localStorage.setItem('isUserSelectedLanguage', '{{ lang }}');
            window.localStorage.setItem('language', '{{ lang }}');
        }
        if(location.host=='industrytech.smartbeop.com'|| location.host == 'industrytech.rnbadopt.com'){
                var rurl = "";
                var links = document.getElementsByTagName("link");
                var link = {};
                for(var i=0;i<links.length;i++){
                    link = links[i];
                    if(link.rel === "shortcut icon"){
                        link.href='/static/images/AdOPT.ico';
                        break;
                    }
                }
                document.querySelector('title').text='AdOPT'
            }
        var _hmt = _hmt || [];
    </script>

    <style>
