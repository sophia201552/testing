            z-index: 1;
            left: 4px;
            border: 1px solid #ddd;
            border-radius: 4px;
        }

        #ulAlarmPanel > .divider {
            margin: 5px 0;
        }

        #ulAlarmPanel > li:last-child {
            text-align: left;
        }

        .divAlarmInfo {
            white-space: normal;
            margin-bottom: 3px;
        }

        .mb0 {
            margin-bottom: 0;
        }

        @keyframes downTip {
            25%,
            80% {
                -webkit-transform: translateY(0);
                transform: translateY(0);
            }
            0%,
            50%,
            100% {
                -webkit-transform: translateY(-10px);
                transform: translateY(-10px);
            }
        }

        .totalDown {
            position: absolute;
            bottom: 10px;
            left: 50%;
            /*font-size:50px;*/
            color: rgba(255, 255, 255, .6);
            margin-left: -15px;
            width: 30px;
            z-index: 2;
            animation: downTip 2s linear infinite;
            display: none;
            cursor: pointer;
        }

        .dividingLine {
            background: -webkit-gradient(linear, left top, left bottom, from(rgba(41, 56, 76, 0.2)), color-stop(0.25, rgba(124, 156, 212, 0.8)), color-stop(0.75, rgba(41, 56, 76, 0.6)), to(rgba(41, 56, 76, 0.2)));
            height: 1px;
        }

        #iconList .badge {
            background-color: #ed5565;
            position: absolute;
            top: 10px;
            right: 15px;
            height: 20px;
            border-radius: 5px;
        }

