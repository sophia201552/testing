    #btnPanelNavToggle {
        position: absolute;
        top: 0;
        left: 240px;
        z-index: 300;
        cursor: pointer;
        background: rgba(0,0,0,0);
        color: white;
        font-size: 20px;
        padding: 5px;
        height: 52px;
        line-height: 36px;
        transition:all 0.3s;
        display:none;
    }
    #btnPanelNavToggle:hover{
        background: rgba(0,0,0,0.7);
    }
    .hideNav #btnPanelNavToggle{
        left:0;
    }
    .hideNav #btnPanelNavToggle:before{
        content: "\e258";
    }
    #containerDisplayboard {
        transition:all 0.3s;
    }
    .hideNav #containerNav{
        transform: translateX(-100%);
    }
    .hideNav #containerDisplayboard{
        width:100%;
    }
</style>
<div id="btnPanelNavToggle" class="glyphicon glyphicon-menu-left"></div>
<div id="containerNav">
    <div class="brand hide" i18n="energyManagement.overview.ENERGY_MANAGEMENT">能源管理</div>
    <div class="timeConfig hide"></div>
    <div class="wrapNavToggle">
        <div class="areaLabel">Function</div>
        <div class="module"></div>
    </div>
    <!--<div id="btnBackToMain"><span class="glyphicon glyphicon-cog"></span><span class="text">Back</span></div>-->
    <!--<div class='splitLine'></div>-->
    <div class="wrapTagTree">
        <div class="areaLabel">Structure</div>
        <div class="tagSearch">
            <input class="iptTagSearch" type="text">
            <span class="btnTagSearch glyphicon glyphicon-search"></span>
        </div>
        <div class="tagTree"></div>
    </div>
</div>
<div id="containerDisplayboard"></div>