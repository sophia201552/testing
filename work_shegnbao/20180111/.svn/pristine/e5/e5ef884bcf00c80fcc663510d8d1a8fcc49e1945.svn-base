<style>
    *::-webkit-scrollbar-track{
        box-shadow: inset 0 0 6px #3C4450;
        background-color: #454f5e;
    }
    *::-webkit-scrollbar-thumb {
        background-color: rgba(255,255,255,.5);;
    }
    *::-webkit-scrollbar {
        width: 8px;
        height: 8px;
        background-color: #454f5e;
    }
    #containerNav {
        position: absolute;
        left: 0;
        top: 0;
        width: 240px;
        height: 100%;
        color:#c6d5f3;
        background:#2d333c;
        transition:all 0.3s;
        transform: translateX(0%);
    }
    #containerNav .splitLine{
        margin: 0 10px;
        height: 1px;
        border-top: 1px solid transparent;
        border-bottom: 1px solid #808797;
    }
    #containerNav .brand {
        font-size: 21px;
        color: white;
        padding: 11px 0;
        padding-left: 29px;
        background: #4c8be4;
    }
    #containerNav .timeConfig {
        position: relative;
        color: white;
        padding: 15px 15px;
        background: #4c8be4;
    }
    #containerNav label{
        position: absolute;
        right: 15px;
        top: 18px;
        cursor: pointer;
        transform: scale(0.7);
    }
    
    #containerNav #picker {
        /*color: #c6d5f3;*/
        background: transparent;
        outline: none;
        box-shadow: none;
        border: none;
        width: 100%;
        cursor: pointer;
        /*padding-left: 30px;*/
    }
    #containerNav .areaLabel{
        color: #4C8BE4;
        font-size: 14px;
        padding: 5px 15px;
        background: #323A46;
    }
    #containerNav .moduleItem {
        padding: 16px 0;
        cursor: pointer;
        padding-left: 60px;
        color:#808797;
        position: relative;
        border-left:3px solid transparent;
    }
    #containerNav .moduleItem.active,
    #containerNav .moduleItem:hover{
        color:#c6d5f3;
        background:#1e232b;
        border-left:3px solid #4c8be4;
    }
    #containerNav .moduleItem .icon{
        line-height: 18px;
        font-size: 18px;
        position: absolute;
        left: 27px;
        top: 18px;
    }
    #containerNav .wrapTagTree{
        position:relative;
    }
    #containerNav .tagSearch{
        position: absolute;
        top: 0;
        right: 5%;
        height: 30px;
        padding: 5px;
        width: 60%;
        color: #ccc;
    }
    #containerNav .iptTagSearch{
        height: 100%;
        width: 100%;
        border-radius: 5px;
        outline: none;
        border: 1px rgba(255,255,255,.3) solid;
        box-shadow: none;
        padding: 0 10px;
        background: transparent;
    }
    #containerNav .iptTagSearch:focus{
        box-shadow:0 0 1px 1px white;
    }
    #containerNav .btnTagSearch{
        position: absolute;
        right: 12px;
        top: 5px;
        line-height: 20px;
        cursor: pointer;
        font-size: 12px;
        color: rgba(255,255,255,.3);
    }
    #containerNav .tagTree{
        height: calc(100% - 30px);
        overflow: auto;
        /*border-top: 1px solid #242932;
        padding-top: 10px;
        margin-top: 10px;*/
    }
    #containerNav .tagTree>.ztree{
        padding:0;
    }
    #containerNav .tagTree .itemList li{
        position: relative;
    }
    #containerNav .tagTree .itemList>li>a>.node_name{
        padding-left:15px;
        font-size:14px;
    }
    #containerNav .tagTree .itemList li span.button.switch{
        width: auto;
        height: auto;
        position: absolute;
        right: 14px;
        top: 10px;
        z-index: 999;
        color: #cacaca;
        transition: all 0.5s;
    }
    #containerNav .tagTree .node_name{
        line-height: 14px;
        font-size: 12px;
        /*color: #c6d5f3;*/
        padding-left: 8px;
        display: inline-block;
        width: calc(100% - 24px);
        overflow: hidden;
        text-overflow: ellipsis;
        vertical-align: middle;
    }
    #containerNav .tagTree .itemList li a{

        cursor: pointer;
        background-color: transparent;
        text-decoration: none;
        vertical-align: top;
        display: inline-block;


        width: 100%;
        height: 36px;
        line-height: 36px;
        font-size: 12px;
        color: #808797;
        padding: 0 20px 0 12.25px;
        /*border-bottom: 1px solid rgba(88,88,88,.1);*/
        /*transition: all 0.5s;*/
        border-left: none;
        border-right: none;
        border-top: none;
    }

    #containerNav .tagTree .itemList li ul{
        padding:0;
    }
    #containerNav .tagTree .itemList li li>a{
        padding-left:45px;
    }
    #containerNav .tagTree .itemList li li li>a{
        padding-left:63px;
    }
    #containerNav .tagTree .itemList li>a{
        padding: 0 30px;
    }
    #containerNav .tagTree .level0[treenode_a]>[treenode_ico]::before{
        content: "\e881";
    }
    #containerNav .tagTree .no-icon{
        display:none;
    }
    #containerNav .tagTree .itemList li a.curSelectedNode,
    #containerNav .tagTree .itemList li a:hover{
        background: #1e232b;
        color: #c6d5f3;
        border: none;
        opacity: 1;
        border-bottom: 1px solid rgba(88,88,88,.1);
    }

    #containerNav .tagTree .itemList li a span.iconfont, 
    #containerNav .tagTree .itemList li span.button.noline_open, 
    #containerNav .tagTree .itemList li span.button.noline_close{
        width: auto;
        height: auto;
        font-size: 18px;
        line-height: 1;
        /*color: #c6d5f3;*/
        background: none;
        font-family: 'Glyphicons Halflings';
    }
    /*#containerNav .tagTree .itemList li span.button.noline_close{
        display:none;
    }*/
    #containerNav .tagTree .itemList li span.button.noline_open, 
    #containerNav .tagTree .itemList li span.button.noline_close{
        font-size: 12px;
        transform: scale(0.75);
        margin-top: 4px;
    }
    #containerNav .tagTree .itemList li span.button.noline_open::before{
        content: "\e260";
    }
    #containerNav .tagTree .itemList li span.button.noline_close::before{
        content: "\e259";
    }
    #containerNav .tagTree .code_name{
        display: inline-block;
        width: 100%;
        text-overflow: ellipsis;
        overflow: hidden;
        vertical-align: middle;
    }
    #containerDisplayboard {
        position: absolute;
        right: 0;
        top: 0;
        width: calc(100% - 240px);
        height: 100%;
    }
    @media screen and (max-width:1400px){
        #containerNav .moduleItem {
            padding: 10px 0;
            padding-left:60px;
        }
        #containerNav .moduleItem .icon{
            top: 12px;
        }
        #containerNav .tagTree {
            /*height: calc(100% - 315px);*/
        }
    }
    /*.wrapNavToggle{
        overflow:hidden;
        transition:height 0.3s;
