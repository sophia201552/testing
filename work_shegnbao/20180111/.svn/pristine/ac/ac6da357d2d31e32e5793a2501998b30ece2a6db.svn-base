<style>
    #createTagsModal .modal-dialog {
        margin-top: 100px;
        width: 400px;
    }

    #tag_rule_edit .modal-content {
        top: 90px;
    }

    #tag_rule_edit .edit-content {
        padding: 20px;
    }

    #tag_rule_edit .ruleTypeBox {
        margin-bottom: 15px;
        overflow: hidden;
    }

    #tag_rule_edit .radio-rule {
        padding-top: 10px;
    }

    #tag_rule_edit .rule-type {
        padding: 5px 8px;
        height: 30px;
        border: 1px solid #CCCCCC;
        cursor: pointer;
        border-right: none;
    }

    #tag_rule_edit .rule-type:first-child {
        border-radius: 6px 0 0 6px;
    }

    #tag_rule_edit .rule-type:last-child {
        border-right: 1px solid #CCCCCC;
        border-radius: 0 6px 6px 0;
    }

    #tag_rule_edit .rule-type:hover {
        color: #0077ee;
    }

    #tag_rule_edit .rule-type.active {
        color: #ffffff;
        background-color: #0077ee;
        border-color: #0077ee;
    }

    #tag_rule_edit .detail-rule {
        margin-left: 9px;
        line-height: 25px;
        color: #ff9600;
        margin-top: 12px;
    }

    #tagCreateBox {
        background-color: #ffffff;
    }

    #tagCreateBox h3 {
        color: #333333;
        font-size: 28px;
        font-family: "Microsoft YaHei", "微软雅黑", "Helvetica", "arial", "sans-serif";
    }

    #dmNext {
        background-color: #0077ee;
        color: #ffffff;
        padding-left: 12px;
        padding-right: 12px;
    }

    #tagLoadingBox .spinIcon {
        width: 90px;
        height: 90px;
        margin: 0 auto 40px;
        line-height: 90px;
        background-color: #6ebefa;
        color: #ffffff;
        border-radius: 8px;
    }

    @-webkit-keyframes loadRotate {
        from {
            -webkit-transform: rotate(0deg)
        }
        to {
            -webkit-transform: rotate(360deg)
        }
    }

    @keyframes loadRotate {
        from {
            transform: rotate(0deg)
        }
        to {
            transform: rotate(360deg)
        }
    }

    #tagLoadingBox .spinIcon img {
        font-size: 50px;
        line-height: 90px;
        -webkit-animation: loadRotate 1.2s linear 0s normal none infinite;
        animation: loadRotate 1.2s linear 0s normal none infinite;
    }

    #tagLoadingBox h3 {
        color: #333333;
        font-size: 28px;
        font-family: "Microsoft YaHei", "微软雅黑", "Helvetica", "arial", "sans-serif";
    }

    #dmCancel {
        font-size: 16px;
        padding: 6px 35px;
        background-color: #ffffff;
        font-family: "Microsoft YaHei", "微软雅黑", "Helvetica", "arial", "sans-serif";
        outline: none;
    }

    #ruleTextBox .control-label {
        width: 10%;
        padding: 0;
    }

    #ruleTextBox .ruleText {
        padding: 0;
        width: 88%;
    }

    #ruleTextBox .ruleCondition {
        display: inline-block;
        border: 1px solid #fcb813;
        opacity: 0.8;
        padding: 3px 12px;
        border-radius: 4px;
    }

    #ruleTextBox .ruleCondition:not(:first-child) {
        margin-left: 15px;
    }

    #ruleForm .btn {
        outline: none;
    }

    #autoIdentifySave {
        padding: 3px 5px;
    }

    #tagEditBox .tag-right-box {
        width: 320px;
    }

    #rule-content h4 {
        font-size: 16px;
        padding-left: 40px;
    }

    #ruleUl {
        margin-top: 5px;
    }

    #tagEditBox .tagEditCenterBox {
        overflow-y: auto;
        max-height: 845px;
    }

    #ruleForm .rule-basic-info label {
        font-weight: normal;
        font-size: 16px;
        vertical-align: middle;
    }

    #markMode-folder-table .widget-sdt-footer {
        padding-bottom: 20px !important;
    }

    #markMode-folder-table .table-header table th,
    #tagMode-table .table-header table th,
    #ruleTable .table-header table th {
        background: #dfdfe0;
        border: 1px solid #ccc;
        border-bottom: none;
    }

    #markMode-folder-table table tbody tr:nth-of-type(even),
    #tagMode-table table tbody tr:nth-of-type(even),
    #ruleTable table tbody tr:nth-of-type(even) {
        background-color: #f9f9f9;
    }

    #markMode-folder-table table tr td:last-child .ellipsis,
    #tagMode-table table tr td:last-child .ellipsis,
    #ruleTable table tr td:last-child .ellipsis {
        text-overflow: inherit;
        overflow: visible;
        white-space: normal;
    }

    .rule-table {
        padding: 0 5px;
        margin: 10px 20px 0;
    }

    #ruleTableBox .rule-text {
        padding-top: 10px;
        border-top: 1px solid #eee;
    }

    #markMode-folder-table .removeTag,
    #ruleTableBox .removeTag {
        color: red;
        position: absolute;
        right: -5px;
        top: -5px;
        z-index: 1;
    }

    #markMode-folder-table .tag-item-box:hover .removeTag {
        display: inline-block;
    }

    #tagEditContent .table-body.gray-scrollbar {
        height: 71vh !important;
    }

    #ruleTable .table-footer.widget-sdt-footer {
        padding-top: 35px;
    }

    .rule-title .searchPointInputBox {
        position: relative;
        right: 19px;
        width: 200px;
        margin-top: -5px;
    }

    .rule-title .searchPointIcon {
        position: absolute;
        right: 10px;
        top: 7px;
        font-size: 12px;
    }

    #isCreateSubFolder {
        padding-left: 8px;
        width: 80px;
        padding-right: 5px;
    }

    #rule-pass-btn-box .notPassedNumBox {
        color: rgb(255, 139, 3);
    }

    #ruleTable tbody tr.move, #keyword-list-table tbody tr.move {
        cursor: move;
    }

    #markMode-folder-table table tbody tr .editTag {
        position: relative;
        top: 2px;
    }

    #modeType-content {
        overflow-y: auto;
        max-height: 845px;
    }

    #showNodeTag {
        margin: 0 0 10px 0;
        position: relative;
    }

    #showNodeTag .col-md-10 {
        width: 88%;
    }

    #allTags {
        list-style: none;
        margin-bottom: 0;
    }

    .pd0 {
        padding: 0;
    }

    #allTags li {
        float: left;
        white-space: nowrap;
        position: relative;
        margin: 0 5px 5px 10px;
        display: inline-block;
        height: 20px;
        padding: 0 10px;
        background: rgb(47, 166, 242);
        color: #ffffff;
        line-height: 21px;
        font-size: 12px;
    }

    #allTags li:before {
        position: absolute;
        background: rgb(47, 166, 242);
        content: "\25CF";
        color: white;
        font-size: 12px;
        line-height: 12px;
        text-indent: 3px;
        top: 3px;
        left: -7px;
        width: 14px;
        height: 14px;
        transform: rotate(45deg);
        border-left: 1px solid rgb(47, 166, 242);
        border-bottom: 1px solid rgb(47, 166, 242);
        border-bottom-left-radius: 3px;
    }

    #allTags li:after {
        position: absolute;
        content: "";
        width: 0;
        height: 0;
        right: 0;
        top: 0;
        border-top: 10px solid transparent;
        border-right: 5px solid #F3F3F3;
        border-bottom: 10px solid transparent;
    }

    #allTags li .deleteTag {
        color: red;
        position: absolute;
        right: -5px;
        top: -5px;
        z-index: 1;
    }

    #allTags li:hover .deleteTag {
        display: block;
    }

    #modeType-content .widget-sdt-container .table-body {
        height: 66vh;
    }

    @media (max-width: 1366px) {
        #tagEditContent.col-sm-8 {
            width: 62%;
        }

        #ruleBtns {
            margin-top: 0;
        }

        #tagEditBox .tag-right-box {
            width: 270px;
        }

        #rule-content h4 {
            font-size: 14px;
            display: inline-block;
            vertical-align: top;
        }

        #ruleUl {
            padding-left: 15px;
            margin-bottom: 0;
        }

        #tagEditBox .tagEditCenterBox {
            max-height: 562px;
        }

        #ruleForm .rule-basic-info label {
            font-size: 14px;
        }

        #ruleTextBox .control-label {
            width: 14%;
        }

        #ruleTextBox #testCondition {
            width: 86%;
        }

        #tagEditContent .table-body.gray-scrollbar {
            height: 65vh !important;
        }

        #ruleTable .widget-sdt-container {
            font-size: 13px !important;
        }

        #isCreateSubFolder {
            padding-left: 5px;
            width: 60px;
        }

        #modeType-content {
            max-height: 562px;
        }

        #modeType-content .widget-sdt-container .table-body {
            height: 60vh;
        }
    }
</style>

