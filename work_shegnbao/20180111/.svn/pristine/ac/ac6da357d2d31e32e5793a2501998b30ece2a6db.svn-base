<script type="text/html" id="tpl_tag_modeEdit">
    <div class="clearfix editTypeBox">
        <div class="fl editType selectType cp" i18n="tag.inspect.DEVICE_PARAMETERS"></div>
    </div>
    <div id="modeEdit-content" class="gray-scrollbar">
