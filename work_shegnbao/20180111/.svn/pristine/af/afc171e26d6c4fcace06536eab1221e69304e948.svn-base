.ctn {
  width: 100%;
  height: 100%;
  background: #f5f8f9;
}
.dropDwonCtn {
  width: 150px;
  padding: 16px;
  height: 48px;
}
.dropDwonCtn > div > div span {
  line-height: 24px;
}
.dropDwonCtn > div > div > span:first-child {
  height: 24px;
  border-radius: 4px;
}
.chartWrap {
  width: 100%;
  height: 360px;
}
.footTable {
  height: calc(100% - 408px);
  width: 100%;
  background: rgba(235, 238, 247, 0.7);
  padding: 15px 20px;
  overflow-y: auto;
  overflow-x: hidden;
}
.footTable table {
  width: 100%;
  height: 100%;
  border-collapse: collapse;
  border: 1px solid rgba(153, 153, 153, 0.15);
}
.footTable thead {
  background: #f5f8f9;
  color: rgba(102, 102, 102, 0.7);
}
.footTable th,
.footTable td {
  text-align: center;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  border-right: 1px solid rgba(153, 153, 153, 0.15);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
}
.footTable th:last-child,
.footTable td:last-child {
  border-right: none;
}
.footTable td {
  color: #333333;
}
.footTable td:firstchild {
  width: 200px;
}
.footTable td span {
  display: inline-block;
  height: 100%;
  border-right: 1px solid rgba(153, 153, 153, 0.15);
}
.footTable td span:last-child{
  border-right: none;
}
.footTable td span:global(.highLight) {
  background: #f1b3be;
  border: none;
}
