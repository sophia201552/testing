<style>
    #tagSearchBox {
        padding: 9px 10px 8px;
        width: 100%;
        border-bottom: 1px solid #eae8ea;
    }

    #tagSearchBox .iconPosition {
        position: absolute;
        top: 15px;
        font-size: 13px;
        right: 26px;
    }

    #tagSearchBox .tagTitle {
        display: inline-block;
        float: left;
        padding-top: 8px;
        color: black;
        font-weight: bold;
    }

    #tagSearchBox input {
        display: inline-block;
        height: 28px;
        padding-right: 26px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    #tagGroup .btn-group {
        width: 100%;
    }

    #tagGroup button {
        width: 43%;
        padding: 3px 8px;
        background: #ffffff;
        outline: none;
        border-radius: 3px;
        text-overflow: ellipsis;
        overflow: hidden;
    }

    #tagGroup button:last-child {
        margin-left: 15px;
    }

    #tagGroup button:hover {
        background: #ffffff;
    }

    #tagGroup button.active {
        color: #ffffff;
        background: #29bb4f;
        border-color: #29bb4f;
    }

    #tagContent {
        width: 100%;
        height: 82vh;
        user-select: none;
        position: relative;
    }

    #tagContent ul {
        list-style: none;
        padding-left: 16px;
        overflow: hidden;
        margin-bottom:3px;
    }

    #tagContent ul li.selectedColor {
        color: #ff9600;
    }

    #tagContent ul li {
        float: left;
        width: 68px;
        height: 26px;
        display: block;
        background: #dedfe0;
        margin: 6px;
        text-align: center;
        line-height: 28px;
        border-radius: 12px;
        color: #333333;
    }

    #tagContent .tag-item-name {
        display: inline-block;
        width: 60px;
        margin: 0 auto;
    }

    #tagGroup {
        padding: 10px;
        border-bottom: 1px solid #eae8ea;
    }

    #tagContent .common {
        padding: 10px;
    }

    #tagContent .tagType {
        padding: 10px;
        border-bottom: 1px solid #eae8ea;
    }

    #tagContent .tagType span:nth-child(1) {
        display: inline-block;
        float: left;
        position: relative;
        left: 12px;
    }

    #tagContent .tagType .tagOpen {
        float: right;
        width: 10px;
        height: 10px;
        border-right: 2px solid #999;
        border-bottom: 2px solid #999;
        transform: rotate(-45deg);
        position: relative;
        top: 5px;
        right: 12px;
    }

    #tagContent .tagTypeBox.active .tagType .tagOpen {
        transform: rotate(45deg);

    }

    #tagContent .tagTypeBox .tagTypeContent {
        display: none;
    }

    #tagContent .tagTypeBox.active .tagTypeContent {
        display: block;
    }

    #tagContent .tagTypeContent {
        border-bottom: 1px solid #eae8ea;
    }

    #tagContent .tag-item {
        cursor: pointer;
    }

    #tagContent .tag-item i {
        margin-right: 5px;
    }

    #tagContent .tagCommon {
        padding: 10px;
    }

    #sortTag, #commonTag {
        width: 100%;
        height: 100%;
        overflow: auto;
        position: relative;
    }

    #latelyUseTag{
        width:100%;
        position: absolute;
        top: 0;
        z-index: 100;
        background: #F3F3F3;
    }

    @media (max-width: 1366px) {
        #tagGroup button {
            padding: 2px;
            font-size: 13px;
        }

        #tagGroup button:last-child {
            margin-left: 5px;
        }

        #tagContent {
            max-height: 74vh;
        }

        #tagContent ul li {
            width: 70px;
            height: 22px;
            line-height: 23px;
            margin-bottom: 0;
        }

        #tagContent .tagType {
            padding-top: 6px;
            padding-bottom: 6px;
        }
    }

</style>
<div id="tagContainer">
    <div id="tagSearchBox" class="clearfix">
        <span class="icon iconfont iconPosition cp" id="tagSearchIcon">&#xe713;</span>
        <input type="text" class="form-control" id="tagSearchValue"
               i18n="placeholder=tag.inspect.ENTER_SEARCH_CONTENT"/>
    </div>
    <div id="tagGroup">
        <div class="btn-group">
            <button type="button" class="btn btn-default active" id="tagByType"
                    i18n="title=tag.inspect.CLASSIFICATION">
                <span  i18n="tag.inspect.CLASSIFICATION"></span>
            </button>
            <button type="button" class="btn btn-default" id="tagByCommon"
                    i18n="title=tag.inspect.COMMON">
                <span  i18n="tag.inspect.COMMON"></span>
            </button>
        </div>
    </div>
    <div id="tagContent">
        <div id="sortTag" class="clearfix gray-scrollbar"></div>
        <div id="commonTag" class="clearfix gray-scrollbar"></div>
    </div>
</div>
<!--@formatter:off-->
<script type="text/html" id="tpl_show_Tag_by_type">
    <!if(latelyUseTag && Object.keys(latelyUseTag).length){ !>
        <div id="latelyUseTag">
            <div class="tagTypeBox active">
                <div class="tagType clearfix">   
                    <span class="tag-text" style="background:#fcb813;" i18n="tag.inspect.RECENTLY"></span>
                    <div class="tagOpen"></div>
                </div>
                <div class="tagTypeContent clearfix">
                    <ul class="clearfix">
                        <! var language = localStorage.getItem('language') == 'zh' ? 'zh' : 'en'; !>
                        <! for(var key in latelyUseTag){!>
                            <li class="tag-item" tag="<!=latelyUseTag[key].name!>" title="<!=latelyUseTag[key][language]!>">
                                <span class="ellipsis tag-item-name">
                                    <!if(latelyUseTag[key].icon){!>
                                        <i class="icon iconfont <!=latelyUseTag[key].icon!>"></i>
                                    <!}!>
                                    <!=latelyUseTag[key].name!>
                                </span>
                            </li>
                        <!}!>
                    </ul>
                </div>
            </div>
        </div>
    <!}!>
    <!for(var i = 0,iLen = list.length;i < iLen;i++){!>
        <!var groupTag = list[i];!>
        <div class="tagTypeBox oldTagTyle" type="<!=groupTag.groupNm!>">
            <div class="tagType clearfix">
                <!var groupNm = I18n.resource.tag.TAG_TYPE[groupTag.groupNm.toUpperCase()] ? I18n.resource.tag.TAG_TYPE[groupTag.groupNm.toUpperCase()] : groupTag.groupNm;!>
                <!if(groupTag.groupNm == 'Other'){!>
                    <span class="tag-text" style="background:#00bbc3;"><!=groupNm!></span>
                <!}else{!>
                    <span class="tag-text"><!=groupNm!></span>
                <!}!>
                <div class="tagOpen"></div>
            </div>
            <div class="tagTypeContent clearfix">
                <ul class="clearfix">
                    <! var language = localStorage.getItem('language') == 'zh' ? 'zh' : 'en'; !>
                    <! for(var j = 0,jLen = groupTag.tags.length;j < jLen;j++){!>
                        <li class="tag-item" tag="<!=groupTag.tags[j].name!>" title="<!=groupTag.tags[j][language]!>">
                            <span class="ellipsis tag-item-name">
                                <!if(groupTag.tags[j].icon){!>
                                    <i class="icon iconfont <!=groupTag.tags[j].icon!>"></i>
                                <!}!>
                                <!=groupTag.tags[j].name!>
                            </span>
                        </li>
                    <!}!>
                </ul>
            </div>
        </div>
    <!}!>
</script>
<script type="text/html" id="tpl_show_Tag_by_Common">
    <div class="tagCommon">
        <span><!=I18n.resource.tag.inspect.COMMON_TAG!></span>
    </div>
    <ul>
        <!var dict = beop.tag.panel.getTagMap();!>

        <! var language = localStorage.getItem('language') == 'zh' ? 'zh' : 'en'; !>
        <!for(var i = 0,iLen = list.length;i < iLen;i++){!>
            <!
                if(!list[i] || !list[i].name){
                    continue;
                }
                var displayNm = '';
                var tag = '';
                var tagTitle = '';
                var tagItem = list[i].name.trim().toUpperCase();
                if(list[i].name && dict[tagItem]){
                    displayNm = dict[tagItem].name;
                    tag = dict[tagItem].name;
                    tagTitle = dict[tagItem][language];
                }else{
                    tag = displayNm = list[i].name;
                }
            !>
            <li class="tag-item" tag="<!=tag!>" title="<!=tagTitle!>">
                <span class="ellipsis tag-item-name">
                    <!if(list[i].icon){!>
                        <i class="icon iconfont <!=list[i].icon!>"></i>
                    <!}!>
                    <!=displayNm!>
                </span>
            </li>
        <!}!>
    </ul>
</script>
<!--@formatter:on-->