/* SearchInput start */
.searchInputWrap {
  height: 42px;
  width: 100%;
  padding: 8px;
  color: rgb(110, 119, 150);
}
.searchInputWrap .searchLabel {
  width: 40px;
  text-align: center;
  height: 26px;
  line-height: 26px;
  display: inline-block;
  vertical-align: top;
}
.searchInputWrap .searchInput {
  display: inline-block;
  width: calc(100% - 40px);
  height: 26px;
  background-color: rgb(25, 31, 55);
  border-radius: 4px;
  overflow: hidden;
}
.searchInputWrap .searchInput > * {
  border: none;
  background-color: transparent;
  color: rgb(110, 119, 150);
}
.searchInputWrap .searchInput > *:focus {
  outline: none;
}
.searchInputWrap .searchInput button {
  height: 26px;
  width: 26px;
  cursor: pointer;
}
.searchInputWrap .searchInput input {
  height: 26px;
  line-height: 26px;
  width: calc(100% - 26px);
  padding: 4px 0 4px 8px;
}
.searchInputWrap .searchInput input::-moz-placeholder {
  color: rgb(110, 119, 150);
}
.searchInputWrap .searchInput input::-webkit-input-placeholder {
  color: rgb(110, 119, 150);
}
/* SearchInput end */

/* skin whiteSkin*/
.searchInputWrap.whiteSkin .searchInput{
  background: transparent;
  border: 1px solid rgb(110, 119, 150);
}
/* skin whiteSkin end */
