.container {
  height: 100%;
  background-color: rgb(245, 248, 249);
}
.leftWrap {
  float: left;
  width: calc(100% - 300px);
  height: 100%;
  padding: 0 20px;
}
.rightWrap {
  float: left;
  width: 300px;
  border-left: 1px solid rgba(153, 153, 153, 0.3);
  height: calc(100% - 60px);
}

/* 头部导航栏 */

.toolsWrap {
  margin: 24px 0;
}
.fuzzy_text {
  float: left;
  line-height: 20px;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
}
.tools {
  float: right;
}
div.toolItem {
  display: flex;
  align-items: center;
  cursor: pointer;
  float: left;
  color: #666;
  font-weight: 500;
}
div.toolItem:first-child {
  margin-left: 12px;
}
.icon-line {
  float: left;
  width: 1px;
  height: 12px;
  opacity: 0.2;
  border-radius: 0.5px;
  background-color: #6d92db;
  margin: 2px 12px 0;
}

.toolItem > i {
  font-size: 16px;
  margin-right: 2px;
}
.toolItem > span {
  line-height: 16px;
}
.container .list {
  width: 100%;
  height: calc(100% - 100px);
  border-radius: 4px;
  background: #fff;
  box-shadow: 0px 4px 8px 0 rgba(216, 224, 237, 0.6);
  overflow-y: auto;
}

/* list页面 */

.other_config {
  height: 26px;
  line-height: 26px;
  margin-top: 8px;
  border-radius: 4px;
  background-image: linear-gradient(
    to right,
    rgb(91, 136, 247),
    rgb(77, 116, 210)
  );
  text-align: center;
  display: inline-block;
  color: #fff;
  font-size: 14px;
  text-overflow: ellipsis;
  overflow: hidden;
  padding: 0 8px;
}
.fuzzy_view {
  width: calc(100% - 12px);
  height: 16px;
  line-height: 16px;
  margin-top: 11px;
  overflow: hidden;
  background-color: rgba(0, 0, 0, 0.58);
}

.container :global(.ms-DetailsHeader) {
  height: 40px;
  line-height: 40px;
  background-color: #fff;
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
}
.container :global(.ms-DetailsHeader) > div {
  height: 40px;
  color: #999;
  font-weight: 700;
  font-size: 14px;
  text-align: left;
}
.container
  :global(.ms-DetailsHeader)
  > div:global(.ms-DetailsHeader-cell:hover) {
  background-color: #fff;
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
}
.container :global(.ms-DetailsList) > div {
  overflow-x: hidden;
  line-height: 2;
}
.container :global(.ms-DetailsHeader) > div > span > button {
  opacity: 1;
}
.container :global(.ms-DetailsHeader:hover) > div {
  color: #999;
}
.container :global(.ms-DetailsHeader) > div > span > span {
  padding: 0;
}
.container :global(.ms-DetailsHeader) > div:active {
  background-color: #fff !important;
}
.container :global(.ms-List-page) > div > div,
.container :global(.ms-List-page) > div > div:before {
  background-color: #fff;
  border: none;
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  height: 40px;
  color: #333;
  font-size: 14px;
}
.container :global(.ms-List-page) > div > div:before {
  display: none;
}
.container :global(.ms-List-page) > div:hover > div {
  background-color: #f1f4f5;
  background-color: rgb(235, 238, 247);
  position: relative;
}
.container :global(.ms-List-page) > div:hover > div::after {
  position: absolute;
  top: 0;
  left: 0;
  content: '';
  display: block;
  width: 3px;
  height: 100%;
  background-color: rgb(16, 184, 249);
  box-shadow: 0px 4px 10px 0 rgba(16, 184, 249, 0.3);
}

.container :global(.ms-List-page) > div > div > div > div {
  padding: 0;
  line-height: 40px;
}
.container :global(.ms-List-page) > div div[aria-colindex='2'] {
  padding-right: 40px;
}

@media (min-width: 1608px) {
  .container :global(.ms-DetailsList) > div > div div[aria-colindex='2'] {
    width: 500px !important;
    padding-right: 100px;
  }
  .container :global(.ms-DetailsList) > div > div div[aria-colindex='1'],
  .container :global(.ms-DetailsList) > div > div div[aria-colindex='3'] {
    width: 270px !important;
  }
}

.container :global(.ms-DetailsList) i:global(.ms-Check-circle) {
  opacity: 0;
}
.container :global(.ms-DetailsList) div:global(.ms-Check::before) {
  border-radius: 0;
  width: 12px;
  height: 12px;
  border-radius: 2px;
  border: solid 1px rgba(130, 144, 179, 0.4);
}
.container :global(.ms-DetailsList) div:global(.ms-Check.is-checked::before) {
  background-color: #4c84ff;
}
.container :global(.ms-DetailsList) i:global(.ms-Check-check) {
  left: -0.8px;
  font-size: 18px;
  line-height: 14px;
}
.container :global(.ms-DetailsHeader) button:focus i,
.container :global(.ms-DetailsRow) button:focus i {
  color: #fff !important;
}
/* 按钮样式 */
.toolBtn {
  width: 88px;
  height: 25px;
  line-height: 25px;
  margin-left: 16px;
  border-radius: 4px;
  float: right;
  color: #fff;
  background-color: #5b8af7;
  text-align: center;
  outline: none;
  border: none;
  cursor: pointer;
}

/* 下拉框样式 */
.Dropdown {
  width: 94px;
  border-radius: 2px;
}
.Dropdown:global(.is-open) {
  border-radius: 2px;
  height: 24px;
}
.Dropdown > span:first-child {
  height: 24px;
  line-height: 20px;
  font-size: 12px;
  letter-spacing: 0;
}
.Dropdown > span:first-child {
  border-radius: 2px;
  border-color: #eee;
}
.Dropdown:global(.isOpen) > span:first-child,
.Dropdown:hover > span:first-child,
.Dropdown:focus > span:first-child,
.formWrap .listWrap .listName:focus input,
.formWrap .listWrap .listName:hover input {
  border-color: #5b8af7;
  color: #5b8af7;
}
.Dropdown > span:last-child {
  height: 24px;
  line-height: 20px;
  right: 4px;
}

/* 下拉框弹出的 cllout 高度限制 */
div:global(.ms-Dropdown-items) {
  max-height: 300px;
  overflow-y: auto;
}
/* 诊断项 form 表单样式 */
.formWrap {
  width: 100%;
  height: 100%;
  padding: 12px 10px;
  box-sizing: border-box;
  position: relative;
  z-index: 1;
  background: #f5f8f9;
}
.formWrap .listWrap {
  margin-bottom: 12px;
}
.listWrap > div:first-child {
  margin-right: 10px;
}
.formWrap .listWrap > div {
  float: left;
  width: calc((100% - 10px)/2);
}
.formWrap .listWrap .listName input {
  width: 100%;
  height: 24px;
  line-height: 22px;
  border-radius: 4px;
  padding-left: 8px;
  border: 1px solid #eee;
  outline: none;
}
.formWrap .listWrap > div > label {
  line-height: 24px;
  color: #999;
}
.formWrap .listWrap > div > span {
  font-size: 10px;
  color: #f04134;
}
.formWrap .listWrap > .status > label {
  float: left;
}
.formWrap .listWrap > .status > div {
  float: left;
  margin-left: 10px;
}

.formWrap .listWrap .Dropdown {
  width: 100%;
}
.formWrap .settings {
  margin-top: 40px;
}
/* addModal start*/
.addModal {
  padding: 15px;
}
.modal-title {
  line-height: 30px;
}
.modal-body {
  height: 40px;
}
.modal-footer {
  height: 60px;
  display: flex;
  align-items: center;
}
.modal-footer button {
  margin-left: 10px;
  border-radius: 4px;
}
.modal-body {
  display: flex;
  align-items: center;
}
.modal-body label {
  width: 50px;
}
.modal-body input {
  width: calc(100% - 50px);
}
/* addModal end*/

/* 导航栏样式 */
.rightWrap > ul {
  height: 40px;
}
.rightWrap li.item {
  float: left;
  width: 50%;
  height: 40px;
  overflow: hidden;
}
.rightWrap li.item > button {
  width: 100%;
  height: 100%;
  padding: 0 26px !important;
  color: #666 !important;
  text-align: center;
  background-color: rgba(153, 153, 153, 0.3) !important;
  border: none;
  outline: none;
  display: flex;
}
.rightWrap li.item > button > i {
  margin-right: 4px;
}
.rightWrap li.item:global(.selected) > button {
  background-color: transparent !important;
  color: #5b8af7 !important;
}
.rightWrap > div {
  height: calc(100% - 40px);
  overflow-y: auto;
}
