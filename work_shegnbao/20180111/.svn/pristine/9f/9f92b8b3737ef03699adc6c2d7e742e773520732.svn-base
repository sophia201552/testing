.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.dataComplementWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;

  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.showDataCtn {
  float: left;
  width: 82%;
  height: 100%;
}
.left > .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left .mWrap {
  width: 100%;
  height: calc(70% - 20px);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  padding: 24px 16px 0 16px;
}
.left .bWrap {
  width: 100%;
  height: 30%;
  padding: 16px 16px;
}
.left .mWrap .api2 label {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
  font-weight: 600
}
.left .mWrap .api2 > div {
  margin-top: 8px;
  position: relative;
}
.left .mWrap .api2 > div > div {
  position: absolute;
  right: 6px;
  top: 4px;
}
.left .mWrap .api2 > div > div i {
  display: block;
  cursor: pointer;
}
.left .mWrap .api2 input {
  font-size: 14px;
  height: 28px;
  width: 100%;
  border-radius: 4px;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
  padding: 4px 6px;
}
