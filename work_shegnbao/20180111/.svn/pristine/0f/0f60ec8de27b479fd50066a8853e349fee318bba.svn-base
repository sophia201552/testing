/* RenameText */
.RenameText{
  width: 100%;
  position: relative;
  cursor: pointer;
}
.RenameText .text{
  width: 100%;
  height: 20px;
  line-height: 20px;
  outline: 0;
  box-shadow: none;
  color: #333333;
  padding: 0 12px 0 12px;
  width: 100%;
  text-overflow: ellipsis;
  border: 1px solid #a6a6a6;
  background: #ffffff;
}
.RenameText .text:hover{
  border-color: #212121;
}
.RenameText .text:focus{
  border-color: #0078d7;
}
.RenameText .text.error{
  border-color: #a80000;
}
.RenameText .errorText{
  color: #a80000;
}
.RenameText .errorText.hide{
  display: none;
}
/* RenameText end */