.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.deduplicationWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;

  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.showDataCtn {
  float: left;
  width: 82%;
  height: 100%;
}
.left > .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left .mWrap {
  width: 100%;
  height: calc(70% - 20px);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  padding: 24px 16px 0 16px;
}
.inputWrap {
  position: relative;
}
.left .inputWrap > label.ChevronUp,
.left .inputWrap > label.ChevronDown {
  position: absolute;
  bottom: 0;
  right: 6px;
  bottom: 10px;
}
.left .inputWrap > label.ChevronDown{
  bottom: -2px;
}

.left .mWrap span {
  position: absolute;
  bottom: -16px;
  color: red;
  left: 4px;
  display: none;
}
.left .mWrap span:global(.errMsg) {
  display: block;  
}
.left .bWrap {
  width: 100%;
  height: 30%;
  padding: 16px 16px;
}
.left .mWrap label {
  font-size: 14px;
}
.left .mWrap input {
  font-size: 14px;
  margin-top: 8px;
  height: 28px;
  width: 100%;
  border-radius: 4px;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
  padding-left: 8px;
}