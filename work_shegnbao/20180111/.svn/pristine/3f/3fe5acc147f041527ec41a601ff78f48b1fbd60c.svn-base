/* ============================================= 团队begin =============================================*/
#wf_team_box {
    padding: 10px;
}

#wf_team_wrapper {
    height: 100%;
    overflow: auto;
}

.wf-add-tags {
    font-size: 15px;
    margin-right: 5px;
    cursor: pointer;
    margin-top: 9px;
}

.wf_team_wrapper .form-team {
    padding: 10px;
    overflow: hidden;
}

.wf_team_wrapper .wf-manager-people {
    padding: 10px;
    background: #ffffff;
    min-height: 75px;
}

.wf_team_wrapper .wf-manager {
    color: #0077ee;
    opacity: 0.7;
}

.wf_team_wrapper .people {
    float: left;
    margin: 0 30px 10px 0;
}

#team_structure .people {
    margin: 0 30px 10px 0;
}

#wf_process_item_box .people {
    float: left;
    margin: 0 10px 10px 0;
    width: 50px;
}

.wf_team_wrapper .people-name {
    width: 50px;
    height: 25px;
    line-height: 25px;
    text-align: center;
    font-size: 12px;
}

.wf_team_wrapper .user, .people img, #wf_process_item_box .people img {
    width: 50px;
    height: 50px;
}

.edit .wf-tag-wrapper {
    padding: 5px 25px;
}

.wf-tag-wrapper {
    padding: 5px 20px;
    margin: 0 5px;
    border: 1px solid #0077ee;
    background: #0077ee;
    color: #ffffff;
    position: relative;
    float: left;
    border-radius: 2px;
}

.add-person-to-process .people {
    float: left;
}

.no-join-box {
    height: 200px;
    text-align: center;
    margin-top: 20%;
}

.wf-delete, .wf-tag-delete {
    top: 7px;
    right: 0;
    display: block;
    height: 16px;
    width: 16px;
    font-size: 12px;
    text-align: center;
    line-height: 16px;
    cursor: pointer;
    z-index: 12;
    border-radius: 50px;
}

#wf_team_wrapper .people .wf-delete {
    color: #fff;
    background-color: #000;
    display: none;
}

#wf_team_wrapper .people:hover .wf-delete {
    display: block;
}

/*.wf-user-name-box:hover .wf-group-name-edit {
    display: inline-block;
}*/

.wf-name-editable {
    border: 1px solid #87cefa;
    display: inline-block;
    min-width: 10px;
}

.wf_team_process {
    padding: 10px;
    background-color: #ffffff;
}

@media (min-width: 768px) {
    .form-horizontal .control-label {
        padding-top: 0;
    }
}

.wf_team_process_ul {
    padding-left: 0;
}

.wf_team_process_ul li {
    padding: 8px;
}

.wf_team_process_ul li:after {
    content: "";
    display: block;
    font-size: 0;
    overflow: hidden;
    clear: both;
}

.wf_team_process_ul .wf_team_process:not(:last-child) {
    border-bottom: 1px solid #DCDCDC;
}

.wf_team_process_ul .glyphicon-arrow-right {
    margin: 0 5px;
}

.wf_team_process_ul .wf_team_process_title {
    width: 150px;
}

.wf_team_process_ul .wf_team_process_control {
    margin: 10px 0 0 10px;
}

.wf_team_process_ul .wf_team_process_control .glyphicon {
    cursor: pointer;
    margin: 0 5px;
}

.wf_process_box {
    margin: 0 auto;
    width: 400px;
}

.wf_process_box .modal-body {
    overflow: auto;
    height: 600px;
}

#wf_process_box .wf-add-process-item, #wf_process_box .wf-delete-process-item {
    font-size: 16px;
}

.wf-arrow-group {
    width: 280px;
}

.wf-line {
    width: 250px;
    height: 2px;
    background: rgb(100, 100, 100);
    overflow: hidden;
}

.wf-arrow-left-down {
    left: 7px;
    top: 0px;
}

.wf-arrow-right-down {
    left: -7px;
    top: 0px;
}

.wf_process_item_box {
    margin-top: 5px;
}

.wf_process_item {
    width: 355px;
    position: relative;
    margin: 5px auto 20px auto;
}

.wf_process_item_box .wf_process_item label {
    display: block;
    float: left;
}

.wf_process_item_box .glyphicon-arrow-right {
    margin: 5px 10px 0 10px;
}

#wf-outline #wf_process_item_box .wf_process_item .wf-team-people-add, #wf-outline #wf_process_item_box .wf_process_item .add-people-to-process {
    position: absolute;
    right: -15px;
    top: 20px;
    float: right;
    font-size: 16px;
}

#wf-outline #wf_process_item_box .wf_process_item .add-people-to-process {
    top: 45px;
}

#wf_team_wrapper .wf_process_item .wf-team-people-add {
    font-size: 16px;
}

#wf_team_wrapper .form-team .wf-team-people-add {
    font-size: 24px;
    margin-top: 12px;
    opacity: 0.4;
}

#wf_add_process_win .modal-dialog {
    margin-top: 50px;
}

#wf_add_process_win .modal-body {
    height: 80vh;
    overflow-x: hidden;
    overflow-y: auto;
}

#wf-outline #wf_add_process_win input, #wf-outline #wf_add_process_win select {
    background: #d8d9db !important;
    color: #333 !important;
    border: 1px solid #d8d9db !important;
}

#wf_team_tags_wrapper {
    float: left;
}

#wf_team_tags_wrapper .glyphicon-tag {
    display: none;
}

.wf_team_show_page #wf_team_tags_wrapper .glyphicon-tag {
    display: inline-block;
}

#wf_team_tags_wrapper .wf-tag-save {
    display: none;
}

#wf_team_tags_wrapper .wf-tag-edit, #wf_team_tags_wrapper .wf-tag-save {
    top: 7px;
    left: 5px;
    cursor: pointer;
}

.wf-tag-name {
    position: relative;
    left: 4px;
}

.wf-tag-name-editable {
    border-color: #87cefa;
}

#wf_process_nodes {
    display: none;
}

#wf_team_wrapper .col-sm-1 {
    text-align: right;
}

#wf_add_team {
    font-size: 30px;
    cursor: pointer;
}

.wf-added-user-list .col-sm-1 {
    width: 10%;
}

.wf-added-user-list .col-sm-11 {
    width: 90%;
}

.wf-added-user-list .form-team {
    padding-left: 0;
}

.wf-user-name-box {
    padding-bottom: 10px;
    padding-left: 0;
}

/* ----- 团队控制显示begin ----- */
.wf_team_show_page .wf-team-people-add, .wf_team_show_page .wf-group-name-edit, .wf_team_show_page #addTeamGroupText,
.wf_team_show_page #addTeamGroupInput, .wf_team_show_page .wf_team_process_edit, .wf_team_show_page .wf_team_process_remove,
.wf_team_show_page .wf_add_process_wrapper, #wfTeamAddSuperAdmin, .wf_member_type_admin .team-edit #wfTeamAddSuperAdmin,
.wf_member_type_common #wf_team_edit, .wf_member_type_admin #wfTeamAddAdmin, .wf_member_type_super_admin #wf_team_quite {
    display: none;
}

.wf_member_type_super_admin #delete_team, .team-create #wfTeamAddSuperAdmin, .team-edit #wfTeamAddSuperAdmin {
    display: inline-block;
}

/* ----- 团队控制显示end ----- */

.toggle-is-show {
    font-size: 14px;
}

.wf_team_process_item {
    position: relative;
    color: #000;
    float: left;
    margin-left: -18px;
    margin-top: 5px;
}

.color-info .chevron-content {
    background: #5bc0de;
    color: #ffffff;
}

.color-default .chevron-content {
    background: #737373;
    color: #ffffff;
}

.ionPosition {
    position: absolute;
    left: 75px;
    top: 24px;
    transform: rotate(90deg);
    font-size: 12px;
}

.item-peoples-box {
    text-align: center;
}

.mAuto {
    margin: 0 auto;
}

.noImplement .imgColor {
    -webkit-filter: grayscale(1);
}

.peopleName {
    display: inline-block;
    padding: 2px 5px;
    border-radius: 5px;
}

.fuLineBox {
    position: absolute;
    top: 12px;
    left: 76px;
}

.fuLine {
    display: inline-block;
    width: 30px;
    height: 1px;
    background: #18aa00;
}

.completed {
    text-align: center;
    line-height: 50px;
    color: #ffffff;
}

.color-success .completed {
    background: #5bc0de;
}

.color-default .completed {
    background: #737373;
}

.color-success .chevron-content {
    background: #5cb95c;
    color: #ffffff;
}

.color-danger .chevron-content {
    background: #c91623;
    color: #ccc;
}

.toggle-is-show.icon-xiazhankai + .toggle-is-show-target > div {
    display: block !important;
}

.toggle-is-show.icon-youzhankai + .toggle-is-show-target > div {
    display: none !important;
}

/* ============================================= 团队end =============================================*/

#taskTags #wf-labelNames {
    margin-top: 7px;
}

.wf-task-split {
    padding-top: 15px;
}

#taskProcess {
    margin: 0 0 20px -12px;
}

#taskContent .taskEditSection {
    min-height: 0;
}

#wf-detail-funUl {
    height: 40px;
    padding-left: 20px;
    border-bottom: 2px solid #ccc;
}

#wf-detail-funUl li {
    min-width: 100px;
    height: 40px;
    line-height: 40px;
    border-bottom: 2px solid #ccc;
    margin-left: 1px;
    float: left;
    cursor: pointer;
    text-align: center;
    background: #fff;
}

#wf-detail-funUl .active {
    color: #337ab7;
    border-bottom: 2px solid #337ab7;
}

/* 历史记录样式开始 */
time {
    margin: 0 12px;
}

.timeLine ul {
    margin-left: 10px;
}

.timeLine li {
    line-height: 25px;
    margin: 10px 0;
    overflow: hidden;
}

.recordtime {
    width: 220px;
    height: 30px;
    line-height: 30px;
    top: -2px;
}

.recordTime time {
    display: inline-block;
    position: relative;
    top: 3px;
}

.recordContent .contentTitle {
    display: inline-block;
    position: relative;
    left: 20px;
    top: 3px;
}

#wf-detail-verticalLine {
    display: block;
    width: 1px;
    background: chocolate;
    left: 15px;
    top: 45px;
    z-index: 0;
}

.timeLineIcon {
    z-index: 4;
}

.horizontalLine {
    display: block;
    top: 16px;
    left: 29px;
    width: 15px;
    height: 1px;
    background: chocolate;
}

#wf-detail-funWrapper {
    margin-top: 25px;
}

#wf-detail-funWrapper ul {
    list-style-type: none;
    padding-left: 0;
}

#wf-detail-progress {
    overflow: auto;
    position: relative;
    padding-top: 20px;
}

#wf-detail-progress img {
    width: 30px;
    height: 30px;
}

#wf-detail-progress .content p {
    margin-left: 175px;
}

#wf-detail-progress .historyEdit {
    margin-left: 40px;
}

#wf-detail-progress .historyEdit li {
    margin: 1px;
}

.peoples-other-box {
    width: 63px;
    margin: 5px 0;
    padding: 5px 0;
    background: #ccc;
    border: 1px solid #999;
    overflow: hidden;
}

#taskContent [class ^= 'col-md-'] {
    border: 0px solid #ccc;
}

#workflow-task .control-label {
    margin-top: 0;
}

#workflow-task .wf-task-detail {
    word-break: break-all;
    border: none;
}

#wf-labelNames {
    min-width: 100px;
}

#wf-outline .form-group {
    margin: 0 0 15px 0;
}

.wf-manager-people .people {
    width: 50px;
    height: 50px;
    border-radius: 4px;
    position: relative;
    float: left;
    margin: 0 5px 5px 5px;
}

.wf-manager-people .wf-people-add {
    border-radius: 50%;
    font-size: 25px;
    padding: 5px;
    margin-left: 5px;
    position: relative;
    left: 0;
    color: #aaa;
}

.wf_add_process_people {
    width: 92px;
    position: relative;
}

.wf_team_process_ul .teamPeople, #taskProcess .teamPeople {
    margin: 0 auto;
    width: 50px;
    height: 50px;
    border-radius: 4px;
    position: relative;
}

.teamPeople img {
    width: 100%;
    height: 100%;
}

.wf-manager-people .wf-people-add {
    font-size: 45px;
}

.wf-manager-people .wf-people-add:hover {
    color: #666;
}

.wf-manager-people .people-name {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.3);
    height: 20px;
    text-align: center;
    line-height: 20px;
    color: #fff;
    font-size: 13px;
}

.wf-attachment-nav {
    padding: 0;
    list-style-type: none;
    display: inline-block;
    margin-bottom: 0;
    vertical-align: top;
}

#wf-attachment-labelWrapper li {
    display: inline-block;
    margin: 0 10px;
}

#wf-attachment-labelWrapper li .files {
    min-width: 150px;
    min-height: 120px;
    text-align: center;
    border: 1px solid #ccc;
    box-sizing: border-box;
    position: relative;
    color: #fff;
}

#wf-attachment-labelWrapper li:hover .files > button {
    display: block;
}

#wf-attachment-labelWrapper .files .file-type {
    border-bottom: 1px solid #ccc;
    height: 80px;
    line-height: 80px;
    background-size: contain !important;
}

#wf-attachment-labelWrapper .files .file-type a {
    display: inline-block;
    width: 100%;
    height: 100%;
}

#wf-attachment-labelWrapper .files .file-info {
    display: block;
    font-size: 12px;
    height: 60px;
    top: 0px;
    color: #333;
}

#wf-attachment-labelWrapper .files > button {
    cursor: pointer;
    color: #ccc;
    font-size: 12px;
    padding: 2px;
    transition: .3s 0s all ease;
    outline: none;
    background: transparent;
    border: none;
    display: none;
    position: absolute;
    right: 0;
}

#wf-attachment-labelWrapper .files > button:nth-of-type(1):hover {
    text-decoration: none;
    color: #f00;
    background: transparent;
}

#wf-attachment-labelWrapper .files .file-info p {
    margin: 0;
    line-height: 20px;
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 150px;
    overflow: hidden;
    text-align: center;
}

#wf-attachment-labelWrapper .files .file-info .file-name {
    font-size: 12px;
}

#wf-attachment-labelWrapper .files .file-info .file-time {
    font-size: 90%;
}

#wf-attachment-labelWrapper .files .file-type i {
    font-size: 36px;
}

#wf-attachment-input-btn {
    position: relative;
    cursor: pointer;
}

#wf-attachment-input-btn:after {
    content: "";
    position: absolute;
    width: 30px;
    height: 30px;
    border: 2px solid #ccc;
    border-radius: 100%;
    left: -6px;
    top: 3px;
    -webkit-font-smoothing: subpixel-antialiased;
}

#wf-taskMine-content {
    padding-top: 21px;
}

.page-info {
    color: rgb(172, 175, 181);
    padding: 10px 5px;
}