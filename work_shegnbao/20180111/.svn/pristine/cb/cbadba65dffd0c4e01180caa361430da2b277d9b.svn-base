import pytestfrom beopWeb.mod_admin import controllersimport jsonglobal id@pytest.mark.p0@pytest.mark.usefixtures("deleteManagement")@pytest.mark.parametrize('data', [{"name_cn":"测试集团管理","name_en":"Test Management","code_name":"test management","phone":"1301234596"}])def test_create_management(data):    rt = controllers.do_create_management(data)    rt=json.loads(bytes.decode(rt.data))    global id    id=rt.get('data')    actual = controllers.get_management(id)    actual=(json.loads(bytes.decode(actual.data))).get('data')    data.update({'id':id})    for key in actual.keys():        assert actual.get(key)==data.get(key),' not equal .actual={0},expected={1}'.format(actual.get(key),data.get(key))@pytest.fixture(scope='function')def deleteManagement(request):    global id    def fin():        controllers.do_delete_management(id)    request.addfinalizer(fin)