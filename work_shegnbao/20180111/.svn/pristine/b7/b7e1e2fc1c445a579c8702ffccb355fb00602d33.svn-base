<style>
    .MeterContainer {
        width: 100%;
        height: 100%;
        background: #f1f4fb;
        overflow: hidden;
    }
    
    .divTimeSelected {
        display: flex;
        justify-content: space-between;
        align-items: center;
        height: 58px;
        line-height: 58px;
        width: 100%;
        background: #fff;
        border: 1px solid #dfe2e5;
        box-shadow: 0px 1px 10px #ddd;
        color: #354052;
    }
    
    .timeQuery {
        display: none;
        /*display: flex;*/
        padding-right: 8px;
    }
    
    .divTabModule {
        display: flex;
        height: 100%;
    }
    
    .ParameterOrTable {
        height: 100%;
        padding: 0px 40px;
        cursor: pointer;
    }
    
    .ParameterOrTable.cur {
        background: #f1f3f7;
        color: #3f7ee3;
    }
    
    .timeWrapper {
        display: flex;
        border: 1px solid #dfe2e5;
        width: 125px;
        height: 30px;
        line-height: 30px;
        border-radius: 3px;
        /*background: linear-gradient(#fefefe, #f3f4f7);*/
        background: none;
    }
    
    .iptSelectTime {
        width: 105px;
        padding: 5px 15px;
        border: none;
        background: none;
        border-right: 1px solid #dfe2e5;
        outline: none;
    }
    
    #spanSelectTime {
        width: 20px;
        background: none;
        border: none;
        outline: none;
        padding: 0px;
        font-size: 12px;
    }
    
    .divQuery {
        height: 28px;
        line-height: 28px;
    }
    
    .mBtnQuery {
        margin-left: 15px;
        padding: 0px 10px;
        color: #354052;
        border-radius: 3px;
        border: none;
        border: 1px solid #dfe2e5;
        /*background: linear-gradient(#fefefe, #f3f4f7);*/
        background: none;
    }
    
    .analyBody {
        display: flex;
        width: 100%;
        height: calc(100% - 60px);
        padding: 8px;
    }
    
    .powTrendy {
        margin-left: 8px;
        background: #fff;
        height: 100%;
        width: 67%;
        border: 1px solid #dfe2e5;
        box-shadow: 0px 0px 10px #ccc;
    }
    
    .analyTable {
        background: #fff;
        height: 100%;
        width: 33%;
    }
    
    .powTrendyTitle,
    .totalMeterTitle,
    .todyEnergyTitle {
        display: flex;
        justify-content: space-between;
        padding: 0 10px;
        height: 46px;
        width: 100%;
        padding-left: 15px;
        line-height: 46px;
        color: #354052;
        border-bottom: 1px solid #dfe2e5;
        /*background: #f1f4f8;*/
        font-weight: 500;
    }
    
    #powTrendyEchart {
        width: 100%;
        height: calc(100% - 48px);
        background: #fff;
    }
    
    #powTrendyEchart div {
        height: 33%;
    }
    
    .totalMeter,
    .energyConsumption {
        width: 100%;
        height: calc(46% - 4px);
        color: #333;
        border: 1px solid #dfe2e5;
        box-shadow: 0px 0px 10px #ccc;
    }
    
    .energyConsumption {
        height: calc(54% - 4px);
        margin-top: 8px;
    }
    
    .totalMeterTable {
        margin: 20px;
        width: calc(100% - 40px);
        height: calc(100% - 86px);
        overflow-y: auto;
        overflow-x: hidden;
    }
    
    .meterTableRow {
        float: left;
        width: 100%;
        height: 25%;
        /*display: flex;*/
        line-height: 80px;
        border-bottom: 1px solid #dfe2e5;
    }
    
    .meterTableRow span {
        width: 16.5%;
        text-align: left;
        padding-left: 5px;
        display: inline-block;
        float: left;
        height: 100%;
    }
    
    .meterTableRow .spValue {
        padding-left: 5px!important;
        color: #3f7ee3;
        cursor: pointer;
        /*width:85px;*/
        width: calc(33.33% - 40px);
    }
    
    .dayEnergyList {
        margin: 0 20px;
        height: calc(100% - 50px);
        width: calc(100% - 40px);
        overflow-y: auto;
    }
    
    .dayEnergyList .iconfont {
        padding-left: 15px;
        font-size: 50px;
        text-shadow: none;
    }
    
    .dayAccumulate,
    .monthAccumulate,
    .yearAccumulate {
        height: 33%;
        width: 100%;
        display: flex;
        align-items: center;
        color: #54cadd;
        font-size: 30px;
        border-bottom: 1px solid #dfe2e5;
    }
    
    .dayAccumulate {
        text-shadow: -1px 1px 2px #BEE0EB;
    }
    
    .monthAccumulate {
        color: #70D35F;
        text-shadow: -1px 1px 2px #BEEFB6;
    }
    
    .yearAccumulate {
        color: #45abff;
        border-bottom: none;
        text-shadow: -1px 1px 2px #C1D9EF;
    }
    
    .rowTitle {
        width: 35%;
        height: 20px;
        font-size: 14px;
        border-left: 3px solid #54cadd;
        color: #354052;
        text-shadow: none;
    }
    
    .dayAccumulate span,
    .monthAccumulate span,
    .yearAccumulate span {
        padding-left: 5px;
        white-space: nowrap;
    }
    
    .spDayValue,
    .spMonthValue,
    .spYearValue {
        width: 50%;
        text-align: left;
    }
    
    .meterTable {
        display: none;
        padding: 8px;
        width: 100%;
        height: calc(100% - 58px);
        color: #333;
    }
    
    .tableWrapper {
        border: 1px solid #dadde4;
        width: 100%;
        height: 100%;
        overflow: hidden;
        position: relative;
        box-shadow: 0px 0px 10px #ccc;
    }
    
    .tableThead {
        width: 100%;
        height: 52px;
        background: #d1e6ff;
        text-align: center;
        line-height: 52px;
        font-weight: 600;
    }
    
    .tableThead span {
        flex: 1;
    }
    
    .tableBody {
        height: calc(100%);
        width: 100%;
        overflow-y: auto;
        background: #fff;
        font-size: 12px;
        color: #596473;
    }
    
    .tableBody table {
        width: 100%;
    }
    
    .tableBody table td {
        text-align: center;
        padding: 0 5px;
    }
    
    .tableRow {
        width: 100%;
        height: 52px;
        text-align: center;
        line-height: 52px;
    }
    
    .tableRow:hover {
        background: #d1e6ff!important;
    }
    
    .tableRow:nth-child(even) {
        background: #f1f3f7;
    }
    
    .tableRow span {
        flex: 1;
    }
    
    @media (min-width: 1650px) {
        .dayEnergyList .iconfont {
            width: 30%;
        }
    }
    
    @media (max-width: 1650px) {
        .spMonthValue,
        .spYearValue,
        .spDayValue {
            font-size: 22px;
        }
        .dayAccumulate,
        .monthAccumulate,
        .yearAccumulate {
            font-size: 22px;
        }
        .meterTableRow {
            line-height: 60px;
        }
        .dayEnergyList .iconfont {
            width: 20%;
            padding-left: 5px;
            font-size: 50px;
            text-shadow: none;
        }
    }
    
    #tableScrollbar::-webkit-scrollbar-thumb {
        background-color: #DFE1E6;
    }
    
    #tableScrollbar::-webkit-scrollbar-track {
        background-color: #EFF1F5;
        box-shadow: none;
    }
    
    .modal {
        display: none;
    }
    
    .pointCurve {
        width: 100%;
        height: 400px;
    }
    
    .todyEnergyTitle .updateTime,
    .totalMeterTitle .updateTime {
        display: none;
    }
    
    .meterTableRow .spName {
        max-width: 55px!important;
        width: 35px;
    }
    
    @media (max-width: 1400px) {
        .meterTableRow .spName {
            width: 30px;
        }
        .meterTableRow .spValue {
            padding-left: 5px!important;
            color: #3f7ee3;
            cursor: pointer;
            /*width:85px;*/
            width: calc(33.33% - 35px);
        }
        .powTrendyTitle,
        .totalMeterTitle,
        .todyEnergyTitle {
            height: 36px;
            line-height: 36px;
        }
        .totalMeterTable {
            margin: 10px;
            width: calc(100% - 20px);
            height: calc(100% - 56px);
            overflow-y: auto;
        }
        .meterTableRow {
            line-height: 45px!important;
        }
        .divTimeSelected {
            height: 54px;
        }
    }
</style>
<div class="MeterContainer">
    <div class="divTimeSelected">
        <div class="divTabModule">
            <div class="ParameterOrTable cur" type-model="EnergyParameter" i18n="energyManagement.subTable.PARAMETERS_ANALYSIS"></div>
            <div class="ParameterOrTable" type-model="EnergyMeter" i18n="energyManagement.subTable.METER_READING"></div>
        </div>
        <div class="timeQuery">
            <div class="timeWrapper">
                <input class="iptSelectTime" size="16" type="text" name="" value="">
                <button type="button" id="spanSelectTime"><span  class="glyphicon glyphicon-chevron-down" datetimepicker=""></span></button>
            </div>
            <div class="divQuery">
                <button class="mBtnQuery" i18n="energyManagement.overview.QUERY"></button>
            </div>
        </div>

    </div>

    <div class="analyBody">
        <div class="analyTable">
            <div class="totalMeter">
                <div class="totalMeterTitle"><span i18n="energyManagement.subTable.TOTAL_METER"></span><span class="updateTime"></span></div>
                <div class="totalMeterTable gray-scrollbar">

                </div>
            </div>
            <div class="energyConsumption">
                <div class="todyEnergyTitle"><span i18n="energyManagement.subTable.TODAY_ENERGY"></span><span class="updateTime"></span></div>
                <div class="dayEnergyList">
                    <div class="dayAccumulate">
                        <span class="iconfont icon-ri1"></span>
                        <span class="rowTitle" style="border-left: 3px solid #54cadd" i18n="energyManagement.subTable.DAILY_ACC_ENERGY">日累计量</span>
                        <span class="spDayValue"></span>
                    </div>
                    <div class="monthAccumulate">
                        <span class="iconfont icon-yue"></span>
                        <span class="rowTitle" style="border-left: 3px solid #91de84" i18n="energyManagement.subTable.MONTHLY_ACC_ENERGY">月累计量</span>
                        <span class="spMonthValue"></span>
                    </div>
                    <div class="yearAccumulate">
                        <span class="iconfont icon-nian"></span>
                        <span class="rowTitle" style="border-left: 3px solid #45abff" i18n="energyManagement.subTable.YEARLY_ACC_ENERGY">年累计量</span>
                        <span class="spYearValue"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="powTrendy">
            <div class="powTrendyTitle" i18n="energyManagement.subTable.POWER_TRENDY">昨今功率趋势</div>
            <div id="powTrendyEchart">
                <div class="powTrendyDay"></div>
                <div class="powTrendyMonth"></div>
                <div class="powTrendyYear"></div>
            </div>
        </div>
    </div>

    <div class="meterTable">
        <div class="tableWrapper">
            <!--            <div class="tableThead">
                <span  >时刻</span><span></span><span></span><span></span><span></span>
                <span></span><span></span><span></span><span></span><span></span>
                <span></span><span></span>
            </div>-->
            <div class="tableBody" id="tableScrollbar">
                <table>
                    <!--                    <thead>
                        <tr class="tableThead">
                            <td i18n="energyManagement.subTable.METER_TABLE_TIME"></td>
                        </tr>
                    </thead>-->
                    <tbody>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<div class="modal in" id="myMetersModal" tabindex="-1" role="dialog" data-backdrop="true">
    <div class="modal-backdrop in"></div>
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" id="closeCurve" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Point Curve</h4>
            </div>
            <div class="modal-body" id="formWrap">
                <div class="form-horizontal">
                    <div class="pointCurve" id="pointCurve"></div>
                </div>
            </div>
        </div>
    </div>
</div>