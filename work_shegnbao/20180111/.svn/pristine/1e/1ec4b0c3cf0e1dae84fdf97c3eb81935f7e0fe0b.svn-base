import pytestfrom beopWeb.mod_algorithm import controllerimport jsonimport bsonglobal id@pytest.mark.p0@pytest.mark.usefixtures("delete")@pytest.mark.parametrize(('data'), [    ({"createTime":"","creator":""}),])def test_AddAlgorithmTemplateFolder(data):    rt = controller.do_SaveAlgorithmTemplate(data)    assert bson.ObjectId.is_valid(rt),'acutal is not objectId'    global id    id = rt    actual=controller.GetAlgorithmTemplateContent(id)    assert actual,'save fail acutal={0}'.format(actual)@pytest.fixture(scope='function')def delete(request):    def fin():        global id        controller.do_delete_template({"id":id})    request.addfinalizer(fin)