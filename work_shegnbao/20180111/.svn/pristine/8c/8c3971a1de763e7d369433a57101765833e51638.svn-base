<style>
    #orderSummary{
        /*padding-top:10px;*/
        color:#797979;
        height:100%;
    }
    #orderSummary .navSummary{
        box-shadow: 5px 5px 5px rgba(0,0,0,0.1);
        background: #fbfbfb;
    }
    #orderSummary .timeSelect{
        /*width:400px;*/
        padding:10px;
    }
    #orderSummary .btnGroups{
        /*width:calc(100% - 400px);*/
        padding:10px;
    }
    #timeConfigSum{
        display:inline-block;
        width:200px;
    }
    #timeCircleSum,#teamSum{
        height: 34px;
        line-height: 34px;
        border-radius: 3px;
        padding: 0 5px;
        margin-left: -10px;
    }
    #teamSum{
        margin-left:0;
    }
    #orderSummary .gropList{
        display:inline-block;
        height:34px;
        line-height: 34px;
        padding:0 6px;
        width:150px;
        border:1px solid #ddd;
        border-radius:3px;
        text-align: center;
    }
    #orderSummary .orderList{
        width: 98%;
        margin: 15px auto;
        background: #fbfbfb;
    }
    #orderSummary .orderSin{
        width:20%;
        float:left;
        padding-bottom: 30px;
    }
    #orderSummary .oderSinNum{
        text-align: center;
        font-size: 38px;
        color: #343434;
    }
    #orderSummary .orderSinTitle{
        width:100%;
        text-align:center;
    }
    #orderSummary .orderListTitle{
        height: 40px;
        line-height: 40px;
        font-size: 18px;
        padding: 0 10px;
    }
    #orderSummary .compResponse{
        width: 98%;
        margin:0 auto;
        height:calc(100% - 243px);
    }
    #orderSummary .complateOrdersBox{
        margin-right:15px;
    }
    #orderSummary .responseTimeBox,#orderSummary .complateOrdersBox{
        width:calc(50% - 7.5px);
        float:left;
        background: #fbfbfb;
        height:100%;
        padding:10px;
    }
    .compRespTitle{
        font-size:18px;
    }
    .complateOrderList,.responseOrderBox{
        height:calc(100% - 30px);
    }
    #btnQueryOrder{
        float:right;
    }
    .complateOrderBox,.responseOrderBox{
        height:calc(100% - 25px);
    }
    .complateOrderList,.responseTimeList{
        overflow-y: auto;
        height:100%;
        width:100%;
    }
    .responseTimeLineBox,.complateOrderLineBox{
        width:100%;
        height:50%;
        display:none;
        position:relative;
    }
    #complateOrderLine,#responseTimeLine{
        width:calc(100% - 30px);
        height:100%;
        box-shadow: 0 0 9px rgba(19, 19, 19, 0.29)
    }
    .compResProgress{
        display:flex;
        cursor: pointer;
    }
    .compResName{
        width:100px;
        margin-left:30px;
    }
    .compResNum{
        width:80px;
        margin:0 20px 0 20px;
    }
    .compResProgress .progress{
        width:calc(100% - 250px);
        height:15px;
        border-radius: 15px;
    }
    .progress-bar{
        border-radius: 15px;
    }
    .closeBtn{
        position:absolute;
        top:10px;
        right:40px;
        z-index:1
    }
</style>

<div id="orderSummary">
    <div class="navSummary clearfix">
        <div class="timeSelect clearfix">
            <input type="text" class="form-control" id="timeConfigSum">
            <select id="timeCircleSum">
                <option value="week" i18n="workflow.team.SUMMARY_WEEK">周</option>
                <option value="month" i18n="workflow.team.SUMMARY_MONTH">月</option>
                <!--<option value="year">年</option>-->
            </select>
            <select id="teamSum"></select>
            <select class="gropList" id="taskGroup"></select>
            <div id="btnQueryOrder" class="btn btn-primary" i18n="workflow.team.SUMMARY_QUERY">查询</div>
        </div>
        <!--<div class="btnGroups col-lg-8 col-md-12 clearfix">
            <select class="gropList" id="taskGroup"></select>
            <div class="gropList">默认任务组</div>
            <div class="gropList">RD</div>
            <div class="gropList">开发</div>
            <div id="btnQueryOrder" class="btn btn-primary">查询</div>
        </div>-->
    </div>
    <div class="orderList">
        <div class="orderListTitle" i18n="workflow.team.MENU_SUMMARY">汇总</div>
        <div class="orderListCon clearfix">
            <div class="orderSin">
                <div class="pendingOrder oderSinNum"></div>
                <div class="orderSinTitle" i18n="workflow.team.SUMMARY_PROCESSED_ORDER">待处理工单</div>
            </div>
            <div class="orderSin">
                <div class="delayOrder oderSinNum"></div>
                <div class="orderSinTitle" i18n="workflow.team.SUMMARY_DELAY_ORDER">延误工单</div>
            </div>
            <div class="orderSin">
                <div class="totalOrder oderSinNum"></div>
                <div class="orderSinTitle" i18n="workflow.team.SUMMARY_ALL_ORDER">总工单数量</div>
            </div>
            <div class="orderSin">
                <div class="averageComp oderSinNum"></div>
                <div class="orderSinTitle" i18n="workflow.team.AVERAGE_COMPLETION_RATE">平均完成率</div>
            </div>
            <div class="orderSin">
                <div class="responseTime oderSinNum"></div>
                <div class="orderSinTitle" i18n="workflow.team.AVERAGE_RESPONSE_TIME">平均响应时间</div>
            </div>
        </div>
    </div>
    <div class="compResponse clearfix">
        <div class="complateOrdersBox">
            <div class="compRespTitle" i18n="workflow.team.COMPLETION_RATE">完成率</div>
            <div class="complateOrderBox">
                <div class="complateOrderList gray-scrollbar" data-type="complateRate"></div>
                <div class="complateOrderLineBox">
                    <span class="glyphicon glyphicon-remove complateClose closeBtn"></span>
                    <div id="complateOrderLine" class="echatsLine"></div>
                </div>
            </div>
        </div>
        <div class="responseTimeBox">
            <div class="compRespTitle" i18n="workflow.team.RESPONSE_TIME">响应时间</div>
            <div class="responseOrderBox">
                <div class="responseTimeList gray-scrollbar" data-type="responseTime"></div>
                <div class="responseTimeLineBox">
                    <span class="glyphicon glyphicon-remove responseClose closeBtn"></span>
                    <div id="responseTimeLine" class="echatsLine"></div>
                </div>
            </div>
        </div>
    </div>
</div>