.clear::after {
  content: '';
  display: block;
  height: 1px;
  width: 0;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.over {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dataExportPanel {
  width: 100%;
  height: 100%;
  position: relative;
  padding-top: 16px;
}

.top-panel {
  width: 100%;
  height: 48px;
}
.bottom-panel {
  width: 100%;
  overflow-y: auto;
  height: calc(100% - 48px);
}
.top-panel > .title {
  width: 100%;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.top-panel .exportBtn{
  margin: 8px 0 8px 16px;
}
.top-panel .exportBtn2{
  margin: 8px 0 8px 16px;
}
.row{
  line-height: 20px;
}