.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.dataSourceWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}

.dataSourceWrap button:focus {
  outline: none;
}
.dataSourceWrap .left {
  /* background-image: linear-gradient(to bottom, #282f48, #171d34); */
  float: left;
  width: 18%;
  height: 100%;
  padding: 0 0 0 0;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.dataSourceWrap .right {
  float: left;
  width: 82%;
  height: 100%;
  overflow: auto;
  padding: 0 16px;
}
.dataSourceWrap .left .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-weight: 500;
  color: rgb(29, 29, 29);
  padding-left: 16px;
  font-size: 20px;
  font-weight: 500;
}
.dataSourceWrap .left .lt {
  width: 100%;
  height: 80px;
  padding: 24px 16px 24px 16px;
}
.dataSourceWrap .left .lt button {
  height: 32px;
  line-height: 32px;
  width: 100%;
  margin: 0 auto;
  text-align: center;
  border-radius: 4px;
  background-image: linear-gradient(93deg, #5b88f7, #4d74d2);
  font-size: 14px;
  border: none;
  color: #ffffff;
  cursor: pointer;
}
.dataSourceWrap .left .lm {
  width: 100%;
  height: calc(100% - 370px);
  padding-left: 16px;
  position: relative;
  overflow-y: auto;
}
.dataSourceWrap .left .lb1 {
  width: 100%;
  height: 230px;
  padding: 8px 16px 0;
  border-top: 1px solid rgba(153, 153, 153, 0.3);
}
.dataSourceWrap .left .lb2 {
  width: 100%;
  height: 40px;
  padding: 10px 16px 0;
  border-top: 1px solid rgba(153, 153, 153, 0.3);
}

.lb2 {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.lb2 .checkbox {
  height: 24px;
}
.checkbox > label > div {
  width: 14px;
  height: 14px;
  border-radius: 2px;
  border-color: #ccc;
}
.checkbox:focus > label > div:global(.ms-Checkbox-checkbox),
.checkbox > label > div:global(.ms-Checkbox-checkbox),
.checkbox:hover > label > div:global(.ms-Checkbox-checkbox) {
  border-color: #ccc;
}

.checkbox:global(.is-checked:hover) > label > div,
.checkbox:global(.is-checked) > label > div,
.checkbox:global(.is-checked:focus) > label > div {
  background-color: #4c84ff;
  border: none;
}

.checkbox:focus > label > span:global(.ms-Checkbox-text),
.checkbox:global(.is-checked:hover) > label > span:global(.ms-Checkbox-text),
.checkbox:hover > label > span:global(.ms-Checkbox-text),
.checkbox > label > span:global(.ms-Checkbox-text) {
  font-size: 12px;
  color: #666;
  line-height: 14px;
}
.checkbox > label > div > i {
  font-size: 12px;
}
.dataSourceWrap .left .btnGroup {
  float: right;
  margin-right: 8px;
}
.dataSourceWrap .left .btnGroup button {
  float: left;
  margin-left: 8px;
  height: 20px;
  line-height: 20px;
  font-size: 16px;
  border: none;
  background-color: transparent;
  color: #999;
  cursor: pointer;
}

.dataSourceWrap .right .btnGroup button {
  float: left;
  margin-right: 16px;
  height: 28px;
  line-height: 28px;
  padding: 0 8px;
  opacity: 0.6;
  border-radius: 4px;
  border: solid 1px #5b88f7;
  color: #5b88f7;
  background: transparent;
  cursor: pointer;
}
.dataSourceWrap .right .btnGroup button i {
  margin-right: 4px;
}
.dataSourceWrap .right .btnGroup button span {
  vertical-align: top;
}
.dataSourceWrap .pointInput {
  width: 100%;
  height: 28px;
  border-radius: 4px;
  background-color: rgba(153, 153, 153, 0.3);
  border: solid 1px rgba(153, 153, 153, 0.3);
  background-color: transparent;
  padding: 0 8px;
}
.dataSourceWrap .pointInput.error,
.dataSourceWrap .pointInput.error:focus {
  border: solid 1px #a80000;
  color: #a80000;
}
.dataSourceWrap .pointInput:focus {
  border-radius: 4px;
  border: solid 1px #5b88f7;
  color: #5b88f7;
  outline: none;
}
.dataSourceWrap .right .item {
  width: 100%;
  height: 28px;
  line-height: 28px;
  margin-bottom: 16px;
}
.dataSourceWrap .right .item label {
  display: block;
  text-align: right;
  padding: 0 8px;
  float: left;
  width: 40%;
}
.dataSourceWrap .right .item input {
  width: 30%;
  height: 100%;
}
.dataSourceWrap .chartIcon {
  cursor: pointer;
}
.tableWrap {
  height: calc(100% - 29px);
  position: relative;
}
.tableWrap .chartWrap {
  width: 100%;
  height: 40%;
  overflow: hidden;
  position: absolute;
  bottom: 0;
  left: 0;
  background: #ebeef7;
  transition: all 0.367s;
}
.tableWrap .chartWrap .configWrap {
  padding-right: 24px;
  height: 20px;
  position: absolute;
  right: 16px;
  top: 16px;
  line-height: 20px;
  z-index: 99999;
}
.tableWrap .chartWrap .chartConfig {
  width: 20px;
  height: 20px;
  position: absolute;
  right: 0;
  top: 0;
  line-height: 20px;
  text-align: center;
  font-size: 16px;
  cursor: pointer;
}
.tagUl li {
  user-select: none;
  height: 24px;
  line-height: 24px;
  border-radius: 12px;
  background-color: rgba(99, 207, 65, 0.1);
  color: #63cf41;
  cursor: default;
  float: left;
  margin-right: 4px;
  max-width: 99%;
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  transition: padding 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  padding: 0 10px 0 10px;
  text-align: center;
  margin-bottom: 4px;
}
/* RenameText */
.RenameText {
  position: relative;
}
.RenameText:hover .toolbar {
  display: block;
}
.RenameText .toolbar {
  position: absolute;
  top: 0;
  right: 0;
  display: none;
}
.RenameText:hover .domWrap {
  padding-right: 50px;
}
.RenameText .domWrap {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.RenameText .toolbar button {
  margin-right: 8px;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  border: none;
  background-color: transparent;
  color: #999;
  cursor: pointer;
}
/* RenameText end */

/* PointText */
.PointText {
  width: 100%;
  position: relative;
  cursor: pointer;
}
.PointText .text {
  width: 100%;
  height: 20px;
  line-height: 20px;
  outline: 0;
  box-shadow: none;
  color: #333333;
  padding: 0 12px 0 12px;
  width: 100%;
  text-overflow: ellipsis;
  border: 1px solid #a6a6a6;
  background: #ffffff;
}
.PointText .text:hover {
  border-color: #212121;
}
.PointText .text:focus {
  border-color: #0078d7;
}
.PointText .text.error {
  border-color: #a80000;
}
.PointText .errorText {
  display: block;
  color: #a80000;
  margin-top: 4px;
}
.PointText .errorText.hide {
  display: none;
}
/* PointText end */
