.nav-tabs {
    width: 1300px;
}

#debugger .debug-container {
    background-color: #f6f6f6;
    color: #333333;
}

.icon-close {
    color: #666666;
    float: right;
    margin-right: 15px;
    margin-top: 15px;
}

textarea {
    resize: vertical;
}

.device-data-content {
    height: 408px;
    border: 1px solid #CCCCCC;
    margin-top: 13px;
    border-radius: 5px;
}

.show-type {
    width: 200px;
}

.content .nav-tabs {
    background: #fff !important;
}

.content .nav-tabs > li > a {
    color: #222 !important;
}

.content .nav-tabs > li:hover > a {
    background-color: #fff !important;
    color: #222;
}

.content .nav-tabs > li.active > a {
    background-color: #ffffff !important;
    color: #0077ee !important;
}

.debug-ul {
    list-style: none;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    overflow-y: auto;
    overflow-x: hidden;
}

#equipmentDebugLog {
    height: calc(100vh - 513px);
}

#commandDebugLog {
    height: calc(100vh - 386px);
}

#commandDebugShowData, #equipmentDebugShowData {
    height: calc(100vh - 250px);
}

.debugForm .control-label {
    padding-top: 5px;
}

.debugForm .fr {
    float: right;
}

#debuggerContent {
    overflow-y: auto;
    overflow-x: hidden;
}

#hexCommandInput {
    width: 200px;
}

#debugger .modal-content {
    width: 70%;
    margin: 60px auto;
}

#debugger .modal-header {
    height: 30px;
    border-bottom: none;
}

#debugger .nav-tabs {
    background: none !important;
}

#debugger .nav-tabs > li > div {
    color: #222 !important;
    padding: 8px;
    cursor: pointer;
}

#debugger .nav-tabs > li:hover > div {
    background-color: #eee !important;
    color: #222;
}

#debugger .nav-tabs > li.active > div {
    background-color: #ffffff !important;
    color: #0077ee !important;
}

#debugger {

}

#debugger .close {
    margin-top: -8px;
}