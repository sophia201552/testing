.container {
  width: 100%;
  height: 100%;
}

.container h3 {
  font-size: 14px;
}
/* slider */
.dependence-slider {
  width: 200px;
  float: left;
  height: 100%;
}
.dependence-select-data {
  width: 100%;
  padding: 15px;
  border-bottom: 1px solid #edeeef;
}

.dependence-select-data h3 {
  margin-bottom: 15px;
}

.date-picker-start {
  margin-top: 6px;
}
.date-picker-end {
  margin-top: 6px;
  margin-bottom: 15px;
}
.date-picker-start > div > div > div > div > div,
.date-picker-end > div > div > div > div > div {
  background-color: #f2f2f2 !important;
  border: 1px solid #e8e8e8 !important;
}
.date-picker-start > div > div > div > div > div input,
.date-picker-start > div > div > div > div > div i,
.date-picker-end > div > div > div > div > div input,
.date-picker-end > div > div > div > div > div i {
  color: #666666 !important;
  font-size: 12px;
}
.dependence-ways {
  height: 84px;
  border-bottom: 1px solid #edeeef;
  padding: 15px;
}
.dependence-ways h3 {
  margin-bottom: 15px;
}

.dependence-args {
  padding: 15px;
}
.dependence-args h3 {
  margin-bottom: 15px;
}
.args-item {
  margin-bottom: 10px;
  font-size: 12px;
  text-align: center;
  border-radius: 2px;
  border: 1px solid #c7d8fe;
  background-color: #c7d8fe;
  cursor: pointer;
}
.args-item.selected {
  background-color: #4c84ff;
}
.args-item > span {
  line-height: 26px;
  width: 80%;
  display: block;
  margin: 0 auto;
  color: #ffffff;
}
/* details */
.dependence-details {
  width: calc(100% - 200px);
  float: left;
  height: 100%;
}

.details-close {
  height: 33px;
}
.details-close > span {
  border-radius: 50%;
  background: #aaaaaa;
  text-align: center;
  color: #ffffff;
  width: 20px;
  height: 20px;
  line-height: 20px;
  float: right;
  margin-right: 16px;
  margin-top: 6px;
}
.details-close > span > i {
  font-size: 12px;
}

.details-container {
  padding: 20px;
  height: calc(100% - 33px);
  background-color: #f1f4f5;
}
.details-echart {
  height: calc(100% - 300px);
  border: 1px solid #e8ebf4;
  background-color: #ffffff;
}
.echart-content {
  height: calc(100% - 30px);
  overflow: hidden;
}
.echart-title {
  height: 30px;
  line-height: 30px;
  padding-left: 8px;
  border-bottom: 1px solid #edeeef;
}
.content-left-table {
  width: 250px;
  height: 100%;
  float: left;
  padding: 15px;
}

.operation {
  width: 100%;
}
.thead-row {
  font-size: 12px;
}
.operation-row {
  line-height: 36px;
  font-size: 12px;
  color: #666666;
}
.operation-row td {
  cursor: context-menu;
}
.content-center-miserables {
  width: calc(100% - 450px);
  height: 100%;
  float: left;
  border-left: 1px solid #edeeef;
  border-right: 1px solid #edeeef;
}
.content-center-miserables .echartsCtn {
  width: 100%;
  height: 100%;
}
.content-right-list {
  width: 200px;
  height: 100%;
  float: left;
  padding: 38px 15px 15px 15px;
}

.content-right-list .list-rows {
 padding: 6px;
}
.content-right-list .list-rows > span {
  display: inline-block;
  width: 50%;
  line-height: 15px;
  font-size: 10px;
  word-wrap: break-word;
  vertical-align: top;
  color: #666666;
  cursor: context-menu;
}

.echart-footer {
  width: 100%;
  height: 280px;
  margin-top: 20px;
  background: #ffffff;
  border: 1px solid #e8ebf4;
}
.echart-footer > div > div:first-child {
  width: 824px;
}
/*checkBox start*/
.checkBoxCtn {
  padding: 2px 0;
}
.checkBoxCtn .check_tick_ctn {
  position: relative;
  display: flex;
  align-items: end;
}
.checkBoxCtn .check_tick_ctn > div {
  position: relative;
}
.checkBoxCtn .check_tick {
  position: absolute;
  visibility: hidden;
  cursor: pointer;
}
.checkBoxCtn .labelName {
  font-size: 12px;
  cursor: pointer;
  color: #555555;
}
.checkBoxCtn .check_tick + label {
  display: inline-block;
  background: #f2f2f2;
  border: 1px solid #e8e8e8;
  border-radius: 4px;
  width: 12px;
  height: 12px;
  margin-right: 6px;
}
.checkBoxCtn .check_tick:checked + label {
  background: #4c84ff;
}
.checkBoxCtn .check_tick:checked + label:after {
  content: '';
  position: absolute;
  left: 3px;
  bottom: 8px;
  width: 4px;
  height: 2px;
  border: 2px solid #ffffff;
  border-top-color: transparent;
  border-right-color: transparent;
  -ms-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
  transform: rotate(-45deg);
}
/*checkBox end*/
