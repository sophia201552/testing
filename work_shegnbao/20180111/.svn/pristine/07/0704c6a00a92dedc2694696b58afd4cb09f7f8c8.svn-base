.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.dataComplementWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.showDataCtn {
  float: left;
  width: 82%;
  height: 100%;
  background: #f5f8f9;
}
.showDataCtn > div {
  height: 50%;
  width: 100%;
}
.left > .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left .mWrap {
  width: 100%;
  height: calc(70% - 20px);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  padding: 24px 16px 0 16px;
}
.left .bWrap {
  width: 100%;
  height: 30%;
  padding: 16px 16px;
}
.left .mWrap .api2 >label:first-child {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
}
.left .mWrap .api2 > div {
  margin-top: 8px;
  position: relative;
}
.tableWrap {
  overflow-y: auto;
  background: rgba(235, 238, 247, 0.7);
  padding: 15px 20px;
}
.tableWrap table {
  width: 100%;
  border-collapse: collapse;
}
.tableWrap thead {
  background: #fff;
}
.tableWrap th,
.tableWrap td {
  padding: 10px 6px;
  border: 1px solid #e8e8e8;
  text-align: center;
}
.tableWrap td:first-child {
  width: 200px;
}
.tableWrap td span {
  display: inline-block;
  text-align: center;
}

.Dropdown:global(.is-open) {
  margin-bottom: 19px;
  background: #f2f2f2;
  border-radius: 4px;
  height: 24px;
}
.Dropdown > span:first-child {
  height: 24px;
  line-height: 22px;
  background: #f2f2f2;
  font-size: 12px;
  letter-spacing: 0;
}
.Dropdown > span:first-child,
.Dropdown:hover > span:first-child,
.Dropdown:focus > span:first-child {
  border-color: #e8e8e8;
  border-radius: 4px;
  color: #999;
}
.Dropdown > span:last-child {
  height: 24px;
  line-height: 22px;
  right: 4px;
}
