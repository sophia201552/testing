<style>
    #projectStandardCtn {
        width: 100%;
        height: 100%;
        display: flex;
        overflow: hiddden;
    }

    .panelFilter {
        width: 300px;
        border-left: none;
        background: rgba(255, 255, 255, 1);
        box-shadow: 2px 0px 10px #ccc;
    }

    .panelFilter .row input,
    .panelFilter .row select {
        background-color: #fff;
        border-radius: 4px;
        color: #5C6777;
        border-color: #DFE2E5;
        box-shadow: none!important;
    }

    .panelBmModule {
        width: calc(100% - 300px);
        background:#eee;
    }

    .panelModules .btnBmModule {
        padding: 24px 10px;
        text-align: center;
        cursor: pointer;
        border-right: 2px solid transparent;
        -webkit-transition: all 0.3s;
        transition: all 0.3s;
        opacity: 0.5;
    }

    .panelModules .btnBmModule:hover {
        background-color: rgba(35, 45, 78, 0.7);
        color: #eee;
        opacity: 1;
    }

    .panelModules .btnBmModule.selected {
        opacity: 1;
        background-color: rgba(35, 45, 78, 0.9);
    }

    .panelModules .btnBmModule.selected:hover {
        color: #eee;
    }

    .panelModules .btnBmModule .moduleName {
        padding-top: 5px;
    }

    .panelModules .btnBmModule .iconfont {
        font-size: 28px;
        border-radius: 50%;
        padding: 7px;
    }

    .panelModules .btnBmModule.selected .iconfont {
        background-color: rgba(210, 159, 11, 0.8);
    }

    @media screen and (max-width: 1366px) {
        #containerKaide .panelModules {
            width: 8%;
        }
        #containerKaide .panelBmModule {}
    }

    .panelFilter .row .btnSearch {
        position: absolute;
        right: 10px;
        top: 6px;
        color: #354052;
        cursor: pointer;
    }

    .panelFilter .row {
        margin: 10px;
        position: relative;
        color:#666;
    }

    .divFilterSel select.form-control {
        display: inline-block;
        width: 32%;
        padding: 6px;
    }

    .divFilterSel select.form-control:nth-child(2) {
        width: 33%;
    }

    .divFilterSel select.form-control option {
        background-color: rgba(0, 0, 0, 0.5);
    }

    #ulFilterResult {
        padding: 15px 10px 10px
    }

    #ulFilterResult .liBranch {
        font-size: 12px;
        padding: 8px 6px;
        border-bottom: 1px solid rgba(136, 136, 136, 0.3);
        cursor: pointer;
        opacity: 0.85;
    }

    #ulFilterResult .liBranch.selected {
        opacity: 1;
        color: #4c8be4;
    }


    .echartPlane.part {
        width: calc(100% - 280px);
        float: left;
    }

    .treePane {
        width: 260px;
        float: left;
        height: calc(100% - 75px);
        border-left: 1px solid #414B5E;
    }

    #ulDetailLegend>li {
        display: none;
    }

    #ulDetailLegend>li.active {
        display: block;
    }

    #ulDetailLegend li span:first-of-type {
        width: 25px;
        height: 14px;
        border-radius: 2px;
        background: #CCCCCC;
    }

    #ulDetailLegend li {
        width: auto;
    }

    .CPRoomCOP.active {
        background-color: #1790CF !important;
    }

    .hostCOP.active {
        background-color: #1C7099 !important;
    }

    .hostAvrgLoad.active {
        background-color: #88B0BB !important;
    }

    .chillSupplyTemp.active {
        background-color: #AD61A4 !important;
    }

    .chillReturnTemp.active {
        background-color: lightseagreen !important;
    }

    .chillDiffTemp.active {
        background-color: #65B1CA !important;
    }

    .coolingSupplyTemp.active {
        background-color: #CA9A49 !important;
    }
    /*tree*/

    .ztree li {
        padding: 2px 4px;
    }

    .ztree li a {
        padding-left: 6px;
    }

    .ztree li a:hover {
        background-color: rgba(0, 0, 0, 0.3);
    }

    .ztree li a.curSelectedNode {
        color: #E0BC2B;
        background-color: rgba(0, 0, 0, 0.1);
    }

    .ztree li span.button.ico_docu {
        display: none;
    }

    .btnLegend ul li {
        float: none;
        margin: 0 auto;
    }

    .echartModule {
        width: calc(100% - 8px);
        height: calc(100% - 90px);
    }
    .crosswiseChart {
        height: 100%;
    }
    /*6个模块的样式*/

    .crosswisePlane {
        width: 100%;
        height: 100%;
        color: #354052;
    }

    .timeQueryPlane {
        display: block;
        width: 100%;
        height: 80px;
        background: #fff;
        box-shadow: 10px 0px 10px #ccc;
        display: flex;
        align-items: center;
        padding: 13px 0px 0px 15px;
        margin-bottom: 10px;
        
    }

    .timeQueryPlane button {
        display: block;
        float: left;
        background: none;
        color: #5C6777;
        border-radius: 4px;
        outline: none;
    }
    .posSelectTimeBox {
        position: relative;
        height: 30px;
        line-height: 30px;
        display: flex;
        background: #FFFFFF;
        padding: 2px 8px;
        border: 1px solid #C3C8D1;
        border-radius: 3px;
        text-align: center;
    }    
    .seletTimeBox {
        display: flex;
        height: 100%;
        align-items: center;
    }

    .selectTime {
        position: relative;
        outline: none;
        font-size: 13px;
        border: none;
        width: 80px;
        height: 100%;
        line-height: 24px;
        text-align: left;
    }

    .selectTime:after {
        content: '';
        display: inline-block;
        border-left: 6px solid transparent;
        border-right: 6px solid transparent;
        border-top: 6px solid #ccc;
        position: absolute;
        top: 10px;
        right: 5px;
    }    
    .totalComparedText {
        font-size: 20px;
    }
    .echartPlane {
        background: #fff;
        margin-left: 8px;
        width: 100%;
        height: 100%;
        overflow-y: auto;
        overflow-x: hidden;
        box-shadow: 0px 0px 10px #ccc;
    }

    .timeStyle {
        display: block;
        float: left;
        width: 175px;
        height: 34px;
        padding: 6px 12px;
        font-size: 20px;
        font-weight: 200;
        text-align: left;
        line-height: 1.42857143;
        color: #5C6777;
        background-color: rgba(255, 255, 255, 0);
        border: none;
        border-radius: 4px;
        outline: none;
    }

    .elec,
    .gas,
    .water {
        width: 25px;
        height: 14px;
        border-radius: 2px;
        background: #CCCCCC;
    }

    .elec.active {
        background: #71D360;
    }

    .gas.active {
        background: #CCCCCC;
    }

    .water.active {
        background: #45ABFF;
    }

    .btnLegend {
        width: 100%;
        height: 55px;
        padding-top: 20px;
    }

    .btnLegend ul {
        margin: 0 auto;
        width: 500px;
        height: 50px;
    }

    .btnLegend ul li {
        list-style: none;
        float: left;
        width: 150px;
    }

    .btnLegend ul li:hover {
        cursor: pointer;
    }

    .btnLegend ul li span {
        display: block;
        float: left;
        width: 10px;
        height: 10px;
        line-height: 10px;
        border-radius: 50%;
        white-space: nowrap;
    }

    .btnLegend ul li span:last-child {
        padding-left: 3px;
    }

    #crosswiseChart {
        width: 100%;
        height: calc(100% - 55px);
        overflow-y: auto;
        overflow-x: hidden;
    }

    .crosswiseChart {
        width: 100%;
        height: 100%;
    }
    /*#crosswiseChart::-webkit-scrollbar{
	width:8px;
}
#crosswiseChart::-webkit-scrollbar-track {
	border:1px gray solid;
	border-radius:10px;
	-webkit-box-shadow:0 0 6px gray inset;
	}

#crosswiseChart::-webkit-scrollbar-thumb {
	border-radius:10px;
	background-color:rgba(155,155,155,0.2);
	-webkit-box-shadow:0 0 6px gray inset;
	}
#crosswiseChart::-webkit-scrollbar-corner {
		  background-color: black;
	}*/

    .topBtnWrap {
        position: absolute;
        top: 20px;
        right: 40px;
    }

    .btn a {
        color: #fff;
    }

    .btn a:hover {
        text-decoration: none;
    }

    .rightContainer {
        padding: 0px!important;
    }

    .divFilterBox {
        height: 80px;
        background: #4c8be4;
        margin: 0px!important;
        padding: 20px 10px;
    }

    .noDataMask {
        z-index: 100;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }

    .noDataBox {
        display: flex;
        width: 50%;
        margin: auto;
        margin-top: 5%;
    }

    .noDataImg .noDataText {
        width: 50%;
    }

    .noDataText {
        margin-left: 10px;
        margin-top:10px;
        text-align: center;
        color: #242B36;
    }

    .noDataText p:first-child {
        font-size: 18px;
        padding-top: 10px;
    }

    .noDataText p:last-child {
        font-size: 16px;
        text-align: center;
        
    }
</style>
<div id="projectStandardCtn">
    <div class="panelFilter divPanel">
        <div class="row divFilterBox"><input type="text" class="form-control iptSearch" placeholder="Search" />
            <!--<i class="iconfont btnSearch">&#xe713;</i>--></div>
        <div class="row divFilterSel" style="display:none">
            <select class="form-control">
			<option i18n="energyManagement.projectBenchmark.WATER">中国</option>
		</select>
            <select class="form-control">
			<option i18n="energyManagement.projectBenchmark.BUSSINESS_TYPE">业务类型</option>
		</select>
            <select class="form-control">
			<option i18n="energyManagement.projectBenchmark.CITY">城市</option>
		</select>
        </div>
        <div class="row gray-scrollbar" style="height: calc(100% - 100px);overflow: auto;">
            <ul class="list-unstyled" id="ulFilterResult">

            </ul>
        </div>
    </div>
    <div class="panelBmModule divPanel">
        <div class="crosswisePlane">
            <div class="timeQueryPlane">
                <!--<span class="totalComparedText">总能耗对比</span>-->
                <input class="timeStyle" id="iptTime" size="16" type="text" data-format="yyyy-mm-dd" readonly>
                <div class="selectTimeBox">
                    <div class="posSelectTimeBox"><button class="selectTime" type="text" data-format="yyyy-mm" value=""></button>
                       
                    </div>
                </div>
            </div>
            <div class="topBtnWrap">
                <button class="btn btn-info hidden" id="btnDownload"><a href="https://beopweb.oss-cn-hangzhou.aliyuncs.com/custom/beop_sales/Energy_Report_2017-01.pdf" target="_blank">下载报表</a></button>
                <button class="btn btn-default hidden" id="btnBack">返回</button>
            </div>
            <div class="echartModule"></div>
        </div>        
    </div>
</div>