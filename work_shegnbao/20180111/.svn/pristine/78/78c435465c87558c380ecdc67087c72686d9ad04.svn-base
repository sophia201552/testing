<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>
    <%= htmlWebpackPlugin.options.title %>
  </title>
  <% for(var i=0, len=htmlWebpackPlugin.files.css.length; i<len; i++) {%>
  <link href="<%= htmlWebpackPlugin.files.css[i] %>" rel="stylesheet" />
  <% } %>
</head>

<body>
  <div id="container" style="width: 100%;height: 100%;">
    <!--Loader Start-->
    <div id="loading">
      <div id="loading-center">
        <div id="loading-center-absolute">
          <div class="object" id="object-four"></div>
          <div class="object" id="object-three"></div>
          <div class="object" id="object-two"></div>
          <div class="object" id="object-one"></div>
        </div>
      </div>
    </div>
    <!--Loader End-->
  </div>
  <input type="hidden" value="{{token if token else ''}}" id="hidToken" />
  <% for(var i=0, len=htmlWebpackPlugin.files.js.length; i<len; i++) {%>
  <script type="text/javascript" src="<%= htmlWebpackPlugin.files.js[i] %>"></script>
  <% } %>
  <script type="text/javascript">
    var appConfig = window.appConfig || {};
    appConfig.project = appConfig.project || {};
    appConfig.project.id = {{ projId if projId else 'null' }};
  </script>
</body>

</html>