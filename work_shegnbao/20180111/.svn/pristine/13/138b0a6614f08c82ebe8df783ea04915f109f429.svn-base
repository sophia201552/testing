.strategy-list-container {
  min-width: 600px;
  width: 100%;
  height: 100%;
  float: right;
  background-color: #f4f8f9;
  float: left;
  overflow: hidden;
}
.strategy-list-header {
  width: 100%;
  height: 40px;
  font-size: 12px;
  color: rgb(152, 166, 183);
  background-color: rgb(35, 45, 71);
}

.strategy-list-content {
  width: 100%;
  height: calc(100% - 40px);
  overflow-x: hidden;
  overflow-y: auto;
}
.strategy-list-content > div {
  width: 100%;
}

.add {
  width: 86px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  cursor: pointer;
  float: left;
  background-color: rgb(78, 120, 224);
  font-size: 14px;
  color: rgb(255, 255, 255);
  margin-right: 10px;
}
.toolName {
  height: 40px;
  line-height: 40px;
  display: flex;
  align-items: center;
  cursor: pointer;
  float: left;
}
.toolName > div {
  width: 32px;
  height: 30px;
  line-height: 30px;
  border-radius: 2px;
  text-align: center;
  font-size: 14px;
}
.toolName:hover > div {
  background-color: rgba(23, 29, 52, 0.4);
  border: solid 1px rgba(58, 62, 76, 0.4);
}
.icon-line {
  float: left;
  width: 1px;
  height: 24px;
  border-radius: 0.5px;
  background-color: rgb(23, 29, 52);
  margin: 8px 4px 0;
}

.publish > i {
  font-size: 16px;
  transform: rotate(-45deg);
  padding-left: 6px;
  padding-top: 1px;
}

.language,
.searchText {
  float: right;
  line-height: 40px;
}
.language {
  position: relative;
  width: 74px;
  height: 24px;
  line-height: 24px;
  margin-right: 24px;
  margin-top: 8px;
  border-radius: 4px;
  background-color: rgba(25, 31, 55, 0.5);
  border: solid 1px rgba(58, 62, 76, 0.5);
  cursor: pointer;
  padding-left: 8px;
}
.language > div > i {
  float: right;
  color: rgb(89, 100, 124);
  font-size: 9px;
  margin-right: 4px;
}
.selectLan {
  position: absolute;
  top: 24px;
  left: 0;
  width: 72px;
  height: 30px;
  z-index: 999;
  padding-left: 12px;
  line-height: 30px;
  border-radius: 2px;
  background-color: rgb(245, 248, 249);
  box-shadow: 0px 6px 10px 0 rgba(103, 107, 112, 0.38);
  -webkit-box-shadow: 0px 6px 10px 0 rgba(103, 107, 112, 0.38);
}

/* 显示隐藏 spinner */
.show {
  display: block;
}
.hide {
  display: none;
}
/* 搜索框样式 */
.search > div:first-child {
  width: 220px;
  height: 26px;
  line-height: 24px;
  position: relative;
  top: 9px;
  left: -34px;
  font-size: 12px;
  border-radius: 4px;
  float: right;
  border: none;
  background-color: rgba(25, 31, 55, 0.5);
  border: solid 1px rgba(58, 62, 76, 0.5);
}
.search:hover > div:first-child {
  border-color: rgba(58, 62, 76, 0.5);
}
.search > div:first-child div:first-child {
  font-size: 14px;
  width: 22px;
  color: #687296;
  border-color: #a6a6a6;
}
.search > div:global(.is-active) div:first-child {
  width: 4px;
}
.search > div:first-child > div:first-child i {
  width: 14px;
  height: 14px;
  line-height: 14px;
}

.search > :global(.can-clear) > :global(.ms-SearchBox-clearButton) {
  display: -webkit-box;
}
.search > div:first-child > :global(.ms-SearchBox-clearButton) i {
  font-size: 10px;
  line-height: 16px;
  color: #687296;
}
.search > div:first-child > input {
  padding-top: 3px;
  font-size: 12px;
  color: #687296;
  border: none;
}

.search > div:first-child > input::-webkit-input-placeholder {
  color: #687296;
  vertical-align: bottom;
}
.search > div:last-child {
  float: right;
  line-height: 40px;
}
.search > div:last-child > span {
  padding: 0 8px;
}

/* DetailsList 样式 */
.strategy-list-content :global(.ms-DetailsHeader) {
  height: 48px;
  line-height: 48px;
  background-color: #f5f8f9;
  border: none;
}

.strategy-list-content :global(.ms-DetailsList) > div {
  overflow-x: hidden;
}
.strategy-list-content :global(.ms-DetailsHeader) > div:hover {
  color: rgb(108, 149, 255);
  background-color: #f5f8f9;
}
.strategy-list-content :global(.ms-DetailsHeader) > div {
  height: 48px;
  color: rgb(108, 149, 255);
}
.strategy-list-content :global(.ms-DetailsHeader) > div > span > button {
  opacity: 1;
}
.strategy-list-content :global(.ms-DetailsHeader:hover) > div {
  color: rgb(108, 149, 255);
}
.strategy-list-content :global(.ms-DetailsHeader) > div > span > span {
  padding: 0;
}
.strategy-list-content :global(.ms-DetailsHeader) > div:active {
  background-color: #f5f8f9 !important;
}
.strategy-list-content :global(.ms-List-page) > div > div,
.strategy-list-content :global(.ms-List-page) > div > div:before {
  background-color: #f5f8f9;
  border: 1px solid transparent;
  border-left: none;
  border-right: none;
}
.strategy-list-content :global(.ms-List-page) > div > div:before {
  display: none;
}
.strategy-list-content :global(.ms-List-page) > div:nth-child(odd) > div {
  background-color: rgb(235, 238, 247);
  opacity: 0.1;
}
.strategy-list-content :global(.ms-List-page) > div:hover > div {
  border-color: #6c95ff;
}

.strategy-list-content :global(.ms-List-page) > div > div:global(.is-selected),
.strategy-list-content
  :global(.ms-List-page)
  > div
  > div:global(.is-selected:focus)
  > div
  > div,
.strategy-list-content
  :global(.ms-List-page)
  > div
  > div:global(.is-selected:hover)
  > div
  > div {
  color: #767676;
}
.strategy-list-content :global(.ms-List-page) > div > div > div > div {
  padding: 0;
  line-height: 40px;
}
.strategy-list-content
  :global(.ms-DetailsList)
  :global(.ms-DetailsRow-cell)
  > div {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.strategy-list-content :global(.ms-DetailsList) i:global(.ms-Check-circle) {
  opacity: 0;
}
.strategy-list-content :global(.ms-DetailsList) div:global(.ms-Check::before) {
  border-radius: 0;
  width: 12px;
  height: 12px;
  border-radius: 2px;
  border: solid 1px rgba(130, 144, 179, 0.4);
}
.strategy-list-content
  :global(.ms-DetailsList)
  div:global(.ms-Check.is-checked::before) {
  background-color: #4c84ff;
}
.strategy-list-content :global(.ms-DetailsList) i:global(.ms-Check-check) {
  left: -0.8px;
  font-size: 18px;
  line-height: 14px;
}
.strategy-list-content :global(.ms-DetailsHeader) button:focus i,
.strategy-list-content :global(.ms-DetailsRow) button:focus i {
  color: #fff !important;
}

.sync-status {
  width: 70px;
  height: 20px;
  text-align: center;
  line-height: 18px;
  border-radius: 2px;
  border: solid 1px #dfe1e5;
}
