.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.clusteringWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;

  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.showDataCtn {
  float: left;
  width: 82%;
  height: 100%;
}
.left > .title {
  text-align: left;
  color: #1d1d1d;
  padding-left: 16px;
}
.left > .title span {
  font-size: 16px;
  font-weight: 550;
  line-height: 26px;
}
.left .mWrap {
  width: 100%;
  height: calc(70% - 20px);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  padding: 16px;
  overflow-y: auto;
}
.left .mWrap > h3 {
  margin-bottom: 8px;
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
}
.left .bWrap {
  width: 100%;
  height: 30%;
  padding: 16px 16px;
}

.item :global(.show) {
  display: block;
}
.item :global(.hide) {
  display: none;
}

.left li {
  margin-bottom: 12px;
}
/* checkbox 样式 */
.checkboxWrap {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.checkboxWrap .checkbox {
  height: 24px;
}
.checkbox > label > div {
  width: 14px;
  height: 14px;
  border-radius: 2px;
  border-color: #ccc;
}
.checkbox:hover > label > div:global(.ms-Checkbox-checkbox) {
  border-color: #ccc;
}
.checkbox:global(.is-checked:hover) > label > div,
.checkbox:global(.is-checked) > label > div {
  background-color: #4c84ff;
}
.checkbox:hover > label > span:global(.ms-Checkbox-text),
.checkbox > label > span {
  font-size: 10px;
  color: #666;
}
.checkbox > label > div > i {
  font-size: 12px;
}
/* input 框样式 */
.inputWrap {
  width: 100%;
  margin-bottom: 4px;
  padding-left: 24px;
}
.inputWrap > label {
  color: #999;
  margin-bottom: 4px;
  display: inline-block;
}
.inputWrap > input {
  width: 100%;
  height: 24px;
  line-height: 22px;
  padding-left: 4px;
  border-radius: 4px;
  background: #ffffff;
  border: 1px solid rgba(204, 204, 204, 0.3);
}
.inputWrap > input:disabled {
  cursor: no-drop;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
}
.inputWrap > label > div > div {
  vertical-align: top
}
.checkboxWrap > div:last-child {
  flex: 1;
  text-align: right
}
.checkboxWrap > button {
  float: left;
}

