/*新增输出参数编辑内容部分 start*/
.editParameterModal {
  width: 64%;
  height: 640px;
  border-radius: 6px;
  font-size: 12px;
}
.editParameterModal .modal-title {
  height: 50px;
  background: linear-gradient(93deg, #5b88f7, #5bb7f7);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 16px;
}
.editParameterModal .modal-title span {
  font-size: 20px;
  color: #ffffff;
}
.editParameterModal .modal-title span {
  font-size: 20px;
  color: #ffffff;
}
.editParameterModal .modal-title .buttonCtn {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.editParameterModal .modal-title .buttonCtn div {
  width: 70px;
  height: 25px;
  border-radius: 4px;
  text-align: center;
  line-height: 25px;
  cursor: pointer;
}
.editParameterModal .modal-title .buttonCtn div:first-child {
  background-color: #ffffff;
  color: #5bb2f7;
  margin-right: 16px;
}
.editParameterModal .modal-title .buttonCtn div:last-child {
  color: #ffffff;
  border: solid 1px #ffffff;
}
.modal-body {
  height: calc(100% - 50px);
  padding: 16px;
  overflow: hidden;
}
.modal-body .bodyLeft {
  width: 60%;
  float: left;
}
.modal-body .bodyRight {
  width: 40%;
  float: left;
  height: 100%;
}
.modal-body .bodyLeft .flexCtn {
  display: flex;
  align-items: center;
}
.modal-body .bodyLeft .flexCtn > div {
  width: 33.33333%;
}
.modal-body .bodyLeft .flexCtn:first-child > div {
  width: 50%;
}
.modal-body .item {
  padding: 8px 15px;
}
.modal-body .item label {
  display: block;
  padding-bottom: 6px;
  color: #999999;
}
.modal-body .item label.requiredName:before {
  display: inline-block;
  margin-right: 4px;
  content: '*';
  line-height: 1;
  font-size: 12px;
  color: #f04134;
}
.modal-body .item input {
  width: 100%;
  height: 28px;
  border-radius: 2px;
  background-color: #f6f8f9;
  border: none;
  padding: 0 6px;
}
.modal-body .item textarea {
  width: 100%;
  height: 106px;
  border-radius: 2px;
  border: none;
  background-color: rgb(246, 248, 249);
  line-height: 24px;
}
.modal-body .item .inputWidth input {
  width: calc(100% - 36px);
  margin-right: 6px;
  color: #1d1d1d;
}
.modal-body .item > div {
  position: relative;
}
.modal-body .item > div > span.warning {
  position: absolute;
  top: -18px;
  left: 0px;
  width: 100%;
  color: #ff8181;
  background: #fff;
  overflow: hidden;
  height: 15px;
  text-overflow: ellipsis
}
.modal-body .item > div > input:global(.warning){
  border: 1px solid rgba(255, 129, 129, .6)
}
.modal-body .item .dropdown > span {
  height: 28px;
  line-height: 28px;
  font-size: 12px;
  background: #f6f8f9;
  border: none;
  color: #1d1d1d;
}
.modal-body .item .faultModalInfo {
  width: 100%;
  height: 100px;
  border-radius: 2px;
  border: solid 1px #eeeeee;
  color: #1d1d1d;
  padding: 4px;
  cursor: context-menu;
}
.modal-body .item .faultModalInfo span {
  width: 100%;
  display: block;
  padding: 4px;
}
.modal-body .bodyRight .addChartCtn {
  width: 130px;
  height: 28px;
  border-radius: 2px;
  color: #5b8af7;
  border: 1px dashed #6b95f8;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  cursor: pointer;
}
.modal-body .bodyRight .addChartCtn i {
  padding-right: 6px;
  font-size: 16px;
}
.modal-body .bodyRight > div {
  height: 50%;
}
.modal-body .bodyRight .item > div {
  height: calc(100% - 20px);
}
.modal-body .bodyRight .item > div.energyConfigDropDwon {
  border-bottom: 1px solid rgba(153, 153, 153, 0.1);
}
.modal-body .bodyRight > div .energyConfigCtn,
.modal-body .bodyRight > div .chartInfo {
  height: calc(100% - 48px);
  overflow-y: auto;
  overflow-x: hidden;
}
.modal-body .bodyRight .energyConfigCtn > div {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 6px;
}
.modal-body .bodyRight .energyConfigCtn > div span {
  width: 40%;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #999999;
}
.modal-body .bodyRight .energyConfigCtn > div input {
  width: 60%;
}
.modal-body .bodyRight > div .chartInfo > div > span {
  background: #cccccc;
  border-radius: 50%;
  position: relative;
  width: 16px;
  height: 16px;
  line-height: 16px;
  display: flex;
  justify-content: center;
  color: #ffffff;
}
.addChartInfo {
  width: 50px;
  height: 26px;
}
.chartInfo > div {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 8px;
}
.chartInfo > div > div {
  width: 45%;
}
/*新增输出参数编辑内容部分 end*/
