.container {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.container li {
  width: 100%;
  clear: both;
}

/* slider  */

.svm-slider {
  width: 260px;
  height: 100%;
  overflow: hidden;
  float: left;
  background-color: #fff;
}

/* 头部标题 */

.svm-title {
  width: 100%;
  height: 32px;
  line-height: 32px;
  padding: 0 20px;
  color: #333;
  background-color: #f2f5f6;
}
.container h3 {
  font-size: 14px;
  color: #333333;
  letter-spacing: 0;
}
.svm-title .cancel-icon {
  float: right;
  width: 18px;
  height: 18px;
  background-color: #b3b3b3;
  border-radius: 50%;
  line-height: 18px;
  margin-top: -25px;
  text-align: center;
}

.svm-title .cancel-icon i {
  color: #f2f5f6;
  font-size: 12px;
  font-weight: 700;
}

/* 选择模块 */
.svm-select {
  width: 100%;
  height: 82px;
  padding: 0 20px 19px 21px;
  border-radius: 4px;
  border-bottom: 1px solid #e8e9eb;
}
.svm-select-name h3 {
  line-height: 30px;
  margin-top: 6px;
  margin-bottom: 12px;
}
.Dropdown-svm:global(.is-open) {
  margin-bottom: 19px;
  background: #f2f2f2;
  border-radius: 4px;
  height: 24px;
}
.Dropdown-svm > span:first-child {
  height: 24px;
  line-height: 22px;
  background: #f2f2f2;
  font-size: 12px;
  letter-spacing: 0;
}
.Dropdown-svm > span:first-child,
.Dropdown-svm:hover > span:first-child,
.Dropdown-svm:focus > span:first-child {
  border-color: #e8e8e8;
  border-radius: 4px;
  color: #999;
}
.Dropdown-svm > span:last-child {
  height: 24px;
  line-height: 22px;
  right: 4px;
}
.svm-select > div:last-child > span {
  border: none;
}

/* 类型 */
.svm-type {
  width: 100%;
  border-bottom: 1px solid #e8e9eb;
  padding: 17px 20px;
}

.container li:nth-child(1) .svm-item {
  margin-top: 10px;
}
.svm-item.check {
  width: 50%;
  margin-top: 10px;
}
.container li::after {
  content: '';
  display: block;
  height: 0;
  line-height: 0;
  visibility: hidden;
  clear: both;
}
.svm-item {
  width: 20%;
  height: 17px;
  line-height: 17px;
  margin-bottom: 5px;
  float: left;
  position: relative;
}
.svm-item :global(.is-checked) i {
  background: #4c83ff;
  border-color: #e8e8e8;
}
.slider-checkbox:hover > label > div,
.svm-item > button:hover > label > div {
  border-color: #e8e8e8 !important;
  background: #f2f2f2;
}
.slider-checkbox > label > div,
.svm-item > button > label > div {
  width: 14px;
  height: 14px;
  opacity: 0.8;
  border-radius: 2px;
  border: solid 1px #e8e8e8;
  background-color: #f2f2f2;
}
.svm-item > button {
  float: left;
}
.svm-item > div {
  position: absolute;
  top: 1px;
}
.svm-item > button > span {
  color: #555 !important;
  font-size: 12px !important;
}

.svm-item-spinbutton {
  float: right;
  margin-top: 7px;
  color: #555;
  display: flex;
}
.svm-item-spinbutton > label {
  margin-right: 10px;
  line-height: 24px;
  font-size: 12px;
  color: #555555;
  letter-spacing: 0;
}
.svm-item-spinbutton > input {
  width: 56px;
  height: 24px;
  padding-left: 5px;
  border-radius: 2px;
  border: 1px solid #e8e8e8;
  background: #f2f2f2;
}
.svm-kernel {
  width: 100%;
  border-bottom: 1px solid #e8e9eb;
  padding: 20px;
}

/* 侧边栏底部 */
.slider-footer {
  font-size: 12px;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  padding: 16px 24px 0 24px;
}
.slider-button {
  background: #4c84ff;
  border-radius: 2px;
  height: 20px;
  line-height: 20px;
  margin-bottom: 8px;
}
.slider-checkbox {
  float: left;
  margin: 4px 10px 0 0 !important;
}
.send-button {
  float: left;
  background: #f2f2f2;
  border: 1px solid #e8e8e8;
  border-radius: 2px;
  width: 188px;
  height: 20px;
  line-height: 18px;
  color: #555555;
}

/* content */
.svm-content {
  width: calc(100% - 260px);
  height: 100%;
  float: left;
  background: #f1f4f5;
}
.close {
  height: 33px;
  background-color: #fff;
  padding-top: 7px;
  padding-right: 16px;
}

.close .cancel-icon {
  float: right;
  width: 18px;
  height: 18px;
  background-color: #b3b3b3;
  border-radius: 50%;
  line-height: 18px;
  text-align: center;
}

.close .cancel-icon i {
  color: #f2f5f6;
  font-size: 12px;
  font-weight: 700;
}

.svm-echart {
  width: 100%;
  height: 360px;
  padding: 32px;
}

.chart {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.svm-list-option li:first-child .svm-item-spinbutton {
  width: 100%;
}
.svm-list-option li:first-child .svm-item-spinbutton > div {
  display: flex;
}
.svm-list-option li:first-child .svm-item-spinbutton > div > div:first-child {
  word-break: normal;
  margin: 0;
  flex: 2;
}
.svm-list-option li:first-child .svm-item-spinbutton > div > div:last-child {
  flex: 1;
}
.svm-list-option .svm-item {
  width: 55%;
  margin-top: 10px;
}
.tableWrap {
  width: calc(100% - 64px);
  height: calc(100% - 392px);
  margin: 0 32px 32px 32px;
  padding: 24px;
  overflow-y: auto;
  background: #fff;
}

.tableWrap table {
  width: 100%;
  border-collapse: collapse;
}
.tableWrap thead {
  background: #f2f2f2;
}
.tableWrap th,
.tableWrap td {
  padding: 10px 6px;
  border: 1px solid #e8e8e8;
  text-align: center;
}
.tableWrap td:first-child,
.tableWrap td:last-child {
  width: 200px;
}
.tableWrap td span {
  display: inline-block;
  text-align: center;
}
