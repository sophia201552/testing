.PCAPanel {
    width: 100%;
    height: 100%;
    overflow: hidden;
  }
  .panelLeft {
    width: 200px;
    height: 100%;
    float: left;
    color: #666;
    margin-top: 24px;
    border-right: 1px solid rgba(153, 153, 153, 0.3);
  }
  .panelRight {
    width: calc(100% - 200px);
    height: 100%;
    float: left;
  }
  .panelLeft .panelTitle {
    text-align: left;
    color: #1d1d1d;
    padding-left: 16px;
  }
  .panelLeft .panelTitle span {
    font-size: 20px;
    font-weight: 500;
    line-height: 26px;
  }
  .panelLeft .slider {
    height: calc(100% - 46px);
    overflow-x: hidden;
    overflow-y: auto;
  }
  .panelLeft .slider .methodCtn {
    height: calc(100% - 200px);
    padding: 16px;
  }
  .panelLeft .slider .dataSetCtn {
    height: 200px;
    padding: 16px;
    border-top: 1px solid rgba(153, 153, 153, 0.15);
  }
  .panelLeft .slider .methodCtn > h3 {
    margin-bottom: 8px;
    width: 100%;
    height: 16px;
    line-height: 16px;
    font-size: 14px;
    color: #1d1d1d;
    padding: 0 0 0 8px;
    border-left: 4px solid #1d1d1d;
    font-weight: 600
  }
  .panelLeft .slider .methodCtn > div {
    width: 100%;
    height: calc(100% - 24px);
    overflow-y: auto;
    overflow-x: hidden;
  }
  .panelLeft .slider .methodCtn .labelInput {
    width: 100%;
    margin-bottom: 6px;
  }
  .panelLeft .slider .methodCtn .labelInput label {
    display: block;
    margin-bottom: 6px;
    color: #999999;
  }
  .panelLeft .slider .methodCtn .labelInput input {
    width: 100%;
    height: 24px;
    line-height: 22px;
    outline: none;
    padding-left: 4px;
    border-radius: 4px;
    background: #ffffff;
    border: 1px solid rgba(204, 204, 204, 0.3);
  }
  .panelRight .echrtasCtn {
    width: 100%;
    height: 360px;
    padding-top: 24px;
  }
  .panelRight .footer {
    width: 100%;
    height: calc(100% - 360px);
    background: rgba(235, 238, 247, 0.7);
    overflow-y: auto;
    overflow-x: hidden;
  }
  .panelRight .footer .footerInfoCtn {
    width: 100%;
    height: 100%;
  }
  .panelRight .footer .footerInfoCtn > div {
    float: left;
    height: 100%;
    padding: 15px;
  }
  .panelRight .footer .footerInfoCtn .matrixCtn {
    width: calc(100% - 300px);
    border-right: 1px solid rgba(153, 153, 153, 0.15);
  }
  .panelRight .footer .footerInfoCtn .paramesCtn {
    width: 300px;
  }
  .panelRight .footer .matrixCtn .tableCtn,
  .panelRight .footer .paramesCtn > div {
    border: 1px solid rgba(153, 153, 153, 0.15);
  }
  .panelRight .footer .matrixCtn .tableHeader {
    background: #f5f8f9;
    color: rgba(102, 102, 102, 0.7);
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    display: flex;
    border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  }
  .panelRight .footer .matrixCtn .tableHeader span,
  .panelRight .footer .matrixCtn .tableBody > div span,
  .panelRight .footer .paramesCtn > div span {
    border-right: 1px solid rgba(153, 153, 153, 0.15);
    padding: 0 6px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
  }
  .panelRight .footer .matrixCtn .tableHeader span:last-child,
  .panelRight .footer .matrixCtn .tableBody > div span:last-child,
  .panelRight .footer .paramesCtn > div span:last-child {
    border-right: none;
  }
  .panelRight .footer .matrixCtn .tableBody > div,
  .panelRight .footer .paramesCtn > div > div {
    height: 40px;
    line-height: 40px;
    display: flex;
    border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  }
  .panelRight .footer .matrixCtn .tableBody > div:last-child,
  .panelRight .footer .paramesCtn > div > div:last-child {
    border-bottom: none;
  }
  .panelRight .footer .paramesCtn > div span:first-child {
    width: 90px;
  }
  .panelRight .footer .paramesCtn > div span:last-child {
    width: calc(100% - 90px);
  }
  label > div > div {
    margin-top: 1px;
  }
  