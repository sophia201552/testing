<!DOCTYPE html>
<html lang="zh">

<head>
    {% if icon %}
        <link rel="shortcut icon" type="image/ico" href="/static/images/{{ icon }}.ico">
    {% else %}
        <link rel="shortcut icon" type="image/ico" href="/static/images/favicon.ico">
    {% endif %}
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    {% if title %}
        <title>{{ title }}</title>
    {% else %}
        <title>Platform Management</title>
    {% endif %}


    {% assets "css_index" %}
    <link rel="stylesheet" type="text/css" href="{{ ASSET_URL }}"> {% endassets %}
    {% assets "platform_css_bundle" %}
    <link rel="stylesheet" type="text/css" href="{{ ASSET_URL }}"> {% endassets %}
    <link id="darkSkin" rel="stylesheet" type="text/css" href="/static/content/index-black.css?1502258875578">

    <script>
        var CompanyConfig = {
            'code':'{{ code }}',
            'logoId':'{{logoID}}',
            'logoStyle':'{{logo_style}}',
        }
        if ('{{ lang }}' === 'en' || '{{ lang }}' === 'zh') {
            // window.localStorage.setItem('isUserSelectedLanguage', '{{ lang }}');
            window.localStorage.setItem('language', '{{ lang }}');
        }
        if(location.host=='industrytech.smartbeop.com'){
                var rurl = "";
                var links = document.getElementsByTagName("link");
                var link = {};
                for(var i=0;i<links.length;i++){
                    link = links[i];
                    if(link.rel === "shortcut icon"){
                        link.href='/static/images/AdOPT.ico';
                        break;
                    }
                }
                document.querySelector('title').text='AdOPT'
            }
        var _hmt = _hmt || [];
    </script>

    <style>
        html, body, #mainContainer {
            margin: 0; padding: 0; width: 100%; height: 100%; overflow: hidden; position:relative;
        }

    </style>

    {% block style %} {% endblock %}
</head>

<body style="font-family: 'Helvetica', Arial, 'Microsoft YaHei', '微软雅黑'; overflow-x:hidden;">

    <div id="mainContainer">
        {% block indexMain %} {% endblock %}
    </div>

    <article class="modal fade modal-light gray-scrollbar" id="dialogModal" tabindex="-1" role="dialog"
            aria-labelledby="myModalLabel"
            aria-hidden="true">
        <div class="modal-dialog modal-lg" id="dialogContent"></div>
    </article>

{% block Spinner %} {% endblock %} {% block script %} {% endblock %}
{% assets "platform_js_bundle" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% assets "js_screen" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% assets "platform_screen_js_bundle" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% assets "web_factory_ob_page_js_bundle" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% assets "web_factory_ob_dashboard_js_bundle" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% assets "web_factory_ob_report_js_bundle" %}<script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}
{% block scriptLoaded %} {% endblock %}

