import pytestfrom beopWeb.mod_admin import controllersfrom beopWeb.mod_admin.FavoriteProject import FavoriteProjectfrom beopWeb.mod_admin.ProjectPermission import ProjectPermissionuserId=2265roleId=1035@pytest.mark.p0@pytest.mark.usefixtures("deleteRoleUser")@pytest.mark.parametrize(('rq_data'), [    ({"roleId":roleId,"userId":userId}),])def test_add_role_user(rq_data):    rt = controllers.do_add_role_user(rq_data)    assert rt,'add role user fail'    userRole_data = ProjectPermission.get_role_user(userId, "userId", "userfullname","roleId")    rq_data.update({"userfullname":"AutoTester"})    assert rq_data in userRole_data,'add role user fail ,expected = {0} is not in user_role table'@pytest.fixture(scope='function')def deleteRoleUser(request):    def fin():        controllers.do_delete_role_user({"roleId":roleId,"userId":userId})    request.addfinalizer(fin)