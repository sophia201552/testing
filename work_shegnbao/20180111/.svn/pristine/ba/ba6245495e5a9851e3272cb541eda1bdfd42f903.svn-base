/* Tabs start */
.tabsWrap {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.tabsWrap .iconTitle {
  padding-top: 20px;
}
.tabsWrap .iconTitle i {
  font-size: 18px;
  margin-bottom: 10px;
}
.tabsWrap > ul {
  background-color: rgb(47, 59, 85);
}
.tabsWrap .vertical {
  display: inline-block;
  vertical-align: top;
}
.tabsWrap .vertical + div {
  display: inline-block;
}
.tabsWrap .tabsTitle {
  background-color: rgb(47, 59, 85);
  color: #98a6b7;;
  text-align: center;
  user-select: none;
  cursor: pointer;
}
.tabsWrap .horizontal .tabsTitle {
  float: left;
}
.tabsWrap .horizontal .tabsTitle.active {
  background-image: linear-gradient(45deg, rgba(24, 30, 54, 0.3) 25%, transparent 25%, transparent 50%, rgba(24, 30, 54, 0.3) 50%, rgba(24, 30, 54, 0.3) 75%, transparent 75%, transparent);
  background-size: 6px 6px;
}
.tabsWrap .vertical .tabsTitle {
}
.tabsWrap .tabsTitle.active {
  background-color: #212a47;
  color: #dcdee2;
 
}
/* Tabs end */
