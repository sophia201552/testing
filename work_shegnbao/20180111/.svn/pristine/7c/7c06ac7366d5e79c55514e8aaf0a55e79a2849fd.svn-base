/* List start */
.listWrap {
  width: 100%;
  height: 100%;
}
.listWrap li {
  border-bottom: 1px solid transparent;
}
.listWrap .titleWrap {
  position: relative;
  cursor: pointer;
}
.listWrap .titleWrap .title {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-box-shadow: none;
  box-shadow: none;
  margin: 0;
  cursor: pointer;
  display: block;
  padding: 0 32px 0 12px;
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 14px;
  background-color: rgb(33, 42, 71);
  font-weight: 400;
  user-select: none;
}
.listWrap .titleWrap.selected .title {
  background-color: rgb(54, 63, 93);
}
.listWrap .titleWrap i {
  position: absolute;
  font-size: 12px;
  top: 1px;
  pointer-events: none;
  right: 12px;
}

/* List end */
