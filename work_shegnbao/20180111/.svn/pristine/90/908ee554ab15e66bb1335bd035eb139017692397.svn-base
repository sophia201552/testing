.configTabPane {
  width: 100%;
  height: 100%;
  padding-top: 16px;
}
.configTabPane .group {
  width: 100%;
  margin-top: 16px;
  padding: 0 16px;
}
.configTabPane .group:first-child {
  margin-top: 0;
}
.configTabPane .group::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.configTabPane .group label {
  float: left;
  color: #fff;
  height: 36px;
  line-height: 26px;
  width: 40%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden
}
.configTabPane .group .dropdownlistWrap {
  width: 60%;
  float: left;
  height: 36px;
}
.configTabPane .group .dropdownlistWrap input {
  background-color: rgb(25, 31, 55);
  height: 24px;
  width: 100%;
  line-height: 24px;
  margin-top: 6px;
  padding: 0 12px;
  border-radius: 4px;
  border: none;
  color: rgb(110, 119, 150);
}
.configTabPane .group .dropdownlistWrap input:focus {
  outline: none;
}
