    #wrapFunction {
        position: absolute;
        left: 0;
        width: 200px;
        height: 100%;
        background: #23283E;
        color: white;
        transition: height 3s ease-out;
        z-index: 13;
        overflow: hidden;
        display: none;
    }
    
    #wrapFunction .navModule::before {
        display: table;
        content: '';
    }
    
    #wrapFunction .navModule::after {
        display: table;
        content: '';
        clear: both;
    }
    
    #wrapFunction.focus {
        display: block;
    }
    
    .wrapFunction {
        top: 100%;
        animation: mymove 0s;
        animation-fill-mode: forwards;
    }
    
    .wrapFunctionNew {
        top: 0% !important;
        /* animation: mymoveNew 1s !important;
        animation-fill-mode: forwards !important; */
    }
    
    @keyframes mymoveNew {
        /* from {top:0%;left: 0;opacity: 1}
            to {top:100%;left: 0;opacity: 0.9;height: 0;} */
        from {
            top: 0%;
            left: 0px;
            opacity: 1
        }
        to {
            top: 0%;
            left: -200px;
            opacity: 1;
        }
    }
    
    @keyframes mymove {
        /* 0% {top:100%;left: 0px;opacity: 0;height: 0;}
            100%{top:0;left: 0;opacity: 1;}  */
        from {
            top: 0%;
            left: -200px;
            opacity: 0.9;
        }
        to {
            top: 0%;
            left: 0;
            opacity: 1;
        }
    }
    
