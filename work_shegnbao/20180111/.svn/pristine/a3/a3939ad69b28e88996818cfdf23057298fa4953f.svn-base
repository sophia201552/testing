    /* 导航动画开始 */
    
    .ciycleDiv {
        position: absolute;
        bottom: 0px;
        background: rgb(253, 253, 253);
        width: 50px;
        height: 50px;
        border-radius: 50px;
        /* top: 100% !important; */
        animation: ciycle 0.6s !important;
        animation-timing-function: linear;
        animation-fill-mode: forwards !important;
    }
    
    @keyframes ciycle {
        0% {
            left: 100px;
            width: 50px;
            height: 50px;
            border-radius: 50px;
            opacity: 0.8;
        }
        100% {
            bottom: -150px;
            width: 1200px;
            height: 1200px;
            left: -500px;
            border-radius: 1200px;
            opacity: 0;
            display: none;
        }
    }
    
    .wrapuser {
        opacity: 0;
        position: relative;
        bottom: 0;
        animation: wrapuser 0.4s;
        animation-delay: 0.4s;
        animation-fill-mode: forwards;
    }
    
    @keyframes wrapuser {
        0% {
            transform: translateY(100px);
            opacity: 0.5;
        }
        100% {
            transform: translateY(0);
            opacity: 1;
        }
    }
    
    .wrapNavModule[data-block="function"] {
        opacity: 0;
        position: relative;
        /* bottom: 0; */
        animation: wrapFunction 0.4s;
        animation-delay: 0.5s;
        animation-fill-mode: forwards;
    }
    
    @keyframes wrapFunction {
        0% {
            transform: translateY(100px);
            opacity: 0.5;
        }
        100% {
            transform: translateY(0);
            opacity: 1;
        }
    }
    
    .wrapNavModule[data-block="project-function"] {
        opacity: 0;
        position: relative;
        /* bottom: 0; */
        animation: wrapProjectFunction 0.4s;
        animation-delay: 0.6s;
        animation-fill-mode: forwards;
        padding-top: 35px!important;
    }
    
    @keyframes wrapProjectFunction {
        0% {
            transform: translateY(100px);
            opacity: 0.5;
        }
        100% {
            transform: translateY(0);
            opacity: 1;
        }
    }
    
    .functionBottomWrap {
        opacity: 0;
        position: relative;
        /* bottom: 0; */
        animation: functionBottomWrap 0.4s;
        animation-delay: 0.7s;
        animation-fill-mode: forwards;
    }
    
    @keyframes functionBottomWrap {
        0% {
            transform: translateY(100px);
            opacity: 0.5;
        }
        100% {
            transform: translateY(0);
            opacity: 1;
        }
    }
    
    .ciycleDivNew {
        position: absolute;
        top: 0px;
        right: 0px;
        background: rgb(253, 253, 253);
        width: 50px;
        height: 50px;
        border-radius: 50px;
        /* top: 100% !important; */
        animation: ciycleDivNew 0.6s !important;
        animation-fill-mode: forwards !important;
    }
    
    @keyframes ciycleDivNew {
        0% {
            top: 0px;
            right: 0px;
            width: 50px;
            height: 50px;
            border-radius: 50px;
            opacity: 0.8;
        }
        100% {
            top: -150px;
            width: 1000px;
            height: 1000px;
            right: -400px;
            border-radius: 1000px;
            opacity: 0;
            display: none;
        }
    }
