.clear::after {
  content: '';
  display: block;
  height: 1px;
  width: 0;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.over {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dataSortingPanel {
  width: 100%;
  height: 100%;
  position: relative;
  padding-top: 16px;
}
.left-panel {
  width: 18%;
  height: 100%;
  overflow-y: auto;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.right-panel {
  width: 82%;
  height: 100%;
}
.left-panel > .title {
  width: 100%;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left-panel > .content{
  width: 100%;
  height: calc(100% - 68px);
}
.left-panel > .foot{
  width: 100%;
  height: 48px;
  padding: 8px;
}
.left-panel .exportBtn{
  display: block;
  margin: 0 auto;
}
.row{
  line-height: 20px;
}
.numberText{
  height: 20px;
  line-height: 20px;
  padding-left: 4px;
  outline: 0;
  box-shadow: none;
  color: #333333;
  border: 1px solid #a6a6a6;
  background: #ffffff;
}
.numberText:hover{
  border-color: #212121;
}
.numberText:focus{
  border-color: #0078d7;
}
/* RenameText */
.RenameText{
  width: 100%;
  position: relative;
  cursor: pointer;
}
.RenameText .text{
  width: 100%;
  height: 20px;
  line-height: 20px;
  outline: 0;
  box-shadow: none;
  color: #333333;
  padding: 0 12px 0 12px;
  width: 100%;
  text-overflow: ellipsis;
  border: 1px solid #a6a6a6;
  background: #ffffff;
}
.RenameText .text:hover{
  border-color: #212121;
}
.RenameText .text:focus{
  border-color: #0078d7;
}
.RenameText .text.error{
  border-color: #a80000;
}
.RenameText .errorText{
  color: #a80000;
}
.RenameText .errorText.hide{
  display: none;
}
/* RenameText end */

/* dropdown */
.dataSortingPanel :global(.ms-Dropdown-title) {
  background: #ffffff;
  border: 1px solid rgba(204, 204, 204, 0.3);
  border-radius: 4px;
}
.dataSortingPanel :global(.ms-Dropdown){
  font-size: 12px;
}
.dataSortingPanel :global(.ms-Dropdown) > span {
  height: 20px;
  line-height: 20px;
}
/* dropdown end */