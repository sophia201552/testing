/* select start */
.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.selectWrap {
  width: 100%;
  position: relative;
  color: rgba(0, 0, 0, 0.65);
  font-size: 12px;
}
.selectBox {
  min-height: 28px;
  cursor: text;
  padding-bottom: 3px;
  zoom: 1;
  user-select: none;
  display: block;
  background-color: #fff;
  border-radius: 4px;
  border: 1px solid #d9d9d9;
  -webkit-transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
}
.selectBox::before {
  content: ' ';
  display: table;
}
.selectBox::after {
  content: ' ';
  display: table;
  clear: both;
  visibility: hidden;
  font-size: 0;
  height: 0;
}
.rendered {
  margin-left: 5px;
  margin-bottom: -3px;
  height: auto;
  display: block;
  margin-right: 7px;
  position: relative;
  line-height: 26px;
}
.rendered::after {
  content: '.';
  visibility: hidden;
  pointer-events: none;
  display: inline-block;
  width: 0;
}
.rendered ul li {
  margin-top: 3px;
  height: 20px;
  line-height: 20px;
}
.rendered ul li.item {
  user-select: none;
  color: rgba(0, 0, 0, 0.65);
  background-color: #f3f3f3;
  border-radius: 4px;
  cursor: default;
  float: left;
  margin-right: 4px;
  max-width: 99%;
  position: relative;
  overflow: hidden;
  -webkit-transition: padding 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: padding 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  padding: 0 20px 0 10px;
}
.rendered ul .searchLi {
  width: auto;
  padding: 0;
  max-width: 100%;
  float: left;
  position: static;
}
.rendered ul .searchLi .searchLiBox {
  width: 100%;
  height: 100%;
  display: inline-block;
  position: relative;
}
.rendered ul .searchLi .searchLiBox input {
  width: 0.75em;
  min-width: 6px;
  max-width: 100%;
  border-width: 0;
  font-size: 100%;
  height: 100%;
  background: transparent;
  outline: 0;
  border-radius: 4px;
  line-height: 1;
}
.rendered ul .searchLi .searchLiBox span {
  position: absolute;
  top: 0;
  left: -9999px;
  white-space: pre;
  pointer-events: none;
}
.rendered ul li.item .itemContent {
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 100%;
  -webkit-transition: margin 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: margin 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
}
.rendered ul li.item .remove {
  font-style: normal;
  vertical-align: baseline;
  text-align: center;
  text-transform: none;
  line-height: 1;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: rgba(0, 0, 0, 0.43);
  line-height: inherit;
  cursor: pointer;
  font-weight: 700;
  -webkit-transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
  display: inline-block;
  -webkit-transform: scale(0.66666667) rotate(0deg);
  -ms-transform: scale(0.66666667) rotate(0deg);
  transform: scale(0.66666667) rotate(0deg);
  -ms-filter: "progid:DXImageTransform.Microsoft.Matrix(sizingMethod='auto expand', M11=1, M12=0, M21=0, M22=1)";
  zoom: 1;
  position: absolute;
  right: 4px;
  padding: 0 0 0 8px;
  -webkit-filter: none;
  filter: none;
  font-size: 12px;
}
.background {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  display: none;
  z-index: 1000001;
}
.dropdownUl {
  overflow: auto;
  position: absolute;
  border: 1px solid #d9d9d9;
  background: #fff;
}
.background.show {
  display: block;
}
.dropdownUl .dropdownLiWrap {
  overflow: auto;
  height: 100%;
}

.dropdownLi {
  height: 30px;
  line-height: 30px;
  box-sizing: border-box;
  cursor: pointer;
  color: rgba(0, 0, 0, 0.65);
  padding: 0 32px 0 12px;
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.dropdownLi.selected {
  position: relative;
}
.dropdownLi.selected::after {
  display: block;
  content: 'âˆš';
  color: green;
  position: absolute;
  right: 16px;
  top: 0;
}
.dropdownLi:hover,
.dropdownLi.active {
  background-color: rgba(0, 0, 0, 0.1);
}
/* select end */

/* black skin */
.black.selectWrap {
  color: rgb(110, 119, 150);
}
.black .selectBox {
  background-color: rgb(25, 31, 55);
  border: 1px solid rgb(25, 31, 55);
}
.black .rendered ul li.item {
  color: #fff;
  background-color: rgba(99, 207, 65, 0.7);
}
.black .rendered ul li.item .remove {
  color: #fff;
}
.black .rendered ul li .searchLiBox input {
  color: rgb(110, 119, 150);
}
.black.dropdownUl {
  border: 1px solid #151b32;
  background: rgb(25, 31, 55);
}

.black .dropdownLi {
  color: rgb(110, 119, 150);
}
.black .dropdownLi.selected::after {
  color: #63cf41;
}
.black .dropdownLi:hover,
.black .dropdownLi.active {
  background-color:#2A3045;
}
/* black skin end */
