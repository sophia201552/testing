.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.dataComplementWrap {
  width: 100%;
  height: 100%;
  padding: 24px 0 0 0;
}
.left {
  float: left;
  width: 18%;
  height: 100%;
  border-right: 1px solid rgba(153, 153, 153, 0.3);
}
.showDataCtn {
  float: left;
  width: 82%;
  height: 100%;
}
.left > .title {
  width: 100%;
  height: 20px;
  line-height: 1;
  font-size: 20px;
  font-weight: 500;
  color: #1d1d1d;
  padding: 0 16px;
}
.left .mWrap {
  width: 100%;
  height: calc(70% - 20px);
  border-bottom: 1px solid rgba(153, 153, 153, 0.15);
  padding: 24px 16px 0 16px;
}
.left .bWrap {
  width: 100%;
  height: 30%;
  padding: 16px 16px;
}
.left .mWrap .api2 > label {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
}
.left .mWrap .api2 > div {
  margin-top: 8px;
  position: relative;
}
.left .mWrap .api2 > input {
  font-size: 14px;
  height: 28px;
  width: 100%;
  border-radius: 4px;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
  padding: 4px 6px;
}

.item :global(.show) {
  display: block;
}
.item :global(.hide) {
  display: none;
}

.left li {
  margin-bottom: 12px;
}

/* checkbox 样式 */
.checkbox {
  height: 24px;
}
.checkbox > label > div {
  width: 14px;
  height: 14px;
  border-radius: 2px;
  border-color: #ccc;
}
.checkbox:hover > label > div:global(.ms-Checkbox-checkbox) {
  border-color: #ccc;
}
.checkbox:global(.is-checked:hover) > label > div,
.checkbox:global(.is-checked) > label > div {
  background-color: #4c84ff;
  vertical-align: middle;
}
.checkbox:hover > label > span:global(.ms-Checkbox-text),
.checkbox > label > span {
  font-size: 10px;
  color: #666;
  vertical-align: middle;
}
.checkbox > label > div > i {
  font-size: 12px;
}
/* input 框样式 */
.inputWrap {
  width: 100%;
  margin-bottom: 4px;
  padding-left: 24px;
}
.inputWrap > label {
  color: #999;
  margin-bottom: 4px;
  display: inline-block;
}
.inputWrap > input {
  width: 100%;
  height: 24px;
  line-height: 22px;
  outline: none;
  padding-left: 4px;
  border-radius: 4px;
  background: #ffffff;
  border: 1px solid rgba(204, 204, 204, 0.3);
}
.inputWrap > input:disabled {
  cursor: no-drop;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
}
.inputWrap > input:disabled {
  cursor: no-drop;
  background-color: rgba(204, 204, 204, 0.3);
  border: none;
}

/* 数据集样式 */

.clear::after {
  display: block;
  content: '';
  clear: both;
  width: 0;
  height: 1px;
}
.datasetWrap {
  width: 100%;
  height: 100%;
}
.datasetWrap > .title {
  width: 100%;
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #1d1d1d;
  padding: 0 0 0 8px;
  border-left: 4px solid #1d1d1d;
}
.datasetWrap > .content {
  width: 100%;
  height: calc(100% - 16px);
  overflow-y: aoto;
  padding-top: 16px;
}
.datasetWrap > .content .item {
  float: left;
  width: 48%;
  height: 28px;
  padding: 0 8px;
  line-height: 26px;
  border-radius: 4px;
  border: solid 1px rgba(204, 204, 204, 0.3);
  background-color: rgba(204, 204, 204, 0.3);
  margin-right: 4%;
  font-size: 12px;
  text-align: center;
  color: #1d1d1d;
  cursor: pointer;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 8px;
}
.datasetWrap > .content .item:hover,
.datasetWrap > .content .item.active {
  background-color: #fff;
  border: solid 1px #10b8f9;
  color: #10b8f9;
}
.datasetWrap > .content .item:nth-child(even) {
  margin-right: 0;
}

.checkboxWrap {
  position: relative;
}
.checkboxWrap > div {
  position: absolute;
  top: 6px;
}
.checkboxWrap > button {
  float: left;
}
