.container {
  height: 40px;
  line-height: 40px;
  background-color: #2f3a54;
  color: rgb(136, 172, 255);
  padding: 0 20px;
}
.container:after {
  clear: both;
}
.title {
  float: left;
}
.toolbox {
  float: right;
}
.container button {
  margin: 0;
  border: none;
  background: transparent;
  cursor: pointer;
  outline: none;
}
.title .btn-back {
  height: 22px;
  line-height: 22px;
  font-weight: normal;
  color: #5b8af7;
  font-size: 14px;
  color: rgb(152, 166, 183);
}
.title .btn-back > span {
  display: inline-block;
}
.title .btn-back span.backIcon {
  width: 22px;
  height: 22px;
  border-radius: 100%;
  vertical-align: bottom;
  margin-right: 10px;
}
.titleText {
  float: left;
  width: calc(100% - 218px);
  height: 40px;
  line-height: 40px;
  color: rgb(152, 166, 183);
  font-size: 14px;
  text-align: center;
}

div.exportBtn,
div.saveBtn {
  width: 86px;
  height: 26px;
  line-height: 26px;
  margin: 7px 0 0 7px;
  border-radius: 4px;
  box-shadow: 2px 0 10px 0 rgba(23, 28, 53, 0.54);
  border: solid 1px rgba(91, 138, 247, 0.6);
  color: rgb(255, 255, 255);
  font-size: 12px;
  float: left;
  text-align: center;
  cursor: pointer;
}

div.saveBtn {
  background-color: rgba(91, 138, 247, 0.6);
}
div.saveBtn:hover {
  background-color: rgb(91, 138, 247);
}
div.exportBtn:hover {
  border-color: rgb(91, 138, 247);
}
