    #hourlyPower,#dailyPower,#monthlyPower{
        height:calc(100% - 61px);
        position: relative;
    }
    .energyStatistic .statisTime{
        margin-left: calc(100% - 320px);
        /*display: none;*/
    }
    .fixedContent .col-xs-4{padding:0}
    /*.arrorDisplyMonthOn{
        margin-left:7px;
    }*/
    .fixedEnergy .tooltipContent{
        border-top-width:1px;
        border-radius:3px;
        background: rgba(245, 245, 246, 0.9);
        border:1px solid #DFE2E5;
        width:180px;
    }
    .fixedEnergy .tooltip-inner{
        background: rgba(245, 245, 246, 0.9);
        border:1px solid #DFE2E5;
        color:#808fa3;
        white-space: nowrap;
    }
    .fixedEnergy .tooltipModal{
        display:flex;
        height:26px;
        justify-content:center;
        align-items:center;
    }
    .fixedEnergy .tooltipModals{
        width:108px;
        height:26px;
        margin:0 auto;
        line-height:26px;
    }
    .compareAllStyleMoM,.compareAllStyleYoY{
        display: inline-block;
        width:27px;
        text-align:right
    }
    .fixedEnergy .tooltip.top .tooltip-arrow{
        border-top-color:#DFE2E5;
    }
    .fixedEnergy .tooltip.bottom .tooltip-arrow{
        border-bottom-color:#DFE2E5;
    }
    .fixedEnergy .tooltip.left .tooltip-arrow{
        border-left-color:#DFE2E5;
    }
    .fixedEnergy .tooltip.right .tooltip-arrow{
        border-right-color:#DFE2E5;
    }
    .fixedContent .col-xs-5,.fixedContent .col-xs-2{
        padding:0
    }
    .compareBox{
        position:relative;
        display:inline-block;
        float:right;
        /*width: 65px;*/
        text-align:right
    }
    /*.compareBox .glyphicon{
        position:absolute;
        top: 2.2px;
        left: -12px;
    }*/
    @media screen and (max-width:1650px){
        #energyOverview .energyCostStyle{
            font-size:18px;
        }
        #energyOverview .compareAllStyle{
