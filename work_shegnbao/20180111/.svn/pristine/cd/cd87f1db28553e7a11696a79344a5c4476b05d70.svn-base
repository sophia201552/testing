<style>
    #energyOverview{
        width:100%;
        height:100%;
        padding:10px;
    }
    #energyOverview .energyOverviewH{
        width:100%;
        height:50%;
        margin-bottom:10px;
    }
    #energyOverview .energyOverviewReal{
        width:100%;
        height:calc(50% - 10px);
    }
    #energyOverview .energyList{
        width:calc(33.33% - 5px);
        height:100%;
        margin-right:10px;
        float:left;
    }
    #energyOverview .energyStatistic{
        width:calc(66.66% - 5px);
        height:100%;
        float:left;
        background: #fff;
        padding: 0 10px;
        box-shadow:0 4px 6px 0 rgba(191,193,201,0.50);
        position: relative;
    }
    #energyOverview .fixedEnergy{
        width:100%;
        padding:10px;
        height:calc(33.33% - 6.667px);
        background: #fff;
        margin-bottom: 10px;
        box-shadow:0 4px 6px 0 rgba(191,193,201,0.50);
    }
    #energyOverview .fixedEnergy.fixedEnergy .energyText{
        color:#808fa3;
        font-size:12px;
        /*padding-left:5px;*/
    }
    #energyOverview .lastFixedEnergy{
        margin-bottom:0;
    }
    #energyOverview .fixedTitle{
        height:16px;
        line-height: 16px;
        padding:0 5px;
        color:#354052;
    }
    #energyOverview .fixedContent{
        width:100%;
        padding:15px;
        height:calc(100% - 16px);
        display:flex;
        align-items: center;
        position:relative;
    }
    #energyOverview .todayEnergy{
        border-left:3px solid #5fd7e9;
    }
    #energyOverview .monthEnergy{
        border-left:3px solid #70d35f;
    }
    #energyOverview .yearEnergy{
