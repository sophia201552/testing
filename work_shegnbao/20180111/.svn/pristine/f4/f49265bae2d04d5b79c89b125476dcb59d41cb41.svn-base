.strategy-list-container {
  min-width: 600px;
  width: 100%;
  height: 100%;
  float: right;
  background-color: #f4f8f9;
  float: left;
  overflow: hidden;
}
.strategy-list-header {
  width: 100%;
  height: 40px;
  font-size: 12px;
  color: #6385c4;
  background-color: #2f3a54;
}

.strategy-list-content {
  width: 100%;
  height: calc(100% - 40px);
  overflow-x: hidden;
  overflow-y: auto;
}
.strategy-list-content > div {
  width: 100%;
}

.strategy-list-header {
  padding-left: 4px;
}

div.delete,
div.add,
div.edit,
div.sync {
  height: 40px;
  line-height: 40px;
  padding-left: 10px;
  display: flex;
  align-items: center;
  cursor: pointer;
  float: left;
}

div.sync {
  margin-left: 20px;
}

div.delete:hover,
div.add:hover,
div.edit:hover,
div.sync:hover,
div.searchText:hover {
  background-color: #292e48;
  color: #17a4df;
}
div.delete.highLight,
div.add.highLight,
div.edit.highLight,
div.sync.highLight {
  background-color: #292e48;
  color: #17a4df;
}

.icon-line {
  float: left;
  width: 1px;
  height: 12px;
  margin-top: 14px;
  opacity: 0.2;
  border-radius: 0.5px;
  background-color: #6d92db;
}

.strategy-list-header div .toolName {
  padding: 0 6px;
}

.add .iconAdd,
.edit .iconEdit,
.delete .iconDelete,
iconsync {
  font-size: 13px;
  height: auto;
  box-sizing: border-box;
}
.language,
.searchText {
  float: right;
  line-height: 40px;
}
.language {
  margin-right: 24px;
  cursor: pointer;
}

/* 显示隐藏 spinner */

.show {
  display: block;
}

.hide {
  display: none;
}
/* 搜索框样式 */
.search > div:first-child {
  width: 220px;
  height: 24px;
  position: relative;
  top: 9px;
  left: -34px;
  font-size: 12px;
  border-radius: 4px;
  float: right;
  border: none;
  background-color: rgba(25, 31, 55, 0.5);
}
.search > div:first-child div:first-child {
  font-size: 14px;
  width: 22px;
  color: #687296;
  border-color: #a6a6a6;
}
.search > div:global(.is-active) div:first-child {
  width: 4px;
}
.search > div:first-child > div:first-child i {
  width: 14px;
  height: 14px;
}

.search > :global(.can-clear) > :global(.ms-SearchBox-clearButton) {
  display: -webkit-box;
}
.search > div:first-child > :global(.ms-SearchBox-clearButton) i {
  font-size: 10px;
  line-height: 16px;
  color: #687296;
}
.search > div:first-child > input {
  padding-top: 3px;
  font-size: 12px;
  color: #687296;
  border: none;
}

.search > div:first-child > input::-webkit-input-placeholder {
  color: #687296;
  vertical-align: bottom;
}
.search > div:last-child {
  float: right;
  line-height: 40px;
}
.search > div:last-child > span {
  padding: 0 8px;
}

/* DetailsList 样式 */
.strategy-list-content :global(.ms-DetailsHeader) {
  height: 48px;
  line-height: 48px;
  background-color: #f5f8f9;
  border: none;
}

.strategy-list-content :global(.ms-DetailsList) > div {
  overflow-x: hidden;
}
.strategy-list-content :global(.ms-DetailsHeader) > div {
  height: 48px;
  color: rgb(108, 149, 255);
}
.strategy-list-content :global(.ms-DetailsHeader) > div > span > button {
  opacity: 1;
}
.strategy-list-content :global(.ms-DetailsHeader:hover) > div {
  color: rgb(108, 149, 255);
}
.strategy-list-content :global(.ms-DetailsHeader) > div > span > span {
  padding: 0;
}
.strategy-list-content :global(.ms-DetailsHeader) > div:active {
  background-color: #f5f8f9 !important;
}
.strategy-list-content :global(.ms-List-page) > div > div,
.strategy-list-content :global(.ms-List-page) > div > div:before {
  background-color: #f5f8f9;
  border: 1px solid transparent;
  border-left: none;
  border-right: none;
}
.strategy-list-content :global(.ms-List-page) > div > div:before {
  display: none;
}
.strategy-list-content :global(.ms-List-page) > div:nth-child(odd) > div {
  background-color: rgb(235, 238, 247);
  opacity: 0.1;
}
.strategy-list-content :global(.ms-List-page) > div:hover > div {
  border-color: #6c95ff;
}

.strategy-list-content :global(.ms-List-page) > div > div:global(.is-selected),
.strategy-list-content
  :global(.ms-List-page)
  > div
  > div:global(.is-selected:focus)
  > div
  > div,
.strategy-list-content
  :global(.ms-List-page)
  > div
  > div:global(.is-selected:hover)
  > div
  > div {
  color: #767676;
}
.strategy-list-content :global(.ms-List-page) > div > div > div > div {
  padding: 0;
  line-height: 40px;
}
.strategy-list-content  :global(.ms-DetailsList) :global(.ms-DetailsRow-cell) > div {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.strategy-list-content :global(.ms-DetailsList) i:global(.ms-Check-circle) {
  opacity: 0;
}
.strategy-list-content :global(.ms-DetailsList) div:global(.ms-Check::before) {
  border-radius: 0;
  width: 12px;
  height: 12px;
  border-radius: 2px;
  border: solid 1px rgba(130, 144, 179, 0.4);
}
.strategy-list-content
  :global(.ms-DetailsList)
  div:global(.ms-Check.is-checked::before) {
  background-color: #4c84ff;
}
.strategy-list-content :global(.ms-DetailsList) i:global(.ms-Check-check) {
  left: -0.8px;
  font-size: 18px;
  line-height: 14px;
}
.strategy-list-content :global(.ms-DetailsHeader) button:focus i,
.strategy-list-content :global(.ms-DetailsRow) button:focus i {
  color: #fff !important;
}

.sync-status {
  width: 70px;
  height: 20px;
  text-align: center;
  line-height: 18px;
  border-radius: 2px;
  border: solid 1px #dfe1e5;
}
