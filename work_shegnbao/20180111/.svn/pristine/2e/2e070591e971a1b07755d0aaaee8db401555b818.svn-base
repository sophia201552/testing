import pytestfrom beopWeb.mod_algorithm import controllerimport jsonimport bson@pytest.mark.p0def test_delete_template():    rt = controller.do_AddAlgorithmTemplateFolder({"createTime":"","creator":""})    rt=json.loads(rt)    assert bson.ObjectId.is_valid(rt),''    delete_rt=controller.do_delete_template({"id": rt})    assert delete_rt.get("n")==1,'delete  template fail'    tem=controller.GetAlgorithmTemplateContent(rt)    assert tem=="null",'delete template fail'