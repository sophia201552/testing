  term: Big Gaussian 15.000 2.000
# ############################################Compound Input Variable########################################
InputVariable: OARatio_MAT
  enabled: true
  range: 0.000 1.000
  term: Big Function exp(~(((MATemp-RATemp)/(OATemp-RATemp)-1.000)^2)/(2*pow(0.3,2)))
  term: Small Function exp(~(((MATemp-RATemp)/(OATemp-RATemp)-0.000)^2)/(2*pow(0.3,2)))
InputVariable: OARatio_SAT
  enabled: false
  range: 0.000 1.000
  term: Big Function exp(~(((SATemp-RATemp)/(OATemp-RATemp)-1.000)^2)/(2*pow(0.3,2)))
  term: Small Function exp(~(((SATemp-RATemp)/(OATemp-RATemp)-0.000)^2)/(2*pow(0.3,2)))
InputVariable: OARADiff
  enabled: true
  range: -3.000 3.000
  term: Big Function exp(~((((OATemp-RATemp)*le(fabs(OATemp-RATemp-0.0),3)-le(OATemp-RATemp,~3)*3+ge(OATemp-RATemp,3)*3)-3.000)^2)/(2*pow(1.500,2)))
  term: Normal Function exp(~((((OATemp-RATemp)*le(fabs(OATemp-RATemp-0.0),3)-le(OATemp-RATemp,~3)*3+ge(OATemp-RATemp,3)*3)-0.000)^2)/(2*pow(0.500,2)))
  term: Small Function exp(~((((OATemp-RATemp)*le(fabs(OATemp-RATemp-0.0),3)-le(OATemp-RATemp,~3)*3+ge(OATemp-RATemp,3)*3)+3.000)^2)/(2*pow(1.50,2)))
InputVariable: OAMADiff
  enabled: true
  range: -3.000 3.000
  term: Big Function exp(~((((OATemp-MATemp)*le(fabs(OATemp-MATemp-0.0),3)-le(OATemp-MATemp,~3)*3+ge(OATemp-MATemp,3)*3)-3.000)^2)/(2*pow(1.50,2)))
  term: Normal Function exp(~((((OATemp-MATemp)*le(fabs(OATemp-MATemp-0.0),3)-le(OATemp-MATemp,~3)*3+ge(OATemp-MATemp,3)*3)-0.000)^2)/(2*pow(0.500,2)))
  term: Small Function exp(~((((OATemp-MATemp)*le(fabs(OATemp-MATemp-0.0),3)-le(OATemp-MATemp,~3)*3+ge(OATemp-MATemp,3)*3)+3.000)^2)/(2*pow(1.500,2)))
InputVariable: RAMADiff
