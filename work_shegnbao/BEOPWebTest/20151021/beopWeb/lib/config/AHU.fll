# General AHU fuzzy diagnosis engine by Matthew Zuo 2014
Engine: AHUFDDEngine
InputVariable: MATemp
  enabled: false
  range: -10.000 50.000
InputVariable: SATemp
  enabled: true
  range: -10.000 50.000
InputVariable: RACO2
  enabled: true
  range: 400.000 1200.000
  term: Small Gaussian 400 80.000
  term: Big Gaussian 1200 150.000
  term: Normal Gaussian 700.000 20.00
InputVariable: OATemp
  enabled: true
  range: -10.000 50.000
  term: Small Trapezoid 15.000 15.000 16.000 22.000
  term: Big Trapezoid 26.000 28.000 30.000 30.000
InputVariable: RATemp
  enabled: true
  range: -10.000 50.000
InputVariable: RATempSetting
  enabled: true 
  range: 20.000 30.0
  term: Small Gaussian 20 2.000
  term: Big Gaussian 30.000 2.000
  term: Normal Gaussian 25.000 1.00
InputVariable: SATempSetting
  enabled: false
  range: -10.000 50.000
InputVariable: RACO2Setting
  enabled: true
  range: 400.000 1200.000
  term: Small Gaussian 400 80.000
  term: Big Gaussian 1200 150.000
  term: Normal Gaussian 700.000 20.00
InputVariable: HWTemp
  enabled: true
  range: 25.000 50.000
  term: Small Gaussian 25.000 8.000
  term: Big Gaussian 50.000 8.000
InputVariable: CWTemp
  enabled: true
  range: 5.000 15.000
  term: Small Gaussian 5.000 3.000
  term: Big Gaussian 15.000 3.000
InputVariable: OperationMode
  enabled: true
  range: -1.000 1.000
  term: Winter Rectangle -1.000 0.000
  term: Summer Rectangle 0.000 1.000
InputVariable: OADamperRatio
  enabled: true
  range: 0.000 100.000
  term: Small Gaussian 0.000 30.000
  term: Big Gaussian 100.000 30.000
  term: Off Gaussian 0.000 5.000
  term: Open Gaussian 100.000 5.000
InputVariable: HCDamperRatio
  enabled: true
