<div id="workflow-insert-container">
    <style type="text/css">
        #workflow-insert-container .modal-dialog {
            margin-top: 60px;
            width: 800px;
        }

        #workflow-insert-container table td, #workflow-insert-container table th {
            padding: 5px;
            color: #333;
        }

        .wf-table-head-fault {
            text-align: right;
        }

        #workflow-insert-container .wf-detail-table tr td, #workflow-insert-container .wf-detail-table tr th {
            border: none !important;
        }

        #workflow-insert-container .wf-detail-table .glyphicon {
            cursor: pointer;
        }

        #workflow-insert-container input, #workflow-insert-container select {
            width: 200px;
        }

        #workflow-insert-container {
            background: #000;
            -webkit-user-select: none;
            -ms-user-select: none;
            -moz-user-select: none;
        }

        #workflow-insert-container .modal-dialog {
            margin: 60px auto;
        }

        .wf-search-box-select li {
            width: auto;
            margin-right: 25px;
            cursor: pointer;
            padding: 5px 0;
            vertical-align: middle;
        }

        .wf-search-box-select .active {
            color: #3071a9;
            border-bottom: 1px solid #3071a9;
        }

        .wf-search-result-container {
            min-height: 430px;
            padding: 10px;
            padding-bottom: 0;
        }

        .wf-search-all {

        }

        .wf-search-all ul {
            border: none;
        }

        .wf-search-all ul .active {
            color: #1543fa;
        }

        .wf-search-all li {
            display: inline-block;
            padding: 0 5px;
            text-transform: capitalize;
            font-size: 12px;
            cursor: pointer;
            transition: .2s all linear 0s;
        }

        .wf-search-result-title ul li:hover {
            color: #fff;
            background: #000;
            border-radius: 5px;
        }

        .wf-search-result {
            position: relative;
            padding: 10px 38px;
            height: 355px;
            border: 1px solid #cccccc;
            -webkit-user-select: none;
            -webkit-font-smoothing: antialiased;
            overflow-x: hidden;
            overflow-y: auto;
        }

        .wf-search-result > div {
            margin: 5px 0;
        }

        .wf-search-result .wf-member-selected {
            background: #F1EDED;
        }

        .wf-search-result .wf-member {
            border-radius: 5px;
            transition: .3s background 0s linear;
            border: 1px solid #ccd;
            position: relative;
            cursor: pointer;
            width: 60px;
            height: 60px;
            padding: 0;
            margin: 10px 15px;
            float: left;
        }

        .wf-search-result .wf-member:hover {
            background: #F1EDED;
        }

        .wf-search-result-title {
            margin: 5px 0;
        }

        .wf-search-result-title .nav-tabs li.active {
            background: #000000;
            color: #fff;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
        }

        .wf-member .wf-team-avatar-name {
            width: 100%;
            display: block;
            background: rgba(0, 0, 0, 0.3);
            color: #fff;
            font-size: 12px;
            margin: 0;
        }

        .wf-member-avatar {
            text-align: center;
            /*padding: 5px ;*/
            position: absolute;
            width: 100%;
            height: 100%;
            z-index: 1;
        }

        .wf-member-avatar .avatar-icon {
            width: 100%;
            height: 100%;
            transition: .3s opacity 0s linear;
            border-radius: 5px;
        }

        .wf-member .wf-member-content {
            position: absolute;
            left: 0;
            right: 0;
            z-index: 10;
            text-align: center;
            border-radius: 5px;
            bottom: 0;
        }

        .wf-member .wf-member-check {
            display: none;
        }

        .wf-member-selected .wf-member-check {
            position: absolute;
            right: 0;
            top: 0;
            width: 20px;
            height: 20px;
            z-index: 11;
            text-align: right;
            font-size: 12px;
            border-color: #A1CF64;
            color: #fff;
            display: block;
        }

        #calendar-time {
            width: 241px;
            height: auto;
            margin: 10px 0;
        }

        .wf-member-selected .wf-member-check:after {
            position: absolute;
            content: "";
            right: 0;
            top: 0;
            z-index: -1;
            width: 0;
            height: 0;
            border-top: 0 solid transparent;
            border-right: 25px solid transparent;
            border-bottom: 25px solid transparent;
            border-left: 0 solid transparent;
            border-right-color: inherit;
            -webkit-transition: border-color .2s ease;
            -moz-transition: border-color .2s ease;
            transition: border-color .2s ease;
        }

        .wf-check-result-container {
            padding: 5px 10px;
            min-height: 60px;
        }

        .wf-check-result-container > span {
            display: inline-block;
            height: 35px;
            width: auto;
            line-height: 35px;
            text-align: center;
            margin-right: 10px;
            margin-top: 5px;
        }

        .wf-check-result {
            list-style: none;
            padding: 5px 0 0 80px;
        }

        #labelName {
            width: 200px !important;
        }

        .wf-check-result li {
            width: 76px;
            height: 25px;
            max-width: 80px;
            background: #A1CF64;
            padding: 5px;
            border-radius: 13%;
            font-size: 12px;
            float: left;
            text-align: center;
            text-transform: none;
            margin: 5px;
            -webkit-user-select: none;
            cursor: default;
            transition: .3s opacity 0s linear;

        }

        .wf-check-result li:hover {
            opacity: .8;
        }

        .wf-check-result li span {
            display: inline-block;
            width: 50px;
        }

        .wf-check-result li .glyphicon {
            vertical-align: top;
            line-height: 16px;
            cursor: pointer;
            color: #fff;
            transform: scale(.87);
        }

        .wf-search-by-name ul .active {
            color: #fff;
            background: #000;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
        }

        .wf-search-by-name ul li:hover {
            color: #fff;
            background: #000;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
            transform: scale(1.1, 1.1);
        }

        .wf-search-by-name ul {
            border: none;
        }

        .wf-search-by-name ul li {
            display: inline-block;
            margin: 0 5px;
            text-transform: capitalize;
            font-size: 12px;
            cursor: pointer;
            padding: 0 5px;
            transition: .1s all 0s linear;
        }

        .wf-detail-userInfo {
            width: 50px;
            height: 50px;
            float: left;
            margin: 3px;
            border-radius: 5px;
            overflow: hidden;

        }

        .wf-person-pic {
            width: 50px;
            height: 50px;
            /*border-radius: 50px;*/
        }

        .wf-detail-userName {
            width: 100%;
            height: 20px;
            line-height: 20px;
            text-align: center;
            font-size: 13px;
            left: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.3);
            color: #fff;
        }

        .wf-add-user {
            font-size: 18px;
        }

        .wf-manager-people .people {
            width: 50px;
            height: 50px;
            float: left;
            margin: 0 5px 5px 5px;
            position: relative;
        }

        .wf-manager-people .wf-people-add {
            border-radius: 50%;
            font-size: 25px;
            padding: 5px;
            margin-left: 5px;
            position: relative;
            left: 0;
            color: #aaa;
            cursor: pointer;
        }

        .wf-manager-people .wf-people-add {
            font-size: 22px;
            cursor: pointer;
        }

        .wf-manager-people .wf-people-add:hover {
            color: #666;
        }

        .wf-manager-people .people img {
            width: 100%;
            height: 100%;
            vertical-align: middle;
        }

        .wf-manager-people .people-name {
            position: absolute;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.3);
            height: 20px;
            text-align: center;
            line-height: 20px;
            color: #fff;
            font-size: 13px;
        }

        #workflow-insert-container textarea {
            resize: none;
            width: 90%;
        }

        #workflow-insert-container #workflow-insert-curve {
            position: relative;
            height: 300px;
            width: 800px;
            margin: 0 auto;
            max-width: 768px;
        }

        #workflow-insert-container .wf-attachment-nav {
            padding-left: 0;
            list-style-type: none;
            display: inline-block;
            margin-bottom: 0;
        }

        #wf-attachment-labelWrapper li {
            display: inline-block;
            margin: 0 10px;
        }

        #wf-attachment-labelWrapper li .files {
            width: 150px;
            min-height: 120px;
            text-align: center;
            border: 1px solid #ccc;
            box-sizing: border-box;
            position: relative;
            color: #000;
        }

        #wf-attachment-labelWrapper li:hover .files > button {
            display: block;
        }

        #wf-attachment-labelWrapper .files .file-type {
            border-bottom: 1px solid #ccc;
            height: 80px;
            line-height: 80px;
            background-size: contain !important;
        }

        #wf-attachment-labelWrapper .files .file-info {
            display: block;
            font-size: 12px;
            text-align: left;
            text-indent: 10px;
            color: #333;
        }

        #wf-attachment-labelWrapper .files > button {
            cursor: pointer;
            color: #ccc;
            font-size: 12px;
            padding: 2px;
            transition: .3s 0s all ease;
            outline: none;
            background: transparent;
            border: none;
            display: none;
            position: absolute;
            right: 0;
        }

        #wf-attachment-labelWrapper .files > button:nth-of-type(1):hover {
            text-decoration: none;
            color: #f00;
            background: transparent;
        }

        #wf-attachment-labelWrapper .files .file-info p {
            margin: 0;
            line-height: 20px;
            width: 100%;
            text-overflow: ellipsis;
            white-space: nowrap;
            max-width: 150px;
            overflow: hidden;
            text-align: center;
        }

        #wf-attachment-labelWrapper .files .file-info .file-name {
            font-size: 12px;
        }

        #wf-attachment-labelWrapper .files .file-info .file-time {
            font-size: 90%;
        }

        #wf-attachment-labelWrapper .files .file-type i {
            font-size: 36px;
        }

        #wf-attachment-input-btn {
            position: relative;
        }

    </style>
    <div class="modal fade" role="document" tabindex="-1"
         data-keyboard="false" aria-labelledby="myModalLabel" style="overflow-y: auto;">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body" data-backdrop="false"></div>
                <div class="modal-footer">
                    <button type="submit" id="workflow-insert-submit-btn" class="btn btn-success fr"
                            i18n="common.CONFIRM"></button>
                    <button type="button" id="workflow-insert-cancel-btn" class="btn btn-default fr mr5 closeWin"
                            i18n="common.CANCEL"></button>
                </div>
            </div>
        </div>
    </div>
    <script type="text/html" id="temp_wf_added_member_personal">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <dl class="pr wf-detail-userInfo">
            <dt class="wf-detail-userPic"><img src="<!=member.userpic!>" class="wf-person-pic"></dt>
            <dd class="pa wf-detail-userName ellipsis"
                title="<!=member.userfullname +'\n' +(member.useremail && member.useremail!='undefined'  ? member.useremail : '')!>">
                <!=member.userfullname!>
            </dd>
            <input type="hidden" name="<!=userListName!>[]" class="<!=userListName!>-person" value="<!=member.id!>">
        </dl>
        <!}!>
    </script>
    <!--新建故障添加工单开始-->
    <script type="text/html" id="temp_fault_new_order">
        <form id="wf-detail-form" action="javascript:void(0);" method="post" class="task-new">
            <table class="mb20 wf-table wf-detail-table">
                <tbody>
                <tr>
                    <td width="15%" class="wf-table-head-fault">
                        <span i18n=workflow.mine.WORK_ORDER_NAME></span>
                    </td>
                    <td width="35%">
                        <input type="text" class="form-control w200" maxlength="25"
                               i18n="placeholder=workflow.task.INPUT_TASK_TITLE_INFO"
                               required
                               name="title" value="<!=fault.name!>"/>
                    </td>
                    <td width="15%" class="wf-table-head-fault">
                        <span i18n="workflow.notice.DEADLINE"></span>
                    </td>
                    <td width="35%">
                        <input type="text" name="dueDate" class="form-control w200 cp" readonly
                               i18n="placeholder=workflow.task.SELECTED_DUE_DATE_INFO"
                               id="dueTime" value="<!=fault.time!>" required/>
                    </td>
                </tr>
                <tr>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.task.TASK_GROUP"></span>
                    </td>
                    <td>
                        <select class="form-control w200" id="wf-user-group-list"
                                i18n="placeholder=workflow.task.SELECT_TASK_GROUP" name="groupId">
                        </select>
                    </td>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.urgencyLevel.URGENCY_LEVEL"></span>
                    </td>
                    <td>
                        <select id="wf-detail-critical" class="form-control" name="critical">
                            <option value="0" i18n="workflow.urgencyLevel.0"></option>
                            <option value="1" i18n="workflow.urgencyLevel.1"></option>
                            <option value="2" i18n="workflow.urgencyLevel.2"></option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.main.FAULT_INFO"></span>
                    </td>
                    <td colspan="3">
                                <textarea class="form-control fl text-detail" id="wfDetail" name="detail"
                                          i18n="placeholder=workflow.task.CHECK_TASK_INFO"
                                          maxlength="500" required><!=fault.description!></textarea>
                    </td>
                </tr>
                <tr class="dn">
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.task.TAGS"></span>
                    </td>
                    <td colspan="3">
                        <div id="wf-detail-labelWrapper" class="mr10 fl"></div>
                        <select class="form-control fl mr10" id="labelName">
                            <option disabled="" selected="" value="0"
                                    i18n="workflow.task.SELECT_TAG_NAME_INFO"></option>
                        </select>
                        <span id="wf-labelName-error" class="dn" i18n="workflow.task.ADD_TAGS_INFO"></span>
                    </td>
                </tr>
                <tr>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.mine.EXECUTOR"></span>
                    </td>
                    <td>
                        <div class="wf-detail-userInfoWrapper" data-type="executor"></div>
                        <span class="glyphicon glyphicon-edit wf-people-add" data-type="executor"></span>
                    </td>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.task.VERIFIERS"></span>
                    </td>
                    <td>
                        <div class="wf-detail-userInfoWrapper" data-type="verifiers"></div>
                        <span class="glyphicon glyphicon-edit wf-people-add" data-type="verifiers"></span>
                    </td>
                </tr>
                <tr>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.task.WATCHERS"></span>
                    </td>
                    <td colspan="3">
                        <div class="wf-detail-userInfoWrapper" data-type="watchers"></div>
                        <span class="glyphicon glyphicon-edit wf-people-add" data-type="watchers"></span>
                    </td>
                </tr>
                <tr>
                    <td class="wf-table-head-fault">
                        <span i18n="workflow.task.ATTACHMENT"></span>
                    </td>
                    <td colspan="3">
                        <div id="wf-attachment-labelWrapper" class="mr10 fl">
                            <ul class="wf-attachment-nav"></ul>
                            <button type="button" id="wf-attachment-input-btn" class="btn btn-success">
                                <span class="glyphicon glyphicon-plus-sign"></span>
                                <span i18n="workflow.task.ATTACHMENT_INFO"></span>
                            </button>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                        <div id="wf-detail-curve-box">
                            <div class="wf-detail-curve" id="workflow-insert-curve"></div>
                        </div>
                    </td>
                </tr>
                </tbody>
            </table>
        </form>
    </script>
    <!--新建故障添加工单结束-->
    <!--新建故障添加 - 任务组表单开始-->
    <script type="text/html" id="temp_fault_group_add">
        <form class="form-horizontal" style="padding: 10px" id="wf-add-group-form" action="javascript:void(0);"
              method="post">
            <div class="tc" style="height: 30px;"><span i18n="workflow.task.NO_GROUP_PROMPT"
                                                        style="color:#d9534f;"></span>
            </div>
            <div class="form-group">
                <label for="wf-group-name" class="col-sm-2 control-label"><span
                        i18n="workflow.task.TASK_GROUP"></span>:</label>

                <div class="col-sm-10">
                    <input type="text" class="form-control" id="wf-group-name"
                           i18n="workflow.task.TASK_GROUP_NAME;placeholder=workflow.task.TASK_GROUP_NAME" name="name"
                           maxlength="50" style="width: 100%;">
                </div>
            </div>
            <div class="form-group">
                <label for="wf-group-des" class="col-sm-2 control-label"><span
                        i18n="workflow.main.ALARM_CONTENT"></span>:</label>

                <div class="col-sm-10">
                <textarea class="form-control text-detail" id="wf-group-des"
                          i18n="placeholder=workflow.task.DESCRIPTION_OF_TASK_GROUP;" name="description"
                          maxlength="120"></textarea>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label"><span i18n="workflow.task.CREATED_PERSON"></span>:</label>

                <div class="col-sm-10 pull-right wf-manager-people" id="wf-workflow-creator">
                    <div class="people">
                        <img src="<!='http://images.rnbtech.com.hk'+AppConfig.userProfile.picture!>"
                             title="<!=AppConfig.userProfile.fullname!>"/>

                        <div class="people-name ellipsis"><!=AppConfig.userProfile.fullname!></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label"><span i18n="workflow.task.MEMBERS"></span>:</label>

                <div class="col-sm-10 pull-right wf-manager-people">
                    <div class="wf-added-user-list" id="wf-workflow-memberList"></div>
                    <span class="glyphicon glyphicon-edit wf-people-add" data-toggle="modal"></span>
                </div>
            </div>
        </form>
    </script>
    <!--新建故障添加 - 任务组表单结束-->
    <script type="text/html" id="wf_file_list_temp">
        <li>
            <div class="files" data-file-id="<!=fileId!>" data-file-uid="<!=uid!>" data-file-name="<!=fileName!>">
                <button type="button" class="pull-right glyphicon glyphicon-remove wf-remove-upload-item"
                        data-file-type="remove-upload-item"></button>
                <!if(fileBase64){!>
                <section class="icon file-type"
                         style="background: url(<!=fileBase64!>) center center no-repeat"></section>
                <!}else{!>
                <section class="icon file-type <!=iconClass!>"></section>
                <!}!>
                <section class="file-name file-info">
                    <p title="<!=file.name!>" style="line-height: 40px;">
                        <!=file.name!>
                    </p>
                </section>
            </div>
        </li>
    </script>
    <!--新建故障添加 - 任务组表单添加人物开始-->
    <script type="text/html" id="temp_wf_added_member">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <div class="people" title="<!=member.userfullname!>" data-user-id="<!=member.id!>">
            <img src="<!=member.userpic!>">

            <div class="people-name ellipsis"><!=member.userfullname!></div>
            <input type="hidden" name="addedUsers[]" value="<!=member.id!>">
        </div>
        <!}!>
    </script>
    <!--新建故障添加 - 任务组表单添加人物结束-->
</div>