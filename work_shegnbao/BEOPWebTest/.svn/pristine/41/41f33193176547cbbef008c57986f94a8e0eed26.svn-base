                        <span class="wf-get-crumbs" i18n="workflow.calendar.YESTERDAY"></span>
                    </li>
                    <li class="secondMenuItem" data-topic="wf-activities-change" data-param="thisWeek">
                        <span class="glyphicon glyphicon-tree-conifer"></span>
                        <span class="wf-get-crumbs" i18n="workflow.calendar.THIS_WEEK"></span>
                    </li>
                    <li class="secondMenuItem" data-topic="wf-activities-change" data-param="thisMonth">
                        <span class="glyphicon glyphicon-tree-conifer"></span>
                        <span class="wf-get-crumbs" i18n="workflow.calendar.THIS_MONTH"></span>
                    </li>
                </ul>
                <ul id="wf-activity-time" class="wf-ul wf-level-ul">
                    <li class="secondMenuItem" data-topic="wf-activities-change" data-param="latestCompleted">
                        <span class="glyphicon glyphicon-tree-conifer"></span>
                        <span class="wf-get-crumbs" i18n="workflow.activities.RECENTLY_COMPLETE"></span>
                    </li>
                    <li class="secondMenuItem" data-topic="wf-activities-change" data-param="latestCreated">
                        <span class="glyphicon glyphicon-tree-conifer"></span>
                        <span class="wf-get-crumbs" i18n="workflow.activities.RECENTLY_CREATED"></span>
                    </li>
                </ul>
            </section>
            <section id="level-menu-task" class="level-menuTask-wrapper">
                <div id="wf-task-mine">
                    <div class="pr">
                        <input type="search" id="wf-level-search" data-topic="wf-search-list" data-param="search"
                               class="form-control" i18n="placeholder=workflow.task.SEARCH" maxlength="12"/>
                        <span id="wf-task-search" data-topic="wf-task-list" data-param="search"
                              class="glyphicon glyphicon-search pr"></span>
                    </div>
                    <ul id="wf-task-type" class="wf-ul wf-level-ul mb10">
                        <li id="wf-mine-create" data-type="default" data-topic="wf-task-list" data-param="workingTask">
                            <span class="glyphicon glyphicon-tree-conifer"></span>
                            <span class="wf-get-crumbs" i18n="workflow.task.WORKFLOW_IN_PROGRESS"></span>
                        </li>
                        <li id="wf-mine-create" data-topic="wf-task-list" data-param="createdBy">
                            <span class="glyphicon glyphicon-tree-conifer"></span>
                            <span class="wf-get-crumbs" i18n="workflow.task.WORKFLOW_CREATED_BY_ME"></span>
                        </li>
                        <li data-topic="wf-task-list" data-param="finishedBy">
                            <span class="glyphicon glyphicon-tree-conifer"></span>
                            <span class="wf-get-crumbs" i18n="workflow.task.WORKFLOW_COMPLETE_BY_ME"></span>
                        </li>
                        <li data-topic="wf-task-list" data-param="joinedBy">
                            <span class="glyphicon glyphicon-tree-conifer"></span>
                            <span class="wf-get-crumbs" i18n="workflow.task.WORKFLOW_INVOLVED_BY_ME"></span>
                        </li>
                    </ul>
                    <button type="button" id="wf-new-task" class="btn btn-primary mb10" data-topic="wf-task-add"
