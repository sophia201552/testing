<style>
    /* ---主菜单样式 --- */
    #wf-outline {
        height: 100%;
        position: relative;
    }

    #wf-outline > section {
        height: 100%;
        float: left;
    }

    #wf-menu {
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
    }

    #wf-menu > section {
        float: left;
    }

    #wf-img-zoom {
        -webkit-user-select: none;
        cursor: -webkit-zoom-out;
        background: #fff;
        overflow: scroll;
        position: fixed;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        margin: auto;
        z-index: 1000
    }

    #wf-img-zoom img {
        position: absolute;
        display: block;
        margin: auto;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
    }

    .wf-activity-person-box .media-body img {
        max-width: 100%;
        display: block;
    }

    .alert[role="alert"] {
        z-index: 10000000;
    }

    #wf-reply-table .wf-reply-info img {
        max-width: 100%;
    }

    #wf-main-menu li, #wf-level-menu {
        cursor: pointer;
    }

    #wf-main-menu {
        left: 0;
        height: 100%;
        width: 80px;
        font-size: 12px;
        background-color: #123454;
        color: #A7B1C2;
    }

    #wf-main-logo .glyphicon-fire {
        text-align: center;
        font-size: 34px;
        height: 80px;
        width: 100%;
        display: block;
        line-height: 80px;
        color: #fff;
    }

    #wf-main-menuWrapper {
        width: 100%;
        height: 100%;
    }

    #wf-add-person {
        -webkit-user-select: none;
        -ms-user-select: none;
        -moz-user-select: none;
    }

    #wf-add-person .modal-dialog {
        margin: 60px auto;
    }

    .wf-search-box-select li {
        width: auto;
        margin-right: 25px;
        cursor: pointer;
        padding: 5px 0;
        vertical-align: middle;
    }

    .wf-search-box-select .active {
        color: #3071a9;
        border-bottom: 1px solid #3071a9;
    }

    .wf-search-result-container {
        min-height: 430px;
        padding: 10px;
        padding-bottom: 0;
    }

    .wf-search-all {

    }

    .wf-search-all ul {
        border: none;
    }

    .wf-search-all ul .active {
        color: #1543fa;
    }

    .wf-search-all li {
        display: inline-block;
        padding: 0 5px;
        text-transform: capitalize;
        font-size: 12px;
        cursor: pointer;
        transition: .2s all linear 0s;
    }

    .wf-search-result-title ul li:hover {
        color: #fff;
        background: #000;
        border-radius: 5px;
    }

    .wf-search-result {
        position: relative;
        padding: 10px 38px;
        height: 434px;
        border: 1px solid #cccccc;
        -webkit-user-select: none;
        -webkit-font-smoothing: antialiased;
        overflow-x: hidden;
        overflow-y: auto;
    }

    .wf-search-result > div {
        margin: 5px 0;
    }

    .wf-search-result .wf-member-selected {
        background: #F1EDED;
    }

    .wf-search-result .wf-member {
        border-radius: 5px;
        transition: .3s background 0s linear;
        border: 1px solid #ccd;
        position: relative;
        cursor: pointer;
        width: 60px;
        height: 60px;
        padding: 0;
        margin: 10px 15px;
        float: left;
    }

    .wf-search-result .wf-member:hover {
        background: #F1EDED;
    }

    .wf-search-result-title {
        margin: 5px 0;
    }

    .wf-search-result-title .nav-tabs li.active {
        background: #000000;
        color: #fff;
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        border-radius: 3px;
    }

    .wf-member .wf-team-avatar-name {
        width: 100%;
        display: block;
        background: rgba(0, 0, 0, 0.3);
        color: #fff;
        font-size: 12px;
        margin: 0;
    }

    .wf-member-avatar {
        text-align: center;
        /*padding: 5px ;*/
        position: absolute;
        width: 100%;
        height: 100%;
        z-index: 1;
    }

    .wf-member-avatar .avatar-icon {
        width: 100%;
        height: 100%;
        transition: .3s opacity 0s linear;
        border-radius: 5px;
    }

    .wf-member .wf-member-content {
        position: absolute;
        left: 0;
        right: 0;
        z-index: 10;
        text-align: center;
        border-radius: 5px;
        bottom: 0;
    }

    .wf-member-selected .wf-member-check {
        position: absolute;
        right: 0;
        top: 0;
        width: 20px;
        height: 20px;
        z-index: 11;
        text-align: right;
        font-size: 12px;
        border-color: #A1CF64;
        color: #fff;
        display: none;
    }

    #calendar-time {
        width: 241px;
        height: auto;
        margin: 10px 0;
    }

    .wf-member-selected .wf-member-check {
        display: block;
    }

    .wf-member-selected .wf-member-check:after {
        position: absolute;
        content: "";
        right: 0;
        top: 0;
        z-index: -1;
        width: 0;
        height: 0;
        border-top: 0 solid transparent;
        border-right: 25px solid transparent;
        border-bottom: 25px solid transparent;
        border-left: 0 solid transparent;
        border-right-color: inherit;
        -webkit-transition: border-color .2s ease;
        -moz-transition: border-color .2s ease;
        transition: border-color .2s ease;
    }

    .wf-check-result-container {
        padding: 5px 10px;
        min-height: 60px;
    }

    .wf-check-result-container > span {
        display: inline-block;
        height: 35px;
        width: auto;
        line-height: 35px;
        text-align: center;
        margin-right: 10px;
        margin-top: 5px;
    }

    .wf-check-result {
        list-style: none;
        padding: 5px 0 0 80px;
    }

    #labelName {
        width: auto !important;
    }

    .wf-check-result li {
        width: 76px;
        height: 25px;
        max-width: 80px;
        background: #A1CF64;
        padding: 5px;
        border-radius: 13%;
        font-size: 12px;
        float: left;
        text-align: center;
        text-transform: none;
        margin: 5px;
        -webkit-user-select: none;
        cursor: default;
        transition: .3s opacity 0s linear;

    }

    .wf-check-result li:hover {
        opacity: .8;
    }

    .wf-check-result li span {
        display: inline-block;
        width: 50px;
    }

    .wf-check-result li .glyphicon {
        vertical-align: top;
        line-height: 16px;
        cursor: pointer;
        color: #fff;
        transform: scale(.87);
    }

    .wf-search-by-name ul .active {
        color: #fff;
        background: #000;
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        border-radius: 3px;
    }

    .wf-search-by-name ul li:hover {
        color: #fff;
        background: #000;
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        border-radius: 3px;
        transform: scale(1.1, 1.1);
    }

    .wf-search-by-name ul {
        border: none;
    }

    .wf-search-by-name ul li {
        display: inline-block;
        margin: 0 5px;
        text-transform: capitalize;
        font-size: 12px;
        cursor: pointer;
        padding: 0 5px;
        transition: .1s all 0s linear;
    }

    .wf-ul {
        padding-left: 0;
        list-style-type: none;
    }

    #wf-main-ul {
        width: 100%;

    }

    #wf-main-ul li {
        height: 60px;
        text-align: center;
        cursor: pointer;
        border: 1px solid #2f4050;
    }

    #wf-main-ul li:hover, #wf-main-ul li.active {
        border: 1px solid #e5f3ff;
        background-color: #e5f3ff;
        color: #337ab7;
        box-shadow: 2px 2px 5px #e5f3ff;
        text-shadow: 0 0 5px #e5f3ff;
        font-size: 14px;
    }

    #wf-main-ul li .icon {
        display: block;
        height: 30px;
        line-height: 40px;
    }

    #level-menu-activity {
        width: 179px;
    }

    /* ---二级单样式 --- */
    #wf-level-menu {
        left: 80px;
        height: 100%;
        /*width: 200px;*/
        padding: 10px;
        background-color: #e5f3ff;
        color: #676a6c;
        border-right: 1px solid #CCC;
        /*background: linear-gradient(top , rgb(131, 140, 133) 1% , rgb(168, 164, 164) 100%);
        background: -o-linear-gradient(top , rgb(131, 140, 133) 1% , rgb(168, 164, 164) 100%);
        background: -ms-linear-gradient(top , rgb(131, 140, 133) 1% , rgb(168, 164, 164) 100%);
        background: -moz-linear-gradient(top , rgb(131, 140, 133) 1% , rgb(168, 164, 164) 100%);
        background: -webkit-linear-gradient(top , rgb(131, 140, 133) 1% , rgb(168, 164, 164) 100%);*/
        overflow-y: hidden;
        overflow-x: hidden;
    }

    #wf-task-mine {
        border-bottom: 1px dashed #ccc;
    }

    #wf-task-group {
        position: relative;
        margin-top: 10px;
    }

    #wf-level-menu > .active {
        display: block;
    }

    #wf-level-menu > section {
        display: none;
    }

    .wf-close-Wrapper {
        height: 30px;
        line-height: 30px;
    }

    #wf-new-task {
        width: 100%;
    }

    #wf-add-taskGroup {
        width: 100%;
    }

    #wf-levelMenu-close {
        display: inline-block;
        width: 30px;
        height: 30px;
        line-height: 30px;
        border: 1px solid #ccc;
        border-radius: 50px;
        background: #333;
        color: #fff;
        text-align: center;
        font-style: normal;
        cursor: pointer;
    }

    #wf-level-search {
        margin-bottom: 10px;
    }

    #wf-task-search {
        position: absolute;
        top: 10px;
        right: 7px;
    }

    .wf-level-ul {
        color: #777777;
    }

    .wf-level-ul li {
        height: 28px;
        line-height: 28px;
    }

    .wf-search-result .wf-member-selected .wf-member-check {
        display: block !important;
    }

    .wf-search-result .wf-member .wf-member-check {
        display: none;
    }

    #wf-level-menu li.active, #wf-level-menu span.active {
        color: #337ab7;
        font-weight: bolder;
    }
    #wf-task-mine div.active {
        color: #337ab7;
        font-weight: bolder;
    }

    #wf-label-header .wf-tags-ul-show {
        color: #337ab7;
        font-weight: bolder;
    }

    #wf-task-groups .glyphicon-edit {
        margin: 2px 10px 0 0;
    }

    .wf-my-lable, .wf-task-label-wrapper {
        padding: 5px 15px;
        margin: 5px;
        border: 1px solid #ccc;
        border-radius: 5px;
        background: #fff;
        color: #666;
        position: relative;
    }

    #wf-add-taskGroup, #wf-new-task {
        width: 170px;
    }

    /* ---主体内容样式 --- */

    #crumbs {
        height: 50px;
        line-height: 50px;
        background: #fff;
        color: #000;
        text-indent: 20px;
    }

    #wf-task-return-activities {
        margin-bottom: 20px;
    }

    #wf-add-label {
        font-size: 20px;
    }

    #wf-my-labelTitle {
        /*color: #d9534f;
        cursor: default;*/
    }

    #wf-calendar-input {
        border: none;
        top: 0;
        left: 28px;
        height: 30px;
        width: 136px;
        line-height: 34px;
        text-indent: 5px;
    }
    #wf-content{
        background: #fff;
    }
    #wf-content-box {
        padding: 20px;
        height: 100%;
        /*height: calc(100% - 50px);*/
        background: #fff;
        overflow-y: auto;
    }

    #wf-content-box > div {
    }

    #wf-content #calendar {
        float: left;
        width: calc(96% - 300px);
    }

    #wf-content #events {
        float: right;
        margin: 0;
        position: relative;
    }

    #wf-content .datetimepicker-container {
        position: relative;
        float: left;
    }

    #crumbs {
        display: none;
    }

    .level-menuTask-wrapper {
        display: none;
        margin-top: 20px;
    }

    .level-menuTask-wrapper.active {
        display: block;
    }

    #wf-task-groups {
        color: #777777;
        overflow-y: scroll;
        overflow-x: hidden;
        max-height: 150px;
    }

    .group-item.active {
        color: #337ab7;
        font-weight: bolder;
    }

    .wf-get-task-group {
        width: 129px;
        display: inline-block;
    }

    .wf-get-task-group i {
        margin-right: 5px;
    }

    #wf-del-win .modal-dialog {
        margin-top: 300px;
    }

    /* ---标签样式 --- */
    #wf-label-header {
        height: 30px;
        line-height: 30px;
    }

    #wf-label-header .glyphicon-tags {
        margin: 6px 20px 0 0;
    }

    #wf-label-ul li {
        padding-bottmo: 5px;
        color: #555;
        text-indent: 7px;
        position: relative;
    }

    #wf-label-ul li .wf-label-delete {
        position: absolute;
        right: 25px;
        top: 10px;
    }

    #wf-label-ul li .wf-label-name {
        margin-bottom: 5px;
    }

    #wf-label-edit, #wf-label-plus {
        margin-top: 7px;
    }

    #wf-label-form {
        max-height: 151px;
        overflow: auto;
        overflow-x: hidden;
    }

</style>
<script src="/static/scripts/lib/jquery-ui.min.js"></script>
<script src="/static/scripts/lib/fullcalendar/moment.min.js"></script>
<script src="/static/scripts/lib/fullcalendar/moment.locale.zh-cn.js"></script>
<script src="/static/scripts/workflow/jquery.twbsPagination.min.js"></script>
<div id="wf-outline" class="clearfix">
    <section id="wf-menu" class="clearfix">
        <section id="wf-main-menu">
            <div id="wf-main-menuWrapper">
                <article id="wf-main-logo">
                    <span class="glyphicon glyphicon-fire"></span>
                </article>
                <ul id="wf-main-ul" class="wf-ul">
                    <li id="wf-main-activity" class="active main-menu" data-topic="wf-activities-change"
                        data-param="today">
                        <span class="glyphicon glyphicon-bullhorn icon"></span>
                        <span i18n="workflow.activities.ACTIVITIES"></span>
                    </li>
                    <li id="wf-main-task" class="main-menu" data-topic="wf-task-list" data-param="workingTask">
                        <span class="glyphicon glyphicon-tasks icon"></span>
                        <span i18n="workflow.mine.TASK"></span>
                    </li>
                    <li id="wf-main-calender" class="main-menu" data-topic="wf-firstMenu-change"
                        data-param="schedulers">
                        <span class="glyphicon glyphicon-calendar icon"></span>
                        <span i18n="workflow.pageInfo.TITLE_SCHEDULER"></span>
                    </li>
                </ul>
            </div>
        </section>
        <section id="wf-level-menu">
            <section id="level-menu-activity" class="active level-menuTask-wrapper">
                <ul id="wf-activity-time" class="wf-ul wf-level-ul mb30">
                    <li class="secondMenuItem active" data-type="default" data-topic="wf-activities-change"
                        data-param="today">
                        <span class="glyphicon glyphicon-tree-conifer"></span>
                        <span class="wf-get-crumbs" i18n="workflow.activities.TODAY"></span>
                    </li>
                    <li class="secondMenuItem" data-topic="wf-activities-change" data-param="yesterday">
