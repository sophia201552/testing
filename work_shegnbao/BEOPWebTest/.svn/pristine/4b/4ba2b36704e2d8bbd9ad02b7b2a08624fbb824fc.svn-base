<style>
    .tagTreeInfoHeader {
        height: 55px;
        line-height: 55px;
        color: #252525;
        font-size: 16px;
        border-bottom: 1px solid #ddd;
        padding: 0 10px;
    }

    .tagTreeBtnBox {
        padding: 10px 0;
        border-bottom: 1px solid #ddd;
    }

    .tagZtreeBox {
        height: calc(100% - 55px);
    }

    #tagTreeUl {
        overflow: auto;
        height: calc(100% - 80px);
    }

    #tagTreeUl .ml5 {
        margin-left: 5px;
    }

    #tagTreeUl li {
        line-height: 24px;
    }

    #tagTreeUl li a {
        color: #333;
        height: 24px
    }

    #tagTreeUl li a:hover {
        text-decoration: none;
    }

    #tagTreeUl li a .addNode, #tagTreeUl li a .addRule {
        margin-top: 2px;
        text-decoration: none;
    }

    #tagTreeUl li span.button.ico_open, #tagTreeUl li span.button.ico_close,
    #tagTreeUl li span.button.edit, #tagTreeUl li span.remove,
    #tagTreeUl li .addNode, #tagTreeUl li .addRule {
        background: none;
        font-family: 'Glyphicons Halflings';
        vertical-align: middle;
        width: 0;
        height: 0;
    }

    #tagTreeUl li span.button.ico_open, #tagTreeUl li span.button.ico_close {
        margin: 0 22px 0 0;
    }

    #tagTreeUl li span.button.edit {
        margin: 0 16px 0 -9px;
    }

    #tagTreeUl li span.remove {
        margin-right: 12px;
    }

    #tagTreeUl li span.addNode {
        margin-right: 6px;
    }

    #tagTreeUl li span.addRule {
        margin-right: 2px;
    }

    #tagTreeUl li span.button.ico_close:before, #tagTreeUl li span.button.ico_open:before {
        font-size: 15px;
    }

    #tagTreeUl li span.button.ico_close:before {
        content: "\e117";
    }

    #tagTreeUl li span.button.ico_open:before {
        content: "\e118";
    }

    #tagTreeUl li span.button.edit:before {
        content: "\270f";
    }

    #tagTreeUl li span.button.remove:before {
        content: "\e115";
    }

    #tagTreeUl li .addNode:before {
        content: "\e110";
    }

    #tagTreeUl li .addRule:before {
        content: "\e019";
    }

</style>
<div class="tagTreeInfo" style="height: 100%;">
    <header class="oh tagTreeInfoHeader">
        <span class="fl">结构</span>
        <button type="button" class="btn btn-default fr mt10" id="finishFolder">完成目录</button>
        <span class="glyphicon glyphicon-edit fr mt20" id="editFolder" title="修改"></span>
    </header>
    <div class="tagZtreeBox">
        <article class="tagTreeBtnBox tc">
            <button type="button" class="btn btn-default" id="tagTreeForward">
                <span class="glyphicon glyphicon-circle-arrow-left"></span>
                <span>向前</span>
            </button>
            <button type="button" class="btn btn-default" id="tagTreeBack">
                <span class="glyphicon glyphicon-circle-arrow-right"></span>
                <span>后退</span>
            </button>
            <button type="button" class="btn btn-default" id="tagTreeAdd">
                <span class="glyphicon glyphicon-plus-sign"></span>
                <span>添加</span>
            </button>
        </article>
        <ul id="tagTreeUl" class="ul ztree ml10 gray-scrollbar"></ul>
    </div>
</div>


<!--@formatter:off-->
<script type="text/html" id="tpl_tag_tree">

</script>
<!--@formatter:on-->