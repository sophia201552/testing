<style type="text/css">
    @keyframes AnimationWarning {
        from {
            opacity: 1;
        }

        to {
            opacity: 0.3;
        }
    }

    @-webkit-keyframes AnimationWarning {
        from {
            opacity: 1;
        }

        to {
            opacity: 0.1;
        }
    }


    .div-pane-icon {
        height: 100%;
        width: 100%;
        z-index: 99;
        overflow-y: auto;
        padding: 15px;
    }

    .div-pane-icon::-webkit-scrollbar-track {
        border-radius: 2px;
        background-color: #ccc;
    }

    .div-pane-icon::-webkit-scrollbar {
        height: 3px;
        width: 5px;
        background-color: #fff;
    }

    .div-pane-icon::-webkit-scrollbar-thumb {
        border-radius: 2px;
        background-color: #888;
    }


    .div-nav-row {
        /*width: 150px;*/
        border-bottom: 1px inset #ccc;
        cursor: pointer;
    }

    .div-nav-row:hover {
        border-bottom: 1px inset #aaa;
    }
    .div-nav-row-hide {
        line-height:40px;
        border-bottom: 0px;
    }

    .div-row-icon-badge {
        min-width: 40px;
        opacity: 0.7;
        margin-right: 8px;
    }

    .div-row-icon {
        margin: 2px 0 5px 0;
        padding: 0 1px 0 1px;
        top: 3px;
    }
    
    #obContainer {
        width: 100%;
        height: 100%;
    }
    .element {
        float: left;
        height: 100%;
    }

    .animation-warning {
        animation: AnimationWarning 0.5s linear 1s infinite alternate;
        -webkit-animation: AnimationWarning 0.5s linear 1s infinite alternate;
    }

    .div-pane-log {
        border-bottom: 1px inset #ccc;
        margin-top: 4px;
        padding-bottom: 2px;
    }

    .span-hover-pointer {
        cursor: pointer;
    }

    @media screen and (min-height: 769px){
        #divPaneNotice .panel-body {
            max-height: 870px;
        }
        /*.diagnosisLogCt{
            width: 16.7%;
        }*/
    }
    @media screen and (max-height: 768px){
        #divPaneNotice .panel-body {
            max-height: 660px;
        }
        /*.diagnosisLogCt{
            width: 19%;
            font-size: 13px;
        }
        .panel-body{
            padding: 3px;
        }*/
    }

    #divPaneNotice .panel-body:last-child {
        border-bottom-width: 0;
        border-bottom-left-radius: 3px;
        border-bottom-right-radius: 3px;
    }
    #divPaneNotice.top{
        box-shadow: 0 0 2px 1px #67aace;
    }

    .span-floor-number {
        display: none;
        font-size: 30px;
        padding: 20px 30px 10px 30px;
    }

    #divPaneNotice .div-pane-log > div > span {
        margin-right: 2px;
        opacity: 0.8;
    }

    #divPaneNotice p {
        margin: 4px 0 0 2px;
    }

    #divPaneNotice p .variable {
        font-weight: bolder;
        text-decoration: underline;
        color: #d9534f;
    }

    #divPaneNotice .span-btn {
        float: right;
        margin-right: 6px !important;
        margin-top: 4px;
        color: #666;
    }

    .grow:hover {
        /*background-color: #ccc;   去除圆圈背景颜色*/
    }
    .indexContent > div {
        padding: 5px;
    }
    .svgIcon{
        width: 20px;
        height: 20px;
        position: relative;
    }
    .svgIcon .badge{
        background-color: #428bca !important;
        position: absolute;
        right: -12px;
        top: -12px;
    }
    .nav-list li:first-child{
        cursor: pointer;
    }
    .panel-heading-btn{
        margin-right: 15px;
        padding-right: 1px;
    }
    #floorCt .glyphicon, #divPaneNotice .glyphicon {
        border-radius: 15px;
    }
    .glyphicon-pushpin{
        margin-right: 0;
        padding: 3px;
        margin-top: -2px;
        margin-left: 18px;
    }
    .glyphicon-pushpin.notTop{
        transform: scale(0.9);
    }
    .glyphicon-pushpin.top{
        transform: rotateZ(-45deg) scale(0.88);
    }
    #btnFloorsSP:hover {
      transform:rotateZ(-45deg) scale(1);  
    }
    .floorTop{
        z-index: 1;
        position: absolute;
        margin-top: 8px;
        padding: 0;
        margin-left: 6px;
        max-height: 100%;
    }
    .floorTop .panel{
        background-color: rgba(0,0,0,0.45);
    }
    .floorTop .panelViewHide {
        background-color:rgba(0,0,0,0);
    }
    .floorTop .div-row-icon-badge{
        background-color: rgba(194, 194, 194, 0.7);
    }
    #floorCt .panel-body{
        padding: 0;
    }
    #btnFloorsSP.btn{
        padding: 0;
        line-height: 1;
        opacity:1;
    }
    #btnChangeModal {
        margin-right:-15px;
        display:none;
    }
    #showSurveyChart {
        margin-right:10px;
        display:none;
    }
    .col-sm-1{
        transition: width 0.3s;
    }
    .col-sm-2{
        transition: width 0.3s;
    }
    .col-sm-3{
        transition: width 0.3s;
    }
    .bigGap{
        margin-right: 28px;
    }
    .minLog{
        width: 230px;
    }
    /* ERROR TIP STYLE */
    #errTip {
        position: absolute;
        max-height: 281px;
        overflow-y: auto;
        opacity: 0;
        background-color: #fff;
        padding: 5px;
        transition: opacity .15s linear;
        box-shadow: 0 0 5px #333;
    }
    #errTip > .div-pane-log { margin-top: 8px; padding-bottom: 0;}
    #errTip > .div-pane-log:first-child { margin-top: 5px; }
    #errTip > .div-pane-log:last-child { border-bottom: none; }
    #errTip > .div-pane-log span { margin: 0 5px; }
    #errTip > .div-pane-log > p { padding: 8px; margin-bottom: 0;}
    #errTip > .div-pane-log > .pFaultName { padding: 2px 8px 0 8px !important; margin-bottom: -6px; }
    /*tabel模板*/
    #panelIconNew tr {
        height:2.2%;vertical-align:middle;
    }
    #panelIconNew td {text-align:center}
    /*#panelIconNew tr:hover td:not(:first) {
        box-shadow:
            0 -5px 5px rgba(255,255,255,0.5),
            1px 0 5px rgba(255,255,255,0),
            0 5px 5px rgba(255,255,255,0.5),
            -1px 0 5px rgba(255,255,255,0);
        color:#000;
    }*/
    /*左侧概览背景色*/
    .defaultWhite {
        background-color: #fff;
    }
    .tdShadow {
        box-shadow:
            0 -5px 5px rgba(255,255,255,0.5),
            1px 0 5px rgba(255,255,255,0),
            0 5px 5px rgba(255,255,255,0.5),
            -1px 0 5px rgba(255,255,255,0);
        color:#000;
    }
    .defaultBlack {
        height: 100%;
        margin-bottom: 0px;
        background-color: rgba(0, 0, 0, 0.45098);
    }
    #panelIconNew {
        width:100%;height:100%;display:none;background:#fff;overflow:auto;
    }
    #panelIconNew table {
        width:50%;margin-left:25%;margin-right:25%; border:1px solid #fff;border-collapse:collapse;position:absolute;z-index:5;
    }
    .tabelHead {
        border-bottom:1px solid #fff;
    }
    .tabelHead td {
        width:5%;
        border-bottom:2px solid #fff;
        color: #636363;
        background: #d0d0d0;
        border-right:1px solid #fff; 
    }
    .tabelHead td:first-child {
        width: 7%;
        border-bottom: 2px solid #fff;
        background:#fff;
    }
    #panelIconNew td[class*=_01],#panelIconNew td[class*=_17]{
        border-left:2px solid #fff!important;
    }
    #panelIconNew td[class*=_05],#panelIconNew td[class*=_10],#panelIconNew td[class*=_14] {
        border-left:2px solid #fff!important;
        border-right:2px solid #fff!important;
    }
    #panelIconNew table .tdData{
        width: 5%;
        border: 1px solid #d0d0d0;
        /*color: #839198;*/
        color:#fff;
    }
    .tdFirst {
        width: 7%;
        border-right: 2px solid #fff;
        border-bottom: 1px solid #fff;
        color: #636363;
        background: #d0d0d0;
        cursor: pointer;
    }
    .tdFirstHover {
       background:#ff7800;
       color:#fff; 
    }
    #errTipNew {
        border:1px solid #e1e1e1;border-radius:5px;width:200px;color:#333;position:absolute;
    }
    .errTipHeader {
        height:40px;line-height:40px;font-size:20px;background:#F5F5F5;border-bottom:1px solid #e1e1e1;padding-left:20px;
    }
    .vavBox, .fptuBox {
        font-size:16px;padding:20px 0;background:#fff; 
    }
    .fptuBox { display:none;}
    .vavBox div, .fptuBox div {
        padding-left:20px;height:24px;line-height:24px;
    }
    #changeDemandPool {
        display:none;
        margin-top:1px;
    }
    #reuseDemand {
        display:none;
    }
</style>
<div class="col-sm-2 floorTop" id="floorCt">
    <div class="panel panel-default" style="height: 100%;margin-bottom: 0;">
        <div class="panel-heading" style="height: 41px;">
            <span i18n="diagnosis.OVERVIEW"></span>
            <span id="btnFloorsSP" class="glyphicon glyphicon-pushpin btn disabled panel-heading-btn grow top" title="Sticky Post"></span>
            <span id="btnChangeModal" class="glyphicon glyphicon-zoom-in panel-heading-btn grow top" title="Change Modal"></span>
            <span id="showSurveyChart" class="glyphicon glyphicon-eye-open panel-heading-btn grow top" title="Show Overview Chart"></span>
        </div>
        <div class="panel-body" id="" style="overflow: auto;height: calc(100% - 80px);">
            <div class="div-pane-icon" id="paneIcon"></div>
        </div>
    </div>
</div>
<div id="st-container" class="st-container st-effect-7 st-effect-1">
    <div class="indexContent st-pusher">

    <div class="col-sm-12 st-content" id="divCanvas" style="height: 100%;">
        <div id="leftCt" data-effect="st-effect-7" class="sideTrans leftCtOpen"><span class="glyphicon glyphicon-chevron-left"></span></div>
        <div id="rightCt" data-effect="st-effect-1" class="sideTrans rightCtOpen"><span class="glyphicon glyphicon-chevron-right"></span></div>
        <div id="obContainer">
            <div id="divMain" style="width: 100%; height: 100%;">
                <div class="div-canvas-ctn" id="divObserverCanvas" style="padding: 0; margin: 0 auto; height: 100%; width: 100%;">
                    <canvas class="canvas-ctn" style="width: 100%; height: 100%;">浏览器不支持</canvas>
                </div>
                <div id="divObserverTools" style="height: 0"></div>
            </div>
        </div>
    </div>

</div>
</div>

<div class="col-sm-3 minLog" id="diagnosisLogCt" style="position: fixed; right: 0; top: 54px; padding: 5px;min-width: 230px;">
    <div class="panel panel-default top" id="divPaneNotice">
        <div class="panel-heading" style="height: 41px;">
            <span id="logTitle" i18n="diagnosis.DIAGNOSIS_LOG" style="display: none;"></span>
            <span id="btnStickyPost" class="glyphicon glyphicon-pushpin panel-heading-btn grow top" title="Sticky Post" style="display: none;"></span>
            <span id="btnMinimize" class="glyphicon glyphicon-minus panel-heading-btn grow" title="Minimize" style="display: none;"></span>
            <span id="btnNoticeConfig" class="glyphicon glyphicon-cog panel-heading-btn grow" title="Configuration"></span>
            <span id="btnNoticeHistory" class="glyphicon glyphicon-list-alt panel-heading-btn grow" title="History"></span>
            <span id="btnAlertLog" class="panel-heading-btn grow svgIcon" title="View critical log">
                <svg enable-background="new 0 0 23 23" viewBox="0 0 23 23" x="0px" y="0px">
                <g>
                    <g>
                        <path fill="#D9534F" d="M 11.5 0 C 5.148 0 0 5.149 0 11.5 C 0 17.852 5.148 23 11.5 23 S 23 17.852 23 11.5 C 23 5.149 17.852 0 11.5 0 Z M 13 18 h -3 v -3 h 3 V 18 Z M 13 13 h -3 L 9 5 h 5 L 13 13 Z" />
                    </g>
                </g>
                </svg>
                <span class="badge"></span>
            </span>
            <span id="btnWarningLog" class="panel-heading-btn grow svgIcon" title="View urgent log">
                <svg enable-background="new 0 0 23 23" viewBox="0 0 23 23" x="0px" y="0px">
                <g>
                    <g>
                        <path fill="#F0AD4E" d="M 11.5 0 C 5.148 0 0 5.149 0 11.5 C 0 17.852 5.148 23 11.5 23 S 23 17.852 23 11.5 C 23 5.149 17.852 0 11.5 0 Z M 13 18 h -3 v -3 h 3 V 18 Z M 13 13 h -3 L 9 5 h 5 L 13 13 Z" />
                    </g>
                </g>
                </svg>
                <span class="badge"></span>
            </span>
            <span id="changeDemandPool" class="glyphicon glyphicon-calendar grow panel-heading-btn" title="Change Demand Pool"></span>
        </div>
    </div>
</div>

<div class="modal fade" id="detailScreenModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" id="detailScreenContent">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Dialog</h4>
            </div>
            <div class="modal-body clearfix">
                <canvas id="canvasDetail" class="col-md-12" style="width: 100%; margin: 0; padding: 0;" i18n="core.BROWSER_NOT_SUPPORT">浏览器不支持</canvas>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="dlgChangeRule" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="padding-top: 100px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel" i18n="diagnosis.config.TITLE">Change rule</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-3 control-label" i18n="diagnosis.config.CHANGE_LEVEL" style="min-width: 100px;"></label>
                        <div class="col-sm-6">
                            <select class="form-control col-sm-6" id="selectAlarmLevel">
                                <option i18n="diagnosis.config.LEVEL_SET.CRITICAL" setVal="1"></option>
                                <option i18n="diagnosis.config.LEVEL_SET.URGENT" setVal="2"></option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label" i18n="diagnosis.config.CHANGE_ALARM_TIME" style="min-width: 100px;"></label>
                        <div class="col-sm-6">
                            <select class="form-control col-sm-6" id="selectAlarmTime">
                                <option i18n="diagnosis.config.defaultPeriod.REAL_TIME" setVal="0"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_1H" setVal="1"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_12H" setVal="2"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_24H" setVal="3"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_7D" setVal="4"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_1M" setVal="5"></option>
                                <option i18n="diagnosis.config.defaultPeriod.DELAY_FOREVER" setVal="6"></option>
                            </select>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="btnChangeAlarm" i18n="diagnosis.config.CONFIRM">Save changes</button>
                <button type="button" class="btn btn-default" data-dismiss="modal" i18n="diagnosis.config.CANCEL">Close</button>
            </div>
        </div>
    </div>
</div>