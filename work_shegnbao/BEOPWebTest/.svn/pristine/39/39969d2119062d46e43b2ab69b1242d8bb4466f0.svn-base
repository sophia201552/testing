<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <title>Asset Management</title>
    {% assets "asset_css_bundle" %}
    <link rel="stylesheet" type="text/css" href="{{ ASSET_URL }}"> {% endassets %}
</head>
<body>
    <div id="wrapper">
        <div id="paneDock"></div>

        <div id="paneAssetFilter">
        </div>

        <div id="paneAssetList" style="display: none;">
            <div class="panel-titlebar disable-selection">
                <div class="panel-titlebar-text"><i class="icon-circle-arrow-right"></i> 设备列表</div> 
                <span id="spanPath" class="panel-titlebar-text" style="margin-left: 6px; font-weight: normal;"></span>
                <div class="panel-titlebar-button-close"><i class="icon-remove"></i><span class="glyphicon glyphicon-export" style="cursor: pointer;" title="导出"></span></div>
            </div>
            <div class="gray-scrollbar" style=" height: 100%; overflow-x: hidden; overflow-y: auto; padding: 5px;">
                <table class="table table-bordered  table-striped table-hover" id="tbAsset" cellpadding="1" cellspacing="1">
                    <thead>
                        <tr>
                            <th>类型</th>
                            <th>名称</th>
                            <th>描述</th>
                            <th>责任人</th>
                            <th>更新时间</th>
                            <th>到期时间</th>
                            <th>状态</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>

            </div>
        </div>

        <!-- 基本信息 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar gray-scrollbar" id="tabBaseInfo" aria-labelledby="baseInfo-tab" style="display: none;">
            <div class="tabPanelToolBox">
                <span id="btnEditBaseInfo" class="btnTool glyphicon glyphicon-edit" title="编辑"></span>
                <span id="btnSaveBaseInfo" class="btnTool glyphicon glyphicon-ok-sign" style="display: none;" title="保存"></span>
                <span id="btnCancelBaseInfo" class="btnTool glyphicon glyphicon-remove-sign" style="display: none;" title="取消"></span>
            </div>

            <div class="form-inline">
                <div class="form-group">
                    <label for="iptAssetName">名称</label>
                    <input type="text" class="form-control divValue" id="iptAssetName" disabled>
                </div>
                <div class="form-group">
                    <label for="selStatus">状态</label>
                    <select class="form-control divValue" id="selStatus" disabled>
                        <option value="0">已停止</option>
                        <option value="1">已启动</option>
                        <option value="2">维修中</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="iptAssetId">ID</label>
                    <input type="text" class="form-control divValue" id="iptAssetId" disabled>
                </div>

                <div class="form-group">
                    <label for="iptDescription">描述</label>
                    <input type="text" class="form-control divValue" id="iptDescription" disabled>
                </div>
                <div class="form-group">
                    <label for="iptBrand">品牌</label>
                    <input type="text" class="form-control divValue" id="iptBrand" disabled>
                </div>
                <div class="form-group">
                    <label for="iptAssetType">资产类型</label>
                    <input type="text" class="form-control divValue" id="iptAssetType" disabled>
                </div>
                <div class="form-group">
                    <label for="iptAssetTag">标签</label>
                    <input type="text" class="form-control divValue" id="iptAssetTag" disabled>
                </div>
                <div class="form-group">
                    <label for="iptSerialNum">序列号</label>
                    <input type="text" class="form-control divValue" id="iptSerialNum" disabled>
                </div>
                <div class="form-group">
                    <label for="iptManager">责任人</label>
                    <input type="text" class="form-control divValue" id="iptManager" disabled>
                </div>

                <br />
                <hr />
                <div class="form-group">
                    <label for="iptSupplier">供应商</label>
                    <input type="text" class="form-control divValue" id="iptSupplier" disabled>
                </div>
                <div class="form-group">
                    <label for="iptPurchaser">采购人</label>
                    <input type="text" class="form-control divValue" id="iptPurchaser" disabled>
                </div>
                <div class="form-group">
                    <label for="iptBuyPrice">购置价格</label>
                    <input type="text" class="form-control divValue" id="iptBuyPrice" onkeyup="this.value=this.value.replace(/\D/g,'')"  onafterpaste="this.value=this.value.replace(/\D/g,'')" disabled>
                </div>
                <div class="form-group">
                    <label for="iptBuyTime">购置时间</label>
                    <input type="text" class="form-control divValue datetimepicker" id="iptBuyTime" disabled>
                </div>
                <div class="form-group">
                    <label for="iptOvertime">过保时间</label>
                    <input type="text" class="form-control divValue datetimepicker" id="iptOvertime" disabled>
                </div>
                <div class="form-group">
                    <label for="iptUseTime">投入使用</label>
                    <input type="text" class="form-control divValue datetimepicker" id="iptUseTime" disabled>
                </div>
                <div class="form-group">
                    <label for="iptEndTime">到期时间</label>
                    <input type="text" class="form-control divValue datetimepicker" id="iptEndTime" disabled>
                </div>
                <br />
                <hr />
                <div class="form-group" style="display: block;width: 100%;">
                    <label for="txtRemark">备注</label>
                    <textarea id="txtRemark" class="form-control divValue" rows="4" style="width: calc(100% - 100px);border: 1px solid #666;" disabled></textarea>
                </div>
                <div class="form-group">
                    <label for="iptAssetPhoto" style="width: 100%;">设备照片</label>
                    <input type="file" class="form-control" id="iptAssetPhoto" accept="image/*" style="display: none;"/>
                    <div id="divAssetImg"></div>
                    <div id="divQRCodeImg"></div>
                </div>
            </div>
        </div>
        <!-- 基本信息 end -->
        <!-- 铭牌信息 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar" id="tabNameplate" aria-labelledby="nameplate-tab">
            <div id="nameplateType" class="form-inline"></div>
            <div id="divNameplate" class="form-inline"></div>
            <div class="tabPanelToolBox">
                <span id="btnEditNameplate" class="btnTool glyphicon glyphicon-edit" title="编辑" ></span>
                <span id="btnSaveNameplate" class="btnTool glyphicon glyphicon-ok-sign" title="保存" style="display:none"></span>
                <span id="btnCancelNameplate" class="btnTool glyphicon glyphicon-remove-sign" title="取消" style="display:none"></span>
            </div>
            <button type="button" class="btn btn-default btn-block active" id="btnAddNameplate" style="display:none;"><span  class="btnTool glyphicon glyphicon-plus" title="增加" ></span></button>
        </div>
        <!-- 铭牌信息 end -->
        <!-- 实时数据 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar" id="tabRealtimeData" aria-labelledby="realtimeData-tab">
            <div class="tabPanelToolBox">
                <span id="btnJoinAnalysis" class="btnTool glyphicon glyphicon-log-out" title="添加到数据分析"></span>
                <span id="btnHistory" class="btnTool glyphicon glyphicon-equalizer" title="查看历史"></span>
                <span id="btnEditDs" class="btnTool glyphicon glyphicon-edit" title="编辑点位" disabled></span>
            </div>

            <div class="form-inline">
                <div class="form-group" style="display: block; text-align: center;">
                    <label id="equipType" style="float:none; font-size: 16px; color: #f90; margin-bottom: 10px;"></label>
                </div>
            </div>

            <div class="form-inline" id="divRealtimeAttrs">

            </div>
        </div>
        <!-- 实时数据 end -->
        <!-- 维修记录 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar" id="tabMaintenance" aria-labelledby="maintenance-tab">
            <div class="tabPanelToolBox">
                <span id="btnAddMaintenance" class="btnTool glyphicon glyphicon-plus-sign" title="新增记录"></span>
            </div>

            <div class="input-group" style="margin-bottom: 10px; width: 100%;">
                <input type="text" class="form-control datetimepicker" id="">
                <div class="input-group-addon">To</div>
                <input type="text" class="form-control datetimepicker" id="">
                <div class="input-group-addon" id="addMaintenance">确认</div>
            </div>

            <div id="listMaintenance">
                <!--
                <div class="paneWfItem" data-id="111151137ddcf32fbc302221" data-toggle="modal" data-target="#wfDetail">
                    <div><span>2015-01-02 15:00</span> -- <span>2015-01-05 08:00</span></div>
