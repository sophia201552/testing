            </table>-->
            </div>
        </div>
        <!-- 维修记录 end -->
        <!-- 诊断记录 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar" id="tabDiagnosis" aria-labelledby="diagnosis-tab">
            <div class="input-group" style="margin-bottom: 10px; width: 100%;">
                <input type="text" class="form-control datetimepicker" id="">
                <div class="input-group-addon">To</div>
                <input type="text" class="form-control datetimepicker" id="">
                <div class="input-group-addon" id="addMaintenance">确认</div>
            </div>
            <div>
                <table class="table table-bordered table-hover" id="tbDiagnosis">
                    <thead>
                        <tr>
                            <th>时间</th>
                            <th>名称</th>
                            <th>状态</th>
                            <th>等级</th>
                        </tr>
                    </thead>
                    <tbody id="diagnosisTBody">
                        <tr>
                            <td>2016-02-15 18:00</td>
                            <td>支管温度无规律波动</td>
                            <td>已处理</td>
                            <td>严重</td>
                        </tr>
                        <tr>
                            <td>2016-02-15 18:00</td>
                            <td>板换二次供水温度无规律波动</td>
                            <td>已处理</td>
                            <td>严重</td>
                        </tr>
                        <tr>
                            <td>2016-02-15 18:00</td>
                            <td>板换一次供水温度无规律波动</td>
                            <td>已处理</td>
                            <td>严重</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <!-- 诊断记录 end -->
        <!-- 发布工单 start -->
        <div role="tabpanel" class="tab-pane paneInfo gray-scrollbar" id="tabWorkflow" aria-labelledby="tabWorkflow-tab">
            <div class="tabPanelToolBox">
                <span id="btnSend" class="btnTool glyphicon glyphicon-send" title="发布"></span>
            </div>

            <div class="form-inline">
                <div class="form-group">
                    <label>工单类型</label>
                    <select class="form-control divValue">
                        <option>检查</option>
                        <option>维修</option>
                        <option>保养</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>工单名</label>
                    <input type="text" class="form-control divValue">
                </div>
                <div class="form-group">
                    <label>状态</label>
                    <select class="form-control divValue">
                        <option>新建</option>
                        <option>已完成</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>任务组</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>截止时间</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>紧急程度</label>
                    <select class="form-control divValue">
                        <option value="0">一般</option>
                        <option value="1">严重</option>
                        <option value="2">紧急</option>
                    </select>
                </div>

                <div class="form-group">
                    <label>截止日期</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>人工成本</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>物料成本</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>税</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>服务商</label>
                    <input type="text" class="form-control divValue">
                </div>

                <div class="form-group">
                    <label>详情</label>
                    <textarea class="form-control divValue"></textarea>
                </div>
            </div>
        </div>
        <!-- 发布工单 end -->

        <!-- 历史数据 start -->
        <div id="paneHisData">
            <div id="divEchart" style="width: 80%;height: 100%;float: left;"></div>
            <div id="divLegendWrap" style="width: 20%;height: 100%;float: left;"></div>
            <div style="clear: both"></div>
        </div>
        <!-- 历史数据 end -->

        <!-- 工单详情 start -->
        <div class="modal fade" id="wfDetail" style="color: black;">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                aria-hidden="true">&times;</span></button>
                        <span class="label label-danger"><span class="glyphicon glyphicon-wrench" aria-hidden="true"></span> 维修</span>
                    </div>
                    <div class="modal-body">
                        <table class="table table-bordered">
                            <tr>
                                <td class="wf-table-head" width="20%">维修项目</td>
                                <td width="80%">9-13F灯具配电箱检查</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">设备名称</td>
                                <td>9-13F灯具</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">维修详情</td>
                                <td>9-13F灯具配电箱检查</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">负责人</td>
                                <td>李道道</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">截止时间</td>
                                <td>2015/01/05 08:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">维护周期</td>
                                <td>6个月一次</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">提前发送时间</td>
                                <td>2015/01/02 15:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">审核人</td>
                                <td>任石峰</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">相关人员</td>
                                <td>岳森，沈兰花</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" id="wfDetail2" style="color: black;">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                aria-hidden="true">&times;</span></button>
                        <span class="label label-success"><span class="glyphicon glyphicon-grain" aria-hidden="true"></span> 保养</span>
                    </div>
                    <div class="modal-body">
                        <table class="table table-bordered">
                            <tr>
                                <td class="wf-table-head" width="20%">保养项目</td>
                                <td width="80%">检查AHU-E-1 温度传感器</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">设备名称</td>
                                <td>AHU-E-1</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">保养详情</td>
                                <td>根据诊断系统反馈，该设备送风温度读数较长时间未变化,可能是传感器故障, 请检查相应传感器</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">负责人</td>
                                <td>golding2</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">截止时间</td>
                                <td>2016/02/26 08:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">维护周期</td>
                                <td>6个月一次</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">提前发送时间</td>
                                <td>2015/02/22 15:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head">审核人</td>
                                <td>irenel</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" id="wfDetail3" style="color: black;">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                aria-hidden="true">&times;</span></button>
                        <span class="label label-success"><span class="glyphicon glyphicon-grain" aria-hidden="true"></span> 保养</span>
                    </div>
                    <div class="modal-body">
                        <table class="table table-bordered">
                            <tr>
                                <td class="wf-table-head" width="20%">保养项目</td>
                                <td  width="80%">清洗5#冷却水泵过滤器。</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">设备名称</td>
                                <td  width="80%">5#冷却水泵过滤器</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">保养详情</td>
                                <td  width="80%">近日发现5#螺杆机组单独运行时，冷凝趋近温度非常高，超过6度，严重影响机组运行。经分析后发现，对应的冷却水流量很低。初步怀疑是过滤器脏堵所致。建议清洗5#冷却泵过滤器。</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">负责人</td>
                                <td  width="80%">golding2</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">截止时间</td>
                                <td  width="80%">2016/02/25 08:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">维护周期</td>
                                <td  width="80%">3个月一次</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">提前发送时间</td>
                                <td  width="80%">2015/02/23 15:00</td>
                            </tr>
                            <tr>
                                <td class="wf-table-head" width="20%">审核人</td>
                                <td  width="80%">irenel</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- 工单详情 end -->
        <div style="clear: both;"></div>
    </div>

    {% assets "asset_js_bundle" %}
    <script type="text/javascript" src="{{ ASSET_URL }}"></script> {% endassets %}

</body>
</html>