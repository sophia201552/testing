<ul class="nav nav-tabs" id="myTab">
    <li class="active">
        <a href="#pane1" data-toggle="tab">实时数据</a>
    </li>
    <li>
        <a href="#pane2" data-toggle="tab">导入数据</a>
    </li>
    <li class="fr">
        <button type="button" id="joinCurve" class="btn btn-warning m5 fr" data-dismiss="modal" i18n="">加入曲线</button>
        <button type="button" id="pointRefresh" class="btn btn-primary m5 fr" data-dismiss="modal" i18n="">刷新</button>
    </li>
</ul>

<div class="tab-content">
    <div class="tab-pane fade in active" id="pane1">
        <div class='container-fluid indexContent' id="divPointManagerPage">
            <div class='row-fluid'>
                <div class='col-xs-12 col-sm-12 col-md-12 col-lg-12'>
                    <div id="divProjectList" class="panel panel-default col-md-6 pointList">
                        <div class="panel-heading">
                            <span class="listTitle">监视列表</span>
                            <button type="button" id="delSelect" class="btn btn-danger fr" data-dismiss="modal" i18n="">删除选中</button>
                        </div>
                        <div class="panel-body gray-scrollbar">
                            <table class="table" id="tableMonitor" overflow="auto">
                                <thead>
                                    <tr>
                                        <th width="50%">点名</th>
                                        <th width="50%">点值</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                    <div id="divPointsList" class="panel panel-default col-md-6 pointList">
                        <div class="panel-heading">
                            <span class="listTitle">点名列表</span>
                            <button type="button" id="joinMonitoring" class="btn btn-success fr" data-dismiss="modal" i18n="">加入监视</button>
                            <div class="pr fr mr5 w150">
                                <span id="serachDel" class="glyphicon glyphicon-remove pa" aria-hidden="true"></span>
                                <input type="text" id="searchPointName" class="form-control" value="" />
                            </div>
                        </div>
                        <div class="panel-body gray-scrollbar">
                            <table class="table" id="tableWatch" overflow="auto">
                                <thead>
                                    <tr>
                                        <th width="10%">编号</th>
                                        <th width="50%">点名</th>
                                        <th width="40%">点值</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                    <div id="divOperationPanel" class="panel panel-default col-md-3 dn">
                        <div class="panel-heading">操作区域</div>
                        <div class="panel-body" style="padding:30px;">

                            <div class="form-group">
                                <label>开始时间</label>
                                <div class="input-group date form_datetime" data-date="" data-link-field="dtp_inputStart">
                                    <input class="form-control" size="16" type="text" value="" id="selStartTime">
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <label>结束时间</label>
                                <div class="input-group date form_datetime" data-date="" data-link-field="dtp_inputStart">
                                    <input class="form-control" size="16" type="text" value="" id="selEndTime">
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                                </div>
                            </div>

                            <div class="form-group">
                                <button type="button" id="btn_hist_curve">
                                    <label>历史曲线</label>
                                </button>
        <!--
                                <button type="button" id="btn_sel_all">
                                    <label>全选</label>
                                </button>
                                <button type="button" id="btn_sel_none">
                                    <label>不选</label>
                                </button>

                                <button type="button" id="btn_sel_save">
                                    <label>保存</label>
                                </button>
                                <button type="button" id="btn_sel_load">
                                    <label>载入</label>
                                </button>
                                <button type="button" id="btn_sel_clear">
                                    <label>清除</label>
                                </button>
        -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="tab-pane fade" id="pane2">
