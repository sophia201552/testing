<style>
    #spUploadInfo {
        font-size: 15px;
        font-weight: 700;
        margin-left: 15px;
    }
    #divUploadWrap {
        background: #fff;
        padding: 0 50px 20px;
    }
    .nav-tabs {
        background: #fff;
    }
    .tab-content {
        margin-top: 20px;
    }
    .div-actions {
        text-align: center;
    }
    .upload-wrap {position: relative; padding: 5px;}
    .drop-handler {
        position: absolute; 
        left: 0; 
        top: 0;
        z-index: 2;
        color:#92AAB0;
        background: #000;
        opacity: .6;
        text-align:center;
        vertical-align:middle;
        font-size:3em;
        width: 100%;
        height: 100%;
    }
    .drop-handler>p {
      opacity: 1;
      color: #fff;
      height: 100%;
      margin: 0;
      line-height: 100px;
    }
    .div-actions { text-align: center; }
    #uploadMask {
        position: fixed;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        z-index: 1;
        background: transparent;
    }

    .pointList {
        padding-bottom: 20px;
    }

    .pointList .panel-body {
        overflow-y: auto;
        height: 613px;
    }

    .pointList .listTitle {
        height:36px;
