        display: block;
        float: left;;
    }

    .pointList>.panel-heading {
        height: 46px;
        padding: 5px 15px;
    }

    .pointList .panel-body {
        padding-top: 30px;
    }

    .pointList tr {
        cursor: pointer;
    }

    #divPointManagerPage .pointList .active td {
        background: #f0ad4e;
        color: #fff;
    }

    .keyword {
        color: orange;
    }

    tr.active .keyword {
        color: #fff;
    }

    #serachDel {
        color: #5bc0de;
        cursor: pointer;
