<style>
    h1, h2, h3, h4, h5 {
        padding: 0;
        margin: 0;
    }

    ul, li {
        list-style: none;
    }

    /*tooltip样式修改*/
    .tooltip.top .tooltip-arrow {
        border-top-color: rgb(165, 42, 42);
    }

    .tooltip.right .tooltip-arrow {
        border-right-color: rgb(165, 42, 42);
    }

    .tooltip-inner {
        background: rgb(165, 42, 42);
    }

    .tooltip.bottom .tooltip-arrow {
        border-bottom-color: rgb(165, 42, 42);
    }

    .infoPrompt {
        text-indent: 30px;
        color: #a94442;
    }

    #benchmarkConfigure {
        margin-top: 30px;
        background: #fff;
        padding: 20px 30px;
        height: 95%;
    }

    #benchmarkConfigureInfo {
        background: #fff;
        max-height: 85%;
        overflow-y: auto;
    }

    #benchmarkConfigure .dropdown-menu {
        top: 52px;
        left: 64px;
    }

    #benchmarkConfigure .btnCon {
        margin-top: 30px;
    }

    #navTop {
        height: 85%;
    }

    #projectSelector {
        width: 200px;
        max-height: 510px;
        overflow-x: hidden;
        overflow-y: auto;
    }

    #itemMainPic {
        width: 30px;
        height: 30px;
        border-radius: 50px;
    }

    #projectSelector li {
        cursor: pointer;;
    }

    #projectSelector a {
        display: inline-block;
        width: 100px;
    }

    #projectSelector a, #projectSelector a:link, #projectSelector a:hover, #projectSelector a:active {
        text-decoration: none;
    }

    #projectSelector img {
        width: 25px;
        height: 25px;
        border-radius: 50px;
        float: right;
    }

    .itemTextContent {
        display: inline-block;
        width: 150px;
    }

    #navTopUl {
        margin-top: 15px;
        padding: 15px;
        max-height: 100%;
        overflow-x: hidden;
        overflow-y: auto;
    }

    #navTopUl .projectIcon {
        width: 12px;
        height: 16px;;
    }

    #navTopUl > li:nth-of-type(odd) {
        background: #f8f8f8;
    }

    #benchmarkConfigure h3{
        margin-bottom: 5px;
    }

    #benchmarkConfigure .configDropDownDenuUl {
        overflow: hidden;
        padding: 20px;
    }

    .configDropDownDenuUl li {
        float: left;
    }

    #benchmarkConfigure li {
        padding: 5px 15px 5px 20px;
        width: 100%;
    }

    .itemIndex {
        display: inline-block;
        width: 50px;
    }

    #benchmarkConfigure li input[readonly] {
        background: #ccc;
        cursor: default;
    }

    #benchmarkConfigure .projectSelector {
        width: 150px;
    }

    #benchmarkConfigure select, #benchmarkConfigure input[type="text"] {
        width: 130px;
        margin-right: 10px;
    }

    #benchmarkConfigure #navTopUl .directoryTypeInput {
        width: 322px;
    }

    #benchmarkConfigure .form-control {
        display: inline-block;
    }

    #benchmarkConfigure .menuOperation {
        line-height: 32px;
        color: #0c8cc0;
        cursor: pointer;
    }

    #benchmarkConfigure .menuOperation:hover {
        color: #0c8cc0;
    }

    .btnCon {
        text-align: center;
    }

    #errorConfigure {
        height: 35px;
        line-height: 35px;
    }
</style>

<div class="col-xs-10 col-xs-offset-1" id="benchmarkConfigure" index="">
    <h3>benchmark</h3>
    <div id="benchmarkConfigureInfo" class="benchmarkConfigureInfo">
        <div id="navTop">
            <ul id="navTopUl"></ul>
        </div>
    </div>
    <div id="errorConfigure" class="infoPrompt"></div>
    <div class="oh btnCon">
        <button type="button" id="saveSet" class="btn btn-primary" i18n="admin.common.SAVE"></button>
    </div>
</div>

<!--初始页面 -->
<script type="text/html" id="benchmark_tmpl">
    <ul id="navTopUl">
        <! for(var i =0; i < nav.length; i++){!>
        <li class="itemLi" nav-id="<!=nav[i]._id!>">
            <span class="itemIndex">第<!=i+1!>项</span>
            <select class="form-control mainMenu typeSelector">
                <! for(var prop in type){!>
                <!if(prop === nav[i].type){!>
                <option value="<!=prop!>" selected><!=type[prop].text!></option>
                <!}else{!>
                <option value="<!=prop!>"><!=type[prop].text!></option>
                <!}!>
                <!}!>
                <option value="">不显示内容</option>
            </select>
                <span class="itemInfo mainInfo">
                    <span class="itemTextContent">
                        <img src="http://images.rnbtech.com.hk/static/images/projectIcon.png" class="projectIcon"/>
                        <em class="mainMenuText menuName"><!=nav[i].name!></em>
                    </span>
                    <button class="btn btn-default benchmarkSetMenuBtn" data-menu-id="<!=nav[i].menuId!>">页面</button>
                </span>

            <div class="menuOperation fr">
                    <span class="itemCancel dn ml10 cp">
                        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                        <em>取消</em>
                    </span>
                    <span class="subItemEdit cp dn">
                        <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                        <em>副指标</em>
                    </span>
            </div>
            <ul class="configDropDownDenuUl dn">
                <!if(nav[i].children && nav[i].children.length > 0){!>
                    <! for(var j=0;j < nav[i].children.length;j++){!>
                        <li parent-id="<!=nav[i].children[j].parent!>" nav-id="<!=nav[i].children[j]._id!>">
                            <span class="itemIndex">第<!=(j+1)!>项</span>
                            <select class="form-control secondaryMenu typeSelector">
                                <! for(var prop in type){!>
                                <!if(prop === nav[i].children[j].type){!>　　　　
                                <option value="<!=prop!>" selected><!=type[prop].text!></option>
                                <!}else{!>
                                <option value="<!=prop!>"><!=type[prop].text!></option>
                                <!}!>
                                <!}!>
                                <option value="">不显示内容</option>
                            </select>
                            <span class="itemInfo">
                                <input type="text"
                                       class="form-control benchmark-name"
                                       placeholder="名称" value="<!=nav[i].children[j].name || ''!>"/>

                                <input type="text"
                                       class="form-control benchmark-points"
                                       placeholder="点"
                                       value="<!=(nav[i].children[j].points && nav[i].children[j].points.join && nav[i].children[j].points.join(',')) || ''!>">

                                <input type="text"
                                       class="form-control benchmark-title"
                                       placeholder="标题" value="<!=nav[i].children[j].title || ''!>">

                                <input type="text"
                                       class="form-control benchmark-description"
                                       placeholder="描述" value="<!=nav[i].children[j].description || ''!>">

                                <input type="text"
                                       class="form-control benchmark-unit"
                                       placeholder="单位" value="<!=nav[i].children[j].unit || ''!>">

                                <select class="form-control benchmark-desc">
                                    <!if(nav[i].children[j].desc == 0){!>
                                    <option value="0" selected>低值优先</option>
                                    <option value="1">高值优先</option>
                                    <!}else{!>
                                    <option value="0">低值优先</option>
                                    <option value="1" selected>高值优先</option>
                                    <!}!>
                                </select>
                                <button class="btn btn-default benchmarkSetMenuBtn" data-menu-id="<!=nav[i].menuId!>">页面</button>
                            </span>
                        </li>
                    <!}!>
                <!}!>
                <! for(; j < defaultNum; j++){!>
                <li parent-id="" nav-id="">
                    <span class="itemIndex">第<!=j+1!>项</span>
                    <select class="form-control secondaryMenu typeSelector">
                        <! for(var prop in type){!>
                            <!if('DropDownList' !== prop){!>　　　　　
                                <option value="<!=prop!>"><!=type[prop].text!></option>
                            <!}!>
                        <!}!>
                        <option value="" selected>不显示内容</option>
                    </select>
                    <span class="itemInfo"></span>
                </li>
                <!}!>
            </ul>
        </li>
        <!}!>
        <! for(; i < defaultNum; i++){!>
        <li nav-id="" class="itemLi">
            <span class="itemIndex">第<!=i+1!>项</span>
            <select class="form-control mainMenu typeSelector">
                <! for(var prop in type){!>
                <option value="<!=prop!>"><!=type[prop].text!></option>
                <!}!>
                <option selected value="">不显示内容</option>
            </select>
            <span class="itemInfo mainInfo"></span>
            <ul class="configDropDownDenuUl dn"></ul>
        </li>
        <!}!>
    </ul>
</script>

<!--文字内容 -->
<script type="text/html" id="itemText_Content_tmpl">
    <span class="itemTextContent">
        <img src="http://images.rnbtech.com.hk/static/images/projectIcon.png" class="projectIcon"/>
        <em class="mainMenuText menuName"><!=text!></em>
    </span>
</script>

<!--选择报表内容 -->
<script type="text/html" id="operating_Report_tmpl">
    <span class="reportContent">
        <select class="form-control reportTypeSelect">
            <option value="0">日报</option>
            <option value="1">月报</option>
        </select>
        <input type="text" value="" class="form-control directoryTypeInput" placeholder="请填入报表目录">
    </span>
</script>

<!--下拉菜单保存和取消 -->
<script type="text/html" id="dropDown_okOrNo_tmpl">
    <span class="itemCancel dn ml10 cp">
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        <em>取消</em>
    </span>
</script>

<!--修改所有 -->
<script type="text/html" id="item_edit_tmpl">
    <span class="subItemEdit cp">
        <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
        <em>修改</em>
    </span>
</script>

<!--下拉菜单内容 -->
<script type="text/html" id="sub_nav_tmpl">
    <! for(var i=0;i < 8;i++){!>
    <li parent-id="">
        <span class="itemIndex">第<!=(i+1)!>项</span>
        <select class="form-control secondaryMenu typeSelector">
            <! for(var prop in type){!>
            <option value="<!=prop!>"><!=type[prop].text!></option>
            <!}!>
            <option value="" selected>不显示内容</option>
        </select>
        <span class="itemInfo"></span>
    </li>
    <!}!>
</script>

<script type="text/html" id="benchmark_editing_tmpl">
    <input type="text"
           class="form-control benchmark-name"
           placeholder="名称" value="<!= nav.name || ''!>"/>

    <input type="text"
           class="form-control benchmark-points"
           placeholder="点" value="<!= nav.points || ''!>">

    <input type="text"
           class="form-control benchmark-title"
           placeholder="标题" value="<!= nav.title || ''!>">

    <input type="text"
           class="form-control benchmark-description"
           placeholder="描述" value="<!= nav.description || ''!>">

    <input type="text"
           class="form-control benchmark-unit"
           placeholder="单位" value="<!= nav.unit || ''!>">

    <select class="form-control benchmark-desc">
        <!if(nav.desc == 0){!>
        <option value="0" selected>低值优先</option>
        <option value="1">高值优先</option>
        <!}else{!>
        <option value="0">低值优先</option>
        <option value="1" selected>高值优先</option>
        <!}!>
    </select>
    <button class="btn btn-default benchmarkSetMenuBtn" data-menu-id="<!=nav.menuId!>">页面</button>
</script>