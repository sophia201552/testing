/* ============================================= 团队begin =============================================*/
#wf_team_box {
    padding: 10px;
}

#wf_team_wrapper {
    height: 100%;
    overflow: auto;
}

#wf_team_edit {
    width: 200px;
    float: right;
}

.wf-add-tags {
    font-size: 20px;
    margin-right: 5px;
    cursor: pointer;
}

.wf_team_wrapper .form-team {
    padding: 10px;
    overflow: hidden;
}

.wf_team_wrapper .wf-manager-people {
    padding-top: 10px;
    min-height: 90px;
    background: #303e5b;
}

.wf_team_wrapper .people {
    float: left;
    margin: 0 30px 10px 0;
}

#wf_process_item_box .people {
    float: left;
    margin: 0 10px 10px 0;
    width: 50px;
}

.wf_team_wrapper .people-name {
    width: 50px;
    height: 25px;
    line-height: 25px;
    text-align: center;
}

.wf_team_wrapper .user, .people img, #wf_process_item_box .people img {
    width: 50px;
    height: 50px;
    border-radius: 50px;
}

.wf-tag-wrapper {
    padding: 5px 22px;
    margin: 5px;
    border: 1px solid #303e5b;
    border-radius: 5px;
    background: #303e5b;
    color: #fff;
    position: relative;
}

.add-person-to-process .people {
    float: left;
}

.no-join-box {
    height: 200px;
    text-align: center;
    margin-top: 20%;
}

.wf-delete, .wf-tag-delete {
    top: 7px;
    right: 0;
    display: block;
    height: 16px;
    width: 16px;
    font-size: 12px;
    text-align: center;
    line-height: 16px;
    cursor: pointer;
    z-index: 12;
    border-radius: 50px;
}

#wf_team_wrapper .people .wf-delete {
    color: #fff;
    background-color: #000;
    display: none;
}

#wf_team_wrapper .people:hover .wf-delete {
    display: block;
}

/*.wf-user-name-box:hover .wf-group-name-edit {
    display: inline-block;
}*/

.wf-name-editable {
    border: 1px solid #87cefa;
}

.wf_team_process {
    padding: 10px;
    background-color: #303e5b;
}

@media (min-width: 768px) {
    .form-horizontal .control-label {
        padding-top: 0;
    }
}

.wf_team_process_ul {
    padding-left: 0;
}

.wf_team_process_ul li {
    padding: 8px;
    overflow: hidden;
}

.wf_team_process_ul .glyphicon-arrow-right {
    margin: 0 5px;
}

.wf_team_process_ul .wf_team_process_title {
    width: 150px;
    margin-top: 10px;
}

.wf_team_process_ul .wf_team_process_control {
    margin: 10px 0 0 10px;
}

.wf_team_process_ul .wf_team_process_control .glyphicon {
    cursor: pointer;
    margin: 0 5px;
}

.wf_process_box {
    margin: 0 auto;
    width: 400px;
}

.wf_process_box .modal-body {
    overflow: auto;
    height: 600px;
}

#wf_process_box .wf-add-process-item, #wf_process_box .wf-delete-process-item {
    font-size: 16px;
}

.wf-arrow-group {
    width: 280px;
}

.wf-line {
    width: 250px;
    height: 2px;
    background: rgb(100, 100, 100);
    overflow: hidden;
}

.wf-arrow-left-down {
    left: 7px;
    top: 0px;
}

.wf-arrow-right-down {
    left: -7px;
    top: 0px;
}

.wf_process_item_box {
    margin-top: 5px;
}

.wf_process_item {
    width: 355px;
    position: relative;
    overflow: hidden;
    margin: 5px auto 20px auto;
}

.wf_process_item_box .wf_process_item label {
    display: block;
    float: left;
}

.wf_process_item_box .glyphicon-arrow-right {
    margin: 5px 10px 0 10px;
}

#wf-outline #wf_process_item_box .wf_process_item .wf-team-people-add {
    position: absolute;
    right: 0;
    top: 20px;
    float: right;
    font-size: 16px;
}

#wf_team_wrapper .wf_process_item .wf-team-people-add {
    font-size: 16px;
}

#wf_team_wrapper .form-team .wf-team-people-add {
    font-size: 25px;
    margin-top: 20px;
}

#wf_add_tag_win .modal-dialog {
    margin-top: 200px;
}

#wf_add_process_win .modal-dialog {
    margin-top: 50px;
}

#wf-outline #wf_add_process_win input, #wf-outline #wf_add_process_win select {
    background: #d8d9db !important;
    color: #333 !important;
    border: 1px solid #d8d9db !important;
}

#wf_team_tags_wrapper {
    float: left;
}

#wf_team_tags_wrapper .glyphicon-tag {
    display: none;
}

.wf_team_show_page #wf_team_tags_wrapper .glyphicon-tag {
    display: inline-block;
}

#wf_team_tags_wrapper .wf-tag-save {
    display: none;
}

#wf_team_tags_wrapper .wf-tag-edit, #wf_team_tags_wrapper .wf-tag-save {
    top: 7px;
    left: 5px;
    cursor: pointer;
}

.wf-tag-name {
    border: 1px solid #303e5b;
    position: relative;
    left: 4px;
}

.wf-tag-name-editable {
    border-color: #87cefa;
}

#wf_process_nodes {
    display: none;
}

#wf_team_wrapper .col-sm-1 {
    text-align: right;
}

#wf_add_team {
    font-size: 30px;
}

/*.wf_team_wrapper .wf-group-name {
    display: inline-block;
    width: 70px;
    position: relative;
    top: 5px;
    left: 0;
}*/

.wf-added-user-list .col-sm-1 {
    width: 10%;
}

.wf-added-user-list .col-sm-11 {
    width: 90%;
}

.wf-added-user-list .form-team {
    padding-left: 0;
}

.wf-user-name-box {
    padding-bottom: 10px;
    padding-left: 0;
}

/* ----- 团队控制显示begin ----- */
.wf_team_show_page .wf-team-people-add, .wf_team_show_page .wf-group-name-edit, .wf_team_show_page #addTeamGroupText,
.wf_team_show_page #addTeamGroupInput, .wf_team_show_page .wf_team_process_edit, .wf_team_show_page .wf_team_process_remove,
.wf_team_show_page #wf_add_process, #wfTeamAddSuperAdmin, .wf_member_type_admin .team-edit #wfTeamAddSuperAdmin,
.wf_member_type_common #wf_team_edit, .wf_member_type_admin #wfTeamAddAdmin {
    display: none;
}

.wf_member_type_super_admin #delete_team, .team-create #wfTeamAddSuperAdmin, .team-edit #wfTeamAddSuperAdmin {
    display: inline-block;
}

/* ----- 团队控制显示end ----- */

/* ============================================= 团队end =============================================*/

.wf-task-split {
    border-top: 2px dashed #192234;
    padding-top: 15px;
}