                            if ('successful' == data.error) {
                                // success
                            }
                        }).error(function () {
                        }).always(function () {
                        });
                    }
                    else if (1 === dragType || 2 === dragType) {
                        // drag item
                        var itemType = 0;
                        var iconColor = '';
                        var custName = '';
                        var projId = 0;
                        var pointName = '';
                        var pointDesc = '';
                        for (var i = 0, len = _this.m_allPointList.length; i < len; i++) {
                            if (draggedID == _this.m_allPointList[i].itemId) {
                                _this.m_allPointList[i].groupId = dstGroupId;
