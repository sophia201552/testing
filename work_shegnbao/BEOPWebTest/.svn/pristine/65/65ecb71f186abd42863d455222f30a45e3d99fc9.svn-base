            });
            $('#selProjectList', this.$modal).empty().html(arrHtml.join(''));
        };

        // 
        this.attachEvents = function () {
            var _this = this;

            this.$modal.off();

            this.$modal.on('hidden.bs.modal', function () {
                _this.reset();
                _this.$modal.detach();
            });

