                        alert(rs.msg);
                    }
                }).always(function () {
                    $this.prop('disabled', false);
                });
            });

            $('#selProjectList', this.$modal).off().on('change', function () {
                var title = $(this).children('[value="'+this.value+'"]')[0].innerHTML;
