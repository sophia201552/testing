.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.infoBox {
    width: 400px;
    overflow-y: hidden;
    z-index: 9999;
    box-shadow: 0 2px 10px 0 rgba(0, 0, 0, 0.25);
    border-radius: 2px;
    position: absolute;
    margin-top: 8%;
    margin-left: calc(50% - 200px);
    min-height: 110px;
    max-width: 500px;
    outline: 0;
    background-color: #fff;
    max-height: 212px;
    font-family: 'Microsoft YaHei', Helvetica;
}

.infoBox ul {
    padding-left: 0;
    list-style: none;
}

.infoBoxRemind, .infoBoxVersion {
    position: fixed;
    bottom: 5px;
    right: 5px;
    display: none;
    max-height: 300px;
}

.infoBox-header {
    color: black;
    font-weight: bold;
    border-bottom: #eee 1px solid;
    border-radius: 2px 2px 0 0;
    padding: 20px 24px;
    background-color: #fff;
    height: 16px;
    position: relative;
}

.infoBox-header.movable {
    cursor: move;
}

.infoBox-close {
    width: 10px;
    height: 10px;
    float: right;
    border: 0;
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: center;
    cursor: pointer;
    right: 17px;
    top: 17px;
    position: absolute;
    background-image: url(data:image/png;base64,);
}

.infoBox-title {
    font-size: 14px;
}

.infoBox-footer {
    background: #fbfbfb;
    border-top: #eee 1px solid;
    border-radius: 0 0 2px 2px;
    padding: 4px;
    min-height: 40px;
}

.infoBox-body {
    color: black;
    min-height: 125px;
    max-height: 240px;
    padding: 20px 20px;
    overflow-y: auto;
    overflow-x: hidden;
}

.infoBox-body .userImg {
    padding: 2px;
    border: 1px solid #ccc;
    border-radius: 50px;
    background: #fff;
    width: 50px;
    height: 50px;
    margin: 5px;
}

.infoBox-body .wfMessageUserName {
    color: #337ab7;
    margin-right: 10px;
}

.infoBox-msg {
    float: left;
    margin-left: 10px;
    margin-top: 15px;
    word-break: break-word;
    display: inline-block;
    width: 300px;
}

.infoBox-icon {
    float: left;
    display: inline-block;
}

.alert-button {
    float: right;
    padding: 5px 25px;
    margin-right: 5px;
}

.infoBoxAlert, .infoBoxConfirm {
    top: 20px;
}

.infoBoxVersion {    font-size: 14px;
}

.infoBoxVersion .infoBox-body {
    padding: 5px;
}

.infoBoxVersion .infoBox-msg {
    margin: 0;
}

.infoBoxVersion .panel-body {
    padding: 10px 20px;
}

.infoBoxVersion pre {
    padding: 0;
    border: none;
    background: #fff;
}

.infoBoxVersion .infoBox-msg {
    width: 390px;
}

.infoBoxRemind .infoBox-msg {
    width: 370px;
}

.infoBoxRemind .infoBox-msg {
    padding: 10px;
}

.infoBoxRemind .infoBox-body {
    padding: 5px;
}

.infoBoxVersion-title {
    font-size: 14px;
    position: relative;
    top: -7px;
}

.infoBox.infoBoxMessage {
    right: 3px;
    top: 55px;
    height: auto;
    max-height: none;
    margin-top: 0;
    position: fixed;
}

.infoBoxMessage ul {
    margin-top: -15px;
}

.infoBoxMessage li {
    overflow: hidden;
    border-bottom: 1px solid #eee;
    padding: 10px 0;
    cursor: pointer;
}

.infoBoxMessage li:hover {
    background: #e5e5e5;
}

.infoBoxMessage .infoBox-body .userImg {
    padding: 2px;
    margin: 0;
}

.infoBoxMessage .infoBox-header {
    background: #fafafa;
}

.infoBoxMessage .header-right {
    color: #a7a7a7;
}

.infoBoxMessage .infoBox-body {
    padding: 15px 0;
    color: #999;
    max-height: none;
}

.infoBoxMessage .infoBox-msg {
    width: 100%;
    margin: 0;
    height: auto;
}

.infoBoxMessage .wdr1 {
    width: 20%;
    text-align: center;
}

.infoBoxMessage .wdr2 {
    width: 55%;
    line-height: 20px;
    padding-top: 5px;
}

.infoBoxMessage .wdr3 {
    width: 25%;
    text-align: center;
    margin-top: 7px;
}

.infoBoxMessage .unread {
    float: right;
    width: 10px;
    height: 10px;
    border-radius: 10px;
    margin-right: 10px;
    background: #f00;
}

.infoBoxMessage .userImg {
    border-radius: 0;
}

.infoBoxMessage .messageUserName {
    color: #ad9384;
}

.infoBoxMessage .messageTime {
    color: #c4c4c4;
}

.infoBoxMessage .messageShowAll {
    color: #b6b6b6;
}

.infoBox .progress {
    height: 2px;
    margin-bottom: 0;
}

.infoBox .progress.active .progress-bar {
    -webkit-transition: none !important;
    transition: none !important;
}

#infoBoxModal {
    width: 100%;
    height: 100vh;
    position: absolute;
    top: 0;
    z-index: 9998;
    opacity: 0.5;
    background: #000;
}