<style>
    #indexMain {
        background-color: white;
    }
    #containerEChartsSlide {
        height: 40%;
        width: 100%;
    }
    #containerEChartsSlide .carousel-inner{
        height: 100%;
    }
    #containerEChartsSlide .carousel-indicators {
        bottom: 0;
        margin-bottom: 0;
    }
    #containerEChartsSlide .carousel-indicators li{
        background-color: #aaa;
    }
    #containerEChartsSlide .carousel-indicators li.active{
        background-color: #2486FC;
    }
    .slideChart {
        position: absolute;
    }
    #divProjectSummary {
        height: 60%;
        width: 100%;
        overflow-y: auto;
        position: relative;
        margin-bottom: 0;
    }
    #divProjectSummary .panel-heading {
        padding: 0;
    }
    .panel-primary>.panel-heading {
        color: #000;
        background-color: rgb(234,239,245);
        border-color: rgb(230,235,241);
    }
    .panel-primary {
        border-color: #ddd;
        box-shadow: 1px 2px 5px 0px #ddd;
        font-family: "Microsoft Yahei";
    }
    .panel-primary>.panel-heading+.panel-collapse>.panel-body {
        border-top-color: transparent;
    }
    .aCollapse {
        display: inline-block;
        width: 100%;
        position: relative;
    }
    .aCollapse:hover, .aCollapse:focus,.aCollapse:active{
        text-decoration: none;
    }
    .aCollapse[aria-expanded="true"] .glyphicon-chevron-down{
        display: none;
    }
    .aCollapse[aria-expanded="false"] .glyphicon-chevron-up{
        display: none;
    }
    .aCollapse span{
        padding: 10px 15px;
        display: inline-block;
    }
    .reportDetail {
        z-index: 1;
        display: none !important;
    }
    .collapseArrow {
        padding: 0 !important;
        right: 3%;
        top: 1rem;
        transform: scale(1.1,0.9);
        color: rgb(199,199,204);
        position: absolute;
        font-size: 1.4rem;
    }
    #adminPhoto {
        height: 4rem;
        width: 4rem;
        display: inline-block;
        background-color: black;
    }
    #adminInfo {
        font-size: 2.4rem;
        width: 50%;
        display: inline-block;
    }
    #btnPhoneAdmin {
        position: relative;
        font-size: 3.2rem;
        left: 5%;
    }
    #btnMessageAdmin {
        position: relative;
        font-size: 3.2rem;
        left: calc(5% + 3.2rem);
    }
    .canvas-container img{
        width: 100%;
        height: 100%;
    }
    @media (max-device-height:667px) and (-webkit-min-device-pixel-ratio:2){/* 兼容iphone4 */
        .report-block{
            padding-left: 0;
            padding-right: 0;
        }
    }
    .btnDetail{
        position: relative;
        left: 75%;
        display: inline-block;
        padding: 1rem;
    }
</style>
<div id="containerEChartsSlide" class="carousel slide" data-ride="carousel" data-interval="10000">
    <ol class="carousel-indicators">
        <li data-target="#containerEChartsSlide" data-slide-to="0" class="active"></li>
        <li data-target="#containerEChartsSlide" data-slide-to="1"></li>
        <li data-target="#containerEChartsSlide" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active" data-interval="10000">
            <div class="slideChart"></div>
        </div>
        <div class="item" data-interval="10000">
            <div class="slideChart"></div>
        </div>
        <div class="item" data-interval="10000">
            <div class="slideChart"></div>
        </div>
    </div>
</div>
<div class="panel-group" id="divProjectSummary" role="tablist" aria-multiselectable="true">
</div>