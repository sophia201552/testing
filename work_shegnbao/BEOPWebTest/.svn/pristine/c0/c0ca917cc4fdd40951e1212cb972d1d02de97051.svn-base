<div id="wf-memberSelected-container">
    <style type="text/css">
        .pr {
            position: relative;
        }

        .pa {
            position: absolute;
        }

        .fl {
            float: left;
        }

        .fr {
            float: right;
        }

        #workflow-insert-container .modal-dialog {
            margin-top: 60px;
            width: 800px;
        }

        #workflow-insert-container table td, #workflow-insert-container table th {
            padding: 5px;
            color: #333;
        }

        .wf-table-head-fault {
            text-align: right;
        }

        #workflow-insert-container .wf-detail-table tr td, #workflow-insert-container .wf-detail-table tr th {
            border: none !important;
        }

        #workflow-insert-container .wf-detail-table .glyphicon {
            cursor: pointer;
        }

        #workflow-insert-container input, #workflow-insert-container select {
            width: 200px;
        }

        #workflow-insert-container {
            background: #000;
            -webkit-user-select: none;
            -ms-user-select: none;
            -moz-user-select: none;
        }

        #workflow-insert-container .modal-dialog {
            margin: 60px auto;
        }

        .wf-search-box-select li {
            width: auto;
            margin-right: 25px;
            cursor: pointer;
            padding: 5px 0;
            vertical-align: middle;
        }

        .wf-search-box-select .active {
            color: #3071a9;
            border-bottom: 1px solid #3071a9;
        }

        .wf-search-result-container {
            min-height: 430px;
            padding: 10px 10px 0 10px;
        }

        .wf-search-all {

        }

        .wf-search-all ul {
            border: none;
        }

        .wf-search-all ul .active {
            color: #1543fa;
        }

        .wf-search-all li {
            display: inline-block;
            padding: 0 5px;
            text-transform: capitalize;
            font-size: 12px;
            cursor: pointer;
            transition: .2s all linear 0s;
        }

        .wf-search-result-title ul li:hover {
            color: #fff;
            background: #000;
            border-radius: 5px;
        }

        .wf-search-result {
            position: relative;
            padding: 10px 38px;
            height: 355px;
            border: 1px solid #cccccc;
            -webkit-user-select: none;
            -webkit-font-smoothing: antialiased;
            overflow-x: hidden;
            overflow-y: auto;
        }

        .wf-search-result > div {
            margin: 5px 0;
        }

        .wf-search-result .wf-member-selected {
            background: #F1EDED;
        }

        .wf-search-result .wf-member {
            border-radius: 5px;
            transition: .3s background 0s linear;
            border: 1px solid #ccd;
            position: relative;
            cursor: pointer;
            width: 60px;
            height: 60px;
            padding: 0;
            margin: 10px 15px;
            float: left;
        }

        .wf-search-result .wf-member:hover {
            background: #F1EDED;
        }

        .wf-search-result-title {
            margin: 5px 0;
        }

        .wf-search-result-title .nav-tabs li.active {
            background: #000000;
            color: #fff;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
        }

        .wf-member .wf-team-avatar-name {
            width: 100%;
            display: block;
            background: rgba(0, 0, 0, 0.3);
            color: #fff;
            font-size: 12px;
            margin: 0;
        }

        .wf-member-avatar {
            text-align: center;
            /*padding: 5px ;*/
            position: absolute;
            width: 100%;
            height: 100%;
            z-index: 1;
        }

        .wf-member-avatar .avatar-icon {
            width: 100%;
            height: 100%;
            transition: .3s opacity 0s linear;
            border-radius: 5px;
        }

        .wf-member .wf-member-content {
            position: absolute;
            left: 0;
            right: 0;
            z-index: 10;
            text-align: center;
            border-radius: 5px;
            bottom: 0;
        }

        .wf-member .wf-member-check {
            display: none;
        }

        .wf-member-selected .wf-member-check {
            position: absolute;
            right: 0;
            top: 0;
            width: 20px;
            height: 20px;
            z-index: 11;
            text-align: right;
            font-size: 12px;
            border-color: #A1CF64;
            color: #fff;
            display: block;
        }

        #calendar-time {
            width: 241px;
            height: auto;
            margin: 10px 0;
        }

        .wf-member-selected .wf-member-check:after {
            position: absolute;
            content: "";
            right: 0;
            top: 0;
            z-index: -1;
            width: 0;
            height: 0;
            border-top: 0 solid transparent;
            border-right: 25px solid transparent;
            border-bottom: 25px solid transparent;
            border-left: 0 solid transparent;
            border-right-color: inherit;
            -webkit-transition: border-color .2s ease;
            -moz-transition: border-color .2s ease;
            transition: border-color .2s ease;
        }

        .wf-check-result-container {
            padding: 5px 10px;
            min-height: 60px;
        }

        .wf-check-result-container > span {
            display: inline-block;
            height: 35px;
            width: auto;
            line-height: 35px;
            text-align: center;
            margin-right: 10px;
            margin-top: 5px;
        }

        .wf-check-result {
            list-style: none;
            padding: 5px 0 0 80px;
        }

        #labelName {
            width: 200px !important;
        }

        .wf-check-result li {
            width: 76px;
            height: 25px;
            max-width: 80px;
            background: #A1CF64;
            padding: 5px;
            border-radius: 13%;
            font-size: 12px;
            float: left;
            text-align: center;
            text-transform: none;
            margin: 5px;
            -webkit-user-select: none;
            cursor: default;
            transition: .3s opacity 0s linear;

        }

        .wf-check-result li:hover {
            opacity: .8;
        }

        .wf-check-result li span {
            display: inline-block;
            width: 50px;
        }

        .wf-check-result li .glyphicon {
            vertical-align: top;
            line-height: 16px;
            cursor: pointer;
            color: #fff;
            transform: scale(.87);
        }

        .wf-search-by-name ul .active {
            color: #fff;
            background: #000;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
        }

        .wf-search-by-name ul li:hover {
            color: #fff;
            background: #000;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
            transform: scale(1.1, 1.1);
        }

        .wf-search-by-name ul {
            border: none;
        }

        .wf-search-by-name ul li {
            display: inline-block;
            margin: 0 5px;
            text-transform: capitalize;
            font-size: 12px;
            cursor: pointer;
            padding: 0 5px;
            transition: .1s all 0s linear;
        }

        .wf-detail-userInfo {
            width: 50px;
            height: 50px;
            float: left;
            margin: 3px;
            border-radius: 5px;
            overflow: hidden;

        }

        .wf-person-pic {
            width: 50px;
            height: 50px;
            /*border-radius: 50px;*/
        }

        .wf-detail-userName {
            width: 100%;
            height: 20px;
            line-height: 20px;
            text-align: center;
            font-size: 13px;
            left: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.3);
            color: #fff;
        }

        .wf-add-user {
            font-size: 18px;
        }

        .wf-manager-people .people {
            width: 50px;
            height: 50px;
            float: left;
            margin: 0 5px 5px 5px;
            position: relative;
        }

        .wf-manager-people .wf-people-add {
            border-radius: 50%;
            font-size: 25px;
            padding: 5px;
            margin-left: 5px;
            position: relative;
            left: 0;
            color: #aaa;
            cursor: pointer;
        }

        .wf-manager-people .wf-people-add {
            font-size: 22px;
            cursor: pointer;
        }

        .wf-manager-people .wf-people-add:hover {
            color: #666;
        }

        .wf-manager-people .people-name {
            position: absolute;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.3);
            height: 20px;
            text-align: center;
            line-height: 20px;
            color: #fff;
            font-size: 13px;
        }

        .btn {
            display: inline-block;
            padding: 6px 12px;
            margin-bottom: 0;
            font-size: 14px;
            font-weight: normal;
            line-height: 1.42857143;
            text-align: center;
            white-space: nowrap;
            vertical-align: middle;
            touch-action: manipulation;
            cursor: pointer;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
            background-image: none;
            border: 1px solid transparent;
            border-radius: 4px;
        }

        .btn-success {
            color: #fff;
            background-color: #5cb85c;
            border-color: #4cae4c;
        }

        #wf-member-comfirm-btn {
            color: #fff;
        }
    </style>
    <div class="modal fade dn" id="wf-add-person" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">×</span></button>
                    <h5 class="modal-title" i18n="workflow.memberSelected.TITLE"></h5>
                </div>
                <div class="modal-body" style="padding-bottom: 0">
                    <div class="form-group" style="margin-bottom: 0">
                        <input type="text" class="form-control" id="input-search-name"
                               i18n="placeholder=workflow.memberSelected.INPUT_INFO">
                    </div>
                    <div class="wf-search-box">
                        <div class="wf-search-content">
                            <!--筛选所有人-->
                            <section class="wf-search-result-container wf-search-all">
                                <div class="wf-search-result-title">
                                    <ul class="nav nav-tabs">
                                        <li i18n="workflow.memberSelected.ALL"></li>
                                        <li>a</li>
                                        <li>b</li>
                                        <li>c</li>
                                        <li>d</li>
                                        <li>e</li>
                                        <li>f</li>
                                        <li>g</li>
                                        <li>h</li>
                                        <li>i</li>
                                        <li>j</li>
                                        <li>k</li>
                                        <li>l</li>
                                        <li>m</li>
                                        <li>n</li>
                                        <li>o</li>
                                        <li>p</li>
                                        <li>q</li>
                                        <li>r</li>
                                        <li>s</li>
                                        <li>t</li>
                                        <li>u</li>
                                        <li>v</li>
                                        <li>w</li>
                                        <li>x</li>
                                        <li>y</li>
                                        <li>z</li>
                                    </ul>
                                </div>
                                <div class="wf-search-result scrollbar"></div>
                                <div class="wf-check-result-container clearfix">
                                    <span class="fl"><span
                                            i18n="workflow.memberSelected.MEMBER_SELECTED"></span>:</span>
                                    <ul class="clearfix wf-check-result">
                                    </ul>
                                    <button type="button" id="wf-member-comfirm-btn" class="btn btn-success fr"
                                            i18n="workflow.common.CONFIRM"></button>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--选择人员状态栏模板-->
    <script type="text/html" id="tpl_wf_dialog_added_member">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <li>
        <span class="ellipsis"
              title="<!=member.userfullname +'\n' + (member.useremail && member.useremail!='undefined' ? member.useremail : '')!>"><!=member.userfullname!></span>
            <i class="glyphicon glyphicon-remove"
               i18n="workflow.main.DELETE" data-user-id="<!=member.id!>"></i>
        </li>
        <!}!>
    </script>
    <!--选择人员模板结束-->
    <script type="text/html" id="tpl_wf_added_member">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <div class="people" title="<!=member.userfullname!>" data-user-id="<!=member.id!>">
            <img src="<!=member.userpic!>">

            <div class="people-name ellipsis"><!=member.userfullname!></div>
            <input type="hidden" name="addedUsers[]" value="<!=member.id!>">
        </div>
        <!}!>
    </script>
    <script type="text/html" id="tpl_wf_added_member_personal">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <dl class="pr wf-detail-userInfo">
            <dt class="wf-detail-userPic"><img src="<!=member.userpic!>" class="wf-person-pic"></dt>
            <dd class="pa wf-detail-userName ellipsis"
                title="<!=member.userfullname +'\n' +(member.useremail && member.useremail!='undefined'  ? member.useremail : '')!>">
                <!=member.userfullname!>
            </dd>
            <input type="hidden" name="<!=userListName!>[]" value="<!=member.id!>">
        </dl>
        <!}!>
    </script>
    <script type="text/html" id="tpl_wf_member">
        <!for(var m=0; m < members.length; m++){!>
        <!var member = members[m];!>
        <!if(addUserMap[''+member.id]){!>
        <div class="wf-member wf-member-selected"
             data-user-id="<!=member.id!>"
             title="<!=member.userfullname +'\n'+ (member.useremail && member.useremail!='undefined' ? member.useremail : '')!>">
            <div class="wf-member-avatar">
                <img class="avatar-icon" src="<!=member.userpic!>">
            </div>
            <div class="wf-member-content">
                <p class=" wf-team-avatar-name ellipsis"><!=member.userfullname!></p>
            </div>
            <div class="wf-member-check">
                <span class="glyphicon glyphicon-ok"></span>
            </div>
        </div>
        <!}else{!>
        <div class="wf-member" data-user-id="<!=member.id!>"
             title="<!=member.userfullname +'\n'+ (member.useremail && member.useremail!='undefined' ? member.useremail : '')!>">
            <div class="wf-member-avatar">
                <img class="avatar-icon" src="<!=member.userpic!>">
            </div>
            <div class="wf-member-content">
                <p class=" wf-team-avatar-name ellipsis"><!=member.userfullname!></p>
            </div>
            <div class="wf-member-check">
                <span class="glyphicon glyphicon-ok"></span>
            </div>
        </div>
        <!}!>
        <!}!>
    </script>
</div>