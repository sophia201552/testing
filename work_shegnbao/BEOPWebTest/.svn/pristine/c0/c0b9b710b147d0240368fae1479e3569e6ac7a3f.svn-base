                         var lyItem = $('#dataSrcPanel .treeRow');
                         var arrTemp = [];
                         $.each(lyItem, function(i, n) {
                         var id = n.id;
                         if ('' != id) {
                         arrTemp.push(id);
                         }
                         });
                         var postData = {
                         datasourceId: _this.m_dataSourceId,
                         list: arrTemp
                         };
                         */
                        var postData = {};
                        if (dstGroupId == srcGroupId) {
                            var srcGroup, arrSrc = [];
                            srcGroup = $('#' + srcGroupId).find('.rows').find('.treeRow');
                            $.each(srcGroup, function (i, n) {
                                arrSrc.push(n.id);
                            });
