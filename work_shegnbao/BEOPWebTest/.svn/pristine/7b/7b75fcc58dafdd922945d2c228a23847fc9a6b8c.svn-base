    display: inline-block;
    margin: 5px 1%;
    padding: 4px 15px;
    border-radius: 5px;
    width: 31%;
}

@media (min-width: 1200px) {
    .divDsWorkspace .divDs {
        width: 23%;
        margin-left: 1%;
        margin-right: 1%;
    }
}

.divDsWorkspace .divDs:hover .btnDsDel {
    color: darkorange;
}

.divDsWorkspace .spDs {
    display: inline-block;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 84%;
    white-space: nowrap;
}

.divDsWorkspace .btnDsDel {
    top: 2px;
    color: #ccc;
    float: right;
}

.divDsWorkspace .divDsTip {
    text-align: center;
    color: #ccc;
    background-color: transparent;
    border: 1px dashed #ccc;
    height: 30px;
}

.divDsWorkspace .divDsTip:hover {
    border-color: #66afe9;
    cursor: pointer;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
}

.divDsWorkspace .divDsTip:hover > span {
    visibility: hidden;
}

.divDsWorkspace .divDsTip.dragover {
    border-color: #66afe9;
    cursor: pointer;
    outline: 0;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);
}

.divDsWorkspace .divDsTip.dragover > span {
    visibility: hidden;
}

/*配置框end*/

[data-h5-draggable-node]:hover {
    cursor: pointer;
    color: #428bca;
    transition: 0.1s;
    -webkit-transition: 0.1s;
}

/*设置模态框里选中成员的头像填充在选择框里*/
#wf-detail-form dl {
    width: 42px;
    height: 41px;
    border-radius: 5px;
}

#wf-attachment-input-btn {
    padding: 0;
}

#wf-attachment-input-btn img {
    width: 54px;
    height: 54px;
}

/*诊断反馈*/
#temp_user_comments {
    background-color: rgba(0, 0, 0, .4);
    color: #666666;
}

#temp_user_comments .modal-content {
    opacity: 1;
}

#feedback_prompt_msg {
    position: absolute;
    width: 360px;
    height: 40px;
    line-height: 30px;
    left: 50%;
    top: 50%;
    border-radius: 5px;
    margin-left: -180px;
    margin-top: -20px;
    background-color: #edeeef;
    padding: 5px 10px;
    color: #ffffff;
    font-size: 16px;
    z-index: 1900;
    overflow: hidden;
    display: none;
}

#temp_user_comments .modal-dialog {
    width: 800px;
}

#temp_user_comments .modal-header {
    background-color: #fafafa;
    border-bottom: 1px solid #dedede;
    border-radius: 6px 6px 0 0;
}

#temp_user_comments_form label {
    color: #666;
}

#temp_user_comments_form textarea,
#temp_user_comments_form input,
#temp_user_comments_form span {
    background-color: #fafafa;
    color: #999999;
}

#temp_user_comments_form textarea {
    resize: vertical;
    width: 650px;
}

@media (min-width: 992px) {
    .modal-lg {
        width: 800px;
    }
}