<div id="declareVariablesModalWrap">
    <style type="text/css">
        #declareVariables{
            width: 100%;
        }
        #declareVariables #dataSourceTable th,#declareVariables #dataSourceTable td{
            padding:5px;
        }
        #declareVariables #dataSourceTable .addMinusBtn{
            width: 10%;
            padding: 5px;
            text-align: center;
            font-size: 20px;
            color: #000;
        }
        .gray-scrollbar::-webkit-scrollbar-track {
            -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
            background-color: #F5F5F5;
        }

        .gray-scrollbar::-webkit-scrollbar {
            width: 8px;
            height: 8px;
            background-color: #F5F5F5;
        }

        .gray-scrollbar::-webkit-scrollbar-thumb {
            background-color: rgba(0, 0, 0, 0.3);
        }
    </style>
    <div class="modal" id="declareVariables" style="display:block;background: rgba(0,0,0,0.4);">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span class="glyphicon glyphicon-remove"></span>
                    </button>
                    <h4 class="modal-title" i18n="report.VAR_DECLARATION">变量声明</h4>
                </div>
                <div class="modal-body gray-scrollbar" style="height:260px;">
                    <table id="dataSourceTable">
                        <thead>
                            <tr>
                                <th style="width:20%;"  i18n="report.VAR_DECLARATION_NAME">变量名</th>
                                <th i18n="report.VAR_DECLARATION_VALUE">对应值</th>
                            </tr>
                        </thead>
                        <tbody>
                            
                        </tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default jsCode" i18n="report.VAR_DECLARATION_EDIT_CODE">编辑代码</button>
                    <button type="button" class="btn btn-primary btn-submit" i18n="report.reportConfig.SURE">确定</button>
                </div>
            </div>
        </div>
    </div>
</div>
