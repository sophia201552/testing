<div class="modal fade" id="pageEditModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title"></h4>
            </div>

            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="inputPageName" class="col-sm-2 control-label">Page Name</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="inputPageName" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="selectPageType" class="col-sm-2 control-label">Page Type</label>
                        <div class="col-sm-10">
                            <select class="form-control" id="selectPageType">
                                <option value="PageScreen">Page</option>
                                <option value="EnergyScreen">Dashboard</option>
                                <option value="FacReportScreen">Report</option>
                                <option value="FacReportWrapScreen">ReportWrap</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group" id="divPageSize">
                        <label for="divPageSize" class="col-sm-2 control-label">Page Size</label>
                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="inputPageWidth" placeholder="">
                        </div>
                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="inputPageHeight" placeholder="">
                        </div>
                    </div>
                    <div class="form-group" id="divPageDisplay">
                        <label for="selectPageDisplay" class="col-sm-2 control-label">Display</label>
                        <div class="col-sm-10">
                            <select class="form-control" id="selectPageDisplay">
                                <option value="0">Stretch</option>
                                <option value="1">Center</option>
                            </select>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" id="btnCancel" style="width:65px">Close</button>
                <button type="button" class="btn btn-primary" id="btnOk" style="width:65px">OK</button>
            </div>
        </div>
    </div>
</div>