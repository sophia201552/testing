<style>
    *{
        font-family: "Microsoft YaHei";
    }
    #containerWorkFlowList{
        height: 93%;
        overflow: auto;
        background-color: #fff;
        margin-top: 9%;
    }
    #btnworkflowSelect {
        font-size: 2rem;
        top: 0.6rem;
    }
    #btnWorkFlowSort {
        font-size: 2rem;
        top: 0.6rem;
        right: calc(6% + 2.4rem);
    }
    #btnWorkFlowAdd {
        font-size: 2rem;
        top: 0.63rem;
    }
    .liWkRecord {
        position: relative;
        height: 9.4rem;
        border-bottom:1px solid #ccc;
    }
    .liWkRecord:last-child {
        border-bottom: none;
    }
    .wkEmergency {
        height: 86%;
        position: absolute;
        left: 1rem;
        width: 0.4rem;
        top: 7%;
    }
    .wkEmergency_0{
        background-color: #63b962;
    }
    .wkEmergency_1{
        background-color: #fead21;
    }
    .wkEmergency_2{
        background-color: red;
    }
    .wkTitle {
        position: absolute;
        left: 2.5rem;
        font-size: 1.8rem;
        font-weight: 500;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        width: calc(100% - 3rem);
        top: 1rem;
        color: #000;
    }
    .wkInfo {
        position: absolute;
        width: calc(100% - 4.5rem);
        left: 2.5rem;
        top: 3.5rem;
        height: 3.6rem;
        color: #8E8E93;
        overflow : hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        word-break: break-all;
    }
    .wkUser {
        position: absolute;
        left: 2.5rem;
        bottom: 0.6rem;
        color: #8E8E93;
    }
    .wkDate {
        position: absolute;
        left: 7rem;
        bottom: 0.6rem;
        color: #8E8E93;
    }
    #workflowTool {
        height: 5%;
        width: 100%;
        position: fixed;
        z-index: 3;
    }
    #workflowSelect {
        position: fixed;
        left: 2%;
    }
    #workflowSelect .dropdown-menu, #workflowSort .dropdown-menu{
        min-width: 96px;
        background-color: rgba(53,126,236,0.92);
        color: #fff;
    }
    #workflowSort{
        position: fixed;
        right: 10%;
    }
    #workflowSelect .dropdown-menu li, #workflowSort .dropdown-menu li{
        padding: 1px 12px;
    }
    .btnWkSelect.selected{

    }
    #workflowTool .dropdown .dropdown-toggle{
        color: rgb(53,126,236);
        background-color: transparent;
        border: none;
        font-size: 1.6rem;
        font-weight: 500;
    }
    #workflowTool .dropdown.open .dropdown-toggle{
        background-color: rgba(53,126,236,0.92);
        color: #fff;
    }
    .wkGetStatus {
        position: absolute;
        height: 1rem;
        width: 1rem;
        right: 1rem;
        bottom: 1rem;
        border-radius: 1rem;
    }
    .get_false {
        background-color: red;
    }
    .true {
        background-color: white;
    }

</style>
<div id="workflowTool">
    <div id="workflowSelect" class="dropdown">
        <button class="btn btn-default dropdown-toggle" type="button" id="ulWkSelect" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
            <span id="wkType">进行中</span>
            <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" aria-labelledby="ulWkSelect">
            <li id="btnWorking" class="btnWkSelect selected">进行中</li>
            <li id="btnCreated" class="btnWkSelect">创建</li>
            <li id="btnFinished" class="btnWkSelect">完成</li>
            <li id="btnJoined" class="btnWkSelect">参与</li>
        </ul>
    </div>
    <div id="workflowSort" class="dropdown">
        <button class="btn btn-default dropdown-toggle" type="button" id="ulWkSort" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
            <span id="wkType">排序</span>
            <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" aria-labelledby="ulWkSort">
            <li id="idSort" class="btnSort">项目编号</li>
            <li id="createTimeSort" class="btnSort">创建时间</li>
            <li id="deadlineSort" class="btnSort">截止时间</li>
            <li id="emergencySort" class="btnSort">紧急程度</li>
            <li id="executorSort" class="btnSort">执行人</li>
            <li id="teamSort" class="btnSort">任务组</li>
            <li id="collectSort" class="btnSort">收藏</li>
        </ul>
    </div>
</div>
<div id="containerWorkFlowList">
    <div id="divWorking" class="divWkList"></div>
    <div id="divCreated" class="divWkList"></div>
    <div id="divFinished" class="divWkList"></div>
    <div id="divJoined" class="divWkList"></div>
</div>