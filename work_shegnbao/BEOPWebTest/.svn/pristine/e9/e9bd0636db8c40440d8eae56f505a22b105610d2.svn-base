                        return;
                    }
                    rs.data.list = this.page.layerList;
                    // 
                    this.init();
                    // 
                    try {
                        this.updateModelSet(rs.data);
                    } catch (e) {
                        Log.error('update model set error!');
                    }
                    //  worker
                    this.initSync();
                    //  "" 
                    this.initHistoryController();
                    //  Spinner
                    Spinner.stop();
                }.bind(this));
            }.bind(this)).always(function () {
                this.painter.onLoad();
            }.bind(this));
