            if(diffArr == null){
                return opt;
            }else{
                for(var j=0,diffLen=diffArr.length;j<diffLen;j++){
                    if(diffArr[j].path[0] == "iotStore"){
                        for(var k in modelOpt){
                            if (opt[k] != modelOpt[k]) {
                                if (k === 'trigger') {
                                    opt[k] = {};
                                } else {
                                    opt[k] = '';
                                }
                            }
                        }
                    }else if(diffArr[j].path[0] == "tag"){
                        opt["tag"] = {}
                    }
                }
            }
            

        }
        return opt;
    };

