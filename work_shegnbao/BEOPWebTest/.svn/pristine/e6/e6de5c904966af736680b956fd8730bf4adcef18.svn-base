<style>
	* {
        -webkit-user-select: none;
        user-select: none;
    }
    .userSelect {
        -webkit-user-select: auto;
        user-select: all;
    }
    #outerContainer {
        -webkit-overflow-scrolling: auto;
    }
    #appConfig{
        -webkit-overflow-scrolling: none;
    }    
    #appConfig .list-group{
        margin-top: 1rem;
    }
    #appConfig .listBorder {
        padding: 15px;
        position: relative;
    }
    #appConfig .listBorder > div {
        display: table-cell;
        vertical-align: middle;
        height: 24px;
    }
    .opt-title { width: 10000px; }
    .btn-switch {
        display: table-cell;
        border-radius: 11px;
        background-color: #ccc;
        border: 1px solid #bbb;
        padding-left: 0;
        padding-right: 22px;
    }
    .btn-open {
        display: table-cell;
        padding-right: 22px;
    }
    .btn-switch.on {
        padding-left: 22px;
        padding-right: 0;
        background-color: #337ab7;
        border-color: #2e6da4;
    }
    /*.btn-switch-slider {
        width: 20px;
        height: 20px;
        background-color: #fff;
        border-radius: 50%;
        border-right: 1px solid #ccc;
        box-shadow: 0 1px 1px #999;
        margin-top: 1px;
        margin-left: 1px;
    }*/
    #btnLogout {
        text-align: center;
        font-size: 2.2rem;
        border-radius: 3rem;
        width: 80%;
        padding: 0.5rem;
        position: fixed;
        bottom: 0;
        left: 10%;
        cursor: pointer;
    }
    #btnLogout.down {
        position: static;
        margin-left: 10%;
    }
    .spaceLogout {
        width: 80%;
        height: 4rem;
    }
    #btnLogout button {
        background: #f3b145;
        color: rgb(70, 70, 70);
        border: none;
    }
    #selLang {
        white-space: nowrap;
    }
	#divTipNewVs{
        position: absolute;
        left: 8rem;
        top: 1.5rem;
        color: #ff0000;
        /*animation: updown 1s linear infinite;*/
    }
    /*@keyframes updown{
        form{top:1.3rem}
        to{top:1.54rem}
    }*/
    .divUserPic{
        width: 5rem;
        height: 5rem;
        margin: 1rem auto;
        border: 3px solid rgb(238, 238, 238);
        border-radius: 50%;
    }
    .divUserPic img{
        width: 100%;
        height: 100%;
        border-radius: 50%;
        border: 1px inset rgba(136, 136, 136, 0.33);
    }	
    #appConfig #setPwdBox {
        width: 80%;
        height: auto;
        background: rgba(255,255,255,1);
        position: fixed;
        top: 10%;
        left: 10%;
        z-index: 1;
        padding: 10%;
    }
    #setPwdBox label {
        font-weight: normal;
    }
    #appConfig label {
        color: #333;
    }
    #mSensorSH.hide_sensor .hide_s,#mSensorSH.show_sensor .show_s {
        display: table-cell;
    }
    #mSensorSH.hide_sensor .show_s,#mSensorSH.show_sensor .hide_s {
        display: none;
    }</style>
<div id="appConfig">
