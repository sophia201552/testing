            <div class="opt-title show_s" i18n="admin.navTitle.SHOW_SENSOR">显示传感器</div>
            <!--<div class="opt-btn btn-open">
                <span class="glyphicon glyphicon-chevron-right"></span>
            </div>-->
        </div>
        <!--<div class="list-group-item" id="mBoot">
            <div class="opt-title">开机</div>
