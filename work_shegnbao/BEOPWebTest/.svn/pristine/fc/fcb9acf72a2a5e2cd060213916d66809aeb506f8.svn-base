<style>
    .cfBackground {
        position: absolute;
        width: 100%;
        height: 100%;
        right: 0;
        bottom: 0;
        top: 0;
        left: 0;
        z-index: -1;
    }

    .cfProcessCanvas {
        padding: 20% 40px 0 40px;
    }

    .cfProcessMain {
        position: relative;
        width: 100%;
        height: 80px;
        border: solid 12px #ffffff;
        border-radius: 80px;
        background: -webkit-gradient(linear, left top, right top, from(#2989cc), color-stop(0.05, #2989cc), color-stop(0.5, #569b2c), color-stop(0.95, #eeb13b), to(#eeb13b));
        box-sizing: border-box;
    }

    .cfProcessScale {
        position: absolute;
        top: -20px;
        left: 0;
        right: 0;
        height: 8px;
    }

        .cfProcessScale td {
            position: relative;
            display: inline-block;
            width: 20%;
            float: left;
            height: 8px;
            border-left: 1px solid #eee;
        }

            .cfProcessScale td:last-child {
                border-right: 1px solid #eee;
            }

    .cfProcessScaleNum {
        position: absolute;
        width: 80px;
        top: -16px;
        left: -40px;
        text-align: center;
        font-family: Arial;
        font-size: 12px;
        color: #eee;
    }

    .cfProcessScaleNumLast {
        left: auto;
        right: -40px;
    }

    .cfProcessMask {
        position: relative;
        float: right;
        margin-right: 0;
        border-top-left-radius: 0;
        border-bottom-left-radius: 0;
        border-top-right-radius: 86px;
        border-bottom-right-radius: 86px;
        background-color: #ccc;
        width: 0px;
        height: 100%;
        box-sizing: border-box;
    }

    .cfFootprint {
        position: absolute;
        top: 2px;
        left: -26px;
        border-radius: 40px;
        height: 50px;
        width: 50px;
        background-color: #eee;
        -webkit-box-shadow: 2px 3px 3px #666;
        box-shadow: 2px 3px 3px #666;
    }

    .cfLegendBar {
        margin: 16px 12px 0 12px;
        width: calc(100% - 24px);
        height: 20px;
        background-color: #ccc;
        border-radius: 4px;
        text-align: center;
    }

    .cfLegendBarWarning {
        position: relative;
        float: right;
        width: 0;
        height: 100%;
        background-color: #ff7676;
        border-top-left-radius: 0;
        border-bottom-left-radius: 0;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 4px;
    }

    .cfFootprintFlag {
        position: absolute;
        width: 1px;
        height: 240px;
        top: -110px;
        left: 0;
        background: #eee;
        -webkit-box-shadow: 1px 1px 1px #666;
        box-shadow: 1px 1px 3px #666;
    }

    .cfFootprintFlagEgg {
        position: absolute;
        bottom: 31px;
        left: -6px;
        width: 13px;
        height: 13px;
        background-color: #8cd580;
        border-radius: 13px;
        border: solid 2px #eee;
        box-sizing: border-box;
        -webkit-box-shadow: 1px 2px 3px #666;
        box-shadow: 1px 2px 3px #666;
    }

    .cfFootprintDashboard {
        position: absolute;
        top: 0;
        border: solid 4px #eee;
        box-sizing: border-box;
        -webkit-box-shadow: 2px 3px 3px #666;
        box-shadow: 2px 3px 3px #666;
    }

    .cfFootprintDashboardCurrent {
        padding-top: 12px;
        width: 70px;
        height: 70px;
        border-radius: 70px;
        background-color: #7db773;
        left: -34px;
    }

    .cfFootprintDashboardStandard {
        padding-top: 16px;
        width: 80px;
        height: 80px;
        border-radius: 80px;
        background-color: #eeb13b;
        left: -40px;
    }

    .cfFont {
        color: #eee;
        font-family: Arial;
        font-size: 22px;
        text-align: center;
    }

        .cfFont span {
            display: block;
        }

    .cfSmall {
        font-size: 16px;
    }

    .cfStandardFlag {
        position: absolute;
        width: 1px;
        height: 300px;
        top: -260px;
        left: 0;
        background: #eee;
        -webkit-box-shadow: 1px 1px 1px #666;
        box-shadow: 1px 1px 3px #666;
    }

    .cfFontField {
        font-size: 16px;
        font-weight: bold;
    }

    .divCFTitle {
        display: block;
        color: #eee;
        font-family: 微软雅黑;
        font-size: 22px;
        padding: 30px 0 0 30px;
    }
</style>

