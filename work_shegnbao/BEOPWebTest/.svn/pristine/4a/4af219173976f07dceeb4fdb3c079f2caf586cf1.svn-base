                //        $('.section-wrap').addClass('put-section-0');
                //        sectionOne();
                //    });
                //}
            }
            function down() {
                z--;
                if (z < 0) {
                    location.href = '/observer';
                    //z = $sectionCount.length - 1
                    //$('#indexMain').css('transform', 'translateY(0)');
                    //$('.totalDown').show();
                    // sectionOne();
                    //return;
                };
            }

            /*页面滑动*/
            function run() {
                $wrap.attr("class", "section-wrap").addClass(function () { return "put-section-" + z; }).find('.section').eq(z).find('.title').addClass('active');
                switch (z) {
                    case 0: sectionOne(); sectionRmvTwo(); sectionRmvThr(); sectionRmvFour(); break;
                    case 1: sectionTwo(); sectionRmvOne(); sectionRmvThr(); sectionRmvFour(); break;
                    case 2: sectionThr(); sectionRmvTwo(); sectionRmvOne(); sectionRmvFour(); break;
                        // case 3: sectionFour(); sectionRmvOne(); sectionRmvTwo(); sectionRmvThr(); break;
                        /*case 4:  sectionFour();sectionRmvOne(); sectionRmvTwo(); sectionRmvThr(); break;*/
                    default: sectionRmvOne(); sectionRmvTwo(); sectionRmvThr(); sectionFour(); break;
                        //default: sectionRmvOne(); sectionRmvTwo(); sectionRmvThr(); sectionRmvFour(); break;
                }
            };

            /*翻页按钮点击*/
            function go() {
                up(); run();
                setTimeout(function () { $arrow.one('click', go) }, 1500);
            };

            /*响应鼠标*/
            $wrap.one('mousewheel', mouse_);
            function mouse_(event) {
                if (event.deltaY < 0) { up() }
                else { down() }
                run();
                setTimeout(function () { $wrap.one('mousewheel', mouse_) }, 1500);
            };

            /*响应键盘上下键*/
            $(document).one('keydown', k);
            function k(event) {
                var e = event || window.event;
                var key = e.keyCode || e.which || e.charCode;
                switch (key) {
                    case 38: down(); run();
                        break;
                    case 40: up(); run();
                        break;
                };
                setTimeout(function () { $(document).one('keydown', k) }, 1500);
            }



            //第一张
