    /*标题动画*/
    @-webkit-keyframes changeTitle{
            0% {
                transform: translate3d(-800%,0,0);
                -webkit-transform: translate3d(-800%,0,0);
            }
            100% {
                 transform: translate3d(0,0,0);
                -webkit-transform: translate3d(0,0,0);
            }
        }
    @keyframes changeTitle{
           0% {
                transform: translate3d(-800%,0,0);
                -webkit-transform: translate3d(-800%,0,0);
            }
            100% {
                 transform: translate3d(0,0,0);
                -webkit-transform: translate3d(0,0,0);
            }
        }
    @-webkit-keyframes changeTitle1{
            0% {
                transform: translate3d(800%,0,0);
                -webkit-transform: translate3d(800%,0,0);
            }
            100% {
                 transform: translate3d(0,0,0);
                -webkit-transform: translate3d(0,0,0);
            }
        }
    @keyframes changeTitle1{
             0% {
                transform: translate3d(800%,0,0);
                -webkit-transform: translate3d(800%,0,0);
            }
            100% {
                 transform: translate3d(0,0,0);
                -webkit-transform: translate3d(0,0,0);
            }
        }
    /*dialog动画*/
    @-webkit-keyframes dialogShow {
        0% {
         opacity: 0;
         -webkit-transform: translateY(2000px);
         transform: translateY(2000px);
        }
        100% {
          opacity: 1;
          -webkit-transform: translateY(0);
            transform: translateY(0);  

        }
    }
    @keyframes dialogShow {
        0% {
           opacity: 0;
            -webkit-transform: translateY(2000px);
            transform: translateY(2000px);
        }
        100% {
            opacity: 1;
           -webkit-transform: translateY(0);
            transform: translateY(0);
        }
    }
    /*第三张动画*/
    @keyframes threeTitle {
        0% {
           opacity: 0;
            -webkit-transform: translateY(-3200px);
            transform: translateY(-3200px);
        }
        100% {
            opacity: 1;
           -webkit-transform: translateY(0);
            transform: translateY(0);
        }
    }
    @-webkit-keyframes threeTitle {
        0% {
           opacity: 0;
            -webkit-transform: translateY(-3200px);
            transform: translateY(-3200px);
        }
        100% {
            opacity: 1;
