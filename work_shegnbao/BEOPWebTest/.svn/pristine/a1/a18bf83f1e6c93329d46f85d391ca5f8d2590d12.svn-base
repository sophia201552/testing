        this.propPanel = new PropertyPanel(this);
        this.propPanel.show();
    };

    FactoryScreen.prototype.initPagePanel = function () {
        this.pagePanel = new PagePanel(this);
        this.pagePanel.show();
    };

    $(function () {
        new FactoryScreen().show();
    });
}.call(this);

