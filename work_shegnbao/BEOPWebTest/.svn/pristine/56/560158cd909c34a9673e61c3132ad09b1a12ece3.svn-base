                height: 150px;
                margin: 5px;
                border-bottom: 1px dashed #666;
            }

        .userName {
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            color: #333;
        }

        .userList {
            margin-left: 65px;
        }

        .proUser[checked] {
            background-color: steelblue;
        }

        .proUser {
            float: left;
            width: 100px;
            height: 100px;
            text-align: center;
            margin: 10px;
            padding: 10px 0;
            border-radius: 5px;
            background: #F5F5F5;
            cursor: pointer;
        }

            .proUser:hover {
                background: #E4E4E4;
            }

            .proUser .dn {
                display: none;
            }

            .proUser .closed {
                position: absolute;
                top: 10px;
                right: 10px;
                cursor: pointer;
            }

            .proUser img {
                padding: 2px;
                border: 1px solid #ccc;
                border-radius: 100px;
                background: #fff;
                width: 65px;
                height: 65px;
                margin: 0 auto;
            }
