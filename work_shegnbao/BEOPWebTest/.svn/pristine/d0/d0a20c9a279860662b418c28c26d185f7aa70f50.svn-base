<div class="col-xs-12 vertical-roll">
    <div class="col-xs-offset-1 col-xs-10 workflow-container" id="workflow-team">
        <div class="row filter-row">
            <div class="col-xs-6 col-xs-offset-1">
                <h4 class="filter-title"><span i18n="workflow.team.WORKFLOW_FILTER"></span></h4>

                <div class="filter-body">
                    <div class="filter-result"></div>
                    <div class="filter-dialog">
                    <span class="icon-chevron-down filter-dialog-icon group-filter-dialog-icon"
                          aria-hidden="true"></span>

                        <div class="mini-dialog">
                            <div class="groups">
                                <div>
                                    <h5 class="title"><span i18n="workflow.team.WORKFLOW"></span></h5>
                                </div>
                                <div class="content">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row title-row">
            <div class="col-xs-12">

                <h3><span i18n="workflow.team.DEPARTMENT_STAFF"></span></h3></div>
        </div>
        <div class="row dpt-row workflow-template">
            <div class="col-xs-10 col-xs-offset-1 dpt-container">
                <div class="dpt-nm"></div>
                <div class="row dpt-content-row">
                    <div class="col-xs-2 role">
                        <h4 class="role-nm"></h4>
                    </div>
                    <div class="col-xs-10 memeber-container">
                        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 member">
                            <div class="media">
                                <a class="media-left">
                                    <div class="avatar-icon"
                                         style="background: url(/static/images/login_feature_icons.jpg) no-repeat 0 0;">
                                    </div>
                                </a>

                                <div class="media-body">
                                    <span class="media-heading team-avatar-name"></span>

                                    <div class="operator hidden">
                                        <a class="btn btn-del-member btn-danger btn-sm" i18n="workflow.team.DELETE"></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row dialog-row">
            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="assign-dialog" aria-hidden="true"
                 id="team-assign-dialog">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal"><span
                                    aria-hidden="true">&times;</span><span
                                    class="sr-only">Close</span></button>

                            <h4 class="modal-title" id="assign-dialog"><span
                                    i18n="workflow.team.STAFF_ASSIGNMENT"></span>
                            </h4>
                        </div>
                        <div class="modal-body clearfix members">
                        </div>
                        <div class="modal-footer">

                            <button type="button" class="btn btn-default cancel" data-dismiss="modal"><span
                                    i18n="workflow.team.CANCEL"></span></button>
                            <button type="button" class="btn btn-primary save"><span i18n="workflow.team.SAVE"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/html"  id="tmpl_selectMember">
    <! for(var i=0;i < obj.length; i++){!>
    <div class="col-xs-3">
        <div class="media member-media">
            <div class="media-left">
                <div class="avatar-icon"
                     style="background: url(/static/images/login_feature_icons.jpg) no-repeat 0 0;"></div>
            </div>
            <div class="media-body">
                <h4 class="media-heading team-avatar-name ellipsis" title="<!=obj[i].username!>"><!=obj[i].username!></h4>

                <div class="team-avatar-desc ellipsis" title="<!=obj[i].email!>"><!=obj[i].email!></div>
            </div>
        </div>
    </div>
    <!}!>
</script>