:not(input):not(select):not(textarea) {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
:focus {
    outline: none;
}
#mainframe .panel-titlebar-button-close {
    display: none;
}
.spinnerMask {
    position: absolute;
    z-index: 1000;
    opacity: 0.4;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: #808080;
}
body, html {
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
    -ms-content-zooming: none;
    font-family: 'Microsoft YaHei','Helvetica';
    font-size: 12px;
    /*line-height: 20px;*/
    vertical-align: middle;
    overflow: hidden;
    background-color: #2b3034;
    color: #c7c7c7;
}
.fl {
    float: left;
}
.fr {
    float: right;
}

/* main frame */
#mainframe {
    width: 100%;
    height: 100%;
    font: 12px 'Microsoft YaHei',sans-serif;
    color: #c7c7c7;
    display:none;
}

#mainframe .frame {
    position: relative;
    width: 100%;
    height: 100%;
}

#header, #mainframe {
    background: #384043;
}

/* header */
#header {
    width: 100%;
    height: 40px;
    min-height: 40px;
}
.navbar-brand {
    font-weight: bold;
    height: 40px;
    padding: 10px 15px;
}
.dropdown-menu {
    font: 12px 'Microsoft YaHei';
}
.dropdown-menu .divider {
    height: 2px;
    padding: 0!important;
    margin: 5px 0;
}
#lkName { padding-right: 20px; }

/* panel */
#panelframe {
    width: 100%;
    height: 100%;
    position: relative;
    padding-top: 45px;
    background-color: #2b3034;
}
#panels {
    width: 100%;
    height: 100%;
    position: relative;
}

/* windows */
#windows {
    background: #2a2b2d;
    position: relative;
}
#windows > .window {
    position: absolute;
    left: 36px;
    top: 0;
    bottom: 0;
    right: 0;
}

/* layer panel */
#layerPanel {
    position: relative;
    overflow-y: auto;
    overflow-x: hidden;
}

/* toolbar */
#toolbar {
    width: 36px;
    height: 100%;
    vertical-align: middle;
    padding: 0 4px;
    background: #313639;
    border-top: 1px solid #2b3034;
    border-bottom: 1px solid #2b3034;
}

#toolbar > .tools-ctn {
    display: inline-block;
    width: 36px;
    height: 100%;
    vertical-align: middle;
}

#toolbar > .tools-ctn.option {
    float: right;
}

.tools-ctn button {
    position: relative;
    padding: 0;
    margin: 0 1px;
    width: 28px;
    border: none;
    height: 28px;
    line-height: 28px;
    box-shadow: none;
    background-color: transparent;
    vertical-align: middle;
    font: 13px SourceSansPro,sans-serif;
}
.tools-ctn > button.t-active {
    background-color: #565d5f;
}

.tools-ctn > button > svg {
    fill: #c7c7c7;
    stroke: #c7c7c7;
    vertical-align: middle;
    text-align: center;
}
.tools-ctn > button.t-active > svg {
    fill: #fff;
    stroke: #fff;
}
#toolbar li a.curSelectedScale{
    text-decoration: none;
    color: #fff;
    background-color: #5a6676;
}
.dropdown .caret {
    margin-left: 15px;
}

/* layer start */
.panel-content .glyphicon{
    font-family: 'Glyphicons Halflings' !important;
    color: #eee;
    cursor: pointer;
}
.panel-content .glyphicon.glyphicon-eye-open, .panel-content  .glyphicon.glyphicon-eye-close{
    margin-left: 8px;
    margin-right: 8px;
}
.panel-content .glyphicon.glyphicon-lock{
    /*margin-left: 10px !important;*/
}
