    .tag-list-item.active {
        background-color: #cce8ff;
        border: 1px solid #99D1FF;
    }

    #tagsExplorer .tag-list-item.dropTarget {
        border: 1px solid #cce8ff;
    }

    .breadcrumb a {
        color: black !important;
    }

    .breadcrumb {
        padding-top: 12px;
    }

    #tag_search {
        -webkit-user-select: none;
    }

    #active_box {
        background: #95c1ed;
        z-index: 999;
        opacity: 0.5;
    }

    #drag_box {
        z-index: 1000;
    }

    #drag_box span {
        display: inline-block;
        float: left;
    }

    #drag_box .tag-list-item-name {
        color: black;
        min-height: 5px;
    }

    #drag_box .tag-list-item-path {
        display: none;
    }

    #drag_box .tag-list-item-type {
        display: none;
    }

    #mark_box {
        position: absolute;
        width: 120px;
        clear: both;
    }

    .btnGroup button {
        margin-left: 10px;
    }

    .btn-default .sub .glyphicon {
        margin-right: 5px;
    }

    #paneIotAdd {
        display: none;
    }

    #tagsExplorer {
        height: 100%;
        color: #333333;
        background: #F0F0F0;
        user-drag: none;
        user-select: none;
        -moz-user-select: none;
        -webkit-user-drag: none;
        -webkit-user-select: none;
        -ms-user-select: none;
    }

    #container-left {
        width: 80%;
        height: inherit;
        float: left;
    }

    #container-right {
        width: 20%;
        height: inherit;
        float: left;
    }

