        display: block;
        font-weight: bold;
        color: #333;
    }

    #auto-identify-table .data-note {
        display: block;
        color: #abb8bf;
    }

    #tagsExplorer .label {
        color: #333333;
    }

    .contextMenu {
        position: absolute;
        display: none;
        -webkit-user-select: none;
    }

    #tagsExplorer .breadcrumb > .active {
        color: #777 !important;
    }

    .multiple-item .menu-rename {
        display: none;
    }

    .multiple-item .menu-auto-identify {
        display: none;
    }

    .hide_mark_as .menu-mark-as {
        display: none;
    }

    .hide_mark_as .divider {
        display: none;
    }

    .has_group_file .menu-move-outermost-layer {
        display: none;
    }

    .not_group_file .menu-copy-file {
        display: none;
    }

    .not_group_file .menu-delete {
        display: none;
    }

    .not_group_file .menu-rename {
        display: none;
    }

    .hide-auto-identify .menu-auto-identify {
        display: none;
    }

    .tags-left-header span {
        display: inline-block;
        float: left;
        padding: 14px 10px;
        cursor: pointer;
    }

    #tagsExplorer #paneIotData li .iconfont {
        background-image: none;
        top: 7px;
        margin-right: 3px;
    }

    .tags-left-header .disable {
        color: #ccc;
    }

    #contextMenu-item li a {
        padding: 3px 5px 3px 20px;
    }

    #contextMenu-item li {
        width: 100%;
        position: relative;
    }

    .glyphicon-chevron-right {
        font-size: 12px;
        display: inline-block;
        float: right;
        margin-right: 10px;
        padding-top: 3px;
    }

    .glyphicon-th {
        margin-top: 3px;
        cursor: pointer;
        padding: 6px 10px;
    }

    .glyphicon-th-list {
        margin-top: 3px;
        cursor: pointer;
        padding: 6px 10px;
    }

    #tag-search-box .btn-default:hover {
        background-color: #fff;
        border-color: #ccc;
    }

    #tag-search-box .btn-default:focus {
        background-color: #fff;
        border-color: #ccc;
    }

    #tag-search-box .btn-primary:hover {
        background-color: #337ab7;
        border-color: #2e6da4;
    }

    #tag-search-box .btn-primary:focus {
        background-color: #337ab7;
        border-color: #2e6da4;
    }

    #tagsExplorer .orderList .tag-list-item .tag-list-icon {
        display: block;
        float: left;
        font-size: 15px;
    }

    #tagsExplorer .orderList .tag-list-item .tag-list-item-name {
        display: block;
        float: left;
        font-size: 13px;
        margin-left: 5px;
        min-height: 0;
    }

    #tagsExplorer .tag-list-header {
        display: none;
    }

    #tagsExplorer .orderList .tag-list-header {
        display: block;
        clear: both;
        width: 100%;
        padding: 3px 5px;
        margin: 20px 0 0 0;
        font-size: 11px;
        float: left;
    }

    #tagsExplorer .orderList .tag-list-header span:not(:nth-child(1)) {
        display: inline-block;
        width: calc(33% - 6px);
        text-align: left;
        white-space: nowrap;
        text-overflow: ellipsis;
        float: left;
    }

    #tagsExplorer .orderList .tag-list-item {
        display: block;
        clear: both;
        width: 100%;
        padding: 3px 5px;
        margin-left: 0;
        font-size: 15px;
    }

    #tagsExplorer .tag-list-item > span:nth-child(3) {
        display: none;
    }

    #tagsExplorer .tag-list-item > span:nth-child(4) {
        display: none;
    }

    #tagsExplorer .orderList .tag-list-item > span:not(:nth-child(1)) {
        display: inline-block;
        width: calc(33% - 6px);
        text-align: left;
        white-space: nowrap;
        text-overflow: ellipsis;
        float: left;
        height: 16px;
        font-size: 13px;
        line-height: 13px;
    }

    .detailed_information {
        display: none;
        position: fixed;
        top: 169px;
        left: 72%;
    }

    #tagsExplorer .orderList .main_contents {
        width: 60%;
    }

    #tagsExplorer .orderList .detailed_information {
        display: block;
    }

    .attrContent:hover {
        background-color: #448aca;
    }

    .detailed_content {
        padding: 10px;
    }

    .detailed_information_pathPic {
        display: inline-block;
        margin: 5px 0 15px;
        padding: 30px;
        border: 1px solid #ddd;
    }


</style>

<div id="tagsExplorer">
    <div class="clearfix tags-header">
        <div class="btnGroup">
            <div class="dropdown" style="float: left">
                <button class="btn btn-default dropdown-toggle" type="button" id="tag-btn-file" data-toggle="dropdown">
                    File
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                    <li><a id="importTags"><span class="glyphicon glyphicon-import"
                                                 aria-hidden="true"></span>Import</a>
                        <input type="file" accept=".csv,.xls,.xlsx" id="iptPjData" multiple="multiple"
                               style="display:none;">
                    </li>
                    <li><a href="#"><span class="glyphicon glyphicon-export" aria-hidden="true"></span>Export</a></li>
                    <li><a id="syncPointToTree"><span class="glyphicon glyphicon-transfer"></span><span
                            i18n="dataManage.SYNCHRONIZE"></span></a>
                    </li>
                </ul>
            </div>
            <a tabindex="-1" class="menu-new-tag btn btn-default" i18n="debugTools.sitePoint.NEW_POINT" style="margin-left: 5px;"></a>
            <a tabindex="-1" class="menu-delete btn btn-default" i18n="common.DELETE"></a>
        </div>
    </div>
    <div class="tag-content clearfix">
        <div class="col-md-2 tagsLeftContainer">
            <div class="tags-left-header">
                <span class="glyphicon glyphicon-arrow-left last-path disable" title="后退"></span>
                <span class="glyphicon glyphicon-arrow-right next-path disable" title="前进"></span>
                <span class="glyphicon glyphicon-arrow-up" title="上一级"></span>
                <span class="glyphicon" id="tagTreeShowAll" i18n="dataManage.ALL"></span>
            </div>
            <div class="tags-left-content">
                <div class="tb-wrap">
                    <ul id="projectDataTree" class="ztree"></ul>
                </div>
            </div>
        </div>
        <div class="col-md-10 tagsRightContainer">
            <div class="tags-right-header clearfix">
                <div id="tags-path" class="col-md-6">
                    <ol class="breadcrumb">
                    </ol>
                </div>
                <div id="tag-search-box" class="col-md-6">
                    <div class="pull-right">
                        <div class="btn-group" role="group">
                            <button class="btn btn-primary glyphicon glyphicon-th"></button>
                            <button class="btn btn-default glyphicon glyphicon-th-list"></button>
                        </div>
                    </div>
                    <div class="col-md-8 pull-right" style="margin-top: 5px;">
                        <div class="input-group pull-right">
                            <input type="text" id="tag_search" class="form-control mr10 fl">
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-search"></span>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tags-right-content">
                <div class="main_contents">
                    <div style="padding-left: 40px">
                        <li class="tag-list-header">
                            <span style="display:block;float:left;height:18px;width:15px"></span>
                            <span style="margin-left:5px" i18n="common.FILE_NAME"></span>
                            <span i18n="common.FILE_PATH"></span>
                            <span i18n="common.FILE_TYPE"></span>
                        </li>
                    </div>
                    <ul class="clearfix tag-list"></ul>
                </div>
                <div class="detailed_information"></div>
            </div>
            <div class="fileNumber"></div>
        </div>
    </div>
</div>

<div id="contextMenu-item" class="dropdown clearfix contextMenu">
    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu"
        style="display:block;position:static;margin-bottom:5px;">
        <li><a tabindex="-1" class="menu-delete" i18n="common.DELETE"></a></li>
        <li><a tabindex="-1" class="menu-rename" i18n="common.RENAME">重命名</a></li>
        <li><a tabindex="-1" class="menu-auto-identify" i18n="common.">识别</a></li>
        <li><a tabindex="-1" class="menu-copy-file" i18n="common.COPY"></a></li>
        <li><a tabindex="-1" class="menu-move-outermost-layer" i18n="common.MOVE_OUTERMOST_LAYER"></a></li>
        <li class="divider"></li>
        <li><a tabindex="-1" class="menu-mark-as"><span i18n="common.TAGS_AS"></span><span class="glyphicon glyphicon-chevron-right markAs"></span></a>
        </li>
    </ul>
</div>

<div id="contextMenu-panel" class="dropdown clearfix contextMenu">
    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu"
        style="display:block;position:static;margin-bottom:5px;">
        <li><a tabindex="-1" class="menu-new-tag" i18n="debugTools.sitePoint.NEW_POINT"></a></li>
        <li><a tenindex="-1" class="menu-paste" style="display:none" i18n="common.PASTE"></a></li>
    </ul>
</div>
