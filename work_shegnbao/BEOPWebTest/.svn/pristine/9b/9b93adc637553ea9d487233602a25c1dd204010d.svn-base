<script type="text/html" id="tpl_tags_path">
    <li class="tag-path"><a href="javascript:void(0);"></a></li>
    <!if(paths && paths.length){!>
        <!for(var i = 0,iLen = paths.length;i < iLen-1;i++){ !>
            <!var path = paths[i];!>
            <li data-tid="<!=paths[i].tId!>" class="tag-path"><a href="#"><!=paths[i].name!></a></li>
        <!}!>
        <li class="active"><!=paths[iLen-1].name!></li>
    <!}!>
</script>
<script type="text/html" id="tpl_tags_fileNumber">
     <!var num = 0;!>
