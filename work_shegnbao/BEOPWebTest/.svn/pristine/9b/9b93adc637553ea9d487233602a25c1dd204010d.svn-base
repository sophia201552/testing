<script type="text/html" id="tpl_auto_identify_list">
    <div style="padding:20px 10px 10px 20px">
        <div>
            <button style="padding: 3px 8px" class="btn btn-default" id="beginToIdentify">开始识别</button>
            <button style="padding: 3px 8px" class="btn btn-default" title="返回" id="backToTags">返回</button>
        </div>
        <table style="width:100%;height: calc(100% - 50px);margin-top: 10px" id="auto-identify-table">
            <tbody>
            <!for(var i = 0,iLen = matchPoints.length;i < iLen;i++){!>
                <tr draggable="true" class="identifyItem" data-id="<!=matchPoints[i]._id!>">
