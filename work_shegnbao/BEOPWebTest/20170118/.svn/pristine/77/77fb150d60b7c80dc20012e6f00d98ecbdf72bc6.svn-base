<style>
    .historyChartBigModel {
        width: 80%;
    }

    #tableOperatingRecord {
        margin: 0 auto;

    }

    #tableOperatingRecord.historyTable {
        background: #fff;
    }

    .historyChartBigModel .modal-header {
        padding: 8px 15px;
        overflow: hidden;
        border-bottom: none;
    }

    #curveSelectBox {
        height: 30px;
        line-height: 30px;
    }

    .historyDataTable {
        height: 90vh;
        overflow-x: hidden;
        overflow-y: auto;
    }

    .modal-content.dark {
        color: #ffffff;
    }

    .modal-content.default {
        color: black;
    }

    .disable {
        color: #ccc !important;
    }

</style>
<div class="modal-content" style="height:800px;">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" style="opacity: 0.3;">
            <span aria-hidden="true">&times;</span>
            <span class="sr-only">Close</span>
        </button>
        <h4 class="modal-title" id="myModalLabel" style="height: 15px;"></h4>
    </div>
    <div class="modal-body">
        <div id="curveSelectBox" class="mb20 clearfix">
            <div class="fr">
                <span class="fl mr10" i18n="common.TIME_RANGE"></span>
                <input class="form-control mr5 fl w170" size="16" type="text" value="" id="curveDateStart"
                       style="border-radius:5px 0 0 5px;" i18n="placeholder=common.SELECT_START_TIME">
                <span class="mr5 fl" i18n="common.TO"></span>
                <input class="form-control mr30 fl w170" size="16" type="text" value="" id="curveDateEnd"
                       style="border-radius:0 5px 5px 0;" i18n="placeholder=common.SELECT_CLOSING_TIME">
                <span class="mr10 fl" i18n="common.TIME_INTERVAL"></span>
                <select id="intervalType" class="mr10 form-control w150 fl">
                    <option value="m1">1min</option>
                    <option value="m5">5min</option>
                    <option value="h1">1h</option>
                    <option value="d1">1d</option>
                </select>

                <div class="btn-group" role="group">
                    <button type="button" class="btn btn-default searchCurveOfTime" dateType="hour"
                            i18n="common.HOUR"></button>
                    <button type="button" class="btn btn-default searchCurveOfTime" dateType="today"
                            i18n="common.TODAY"></button>
                    <button type="button" class="btn btn-default searchCurveOfTime" dateType="week"
                            i18n="common.THIS_WEEK"></button>
                    <button type="button" class="btn btn-default searchCurveOfTime" dateType="month"
                            i18n="common.THIS_MONTH"></button>
                </div>
                <button type="button" class="btn btn-default mr10" id="repairData" data-toggle="tooltip"
                        i18n="dataManage.GENERATE_DATA"></button>
                <button type="button" class="btn btn-success mr10" id="filterCurveConfirm"
                        i18n="common.CONFIRM"></button>
            </div>
        </div>
        <div id="hcPointTreeWrapper" class="oh">
            <div id="hcPointTreeBox" class="fl gray-scrollbar"
                 style="width: 16%; height: 600px; overflow-x: hidden; overflow-y: auto;">
                <ul id="hcPointTree" class="ul ztree"></ul>
            </div>
            <div id="tableOperatingRecord" class="tableOperatingRecord fr"
                 style="height:680px; width:80%;"></div>
        </div>
    </div>
</div>