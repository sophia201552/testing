<style type="text/css">
.bodyContain {
    margin: 5px 50px 50px 50px;
}
.checkboxShow {
    width: 33%;
    font-weight: 400;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
</style>

<div class="modal">
    <div class="modal-dialog modal-lg" style="margin-top: 100px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" i18n="dashboard.modalJumpPages.IMPORT_PAGE">Import Page</h4>
            </div>

            <div class="modal-body bodyContain">

                <div class="row">
                    <form>
                        <div class="form-group">
                            <label for="selectGroup" i18n="dashboard.modalJumpPages.GROUP_LIST">Group List</label>
                            <select id="selectGroup" class="form-control"></select>
                        </div>
                        <input type="text" class="form-control" id="inputNewGroup" placeholder="New group name" style="display:none;">
                    </form>
                </div>

                <div class="row" style="margin-top: 20px;">
                    <label i18n="dashboard.modalJumpPages.POINT_LIST">Point List</label>
                    <button class="btn btn-default" id="btnSelAll" type="button" i18n="dashboard.modalJumpPages.SELECT_ALL">All</button>
                    <button class="btn btn-default" id="btnSelRevert" type="button" i18n="dashboard.modalJumpPages.SELECT_REVERT">Revert</button>
                </div>
                <div class="row" style="margin-top: 20px;">
                    <div id="ptList" style="height: calc(100%);overflow-y: auto;"></div>
                </div>

                <div class="row" style="margin-top: 20px;position: relative;float: right;">
                    <button class="btn btn-default" type="button"  id="btnImportNoJump" style="width:120px;" i18n="dashboard.modalJumpPages.CANCEL">Sure</button>
                    <button class="btn btn-primary" type="button"  id="btnImportJump" style="width:120px;" i18n="dashboard.modalJumpPages.SURE">Sure and Jump</button>
                </div>

            </div>
        </div>
    </div>
</div>