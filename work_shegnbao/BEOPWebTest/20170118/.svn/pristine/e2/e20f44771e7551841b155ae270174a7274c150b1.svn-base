        width: 420px;
        vertical-align: bottom;
    }
    #TimeQuery .timeText {
        display: block;
        float: left;
        width: 30px;
        height: 34px;
        text-align: center;
        line-height: 34px;
    }
    .timeStyle {
        display: block;
        float: left;
        width: 110px;
        height: 34px;
