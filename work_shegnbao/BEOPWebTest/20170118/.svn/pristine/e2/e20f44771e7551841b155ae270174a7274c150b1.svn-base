    }
    .commentItem .imgAsset{
        max-width: 100px;
        height: 40px;
        width: auto;
    }
    .commentItem .oprtInfo{
        margin-right: 20px;
    }
    #btnImgGallery{
        position: absolute;
        right: 16px;
