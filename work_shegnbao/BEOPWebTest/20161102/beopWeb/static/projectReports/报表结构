报表结构:

[{name:'',//章节名称
	units:[{//章节图例
		unitName:'',小节名
		subUnits:[{
			summary:'',//图例描述
			title:'',//图例名称
			type:'',//图例类型:"line","pie","bar","scatter","table"
			options:'',//图例配置: 如'{x:x轴名称,y:[y轴名称1, y轴名称2 ...]}',
			commonChartOptions:{},//图例配置,配置显示
			theme:'',图例主题
			chartItems:{//图例中每一条线
				x:[],
				y:[{yAxisIndex:0, name:key, value:[], type:'', stack:'1', otherOptions:{}},{yAxisIndex:1, name:key, value:[], type:'', stack:'2', otherOptions:{}}]
			}
		}]
}]}]